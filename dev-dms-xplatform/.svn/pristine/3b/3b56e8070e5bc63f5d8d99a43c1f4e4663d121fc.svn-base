/*----------------------------------------------------------------------------------------------
 * 설명   : 전표생성
 * param   : obj - Button
 * param   : e  - ClickEventInfo
 * return  : void
-----------------------------------------------------------------------------------------------*/
function btn_slipI_onclick(obj:Button,  e:ClickEventInfo)
{
	var msg = "렌탈매출확정 전표현황의 전표를 생성하시겠습니까?";
	
	var yd = ds_search.getColumn(0,"YDATE");
	
		if(ds_list00.findRow("CHK","1") == -1){
			//체크대상없음
			gfn_message("A","DMS00020","I"); // 선택된 정보가 없습니다.
			return;
		}
	  
		for(i=0; i<ds_list00.getRowCount(); i++){
			if(ds_list00.getColumn(i,"CHK")=='1')
			{
			  ds_list00.setColumn(i,"YYYYMM",yd);
			
				if(ds_list00.getColumn(i,"SLIP_ST_CD") == '10' || ds_list00.getColumn(i,"SLIP_ST_CD") == '20' || ds_list00.getColumn(i,"SLIP_ST_CD") == '30' )
				{
					gfn_message("A","DMS00087","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
					return false;
				}
				 var pRow = ds_listtemp.addRow();
				 ds_listtemp.copyRow(pRow,ds_list00,i);

				if(gfn_message("A",msg,"I","","")){
					if(ds_listtemp.getColumn(i,"SALE_DEALCO_FISCL_ST_CD") == "S"){
							fn_saveSlip();
					}else{
						gfn_message("A","DMS00087","E","ERROR" ,"전표발행 불가능한 거래처입니다. 해당"); //{해당} 건은 처리가 불가능 합니다.
					}
				}else{
					return false;
			}
		}
	}
				
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 전표 삭제
 * param   : obj - Button
 * param   : e  - ClickEventInfo
 * return  : void
-----------------------------------------------------------------------------------------------*/
function btn_slipC_onclick(obj:Button,  e:ClickEventInfo)
{
	var msg = "해당전표를 삭제하시겠습니까?"; 
	if(ds_list00.findRow("CHK","1") == -1){
		//체크대상없음
		gfn_message("A","DMS00020","I"); // 선택된 정보가 없습니다.
		return;
	}
	
	var yn =  div_search.cal_Ydate.getDate();
	var year = yn.substring(0,4);
	ds_list00.setColumn(ds_list00.rowposition,"YYYY",year);
	
	for(i=0; i<ds_list00.getRowCount(); i++)
	{
		if(ds_list00.getColumn(i,"CHK")=='1')
		{
			if(ds_list00.getColumn(i,"SLIP_ST_CD") == '10' )
			{
				if(ds_list00.setColumn(i,"YYYY",year) != null )
				{
								
					var pRow = ds_listtemp.addRow();
					ds_listtemp.copyRow(pRow,ds_list00,i);
				
					if(gfn_message("A",msg,"I","",""))
					{
						fn_deleteSlip();
					}
				}
				return false;
		    } else {
				 gfn_message("A","DMS00087","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
				return false;
		    }  
		}
	}
}

