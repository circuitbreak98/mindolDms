/*===============================================================================================
 * [ PART 5 ] Transaction 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	var sSvcID		 = "searchMain";
	var sTrID		 = "PNREqpPolyMgmt_pInqEqpPolyLst";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sOutputDs	 = "ds_main=RS_EQP_POLY_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";

	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인저장 (insert, update)
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_saveMain()
{	
	var sSvcID		 = "saveMain";
	var sTrID		 = "PNREqpPolyMgmt_pSaveEqpPoly";
	var sPtrn		 = ds_main.getRowType(ds_main.rowposition) == Dataset.ROWTYPE_INSERT? "I" : "U";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RS_EQP_POLY_LIST=ds_main:u";
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	pv_sPtrn         = sPtrn;  // 저장Type을 전역변수로 저장
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
 	switch(sSvcID) {
		case "searchMain" : 
				
			cgp_pageNo.setPageNo(page_size, total_record_cnt, page);	//PageNo
			cgp_pageNoCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
			gfn_setGridTitleCountPaging(stc_subTitle01, total_record_cnt);
			gfn_message("B","DMS00001");	//MSGI0003 :정상 조회되었습니다.
			
			div_search.edt_eqpMdlCd.setFocus();
			break;
			
		case "saveMain" : 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//MSGI0001 : 정상 처리되었습니다.
				if (pv_sPtrn == "I") {		// 신규등록일 경우에만 재조회
					ds_search.setColumn(0,"EQP_MDL_CD", ds_main.getColumn(ds_main.rowposition,"EQP_MDL_CD"));
					ds_search.setColumn(0,"EQP_MDL_NM", ds_main.getColumn(ds_main.rowposition,"EQP_MDL_NM"));
					ds_search.setColumn(0,"POLY_NM", ds_main.getColumn(ds_main.rowposition,"POLY_NM"));
					div_search.CommCalendarPred00.setData( ds_main.getColumn(ds_main.rowposition,"RP_EFF_PRD_FR_DT") , ds_main.getColumn(ds_main.rowposition,"RP_EFF_PRD_TO_DT"));
					
					btn_search_onclick();
				 }else if (pv_sPtrn == "U") {
					btn_search_onclick();
				   }
			  }
			break;
			
		default :
			break;
	}
}

