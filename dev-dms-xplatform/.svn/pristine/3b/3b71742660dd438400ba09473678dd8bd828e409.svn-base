	if ( gfn_isNull(sRtnVal) || sRtnVal == "cancel" ) {
		//취소시 값 제거
		div_detail.edt_eqpMdlCd.value  = ""; //단말코드
		div_detail.edt_eqpMdlNm.value  = ""; //단말명
		div_detail.cbo_opClCd.value = "";      //업무구분
		div_detail.cbo_capaCd.value = "";      //저장용량코드  
	
	} else {	//Object이용시
		div_detail.edt_eqpMdlCd.value  = sRtnVal["supEqpMdlCd"]; //단말코드
		div_detail.edt_eqpMdlNm.value  = sRtnVal["supEqpMdlNm"]; //단말명
		div_detail.cbo_opClCd.value = sRtnVal["supOpClCd"];      //업무구분
		div_detail.cbo_capaCd.value = sRtnVal["supCapaCd"];      //용량
		
		}
}
