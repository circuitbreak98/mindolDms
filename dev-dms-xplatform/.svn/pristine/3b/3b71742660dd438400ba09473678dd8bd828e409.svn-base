/*----------------------------------------------------------------------------------------------
 * 설명   : search
-----------------------------------------------------------------------------------------------*/
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	
	
	div_detail.enable=true;
	div_detail.btn_eqpMdlNmSearch.enable=false;
	div_detail.btn_reset.enable=false;
		
	var arrTerm = div_search.CommCalendarPred00.getData();
	 ds_search.setColumn(0,"RP_EFF_PRD_FR_DT",arrTerm[0]);
	 ds_search.setColumn(0,"RP_EFF_PRD_TO_DT",arrTerm[1]);
	
	// 입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	
	
	ds_main.clearData();
	page = 1;
	fn_searchMain();
	
	
}

/*----------------------------------------------------------------------------------------------
 * 설명   : reset
-----------------------------------------------------------------------------------------------*/
function btn_reset_onclick(obj:Button,  e:ClickEventInfo)
{
	if ( ds_main.rowposition != -1 && ds_main.getRowType(ds_main.rowposition) != Dataset.ROWTYPE_NORMAL) {	
		ds_main.reset();
		var row = ds_main.addRow();
		ds_main.rowposition = row;
		
		div_detail.cal_effPrdFrDt.enable=true;
		div_detail.cal_effPrdFrDt.value = gfn_addDate(gfn_today(), 1);
		div_detail.cal_EffPrdToDt.value='99991231';
		div_detail.cal_EffPrdToDt.enable=false;	
		ds_main.setColumn(ds_main.rowposition,"CNTRT_PRD",'');

		div_detail.edt_PolyNm.setFocus();
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명   : new
-----------------------------------------------------------------------------------------------*/
function btn_new_onclick(obj:Button,  e:ClickEventInfo)
{
 	div_detail.enable=true;
 	div_detail.btn_eqpMdlNmSearch.enable=true;
 	div_detail.btn_reset.enable=true;
 	 	
 		
	if ( ds_main.rowposition != -1 && ds_main.getRowType(ds_main.rowposition) != Dataset.ROWTYPE_NORMAL) {	
		if (gfn_message("C","DMS00024","Q")){ //편집중인 데이터가 있습니다. 계속하시겠습니까?
			ds_main.reset();
			var row = ds_main.addRow();
			ds_main.rowposition = row;
			
			div_detail.cal_effPrdFrDt.enable=true;
			div_detail.cal_effPrdFrDt.value = gfn_addDate(gfn_today(), 1);
			div_detail.cal_EffPrdToDt.value='99991231';
			div_detail.cal_EffPrdToDt.enable=false;
			ds_main.setColumn(ds_main.rowposition,"CNTRT_PRD",'');
						
			div_detail.edt_PolyNm.setFocus();			
		}
	} else {
		var row = ds_main.addRow();
			ds_main.rowposition = row;
			
			div_detail.cal_effPrdFrDt.enable=true;
			div_detail.cal_effPrdFrDt.value = gfn_addDate(gfn_today(), 1);
			div_detail.cal_EffPrdToDt.value='99991231';
			div_detail.cal_EffPrdToDt.enable=false;	
			ds_main.setColumn(ds_main.rowposition,"CNTRT_PRD",'');
				
			div_detail.edt_PolyNm.setFocus();	
	}
}

