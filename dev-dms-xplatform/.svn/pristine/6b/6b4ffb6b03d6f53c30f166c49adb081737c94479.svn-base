function fn_provideSub()
{	
	ds_sub.clearData();
	if(Radio00.value != "") {
		ds_main.setColumn(ds_main.rowposition,'APPLF_TYP',Radio00.value);
	}
	if(Radio01.value != "") {
		ds_main.setColumn(ds_main.rowposition,'APPLF_TYP',Radio01.value);
	}
	if(Radio02.value != "") {
		ds_main.setColumn(ds_main.rowposition,'APPLF_TYP',Radio02.value);
	}
	if(Radio03.value != "") {
		ds_main.setColumn(ds_main.rowposition,'APPLF_TYP',Radio03.value);
	}
	if(Radio04.value != "") {
		ds_main.setColumn(ds_main.rowposition,'APPLF_TYP',Radio04.value);
	}
	if( chk_DMS253_01.value == "Y" || 
		chk_DMS253_02.value == "Y" || 
		chk_DMS253_03.value == "Y" || 
		chk_DMS253_04.value == "Y" || 
		chk_DMS253_05.value == "Y" || 
		chk_DMS254_01.value == "Y" || 
		chk_DMS254_02.value == "Y" || 
		chk_DMS254_03.value == "Y" || 
		chk_DMS254_04.value == "Y" || 
		chk_DMS255_01.value == "Y" || 
		chk_DMS255_02.value == "Y" || 
		chk_DMS255_03.value == "Y" || 
		chk_DMS255_04.value == "Y" || 
		chk_DMS255_05.value == "Y" || 
		chk_DMS256_01.value == "Y" || 
		chk_DMS256_02.value == "Y" || 
		chk_DMS256_03.value == "Y" || 
		chk_DMS256_04.value == "Y" || 
		chk_1C.value == "Y" || 
		chk_1D.value == "Y" || 
		chk_2C.value == "Y" || 
		chk_2D.value == "Y" || 
		chk_3A.value == "Y" || 
		chk_3B.value == "Y" || 
		chk_3C.value == "Y" || 
		chk_3D.value == "Y")
	{
		ds_main.setColumn(ds_main.rowposition,'APPLF_IMG_ERR_YN','Y');
	}
	else
	{
		ds_main.setColumn(ds_main.rowposition,'APPLF_IMG_ERR_YN','N');
	}
	
	for(var i=0; i < 18; i++){
		var j = i+1;
		var k = "DMS253_0"+j;
		var l = "1A";
		if(i >= 5 && i <= 8) {
			j = i - 4;
			k = "DMS254_0"+j;
			l = "1B";
		}
		if(i >= 9 && i <= 13) {
			j = i - 8;
			k = "DMS255_0"+j;
			l = "2A";
		}
		if(i >= 14) {
			j = i - 13;
			k = "DMS256_0"+j;
			l = "2B";
		}
		if (ds_main.getColumn(ds_main.rowposition,k) == "Y") {
			var row = ds_sub.addRow();
			ds_sub.rowposition = row;
			ds_sub.setColumn(ds_sub.rowposition,'APPLF_TYP', l);
			ds_sub.setColumn(ds_sub.rowposition,'CNTRT_NO', ds_main.getColumn(ds_main.rowposition,"CNTRT_NO"));
			ds_sub.setColumn(ds_sub.rowposition,'APPLF_ITM_CD', '0'+j);
			ds_sub.setColumn(ds_sub.rowposition,'APPLF_IMG_ERR_YN', ds_main.getColumn(ds_main.rowposition,k));
		}
	}
}

/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
 	switch(sSvcID) {
		case "searchSub":
			if ( iErrorCode == 0 ) {
				//gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				Radio00.value = ds_main.getColumn(ds_main.rowposition,"APPLF_TYP");
				Radio01.value = ds_main.getColumn(ds_main.rowposition,"APPLF_TYP");
				Radio02.value = ds_main.getColumn(ds_main.rowposition,"APPLF_TYP");
				Radio03.value = ds_main.getColumn(ds_main.rowposition,"APPLF_TYP");
				Radio04.value = ds_main.getColumn(ds_main.rowposition,"APPLF_TYP");
				if(ds_main.getColumn(ds_main.rowposition,"APPLF_TYP") == "1C" && ds_main.getColumn(ds_main.rowposition,"APPLF_IMG_ERR_YN") == "Y"){
					chk_1C.value = "Y";
				}
				if(ds_main.getColumn(ds_main.rowposition,"APPLF_TYP") == "1D" && ds_main.getColumn(ds_main.rowposition,"APPLF_IMG_ERR_YN") == "Y"){
					chk_1D.value = "Y";
				}
				if(ds_main.getColumn(ds_main.rowposition,"APPLF_TYP") == "2C" && ds_main.getColumn(ds_main.rowposition,"APPLF_IMG_ERR_YN") == "Y"){
					chk_2C.value = "Y";
				}
				if(ds_main.getColumn(ds_main.rowposition,"APPLF_TYP") == "2D" && ds_main.getColumn(ds_main.rowposition,"APPLF_IMG_ERR_YN") == "Y"){
					chk_2D.value = "Y";
				}
				if(ds_main.getColumn(ds_main.rowposition,"APPLF_TYP") == "3A" && ds_main.getColumn(ds_main.rowposition,"APPLF_IMG_ERR_YN") == "Y"){
					chk_3A.value = "Y";
				}
				if(ds_main.getColumn(ds_main.rowposition,"APPLF_TYP") == "3B" && ds_main.getColumn(ds_main.rowposition,"APPLF_IMG_ERR_YN") == "Y"){
					chk_3B.value = "Y";
				}
				if(ds_main.getColumn(ds_main.rowposition,"APPLF_TYP") == "3C" && ds_main.getColumn(ds_main.rowposition,"APPLF_IMG_ERR_YN") == "Y"){
					chk_3C.value = "Y";
				}
				if(ds_main.getColumn(ds_main.rowposition,"APPLF_TYP") == "3D" && ds_main.getColumn(ds_main.rowposition,"APPLF_IMG_ERR_YN") == "Y"){
					chk_3D.value = "Y";
				}
			}
		break;
		
		case "saveMain":
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				//fn_setInitForm();
			}
		break;
				
		default:
		break;
	}
}

