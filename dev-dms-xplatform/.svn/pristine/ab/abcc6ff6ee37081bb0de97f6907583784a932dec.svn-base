{

	ds_eqpIn.clearData();
	
	for(var i=0; i<ds_list.getRowCount(); i++){
		var strChk = ds_list.getColumn(i, "CHECKED");
		if (strChk == "1"){
			
			var strAssetNo = ds_list.getColumn(i, "ASSET_NO");	//자산번호
