	if(fvOpGb == "NO" && (fvSubOpGb == "31" || fvSubOpGb == "32" || fvSubOpGb == "34")){ //일반감정 송금확인,요금공제,강제매입
		sTrID		 = "PEPDpstMgmt_pUDpstMgmtProcRegUpd";
	    sInputDs	 = "REG_OBJ_LIST=ds_save";
	    sInputDs	 += " REG_OBJ_DTL_LIST=ds_dtlSave";
	}else if(fvOpGb == "RE" && fvSubOpGb == "31"){//재감정 송금확인
		sTrID		 = "PEPDpstMgmt_pUDpstMgmtRJdgSendAmtReg";
	    sInputDs	 = "RQ_REG_OBJ_LIST=ds_rsave";
	}else if(fvOpGb == "RE" && fvSubOpGb == "32"){//재감정 요금공제
		sTrID		 = "PEPDpstMgmt_pUDpstMgmtRJdgFeeDedcReg";
	    sInputDs	 = "RQ_REG_OBJ_LIST=ds_rsave";
	}else{
		return;
	}
	var sPtrn		 = "U";  //입력C,조회R,수정U,삭제D,복합M
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";

	fvOldPage = page;
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
