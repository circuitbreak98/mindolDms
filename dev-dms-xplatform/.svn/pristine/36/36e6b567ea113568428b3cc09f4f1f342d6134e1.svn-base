/*----------------------------------------------------------------------------------------------
 * 설명   : 버튼 Event
-----------------------------------------------------------------------------------------------*/
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj.name) 
	{				
		var arrTerm  = div_search.CommCalendarPred00.getData();
		var procTerm = div_search.cal_strdDt.text;
			
		if(gfn_isNull(arrTerm[0])){
			gfn_message("A","DMS00023","E","","조회일자 FROM");
			return;
		}
			
		if(gfn_isNull(arrTerm[1])){
			gfn_message("A","DMS00023","E","","조회일자 TO");
			return;
		}
			
		var objRtn = new Object;
		objRtn["opGbCd"] = div_search.cbo_opGb.value;    //업무구분
		objRtn["subOpGb"]= div_search.cbo_subOpGb.value; //업무구분sub
		objRtn["dtGb"]   = div_search.cbo_dtGb.value;    //조회일자구분				
		objRtn["inFrDt"] = arrTerm[0];
		objRtn["inToDt"] = arrTerm[1];
		objRtn["procDt"] = procTerm;
		this.close(objRtn);	
		
		case "btn_confirm" :	//확인 >> 전체저장
			trace("전체저장");
			//fn_saveMain();
			if(fvOpGb == "NO" && fvSubOpGb == "31"){ //일반감정 송금확인
				fn_sendAmt();
			}else if(fvOpGb == "RE" && fvSubOpGb == "31"){ //재감정 송금확인
				fn_rJdgSendAmt();
			}else if(fvOpGb == "NO" && fvSubOpGb == "32"){ //일반감정 요금공제
				fn_feeDedc();
			}else if(fvOpGb == "RE" && fvSubOpGb == "32"){ //재감정 요금공제
				fn_rJdgFeeDedc();
			}else{
				gfn_message("A","현재의 조회 데이터는 대상 데이터가 아닙니다. 확인 바랍니다.","E","","");
				return;
			}	
			break;
			
		case "btn_confirmBack" : //전체취소			
			trace("전체취소");
			//fn_saveCancelMain();
			if(fvOpGb == "NO" && fvSubOpGb == "31"){ //일반감정 송금확인
				fn_sndAmtCncl();
			}else if(fvOpGb == "RE" && fvSubOpGb == "31"){ //재감정 송금확인
				fn_rJdgSendAmtCncl();
			}else if(fvOpGb == "NO" && fvSubOpGb == "32"){ //일반감정 요금공제
				fn_feeDedcCncl();
			}else if(fvOpGb == "RE" && fvSubOpGb == "32"){ //재감정 요금공제
				fn_rJdgFeeDedcCncl();
			}else{
				gfn_message("A","현재의 조회 데이터는 취소대상 데이터가 아닙니다. 확인 바랍니다.","E","","");
				return;
			}
			break;
		
		case "btn_cancel" : 	//취소 = close
			close("cancel");
			break;
			
		case "btn_close" : 		//버튼X		
			close("cancel");
			break;	
	}
}

