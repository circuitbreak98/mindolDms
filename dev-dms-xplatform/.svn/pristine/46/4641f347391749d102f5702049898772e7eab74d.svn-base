/*----------------------------------------------------------------------------------------------
 * 설명   : 메인저장 (insert, update)
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_saveMain()
{	
	//bg_detail.chk_imgOmitYn.value == true ? ds_main.setColumn(0, "APPLF_IMG_OMIT_YN", "Y") : ds_main.setColumn(0, "APPLF_IMG_OMIT_YN", "");
	//bg_detail.chk_imgErrYn.value == true ? ds_main.setColumn(0, "APPLF_IMG_ERR_YN", "Y") : ds_main.setColumn(0, "APPLF_IMG_ERR_YN", "");
	//bg_detail.chk_imgItmYn.value == true ? ds_main.setColumn(0, "APPLF_IMG_ITM_ERR_YN", "Y") : ds_main.setColumn(0, "APPLF_IMG_ITM_ERR_YN", "");
	fn_provideSub();
	var sSvcID		 = "saveMain";
	var sTrID		 = "PNRCntrtPrstMgmt_pSaveCntrtPrstUscan";
	var sPtrn		 = ds_main.getRowType(ds_main.rowposition) == Dataset.ROWTYPE_INSERT? "I" : "U";  //입력C,조회R,수정U,삭제D,복합M
	sPtrn            = "M";
	var sInputDs	 = "RS_CNTRT_USCAN_LIST=ds_main:u RS_CNTRT_USCAN_DTL_LIST=ds_sub";
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

function fn_provideSub()
{	
	ds_sub.clearData();
	if(Radio00.value != "") {
		ds_main.setColumn(ds_main.rowposition,'APPLF_TYP',Radio00.value);
	}
	if(Radio02.value != "") {
		ds_main.setColumn(ds_main.rowposition,'APPLF_TYP',Radio02.value);
	}
	for(var i=0; i < 18; i++){
		var j = i+1;
		var k = "DMS253_0"+j;
		var l = "1A";
		if(i >= 5 && i <= 8) {
			j = i - 4;
			k = "DMS254_0"+j;
			l = "1B";
		}
		if(i >= 9 && i <= 13) {
			j = i - 8;
			k = "DMS255_0"+j;
			l = "2A";
		}
		if(i >= 14) {
			j = i - 13;
			k = "DMS256_0"+j;
			l = "2B";
		}
		if (ds_main.getColumn(ds_main.rowposition,k) == "Y") {
			var row = ds_sub.addRow();
			ds_sub.rowposition = row;
			ds_sub.setColumn(ds_sub.rowposition,'APPLF_TYP', l);
			ds_sub.setColumn(ds_sub.rowposition,'CNTRT_NO', ds_main.getColumn(ds_main.rowposition,"CNTRT_NO"));
			ds_sub.setColumn(ds_sub.rowposition,'APPLF_ITM_CD', '0'+j);
			ds_sub.setColumn(ds_sub.rowposition,'APPLF_IMG_ERR_YN', ds_main.getColumn(ds_main.rowposition,k));
		}
	}
}

/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
 	switch(sSvcID) {
		case "searchSub":
			if ( iErrorCode == 0 ) {
				//gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				Radio00.value = ds_main.getColumn(ds_main.rowposition,"APPLF_TYP");
				Radio02.value = ds_main.getColumn(ds_main.rowposition,"APPLF_TYP");
			}
		break;
		
		case "saveMain":
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				//fn_setInitForm();
			}
		break;
				
		default:
		break;
	}
}

