function fn_getLastDay1(sDate)
{
	if(gfn_isNull(sDate)) return "";
    
	var iMonth = parseInt(String(sDate).substr(4, 2), 10);
	
	switch(iMonth){
		case 1: case 3: case 5: case 7: case 8: case 10: case 12:
			return 31;
		case 2:
			if(gfn_isLeapYear(sDate)) return 29;
			else return 28;
		default:
			return 30;
	}
}

