/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
 	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {			
			}
		break;
		
		case "searchDtl": 
			if ( iErrorCode == 0 ) {
				ds_excel.appendData(ds_excelTmp,true);
				fn_nextLoop();
			}
		break;
		
		case "saveSlip":
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				ds_main.clearData();
				btn_search_onclick();
			}else{
				gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
			}
		break;
		
		case "delSlip":
			if (iErrorCode == 0) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				ds_main.clearData();
				btn_search_onclick();
			}else{
				gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
			}
		break;
		
		case "ReState":
			if (iErrorCode == 0) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				btn_search_onclick();
			}
		break;
				
		default:
		break;
		
	}
}

