/*----------------------------------------------------------------------------------------------
 * 설명   : fn_nextLoop
-----------------------------------------------------------------------------------------------*/
function fn_nextLoop()
{
	if ( total_record_cnt <=  page * page_size ) {
		page = curPage;
		page_size = 10000;		
		gfn_baseExcelExpert(grd_excel,"판매정산관리상세목록","판매정산관리상세목록");	
	} else {
		//다음페이지 조회
		page++;
		ds_excelTmp.clearData();
		fn_searchDtl();
	} 
}

