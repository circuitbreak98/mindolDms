-----------------------------------------------------------------------------------------------*/
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	var sellDt = div_search.cal_sell_dt.getData();
	
	if(gfn_isNull(sellDt[0]) || gfn_isNull(sellDt[1])){
		gfn_message("A","DMS00002","E","","조회기간");  //필수입력항목 {0}이/가 누락되었습니다.
		return false;
	}
	
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
		
	ds_search.setColumn(0, "SELL_STA_DTM", sellDt[0]);
	ds_search.setColumn(0, "SELL_END_DTM", sellDt[1]);
		
	ds_main.clearData();
	fn_searchMain();
	
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 전표발행
-----------------------------------------------------------------------------------------------*/
function btn_slipI_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_main.findRow("CHK","1") == -1){
		//체크대상없음
		gfn_message("A","DMS00020","I"); // 선택된 정보가 없습니다.
		return;
	}
	
	for(i=0; i<ds_main.getRowCount(); i++){
		if(ds_main.getColumn(i,"CHK")=='1'){
			if(ds_main.getColumn(i,"SLIP_ST") == '10' || ds_main.getColumn(i,"SLIP_ST") == '20' ){
				gfn_message("A","DMS00087","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
				return false;
		    }
		}
	}
		
	if(gfn_message("A","DMS00147","I","","판매정산")){
		fn_saveSlip();
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 전표취소
