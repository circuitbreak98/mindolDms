/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {

		case "searchSecureInfo" : 
			if ( iErrorCode == 0 ) {
			    //상세사용자설정
			    fn_setDetailUserInfo(true);
			}
			break;	
				
		case "searchMain": 
			if ( iErrorCode == 0 ) {
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);

				if (ds_boxInfo.getRowCount() > 0){
					
					fn_searchDtl();
				}else{
					ds_main.clearData();
					var srtProdQty = "";
					srtProdQty = ds_main.getRowCount(); 				
					ds_search.setColumn(0,"PROD_QTY", srtProdQty);
					ds_boxInfo.setColumn(0,"PROD_QTY", srtProdQty);
					ds_search.setColumn(0,"QTY", ds_boxInfo.getColumn(0,"QTY"));
				}
			}
		break;
		
		case "searchDtl": 
			if ( iErrorCode == 0 ) {
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
 				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
 				
 				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
 				
 				if (ds_boxInfo.getRowCount() == 0){
					ds_boxInfo.addRow();
				}
				var srtProdQty = "";
				/*srtProdQty = ds_main.getRowCount(); 				*/
				srtProdQty = ds_boxInfo.getColumn(0,"PROD_QTY");
				ds_search.setColumn(0,"PROD_QTY", ds_boxInfo.getColumn(0, "PROD_QTY"));
				/*ds_search.setColumn(0,"PROD_QTY", total_record_cnt);*/
				/*ds_boxInfo.setColumn(0,"PROD_QTY", total_record_cnt);*/
				ds_search.setColumn(0,"QTY", ds_boxInfo.getColumn(0,"QTY"));
				
				
			}
		break;
		
		case "saveMain": 
			if ( iErrorCode == 0 ) {

				fn_searchMain();
			}
		break;
	}
}

