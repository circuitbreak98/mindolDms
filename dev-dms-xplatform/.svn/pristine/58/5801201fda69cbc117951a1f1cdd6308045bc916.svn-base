/*----------------------------------------------------------------------------------------------
 * 설명   : delete
-----------------------------------------------------------------------------------------------*/
function btn_delete_onclick(obj:Button,  e:ClickEventInfo)
{
 	ds_boxInfo.clearData();	
 	if(ds_main.rowcount < 1){
 		gfn_message("A","DMS00021","I");	//조회정보가 없습니다.	
 		return false;
 	}
	
 	var del_cnt = 0;
 	
 	for(var i=0 ; i < ds_main.rowcount ; i++){
 		var strChk = ds_main.getColumn(i, "CHECKED");
 		if (strChk == "1"){
			
 			del_cnt ++;
 		} 
 	}
	
 	if (del_cnt < 1){		
 		gfn_message("A","DMS00020","I");	//선택된 정보가 없습니다.
 		return false;
 	}

	var qty = toNumber(ds_search.getColumn(0, "PROD_QTY"));
	var cnt = toNumber(del_cnt);

	var srtProdQty = qty - cnt;
	
 	if ( ds_boxInfo.rowcount < 1 ) {
 		ds_boxInfo.addRow();
 	}
 	
 	ds_main.setColumn(ds_main.rowposition, "CHECKED", "1");
 	
	ds_boxInfo.setColumn(0, "PROD_QTY", srtProdQty);
	ds_boxInfo.setColumn(0, "QTY", ds_search.getColumn(0,"QTY"));
	ds_boxInfo.setColumn(0, "BOX_NO", ds_search.getColumn(0,"BOX_NO"));

	fn_goSave(); 
}

