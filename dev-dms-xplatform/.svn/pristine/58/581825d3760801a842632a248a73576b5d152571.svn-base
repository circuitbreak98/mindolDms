================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitCode(obj:Form, e:LoadEventInfo)
{
	var strCode =  "DMS111=ds_DMS111=A";  //구성품코드
	
	gfn_getCodeDs( this
				  , strCode
				  ,"fn_afterCodeCallBack");	
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 호출 후 처리
-----------------------------------------------------------------------------------------------*/
function fn_afterCodeCallBack()
{
	ds_DMS111.filter("MGMT_ITEM_CD2=='Y' || CM_CD_ID == '%'");

}
/*===============================================================================================
 * [ PART 5 ] Transaction 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인저장
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_saveMain()
{
	var sSvcID		 = "saveMain";
	var sTrID		 = "PPRConstInMgmt_pSaveConstIn";
	var sPtrn		 = "I";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RS_CONST_IN_LIST=ds_main";
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "saveMain" : 
			if ( iErrorCode == 0 ) {
			    Err_chk = "";  //오류검증 초기화
				gfn_message("A","DMS00000","I");	//HPC00001 : 정상 처리되었습니다.
				ds_main.clearData();
			} else {
				gfn_message("A",ncMsg);
			}
			break;
		default :
			break;
	}
}
