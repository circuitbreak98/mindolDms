function fn_chkBeforeSave()	
{	
    //그리드 null체크
	if((ds_main.rowcount) == "0"){
	   gfn_message("A","DMS00004");	//데이터가 존재하지 않습니다. 데이터를 확인해 주세요.
		   return false;
	} 

	if(Err_chk != "03"){
		gfn_message("A","DMS00068");	//엑셀 오류검사를 해주시기 바랍니다.
		return false;
	}

	return true;
}
