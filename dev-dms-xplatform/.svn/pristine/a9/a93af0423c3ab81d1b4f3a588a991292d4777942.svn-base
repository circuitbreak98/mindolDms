 * param   : obj - Grid Object
 * param   : e  - ScrollEventInfo
 * return  : void
-----------------------------------------------------------------------------------------------*/
function grd_eqplist_onvtracklast(obj:Grid, e:ScrollEventInfo)
{
	
	if ( total_record_cnt <=  page * page_size ) {
	//	gfn_message("A","DMS00028","I"); //조회 마지막 페이지 입니다.   
		return;
	} else {
		//다음페이지 조회
		page++;
		fn_searchMain();
	} 
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 조회조건 제조사 검색
-----------------------------------------------------------------------------------------------*/
function div_search_btn_cmCdSearch01_onclick(obj:Button,  e:ClickEventInfo)
{
	var sParam01 = "DMS101";
	var sParam02 = "";	
	var sParam04 = "Y";  //선택버튼 활성화여부 
	if ( e == null ) {
		sParam03 = "key";
	} else {
		sParam03 = "button";
	}
	
	var arrArgs = {argAutoSelect:true, argCallProgram:"FNR000P",argParam01:sParam01, argParam02:sParam02, argParam04:sParam04};
	var sRtnVal = gfn_dialog("FSC007P", arrArgs);

	//팝업에서 입력받은 값 리턴
	if ( gfn_isNull(sRtnVal) || sRtnVal == "cancel" ) {
		//취소시 값 제거
		div_search.edt_mFactCd.value = "";
		div_search.edt_mFactNm.value = "";
	} else {	//Object이용시
		div_search.edt_mFactCd.value  = sRtnVal["supCmCd"];
		div_search.edt_mFactNm.value = sRtnVal["supCmCdNm"];
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명   : reset
-----------------------------------------------------------------------------------------------*/
function div_btn_reset_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj.name) 
	{				
		case "btn_reset01" : //제조사리셋
			div_search.edt_mFactCd.value = null;
			div_search.edt_mFactNm.value = null;
			break;	
	}
}
