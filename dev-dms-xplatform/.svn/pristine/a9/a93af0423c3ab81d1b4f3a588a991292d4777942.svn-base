/*===============================================================================================
 * [ PART 1 ] Form에서 Include 할 라이브러리(xjs)파일 정의
================================================================================================*/
include "LIB::common.xjs"; //공통함수 호출

/*===============================================================================================
 * [ PART 2 ] Form에서 사용될 전역변수를 선언
================================================================================================*/
  var inputType;
/*===============================================================================================
 * [ PART 3 ] Form Event 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Form Load Event
-----------------------------------------------------------------------------------------------*/
function Form_onload(obj:Form, e:LoadEventInfo)
{
	//공통 Form 초기화
	gfn_setInitPop(obj, e, "FNR000P");
	
 	//Form 초기 개발자 셋팅
 	//공통코드 초기화
 	fn_setInitCode(obj, e);
 	//그리드 초기화
 	fn_setInitGrid(obj, e);
 	//기본값 초기화
 	fn_setInitForm(obj, e);
 	
 	if( ds_main.getRowCount() == 1){
		fn_selectValue(0);
	}
}

/*===============================================================================================
 * [ PART 4 ] Load 후 초기설정
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitCode(obj:Form, e:LoadEventInfo)
{
	var strCode =  "DMS110=ds_DMS110=A";
		strCode += ",DMS101=ds_DMS101=A";
						
	gfn_getCodeDs( this
				  , strCode
				  ,"fn_afterCodeCallBack");
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 호출 후 처리
-----------------------------------------------------------------------------------------------*/
function fn_afterCodeCallBack()
{		  	
	
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 그리드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitGrid(obj:Form, e:LoadEventInfo)
{

}

/*----------------------------------------------------------------------------------------------
 * 설명   : 기본값 설정
-----------------------------------------------------------------------------------------------*/
function fn_setInitForm(obj:Form, e:LoadEventInfo)
{
	ds_search.setColumn(0, "OP_CL_CD", argParam04);
	div_search.edt_mdlCd.setFocus();
	//자동조회시 파라메타 전달예제
	if ( !gfn_isNull(argParam01) ) {
		div_search.edt_mdlCd.value = argParam01;
	}
	if ( !gfn_isNull(argParam02) ) {
		div_search.edt_mdlNm.value = argParam02;
	}
	
	try {
		if ( !gfn_isNull(argParam03) ) {
			inputType = argParam03;
		} else {
			inputType = "button";
		}
	}
	catch(e) {

	}
	
	div_search.btn_search.click();
}

/*===============================================================================================
 * [ PART 5 ] Transaction 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	var sSvcID		 = "searchMain";
	var sTrID		 = "PNREqpStdMgmt_pInqEqpStdLst";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sOutputDs	 = "ds_main=RS_EQP_STD_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{

	switch(sSvcID) {
		case "searchMain" : 
			if ( iErrorCode == 0 ) {
				//페이징 관련셋팅
				//ds_eqp.appendData(ds_eqpTemp,true);	
				//조회된 첫번째row로 setFocus 
				ds_main.rowposition = (page-1) * page_size ;

				gfn_setGridTitleCountPaging(stc_subTitle01, total_record_cnt);
				div_search.edt_mdlNm.setFocus();
				//Data 1건일 경우 close
				if( ds_main.getRowCount() == 1 && inputType == "key"){
					fn_selectValue(0);
				}
			}
			break;	
		
		default :
			break;
	}
}

/*===============================================================================================
 * [ PART 7 ] Main Button Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 버튼 Event
-----------------------------------------------------------------------------------------------*/
function button_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj.name) 
	{	
		case "btn_search" :		//조회
		    // 입력 항목 Validate Check
			if (!gfn_getValidateCompList(div_search)) {	
				return false;
			}
			ds_main.clearData();
			page = 1;
			fn_searchMain();
			break;
			
		case "btn_confirm" :	//확인
			fn_selectValue(ds_main.rowposition);
			break;
			
		case "btn_cancel" : 	//취소
			close("cancel");
			break;
			
		case "btn_close" : 		//버튼X
			close("cancel");
			break;	
	}
}

/*===============================================================================================
 * [ PART 8 ] Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Grid Double Click Event
-----------------------------------------------------------------------------------------------*/
function grd_eqplist_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	fn_selectValue(e.row);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 조회조건에서 엔터키가 눌러졌을때 조회
-----------------------------------------------------------------------------------------------*/
function div_Search_edt_mdlCd_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == KEYCODE_ENTER) {
		div_search.btn_search.setFocus();
		ds_main.clearData();
		page = 1;
		fn_searchMain();
	}	
}

/*===============================================================================================
 * [ PART 9 ] User Defined
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 결과값 리턴후 Close
-----------------------------------------------------------------------------------------------*/
function fn_selectValue(nRow)
{
	//단건(Object사용)
	if ( nRow == -1 ) return;
	
	var objRtn = new Object;
		objRtn["supEqpMdlCd"] 	= ds_main.getColumn(nRow,"EQP_MDL_CD");		//단말기모델코드
		objRtn["supEqpMdlNm"] 	= ds_main.getColumn(nRow,"EQP_MDL_NM");		//단말기모델명
		objRtn["supMfactCd"]   	= ds_main.getColumn(nRow,"MFACT_CD");		//제조사코드
		objRtn["supFstRgstDt"] 	= ds_main.getColumn(nRow,"FST_RGST_DT");	//출시일
		objRtn["supPetNm"] 	    = ds_main.getColumn(nRow,"PET_NM");			//펫네임
		objRtn["supCapaCd"] 	= ds_main.getColumn(nRow,"CAPA_CD");		//저장용량코드
		//objRtn["supCapaNm"] 	= ds_main.getColumn(nRow,"CAPA_NM");		//저장용량명
		objRtn["supOutPrc"] 	= ds_main.getColumn(nRow,"OUT_PRC");		//출고가
		objRtn["supRmk"]  	    = ds_main.getColumn(nRow,"RMK");			//비고
		objRtn["supOpClCd"]  	= ds_main.getColumn(nRow,"OP_CL_CD");		//업무구분코드
		//objRtn["supOpClNm"]  	= ds_main.getColumn(nRow,"OP_CL_NM");		//업무구분명
		
	this.close(objRtn);		
	
}

