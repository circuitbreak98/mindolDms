/*----------------------------------------------------------------------------------------------
 * 설명   : search
-----------------------------------------------------------------------------------------------*/
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{


	 //입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	
	ds_main.clearData();
	page = 1;
	fn_searchMain();

}

/*----------------------------------------------------------------------------------------------
 * 설명   : 버튼 Event
-----------------------------------------------------------------------------------------------*/
function button_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj.name) 
	{	
		case "btn_search" :		//조회
		    // 입력 항목 Validate Check
			if (!gfn_getValidateCompList(div_search)) {	
				return false;
			}
			ds_main.clearData();
			page = 1;
			//fn_searchMain();
			break;
			
		case "btn_confirm" :	//확인
			fn_selectValue(ds_main.rowposition);
			break;
			
		case "btn_cancel" : 	//취소
			close("cancel");
			break;
			
		case "btn_close" : 		//버튼X
			close("cancel");
			break;	
	}
}

