}


/*******************************************************************************
 * 설명    :  저장시 validation check
*******************************************************************************/
function fn_saveValidationCheck()
{
    var bFlag = true;

	if ( div_main6.div_sub1.cbo_prchCl.value == "Y" && div_main5.chk_polyObjYn.value == '1' 
		&& toNumber(div_main6.div_sub1.mae_cnslAmt.value,0) > 50000 ) {
		gfn_message("A","DMS00118","E"); /* 본단말기는 정책 대상이 아닙니다. 정책대상여부를 확인하세요. */
		return false;				
	}	

	if ( div_main6.div_sub1.cbo_prchCl.value == "C") {
		gfn_message("A","DMS00002","W","",div_main6.div_sub1.stc_msgInDate.text); /*필수입력항목 {0}이/가 누락되었습니다. */
		div_main6.div_sub1.cbo_prchCl.setFocus();
		return false;
	}

	if ( div_main6.div_sub1.cbo_prchCl.value == "Y" && gfn_isNull(div_main4.edt_sellGrade.text)) {
		gfn_message("A","DMS00002","W","",div_main4.stc_05.text); /*필수입력항목 {0}이/가 누락되었습니다. */
		div_main4.edt_sellGrade.setFocus();
		return false;
	}	
	
    if ( gfn_isNull(div_main2.edt_dealCoNm2.text) || gfn_isNull(div_main2.edt_dealCoCd2.text) ) {
		gfn_message("A","DMS00002","W","",div_main2.stc_msgNm.text); /*필수입력항목 {0}이/가 누락되었습니다. */
		div_main2.edt_dealCoNm2.setFocus();
		return false;
	}
	
	if ( gfn_isNull(div_main3.edt_eqpSerNo.text) ) {
		gfn_message("A","DMS00002","W","",div_main3.stc_msgNm01.text); /*필수입력항목 {0}이/가 누락되었습니다. */
		div_main3.edt_eqpSerNo.setFocus();
		return false;
	}
	
	if ( gfn_getLength (div_main3.edt_eqpSerNo.text) != 6 && 
	     gfn_getLength (div_main3.edt_eqpSerNo.text) != 7 && 
		 gfn_getLength (div_main3.edt_eqpSerNo.text) != 11 &&
		 gfn_getLength (div_main3.edt_eqpSerNo.text) != 12) {
