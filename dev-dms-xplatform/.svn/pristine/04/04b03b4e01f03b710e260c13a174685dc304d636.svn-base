-----------------------------------------------------------------------------------------------*/
function fn_setInitForm(obj:Form, e:LoadEventInfo)
{
	div_search.cal_prchDt.cal_calFrom.value= gfn_getFirstDate();
	div_search.cal_prchDt.cal_calTo.value = gfn_today();
	div_search.stc_slipTs.visible = false;
	div_search.cbo_slipTs.visible = false;
}

/*===============================================================================================
 * [ PART 5 ] Transaction 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	var sSvcID		 = "searchMain";
	var sTrID		 = "PNRAgnEqpStlMgmt_pInqAgnEqpStlLst";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sOutputDs	 = "ds_main=RS_AGN_EQP_STL_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";

	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 전표생성 (insert, update)
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_saveSlip()
{	
	var sSvcID		 = "saveSlip";
	var sTrID		 = "PNRAgnEqpStlMgmt_pSaveAgnEqpStlSlip";
	var sPtrn		 = "I";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RS_AGN_EQP_STL_LIST=ds_main:u";
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
 	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
			}
		break;
		
		case "saveSlip":
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				btn_search_onclick();
			}
		break;
				
		default:
		break;
	}
}

/*===============================================================================================
 * [ PART 7 ] Main Button Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : search
-----------------------------------------------------------------------------------------------*/
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	var prchDt = div_search.cal_prchDt.getData();
	
	if(gfn_isNull(prchDt[0]) || gfn_isNull(prchDt[1])){
		gfn_message("A","DMS00002","E","","매입일자");  //필수입력항목 {0}이/가 누락되었습니다.
		return false;
	}
	
	ds_search.setColumn(0, "PRCH_STA_DT", prchDt[0]);
	ds_search.setColumn(0, "PRCH_END_DT", prchDt[1]);

	ds_main.clearData();
	page = 1;
	fn_searchMain();
	
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 전표생성
-----------------------------------------------------------------------------------------------*/
function btn_slipI_onclick(obj:Button,  e:ClickEventInfo)
{	
	if(ds_main.findRow("CHK","1") == -1){
		//체크대상없음
		gfn_message("A","DMS00020","I"); // 선택된 정보가 없습니다.
		return;
	}
	
	var result = gfn_message("A","DMS00095","I", "", "전표생성");
	if(result){
		ds_main.filter("CHK == '1'");
		fn_saveSlip();
		//ds_main.filter("");
	}
}

