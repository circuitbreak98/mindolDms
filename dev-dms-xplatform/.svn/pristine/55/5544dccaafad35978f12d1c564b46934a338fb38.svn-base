-----------------------------------------------------------------------------------------------*/
function btn_excel_onclick(obj:Button,  e:ClickEventInfo)
{
	if (grd_main.rowcount == 0){
		gfn_message("A","DMS00021","I"); //조회된 정보가 없습니다.
	} else {
		gfn_baseExcelExpert(grd_main,"재고상품목록","재고상품목록");
	}
}

/*===============================================================================================
 * [ PART 8 ] Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Confirm
-----------------------------------------------------------------------------------------------*/
function btn_confirm_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_main.findRow("CHK","1") == -1){
		//체크대상없음
		gfn_message("A","DMS00020","I"); // 선택된 정보가 없습니다.
		return;
	}
	
	ds_main.filter("CHK == '1'");
	for(var i = ds_main.getRowCount()-1; i >= 0 ; i--){
		opener.ds_main.filter("PROD_CL=='"+ds_main.getColumn(i,"PROD_CL")+"'&&PROD_SER_NO=='"+ds_main.getColumn(i,"PROD_SER_NO")+"'&&EQP_MDL_CD=='"+ds_main.getColumn(i,"EQP_MDL_CD")+"'");
	
		if(opener.ds_main.getRowCount() > 0 ){
			ds_main.deleteRow(i);
		}
	}
	
	opener.ds_main.filter("");
	
	if(ds_main.getRowCount() == 0){
		gfn_message("A","선택된 상품이 이미 등록 되어있습니다.");
		ds_main.filter("");
		btn_search_onclick();
		return;
	}
	
	var objRtnDS = new Dataset;
 	objRtnDS.copyData(ds_main, true);
	this.close(objRtnDS);		
}


