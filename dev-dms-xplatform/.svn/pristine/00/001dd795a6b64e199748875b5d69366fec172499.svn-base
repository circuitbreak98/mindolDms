-----------------------------------------------------------------------------------------------*/
function grd_main_oncellclick(obj:Grid, e:GridClickEventInfo)
{


	const const_cc = 14;	//취득취소전표(생성)
	const const_dd = 15;	//취득취소전표(취소)
	const const_jj = 29;		//감가상각취소전표(생성)
	const const_kk = 30;	//감가상각취소전표(취소)
	//
	if ((ds_main.rowposition >= 0 ) && (grd_main.currentcol == const_cc || grd_main.currentcol == const_dd || grd_main.currentcol == const_jj || grd_main.currentcol == const_kk))
	{
		ds_handling.clearData();
		ds_handling.copyRow(ds_handling.addRow(),ds_main,ds_main.rowposition);

		//취소 전표생성
		var slip_st_cd = "";
		if(grd_main.currentcol == const_cc || grd_main.currentcol == const_jj ){
			if(grd_main.currentcol == const_cc){
				ds_handling.setColumn(0, "SLIP_TYPE", "NR_OA");		//취득전표 취소
				slip_st_cd =  ds_main.getColumn(ds_main.rowposition, "ACQR_CNCL_SLIP_ST_CD");		//취득취소전표상태
			} else if (grd_main.currentcol == const_jj){
				ds_handling.setColumn(0, "SLIP_TYPE", "NR_OC");		//감가상각 취소
				slip_st_cd =  ds_main.getColumn(ds_main.rowposition, "DEPR_CNCL_SLIP_ST_CD");		//감가상각취소전표상태
			}
			//
			if ( slip_st_cd == '00' || slip_st_cd == '05'){
				var sSvcID		= "prchCnclXclSlip";
				var sTrID		 	= "PNRPrchCnclXclMgmt_pSlipPrchCnclXcl";
				var sPtrn		 	= "R";  //입력C,조회R,수정U,삭제D,복합M
				var sInputDs	 	= "ncFieldMap=ds_handling";
				var sOutputDs	= "";
				var sArgs		 	= "";
				var sCallBackFnc = "fn_callBack";
				gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
			}else{
				gfn_message("A","DMS00071","I");// 이미 처리된 건이 있습니다.
				return;
			}
		} 
		
		//취소 전표취소
		if(grd_main.currentcol == const_dd || grd_main.currentcol == const_kk ){
			if(grd_main.currentcol == const_dd){
				ds_handling.setColumn(0, "SLIP_TYPE", "NR_OA");		//취득전표 취소
				slip_st_cd =  ds_main.getColumn(ds_main.rowposition, "ACQR_CNCL_SLIP_ST_CD");	//취득취소전표상태
			} else if (grd_main.currentcol == const_kk){
				ds_handling.setColumn(0, "SLIP_TYPE", "NR_OC");		//감가상각 취소
				slip_st_cd =  ds_main.getColumn(ds_main.rowposition, "DEPR_CNCL_SLIP_ST_CD");		//감가상각취소전표상태
			}
			if( slip_st_cd == '00' ){
				gfn_message("A","DMS00131","I","","미발행건은");	//"미발행건은 취소할수 없습니다."
				return;
			}else if( slip_st_cd  == '05'){
				gfn_message("A","DMS00071","I");// 이미 처리된 건이 있습니다.
				return;
			}else if( slip_st_cd == '20'   || slip_st_cd  == '30'){
				gfn_message("A","DMS00131","I","","승인완료 또는 반대전표는");//"승인완료 또는 반대전표는 취소할수 없습니다."
				return;
			}
			//
			var sSvcID		= "prchCnclXclSlip";
			var sTrID		 	= "PNRPrchCnclXclMgmt_pSlipPrchCnclXcl";
			var sPtrn		 	= "R";  //입력C,조회R,수정U,삭제D,복합M
			var sInputDs	 	= "ncFieldMap=ds_handling";
			var sOutputDs	= "";
			var sArgs		 	= "";
			var sCallBackFnc = "fn_callBack";
			gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
		}
	}
		
}

