	ds_search.setColumn(0, "YDATE", div_search.cal_Ydate.getDate());
	if( gfn_isNull( ds_search.getColumn(0,"YDATE") )){
		gfn_message("A","DMS00023","I","","매입 년월");
		return;
	}	

	if (!gfn_isNull(div_search.edt_assetSlipNo.value)) {	
		ds_search.setColumn(0,"ASSET_SLIP_NO",div_search.edt_assetSlipNo.value);
	}
	
	ds_main.clearData();
	page = 1;
	fn_searchMain();
}

