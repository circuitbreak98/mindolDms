function div_search_btn_searchMdl_onclick(obj:Button,  e:ClickEventInfo)
{
	var div = null;
	if(obj.name == 'btn_searchMdl'){	//조회조건에서 모델검색일때
		div = div_search;
	}else if(obj.name == 'btn_searchEqpMdl'){ //입력DETAIL 에서 모델검색일때
		div = div_detail;
	}
	
	var sParam01 = div.edt_eqpMdlCd.value;
	var sParam02 = div.edt_eqpMdlNm.value;
	var sParam03 = "";
	var sParam04 = "NR";
	
	if ( e == null ) {
		sParam03 = "key";
	} else {
		sParam03 = "button";
	}
	var arrArgs = {argAutoSelect:true, argCallProgram:"FNR064M", argParam01:sParam01, argParam02:sParam02, argParam03:sParam03, argParam04:sParam04};
	var sRtnVal = gfn_dialog("FNR000P", arrArgs);

	//팝업에서 입력받은 값 리턴
	if ( gfn_isNull(sRtnVal) || sRtnVal == "cancel" ) {
		//취소시 값 제거
		div.edt_eqpMdlCd.value = "";		//단말코드
		div.edt_eqpMdlNm.value = "";		//단말이름
		
	} else {	//Object이용시
		div.edt_eqpMdlCd.value  = sRtnVal["supEqpMdlCd"];
		div.edt_eqpMdlNm.value  = sRtnVal["supEqpMdlNm"];
	
		}
}
