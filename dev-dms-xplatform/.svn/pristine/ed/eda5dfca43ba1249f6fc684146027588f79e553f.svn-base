/*===============================================================================================
 * [ PART 5 ] Transaction 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	var sSvcID		 = "searchMain";
	var sTrID		 = "PNREqpTransportMgmt_pInqEqpTransportLst";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sOutputDs	 = "ds_main=RS_TRANSPORT_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";

	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인저장
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_saveMain(strObj)
{
	if(!fn_chkBeforeSave(strObj)) return false;

	if(strObj == 'I'){
		ds_temp.addRow();
		ds_temp.copyRow(0,ds_main,ds_main.rowposition);
		ds_temp.setColumn(0,"CHK","I");
	
	
	}else if(strObj == 'U'){
		ds_temp.addRow();
		ds_temp.copyRow(0,ds_main,ds_main.rowposition);
		ds_temp.setColumn(0,"CHK","U");
	
	}
	
	var sSvcID		 = "saveMain";
	var sTrID		 = "PNREqpTransportMgmt_pEqpTransportSave";
	var sPtrn		 = "M";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RS_TRANSPORT_LIST=ds_temp";
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*----------------------------------------------------------------------------------------------
 * 설명   :삭제
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_deleteMain()
{
	ds_temp.addRow();
	ds_temp.copyRow(0,ds_main,ds_main.rowposition);
	ds_temp.setColumn(0,"CHK","D");
	
	var sSvcID		 = "deleteMain";
	var sTrID		 = "PNREqpTransportMgmt_pEqpTransportDelete";
	var sPtrn		 = "D";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RS_TRANSPORT_DELETE=ds_temp";
	var sOutputDs	 = ""; 
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain" : 
			if ( iErrorCode == 0 ) {
 				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
 				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count			
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
				
				fn_btnEnableSet();		// 버튼활성화		
			} 
			
			break;
			
		case "saveMain" : 
			if ( iErrorCode == 0 ) {
				if(ds_temp.getColumn(0,"CHK")== "I"){
					fn_callRCenter();
				}
						
			}else {
				return false;	//저장 실패시....
			}
			fn_searchMain();
			break;
		
		case "deleteMain" :
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I"); /*정상 처리되었습니다.*/
			}
			fn_searchMain();
			break;
		
		default :
			break;
	}
}

