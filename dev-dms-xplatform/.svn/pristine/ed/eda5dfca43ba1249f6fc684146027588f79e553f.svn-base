-----------------------------------------------------------------------------------------------*/
function btn_delete_onclick(obj:Button,  e:ClickEventInfo)
{
	var nRow = ds_main.rowposition;
	
	var msgu = "수정중인 데이터를 삭제합니다. 계속하시겠습니까?" ;
	var msgi = "신규 작성중인 데이터를 삭제합니다. 계속하시겠습니까?" ;
	var msgd = ds_main.getColumn(nRow,"ASSET_NO")+"의 운송정보 데이터를 삭제합니다. 계속하시겠습니까?" ;
	
	if(ds_main.getRowType(nRow) == "1"){	//초기 행의 상태
		if(ds_main.getColumn(nRow,"IN_OBJ_DTL_CD")=='04'){
			gfn_message("A","DMS00025","I");	//삭제할 수 없는 데이터 입니다.
			return false;	
		}else if(gfn_message("A",msgd,"I","CONFIRM","")){
			ds_temp.clearData();
			fn_deleteMain();
		}
		
	}else if(ds_main.getRowType(nRow) == "2" ){	//insert 글은 바로 삭제
		
		if ( gfn_message("A",msgi,"I","CONFIRM","") ) {
			ds_main.deleteRow(nRow);	
			fn_searchMain(); 
		}else{
			return false;
		}
		
	}else if(ds_main.getRowType(nRow) == "4" ){  // 업데이트 글은 Temp 삭제여부만 갱신하고 더미만 삭제 ?????????????????
				
		if ( gfn_message("A",msgu,"I","CONFIRM","") ) {
			fn_searchMain(); 			
		}else{
			return false;
		}
	}

}
