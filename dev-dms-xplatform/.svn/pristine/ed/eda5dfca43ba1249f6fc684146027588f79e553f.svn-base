-----------------------------------------------------------------------------------------------*/
function btn_save_onclick(obj:Button,  e:ClickEventInfo)
{
	var nRow = ds_main.rowposition;
	var check = ds_main.getRowType(nRow);
	var msgi = "신규 작성된 데이터를 저장합니다. 계속하시겠습니까?" ;
	var msgu = "수정된 데이터를 저장합니다. 계속하시겠습니까?" ;
	var msgC = "수정할수 없는 데이터 입니다." ;
		
	
		
	if(check == '1'){
		if(gfn_message("A","DMS00022","I")){ /*변경된 정보가 없습니다.*/
			return false;
		}
	}else if(check == '2'){//insert이면
		
		if ( gfn_message("A",msgi,"I","CONFIRM","") ) {	
			section = "I";
		}
		
	}else if(check == '4'){ // 수정된상태
	
		if(ds_main.getColumn(nRow,"IN_OBJ_DTL_CD")=='04'){
			gfn_message("A",msgC,"I");	//수정할 수 없는 데이터 입니다.
			fn_searchMain();
		}
		if ( gfn_message("A",msgu,"I","CONFIRM","") ) {	
			section = "U";
		}
		
	}
	
	
	ds_temp.clearData();
	fn_saveMain(section);
	
}

