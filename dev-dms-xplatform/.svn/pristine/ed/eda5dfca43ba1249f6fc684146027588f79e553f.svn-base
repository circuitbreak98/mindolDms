/*----------------------------------------------------------------------------------------------
 * 설명   : 버튼 활성화 여부 
-----------------------------------------------------------------------------------------------*/
function fn_btnEnableSet()	
{	
	var rp = ds_main.rowposition;
	
	if(ds_main.getRowType(rp)=='0'){		//조회된애가 없을때
		div_detail.enable = false;
	
	}else if(ds_main.getRowType(rp)=='1'){	//수정대기중일때
		div_detail.enable = true;
			
		div_detail.edt_eqpMdlCd.enable = false;
		div_detail.edt_eqpMdlNm.enable = false;
		div_detail.btn_searchEqpMdl.enable = false;
		div_detail.btn_resetDMdl.enable = false;
		div_detail.edt_eqpSerNum.enable = false;
		
		if(ds_main.getColumn(ds_main.rowposition,"IN_OBJ_DTL_CD")=='04'){
			div_detail.enable = false;
		}
	
	}else if(ds_main.getRowType(rp)=='2'){	//신규입력일때
		div_detail.enable = true;
		
		div_detail.edt_eqpMdlCd.enable = true;
		div_detail.edt_eqpMdlNm.enable = true;
		div_detail.btn_searchEqpMdl.enable = true;
		div_detail.btn_resetDMdl.enable = true;
		div_detail.btn_searchHdlvcd.enable = true;
		div_detail.btn_resetHdlvcd.enable = true;
		div_detail.edt_eqpSerNum.enable = true;
	}
}
