-----------------------------------------------------------------------------------------------*/
function btn_excel_onclick(obj:Button,  e:ClickEventInfo)
{
	if (grd_ifLogList.rowcount == 0){
		gfn_message("A","DMS00021","I"); //조회된 정보가 없습니다.
	} else {
		gfn_baseExcelExpert(grd_ifLogList,"시스템로그","시스템로그");
	}
}

