/*----------------------------------------------------------------------------------------------
 * 설명   : BoxNo 등록에서 바코드 입력 및 키보드 ENTER입력시
-----------------------------------------------------------------------------------------------*/
function edt_boxBarCd_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == KEYCODE_ENTER) {
		edt_mgmtBarCd.setFocus();
		fn_searchMain();
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 관리번호 등록에서 바코드 입력 및 키보드 ENTER입력시
-----------------------------------------------------------------------------------------------*/
function edt_mgmtBarCd_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == KEYCODE_ENTER) {		
		// 입력 항목 Validate Check _중고폰 관리번호
		//if(((obj.value.substr(0,1) == 'T') && (Length(obj.value) = 6 || Length(obj.value) = 7))
		//||((obj.value.substr(0,3) == 'CON') && (Length(obj.value) = 12) )
		//||((obj.value.substr(0,1) == 'C') && (obj.value.substr(0,3) != 'CON') && (Length(obj.value) = 10))
		//||((obj.value.substr(0,1) == 'U') && Length(obj.value) = 8)){    
		
			if (_gfn_getValidItem(edt_boxBarCd, edt_boxBarCd.validate, "value") == false) {
				edt_boxBarCd.setFocus();
				return false;
			}
			
			if (_gfn_getValidItem(edt_mgmtBarCd, edt_mgmtBarCd.validate, "value") == false) {
				edt_mgmtBarCd.setFocus();
				return false;
			}
			
			if(ds_sub.rowcount == 0 || (ds_search.getColumn(0,"BOX_BAR_CD") != ds_sub.getColumn(0,"BOX_BAR_CD"))) {
				fn_searchMain();
				return false;
			}
			
			if(ds_main.rowcount >= toNumber(ds_sub.getColumn(0,"MAX_QTY"))){
				gfn_message("A","DMS00064"); //최대입력건수를 초과하였습니다.
				return false;
			}
			
			var fRow = ds_main.findRow("MGMT_BAR_CD",obj.value);
			if(fRow >= 0) {
				gfn_message("A","DMS00003","I"); //중복 된 데이터가 존재합니다.
				ds_main.rowposition = fRow;
				return false;
			}
			
			nRow = ds_main.addRow();
			ds_main.setColumn(nRow, "MGMT_BAR_CD", obj.value);
			ds_main.setColumn(nRow, "BOX_BAR_CD", ds_search.getColumn(0,"BOX_BAR_CD"));
			
			fn_getProdInfo(obj.value);
			
			obj.value = "";
			
			fn_setGridTitleCount(stc_subTitle, ds_main.rowcount, toNumber(ds_sub.getColumn(0,"MAX_QTY")));
		//} else {
		//    gfn_message("A","DMS00086","W","","관리번호"); //유효한 {0} 코드가 아닙니다
		//}
	}
}

/*******************************************************************************
 * @description    :  상품 정보 조회
*******************************************************************************/
function fn_getProdInfo(o:String)
{
	if (ds_search.getColumn(0,"MGMT_BAR_CD") == null){
		ds_search.setColumn(0, "MGMT_BAR_CD", o);
	}

	var sSvcID		 = "getProdInfo";
	var sTrID		 = "PBIBoxBarcdMgmt_pGetProdInfo";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sOutputDs	 = "ds_prodInfo=RS_PRODINFO";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}  

/*******************************************************************************
 * @description    :  중고폰 정보 추가
*******************************************************************************/
function fn_setEpInfo()
{   
	for(var i=0; i<ds_main.getRowCount(); i++){
		for(var j=0; j<ds_epInfo.getRowCount(); j++){
			if (ds_main.getColumn(i,"MGMT_BAR_CD") == ds_epInfo.getColumn(j,"MGMT_BAR_CD")){				
			    ds_main.setColumn(i,"S_GB",ds_epInfo.getColumn(j,"S_GB"));
			}			
		}
	}
} 

