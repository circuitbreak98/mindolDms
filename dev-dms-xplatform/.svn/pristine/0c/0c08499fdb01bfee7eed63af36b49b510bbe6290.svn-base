/*
삭제버튼
*/
function btn_delete_onclick(obj:Button,  e:ClickEventInfo)
{
	if(grd_main.currentrow < 0){
		return;
	}
	ds_upLoadXlsList.deleteRow(grd_main.currentrow);
	if(ds_upLoadXlsList.rowcount <= 0){
		Div00.btn_excelSave.enable = false;
	}
}
