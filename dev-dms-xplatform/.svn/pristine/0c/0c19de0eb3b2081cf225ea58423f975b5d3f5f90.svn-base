/*----------------------------------------------------------------------------------------------
 * 설명   : search
-----------------------------------------------------------------------------------------------*/
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{	
	 ds_search.setColumn(0,"SALEFROMDTM","");
	 ds_search.setColumn(0,"SALETODTM","");
	 ds_search.setColumn(0,"TLYFROMDT","");
	 ds_search.setColumn(0,"TLYTODT","");
	 ds_search.setColumn(0,"PRCHFROMDT","");
	 ds_search.setColumn(0,"PRCHTODT","");
	 if(div_search.cbo_ProgrSt.value != "%"){ 
		//판매기간
		var arrTerm = div_search.ccp_sale.getData();
		if(div_search.cbo_ProgrSt.value == "30" || div_search.cbo_ProgrSt.value == "40"){ 
			ds_search.setColumn(0,"SALEFROMDTM",arrTerm[0]);
			ds_search.setColumn(0,"SALETODTM",arrTerm[1]);
		}else if(div_search.cbo_ProgrSt.value == "10"){
			ds_search.setColumn(0,"TLYFROMDT",arrTerm[0]);
			ds_search.setColumn(0,"TLYTODT",arrTerm[1]);
		}else if(div_search.cbo_ProgrSt.value == "20"){
			ds_search.setColumn(0,"PRCHFROMDT",arrTerm[0]);
			ds_search.setColumn(0,"PRCHTODT",arrTerm[1]);
		}
		 
		if(gfn_isNull(arrTerm)){
			gfn_message("A","DMS00002","E","",div_search.stc_fromToDate.text);  //필수입력항목 {0}이/가 누락되었습니다.
			
			div_search.ccp_sale.setData(gfn_addDate(gfn_today(),-183),gfn_today(),true); //6개월만 검색 가능하도록 
			div_search.ccp_sale.setFocus();
			return false;
		}
		 
		if ((gfn_diffDate(arrTerm[0], arrTerm[1])) > 183 ) {
			gfn_message("A","DMS00106","E","", new Array('시작일자와 종료일자','6개월'));  //{0}를 {1} 이내로 지정하세요.
			return false;
		}
	}

	ds_search.setColumn(0,"MQ_SELL_YN","%");
	if(div_search.cbo_SellTarget.value == "MQ" || div_search.cbo_SellTarget.value == "PR"){
		ds_search.setColumn(0,"MQ_SELL_YN","Y");
	}
	if(div_search.cbo_SellTarget.value == "GN"){
		ds_search.setColumn(0,"MQ_SELL_YN","N");
	}

	ds_main.clearData();
	page = 1;
	fn_searchMain();
}

