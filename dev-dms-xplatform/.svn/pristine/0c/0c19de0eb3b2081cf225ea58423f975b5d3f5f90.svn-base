		return;
	}
	
	// 입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	curPage = page;
	gfn_dsTrim(ds_search);
	ds_mainExcel.clearData();
	page = 1;
	page_size = 10000;

	var nTotCnt = gfn_toFixedLocaleString(total_record_cnt,0);
	div_downInfoBase.div_downPbr.sta_totDownCnt.text = nTotCnt+" 건";
	div_downInfoBase.div_downPbr.sta_nowDownCnt.text = "0 건";
	div_downInfoBase.visible = true;
	
	gfn_initGrid(grd_mainExcel, false, true, true, "row", null);
	fn_searchMainAll();
}
//상품판매등록양식다운로드
function btn_saleExcelAll_onclick(obj:Button,  e:ClickEventInfo)
{
	if (ds_main.rowcount == 0){
		gfn_message("A","DMS00021","I"); //조회된 정보가 없습니다.
		return;
	}

    curPage = page;
    gfn_dsTrim(ds_search);
    ds_saleExcel.clearData();
	page = 1;
	page_size = 10000;
	
	gfn_initGrid(grd_excel, false, true, true, "row", null);
	fn_searchExcelAll();
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 엑셀다운로드 디비전 사이즈 변경 적용
-----------------------------------------------------------------------------------------------*/
function div_downInfoBase_onsize(obj:Div, e:SizeEventInfo)
{
	var nSpace = (div_downInfoBase.position.width - div_downInfoBase.div_downPbr.position.width) / 2;
	var nWidth = div_downInfoBase.div_downPbr.position.width;
	div_downInfoBase.div_downPbr.position.left = nSpace;
	div_downInfoBase.div_downPbr.position.width = nWidth;
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 진행상태 비판매 선택시 판매기간 초기화
-----------------------------------------------------------------------------------------------*/
function div_search_cbo_ProgrSt_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	if(div_search.cbo_ProgrSt.value == "%"){
		div_search.ccp_sale.cal_calFrom.value = "";	
		div_search.ccp_sale.cal_calTo.value = "";	
		div_search.ccp_sale.enable = false;
	}else if(div_search.cbo_ProgrSt.value == "10" || div_search.cbo_ProgrSt.value == "20"){
		div_search.ccp_sale.enable = true;
	}else{
		div_search.ccp_sale.enable = true;
	}	
	//진행상태 변경시 판매대상은 전체로
	if(div_search.cbo_SellTarget.value != '%'){
		div_search.cbo_SellTarget.value = '%';
	}
	
	if(div_search.cbo_ProgrSt.value == "10"){
		div_search.stc_fromToDate.text = "감정기간";
	}else if(div_search.cbo_ProgrSt.value == "20"){
		div_search.stc_fromToDate.text = "매입기간";
	}else if(div_search.cbo_ProgrSt.value == "40"){
		div_search.stc_fromToDate.text = "판매대행기간";
	}else {
		div_search.stc_fromToDate.text = "판매기간";
	}
	if(div_search.cbo_ProgrSt.value != "%"){
		var arrTerm = div_search.ccp_sale.getData();
		if(gfn_isNull(arrTerm)){
			div_search.ccp_sale.setData(gfn_addDate(gfn_today(),-183),gfn_today(),true); //6개월기본기간 설정
		}
	}	
	div_search.ccp_sale.tooltiptext = div_search.stc_fromToDate.text;
	div_search.stc_fromToDate.tooltiptext = div_search.stc_fromToDate.text;
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 판매대상 선택시 진행상태변경
-----------------------------------------------------------------------------------------------*/
function div_search_cbo_SellTarget_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	if(div_search.cbo_SellTarget.value == "MQ" || div_search.cbo_SellTarget.value == "GN"){
		div_search.cbo_ProgrSt.value = "20";
	}else if(div_search.cbo_SellTarget.value == "PR"){
		div_search.cbo_ProgrSt.value = "30"
	}
	//판매대상 변경시 판매기간 초기화
	if(div_search.cbo_ProgrSt.value == "%"){
		div_search.ccp_sale.cal_calFrom.value = "";	
		div_search.ccp_sale.cal_calTo.value = "";	
		div_search.ccp_sale.enable = false;
	}else if(div_search.cbo_ProgrSt.value == "10" || div_search.cbo_ProgrSt.value == "20"){
		div_search.ccp_sale.enable = true;
	}else{
		div_search.ccp_sale.enable = true;
	}	

	if(div_search.cbo_ProgrSt.value == "10"){
		div_search.stc_fromToDate.text = "감정기간";
	}else if(div_search.cbo_ProgrSt.value == "20"){
		div_search.stc_fromToDate.text = "매입기간";
	}else if(div_search.cbo_ProgrSt.value == "40"){
		div_search.stc_fromToDate.text = "판매대행기간";
	}else {
		div_search.stc_fromToDate.text = "판매기간";
	}
	if(div_search.cbo_ProgrSt.value != "%"){
		var arrTerm = div_search.ccp_sale.getData();
		if(gfn_isNull(arrTerm)){
			div_search.ccp_sale.setData(gfn_addDate(gfn_today(),-183),gfn_today(),true); //6개월기본기간 설정
		}
	}	
	
	div_search.ccp_sale.tooltiptext = div_search.stc_fromToDate.text;
	div_search.stc_fromToDate.tooltiptext = div_search.stc_fromToDate.text;
}
/*----------------------------------------------------------------------------------------------
 * 설명   :  form 내에서 esc버튼 클릭시 그리드 데이터셋 초기화 
 * param   : obj - Form
 * param   : e  - KeyEventInfo
 * return  : true/false
-----------------------------------------------------------------------------------------------*/
function Form_onkeyup(obj:Form, e:KeyEventInfo)
{
	if(e.keycode == KEYCODE_ESC) {	//ESC
		ds_main.clearData();
		stc_subTitle.text = "조회결과";
		fn_setInitGrid();
		fn_setInitForm();

		ds_search.clearData();
		ds_search.addRow();
		ds_search.setColumn(0,"SELL_YN","%");
		fn_afterCodeCallBack();
		
		div_search.ccp_sale.cal_calFrom.value = "";	
		div_search.ccp_sale.cal_calTo.value = "";	
		div_search.ccp_sale.enable = false;
		div_search.stc_fromToDate.text = "판매기간";
		div_search.ccp_sale.tooltiptext = div_search.stc_fromToDate.text;
		div_search.stc_fromToDate.tooltiptext = div_search.stc_fromToDate.text;
	}
}
