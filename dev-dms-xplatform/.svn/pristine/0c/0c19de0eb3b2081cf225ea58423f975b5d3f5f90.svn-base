}
// 액셀전체다운로드 다음처리
function fn_nextLoop()
{
	
	var nTotCnt = gfn_toFixedLocaleString(total_record_cnt,0);
	var nNowCnt = gfn_toFixedLocaleString(page * page_size,0);
	div_downInfoBase.div_downPbr.sta_totDownCnt.text = nTotCnt+" 건";
	div_downInfoBase.div_downPbr.sta_nowDownCnt.text = nNowCnt+" 건";
	if ( total_record_cnt <=  page * page_size ) {
		div_downInfoBase.visible = false;
		page_size = 50;
		page = curPage;
		gfn_baseExcelExpert(grd_mainExcel,"FPA가용재고목록","FPA가용재고목록");
	} else {
		//다음페이지 조회
		page++;
		fn_searchMainAll();
	} 
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 상품판매양식다운
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchExcelAll()
{
	var sSvcID		 = "searchExcelAll";
	var sTrID		 = "PEPFPAAvailInveMgmt_pInqFPAAvailInveList";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
