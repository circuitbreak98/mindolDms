-----------------------------------------------------------------------------------------------*/
function div01_btn_02_onclick(obj:Button,  e:ClickEventInfo)
{
	var sParam01 = div01.edt_eqpMdlCd.value;
	var sParam02 = div01.edt_eqpMdlNm.value;
	var sParam03 = "";
	var sParam04 = "";
	if ( e == null ) {
		sParam03 = "key";
	} else {
		sParam03 = "button";
	}

	var arrArgs = {argAutoSelect:true, argCallProgram:"FNR001P", argParam01:sParam01, argParam02:sParam02, argParam03:sParam03, argParam04:sParam04};
	var sRtnVal = gfn_dialog("FBI008P", arrArgs);

	//팝업에서 입력받은 값 리턴
	if ( gfn_isNull(sRtnVal) || sRtnVal == "cancel" ) {
		//취소시 값 제거
		div01.edt_eqpMdlCd.value = "";
		div01.edt_eqpMdlNm.value = "";
		div01.cbo_mFactCd.value  = "";
		div01.cal_fstRgstDt.value = "";
		div01.edt_petNm.value  = "";
	} else {	//Object이용시
		div01.edt_eqpMdlCd.value 	= sRtnVal["supEqpMdlCd"];
		div01.edt_eqpMdlNm.value  = sRtnVal["supEqpMdlNm"];
		div01.cbo_mFactCd.value  = sRtnVal["supMfactCd"];
		div01.cal_fstRgstDt.value  = sRtnVal["supFstRgstDt"];
		div01.edt_petNm.value  = sRtnVal["supPetNm"];
	}
}

