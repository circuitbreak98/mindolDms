/*===============================================================================================
 * [ PART 5 ] Transaction 처리
================================================================================================*/

/*----------------------------------------------------------------------------------------------
 * 설명   : 메인저장 (insert, update)
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_saveMain()
{	
	var sSvcID		 = "saveMain";
	var sTrID		 = "PNREqpStdMgmt_pSaveEqpStd";
	var sPtrn		 = "I";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RS_EQP_STD_FRM=ds_frm";
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}


/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
 	switch(sSvcID) {
		case "saveMain":
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				opener.btn_search_onclick();
				close(true);
			}
			break;
		
		default :
			break;
	}
}

/*===============================================================================================
 * [ PART 7 ] Main Button Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : save
-----------------------------------------------------------------------------------------------*/
function DivBtn_btn_confirm_onclick(obj:Button,  e:ClickEventInfo)
{
	if ( fn_chkBeforeSave() ) fn_saveMain();
}

