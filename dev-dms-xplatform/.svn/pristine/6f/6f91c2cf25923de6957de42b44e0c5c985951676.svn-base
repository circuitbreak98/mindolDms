function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{	
	var sCK_ColId = "BF_SER_NO=일련번호";		
	if (!gfn_checkValueDs(ds_search, sCK_ColId)){		//조회 필수값체크
	
