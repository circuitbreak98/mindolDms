/*----------------------------------------------------------------------------------------------
 * 설명   : save
-----------------------------------------------------------------------------------------------*/
function btn_save_onclick(obj:Button,  e:ClickEventInfo)
{
	if(gfn_getLengthB2(ds_sub1.getColumn(0,"CNSL_MEMO")) >100){
		gfn_message("A","DMS00096","E","",new Array('메모','100'));	//DMS00096 : {0}은/는 {1}자 이상 초과 할수 없습니다.
		div_07.grd_main.setFocus();
		return;
	}
	if ( fn_chkBeforeSave() ) fn_saveMemo();
	
}

/*===============================================================================================
 * [ PART 8 ] Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Form onsetFocus 이벤트
-----------------------------------------------------------------------------------------------*/
function Form_onsetfocus(obj:Form, e:SetFocusEventInfo)
{
	var frmParam = gfn_getFrm("FNR031M");
	if (!gfn_isNull(frmParam)){
		//ds_search.copyRow(0, frmParam.ds_main, frmParam.ds_main.rowposition);
		var sCntrtNo = frmParam.ds_main.getColumn(frmParam.ds_main.rowposition,"CNTRT_NO");
		if(!gfn_isNull(sCntrtNo)) {
			if(sCntrtNo != ds_search.getColumn(0, "CNTRT_NO")) {
				ds_search.setColumn(0, "CNTRT_NO", sCntrtNo);
				fn_searchMain();
			}
		}
	}
}

