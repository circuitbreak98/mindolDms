/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {
			
				for(var i=0; i<ds_poly.getRowCount();++i){
					if(ds_poly.getColumn(i,"PEN_POLY_CL")=="01"){ //중도해지
					
						div_02.cal_prOccrDt1.value = ds_poly.getColumn(i,"RP_OCCR_DT"); //중도해지위약금발생일
						div_02.mae_dmsPenAmt01.value = ds_poly.getColumn(i,"DMS_PEN_AMT"); //DMS 금액
						div_02.mae_ukyPenAmt01.value = ds_poly.getColumn(i,"UKEY_PEN_AMT");//UKEY 금액
						div_02.mae_rpPayAmt01.value = ds_poly.getColumn(i,"RP_PAY_AMT"); //수납금액
						
					}else if(ds_poly.getColumn(i,"PEN_POLY_CL")=="02"){ //반납지연
					
						div_02.cal_prOccrDt2.value = ds_poly.getColumn(i,"RP_OCCR_DT");//반납지연위약금발생일
						div_02.mae_dmsPenAmt02.value = ds_poly.getColumn(i,"DMS_PEN_AMT");//DMS 금액
						div_02.mae_ukyPenAmt02.value = ds_poly.getColumn(i,"UKEY_PEN_AMT");//UKEY 금액
						div_02.mae_rpPayAmt02.value = ds_poly.getColumn(i,"RP_PAY_AMT"); //수납금액
						
					}else if(ds_poly.getColumn(i,"PEN_POLY_CL")=="03"){ //연체가산금
					
						div_02.cal_prOccrDt3.value = ds_poly.getColumn(i,"RP_OCCR_DT");//연체가산금위약금발생일
						div_02.mae_dmsPenAmt03.value = ds_poly.getColumn(i,"DMS_PEN_AMT"); //DMS 금액
						div_02.mae_ukyPenAmt03.value = ds_poly.getColumn(i,"UKEY_PEN_AMT");//UKEY 금액
						div_02.mae_rpPayAmt03.value = ds_poly.getColumn(i,"RP_PAY_AMT");//수납금액
					}
				}
			///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
				div_00.ccp_term.setData(ds_main.getColumn(0,"RENTAL_CNTRT_STA_DT"),ds_main.getColumn(0,"RENTAL_CNTRT_END_DT"));
				div_02.ccp_term.setData(ds_main.getColumn(0,"PP_EFF_PRD_FR_DT"),ds_main.getColumn(0,"PP_EFF_PRD_TO_DT"));
				
				div_01.cal_submitDtm10.value = "";
				div_01.cal_submitDtm20.value = "";
				div_01.cal_submitDtm30.value = "";
				div_01.cal_submitDtm40.value = "";
				
				for(var i=0; i<ds_sub.rowcount; i++) {
					switch (ds_sub.getColumn(i,"APPLF_TYP")) {
						case "10":
							div_01.cal_submitDtm10.value = ds_sub.getColumn(i,"APPLF_SUBMIT_DTM");
							break;
						case "20":
							div_01.cal_submitDtm20.value = ds_sub.getColumn(i,"APPLF_SUBMIT_DTM");
							break;
						case "30":
							div_01.cal_submitDtm30.value = ds_sub.getColumn(i,"APPLF_SUBMIT_DTM");
							break;
						case "40":
							div_01.cal_submitDtm40.value = ds_sub.getColumn(i,"APPLF_SUBMIT_DTM");
							break;
						default:
							break;
					}
				}
				
				var nMax;
				if(ds_sub1.rowcount == 0) {
					nMax = 0;
				} else {
					nMax = toNumber(ds_sub1.getColumn(0,"CNSL_MEMO_SEQ"));
					nMax += 1;
				}
				
				ds_sub1.updatecontrol = false;
				
				ds_sub1.insertRow(0);
				ds_sub1.setColumn(0,"CNTRT_NO",ds_search.getColumn(0,"CNTRT_NO"));
				ds_sub1.setColumn(0,"CNSL_MEMO_SEQ",nMax);
				
				ds_sub1.updatecontrol = true;
			}
			break;
		case "fn_saveMemo":
			if ( iErrorCode == 0 ) {
				var nMax;
				if(ds_sub1.rowcount == 0) {
					nMax = 0;
				} else {
					nMax = toNumber(ds_sub1.getColumn(0,"CNSL_MEMO_SEQ"));
					nMax += 1;
				}
				
				ds_sub1.updatecontrol = false;
				
				ds_sub1.insertRow(0);
				ds_sub1.setColumn(0,"CNTRT_NO",ds_search.getColumn(0,"CNTRT_NO"));
				ds_sub1.setColumn(0,"CNSL_MEMO_SEQ",nMax);
				
				ds_sub1.updatecontrol = true;
				
				gfn_message("A","DMS00000","I");	//MSGI0001 : 정상 처리되었습니다.
			}
			break;
		default:
			break;
	}
}

