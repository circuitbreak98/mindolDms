function fn_chkBeforeSave()	
{
	if(!gfn_isUpdated(ds_sub1))//저장할 테이타셋 ds_Save
	{
		gfn_message("A","DMS00022","I");	//MSGI0020 : 변경된 정보가 없습니다.
		return false;
	}	
	return true;
}


