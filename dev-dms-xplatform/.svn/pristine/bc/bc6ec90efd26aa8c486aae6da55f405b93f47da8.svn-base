/*===============================================================================================
 * [ PART 1 ] Form에서 Include 할 라이브러리(xjs)파일 정의
================================================================================================*/
include "LIB::common.xjs"; //공통함수 호출

/*===============================================================================================
 * [ PART 2 ] Form에서 사용될 전역변수를 선언
================================================================================================*/
 var inputType;
/*===============================================================================================
 * [ PART 3 ] Form Event 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Form Load Event
-----------------------------------------------------------------------------------------------*/
function Form_onload(obj:Form, e:LoadEventInfo)
{
	//공통 Form 초기화
	gfn_setInitPop(obj, e, "FSC011P");
	
 	//Form 초기 개발자 셋팅
 	//공통코드 초기화
 	fn_setInitCode(obj, e);
 	//그리드 초기화
 	fn_setInitGrid(obj, e);
 	//기본값 초기화
 	fn_setInitForm(obj, e);
 	
 	//fn_searchMain();
}

/*===============================================================================================
 * [ PART 4 ] Load 후 초기설정
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitCode(obj:Form, e:LoadEventInfo)
{
	var strCode =  "DMS002=ds_DMS002=A";
		strCode +=  ",DMS002=ds_DMS002G=N";				
	gfn_getCodeDs( this
				  , strCode
				  ,"fn_afterCodeCallBack");				  
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 호출 후 처리
-----------------------------------------------------------------------------------------------*/
function fn_afterCodeCallBack()
{		  	
	Div02.cob_cl.index=0;
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 그리드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitGrid(obj:Form, e:LoadEventInfo)
{
	argCallProgram == "AutrRolMgmtM" ? UserGrid00.selecttype ="multirow" : UserGrid00.selecttype ="row";

}

/*----------------------------------------------------------------------------------------------
 * 설명   : 기본값 설정
-----------------------------------------------------------------------------------------------*/
function fn_setInitForm(obj:Form, e:LoadEventInfo)
{
	//자동조회시 파라메타 전달예제
	page = 1;
	page_size = 30;
	
	if ( !gfn_isNull(argParam01) ) Div02.Edit00.value = argParam01;
	if ( !gfn_isNull(argParam02) ) Div02.Edit01.value = argParam02;
	
	try {
		if ( !gfn_isNull(argParam03) ) {
			inputType = argParam03;
		} else {
			inputType = "button";
		}
		
		if ( !gfn_isNull(argParam04) ) Div02.edt_login_id.value = argParam04;
	}
	catch(e) {

	}
	
	if (!gfn_isNull(argParam01) || !gfn_isNull(argParam02) || !gfn_isNull(argParam04)) {
			btn_search_onclick();
	} else {
		Div02.Edit01.setFocus();
	}
}



/*===============================================================================================
 * [ PART 5 ] Transaction 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	//gfn_setGridTitleCount(stc_subTitle01, ds_userLst_org);	//데이타 건수 clear
	var sSvcID		 = "searchMain";
	var sTrID		 = "PSCUserMgmt_pInqUserLst";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_ncFieldMap";
	var sOutputDs	 = "ds_userLstTemp=RS_USER_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{

	switch(sSvcID) {
		case "searchMain" : 	
			ds_userLst.appendData(ds_userLstTemp,true);	
			gfn_setGridTitleCountPaging(stc_subTitle01, total_record_cnt);
			//div_search.edt_frmId.setFocus();
			
			//Data 1건일 경우 close
			if( ds_userLst.getRowCount() == 1 && inputType == "key"){
				Div00.btn_confirm.click();
			}
			break;
			
		default :
			break;
	}
}

/*===============================================================================================
 * [ PART 7 ] Main Button Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 버튼 Event
-----------------------------------------------------------------------------------------------*/
function button_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj.name) 
	{	
			
		case "btn_confirm" :	//확인
			fn_selectValue(ds_userLst.rowposition);
			break;
			
		case "btn_cancel" : 	//취소
			close("cancel");
			break;
			
		case "btn_close" : 		//버튼X
		
			close("cancel");
			break;	
	}
}

