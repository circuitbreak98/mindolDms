/*----------------------------------------------------------------------------------------------
 * 설명   : 결과값 리턴후 Close
-----------------------------------------------------------------------------------------------*/
//대상값 수정

function fn_selectValue(nRow)
{
	//단건(Object사용)
	if ( nRow == -1 ) return;
	
	var objRtn;
	
	if(argCallProgram == "AutrRolMgmtM"){	 
	   objRtn = new Array;
		var totCnt = ds_userLst.getRowCount();	   
		var idx = 0;
		for(var i = 0; i < totCnt; i++){
			if(ds_userLst.getSelect(i)){
				objRtn[idx] = ds_userLst.getColumn(i,"USER_NO")
							  +","+ds_userLst.getColumn(i,"USER_NM")
							  +","+ds_userLst.getColumn(i,"LGIN_ID")
				idx++;
			}
		}
	 
	} else {	 
	 objRtn = new Object;	 
	 objRtn["value1"] = ds_userLst.getColumn(nRow,"USER_NO");
	 objRtn["value2"] = ds_userLst.getColumn(nRow,"USER_NM");
	 objRtn["value3"] = ds_userLst.getColumn(nRow,"LGIN_ID");
	}
	this.close(objRtn);		

}

function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	// 입력 항목 Validate Check
	if (!gfn_getValidateCompList(Div02)) {	
		return false;
	}
	//trace(ds_DMS002.saveXML());
	//trace(ds_ncFieldMap.getColumn(0,"USER_CL_CD"));
	if(gfn_isEmpty(ds_ncFieldMap.getColumn(0,"USER_CL_CD")))ds_ncFieldMap.setColumn(0,"USER_CL_CD","%"); // USER_CL_CD 항상 '%'  2014-10-13 
	ds_ncFieldMap.setColumn(0,"USER_ST_CD","%"); // USER_ST_CD 항상 '%'  2014-11-28 
	ds_ncFieldMap.setColumn(0,"GRPC_CO_CD","%"); // GRPC_CO_CD 항상 '%'  2014-11-28 
	ds_userLst.clearData();
	page = 1;
	fn_searchMain();
}


/*----------------------------------------------------------------------------------------------
 * 설명   : 수직 스크롤바가 마지막에 도달했을때 페이지 조회 이벤트
 * param   : obj - Grid Object
 * param   : e  - ScrollEventInfo
 * return  : void
-----------------------------------------------------------------------------------------------*/
function grd_main_onvtracklast(obj:Grid, e:ScrollEventInfo)
{
	if ( gfn_isUpdated(ds_userLst) ) {
		gfn_message("A","DMS00027"); //변경중인 데이터가 있어 페이지 조회를 사용 할 수 없습니다.
		return;
	}
	
	if ( total_record_cnt <=  page * page_size ) {
		gfn_message("B","DMS00028","I"); //조회 마지막 페이지 입니다.
		return;
	} else {
		//다음페이지 조회
		page++;
		fn_searchMain();
	} 
}

