	div_search.edt_mdlNm.setFocus();
}

/*===============================================================================================
 * [ PART 5 ] Transaction 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	var sSvcID		 = "searchMain";
	var sTrID		 = "PBIEqpMdlMgmt_pInqEqpMdlLst";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_ncFieldMap";
	var sOutputDs	 = "ds_main=RS_EQP_MDL_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";

	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 단말기 색상조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchSub()
{
	var sSvcID		 = "searchSub";
	var sTrID		 = "PBIEqpMdlMgmt_pInqEqpColorLst";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_ncFieldMap";
	var sOutputDs	 = "ds_eqpColorList=RS_EQP_COLOR_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";

	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 메인저장 (insert, update)
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_saveMain()
{	
	var sSvcID		 = "saveMain";
	var sTrID		 = "PBIEqpMdlMgmt_pSaveEqpMdl";
	var sPtrn		 = ds_main.getRowType(ds_main.rowposition) == Dataset.ROWTYPE_INSERT? "I" : "U";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "";
	if(!gfn_isUpdated(ds_eqpColorList)) {
		sInputDs	 = "RS_EQP_MDL_LIST=ds_main:u";
	} else {
		sInputDs	 = "RS_EQP_MDL_LIST=ds_main:u RS_EQP_COLOR_LIST=ds_eqpColorList:u";
	}
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	pv_sPtrn         = sPtrn;  // 저장Type을 전역변수로 저장
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
 	switch(sSvcID) {
		case "searchMain" : 
			//페이징 관련셋팅
			//ds_main.appendData(ds_eqpTempPage, true);	
			//조회된 첫번째row로 setFocus 
			ds_main.rowposition = (page-1) * page_size ;
			gfn_setGridTitleCountPaging(stc_subTitle01, total_record_cnt);
			cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
			cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
			div_search.edt_mdlNm.setFocus();
			fn_searchSub();
			break;
			
		case "searchSub" : 
			ds_eqp_onrowposchanged();	//단말기색상목록필터
			break;
			
		case "saveMain" : 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//MSGI0001 : 정상 처리되었습니다.
				if (pv_sPtrn == "I") {		// 신규등록일 경우에만 재조회
					ds_ncFieldMap.setColumn(0,"EQP_MDL_CD", ds_main.getColumn(ds_main.rowposition,"EQP_MDL_CD"));
					ds_ncFieldMap.setColumn(0,"EQP_MDL_NM", ds_main.getColumn(ds_main.rowposition,"EQP_MDL_NM"));
					//ds_ncFieldMap.setColumn(0,"MFACT_CD", ds_main.getColumn(ds_main.rowposition,"MFACT_CD"));
					btn_search_onclick();
				} else if (pv_sPtrn == "D") {
					btn_search_onclick();
				}
				Div01.Div02.edt_mdlCd.readonly = true;
			}
			break;
			
		default :
			break;
	}
}

/*===============================================================================================
 * [ PART 7 ] Main Button Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : search
-----------------------------------------------------------------------------------------------*/
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	// 입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	
	ds_main.clearData();
	page = 1;
	fn_searchMain();
	
}

/*----------------------------------------------------------------------------------------------
 * 설명   : reset
-----------------------------------------------------------------------------------------------*/
function btn_reset_onclick(obj:Button,  e:ClickEventInfo)
{
	if ( ds_main.rowposition != -1 && ds_main.getRowType(ds_main.rowposition) != Dataset.ROWTYPE_NORMAL) {		
		if (gfn_message("C","DMS00024","Q")){ //편집중인 데이터가 있습니다. 계속하시겠습니까?
			ds_main.reset();
			Div01.Div02.edt_mdlCd.readonly = true;
		}
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명   : new
-----------------------------------------------------------------------------------------------*/
function btn_new_onclick(obj:Button,  e:ClickEventInfo)
{
	if ( ds_main.rowposition != -1 && ds_main.getRowType(ds_main.rowposition) != Dataset.ROWTYPE_NORMAL) {		
		if (gfn_message("C","DMS00024","Q")){ //편집중인 데이터가 있습니다. 계속하시겠습니까?
			ds_main.reset();
			var row = ds_main.addRow();
			fn_chk_eqpid();
			ds_main.rowposition = row;
			ds_main.setColumn(ds_main.rowposition,"DSTRB_EQP_YN","Y");
			Div01.Div02.edt_mdlNm.setFocus();			
		}
	} else {
		var row = ds_main.addRow();
		fn_chk_eqpid();
		ds_main.rowposition = row;
		ds_main.setColumn(ds_main.rowposition,"DSTRB_EQP_YN","Y");
		Div01.Div02.edt_mdlNm.setFocus();	
	}
	
	
}

