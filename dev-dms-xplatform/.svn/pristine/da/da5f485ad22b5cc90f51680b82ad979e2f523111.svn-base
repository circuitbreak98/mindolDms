/*----------------------------------------------------------------------------------------------
 * 설명   : 오류검증  (fn_checkValueDs,fn_checkValueDsRow 호출)
-----------------------------------------------------------------------------------------------*/
function btn_errChk_onclick(obj:Button,  e:ClickEventInfo)
{
	//그리드 null체크
	if(ds_main.rowcount == "0"){
		gfn_message("A","DMS00004");	//데이터가 존재하지 않습니다. 데이터를 확인해 주세요.
		return false;
	}
	
	for(var i=0; i<ds_main.rowcount; i++){
		ds_main.setColumn(i,"ERR_YN","");
		ds_main.setColumn(i,"ERR_DTL","");
	}
	
	gfn_dsExcelValidCheck(grd_main, ds_mainValid);
	
	var nRow;
	
	ds_constList.clearData();
	
	for(var i=0; i<ds_main.rowcount; i++){
		nRow = ds_dealCo.findRow("DEAL_CO_CD",ds_main.getColumn(i,"IN_DEALCO_CD"));
		if(nRow == -1) {
			if(gfn_isNull(ds_main.getColumn(i,"ERR_YN"))) {
				ds_main.setColumn(i,"ERR_DTL","매입처코드오류");
			} else {
				ds_main.setColumn(i,"ERR_DTL",ds_main.getColumn(i,"ERR_DTL") + "/" + "매입처코드오류");
			}
			ds_main.setColumn(i,"IN_PLC_NM","");
			ds_main.setColumn(i,"ERR_YN","Y");
		} else {
			ds_main.setColumn(i,"IN_PLC_NM",ds_dealCo.getColumn(nRow,"DEAL_CO_NM"));
		}
		
		nRow = ds_eqp.findRow("EQP_MDL_CD",ds_main.getColumn(i,"EQP_MDL_CD"));
		if(nRow == -1) {
			if(gfn_isNull(ds_main.getColumn(i,"ERR_YN"))) {
				ds_main.setColumn(i,"ERR_DTL","단말기모델코드오류");
			} else {
				ds_main.setColumn(i,"ERR_DTL",ds_main.getColumn(i,"ERR_DTL") + "/" + "단말기모델코드오류");
			}
			ds_main.setColumn(i,"EQP_MDL_NM","");
			ds_main.setColumn(i,"ERR_YN","Y");
		} else {
			ds_main.setColumn(i,"EQP_MDL_NM",ds_eqp.getColumn(nRow,"EQP_MDL_NM"));
		}
		
		nRow = ds_DMS100.findRow("CM_CD_ID",ds_main.getColumn(i,"EQP_COLOR_CD"));
		if(nRow == -1) {
			if(gfn_isNull(ds_main.getColumn(i,"ERR_YN"))) {
				ds_main.setColumn(i,"ERR_DTL","색상코드오류");
			} else {
				ds_main.setColumn(i,"ERR_DTL",ds_main.getColumn(i,"ERR_DTL") + "/" + "색상코드오류");
			}
			ds_main.setColumn(i,"ERR_YN","Y");
		}
		
		var strCmptCodes = ds_main.getColumn(i,"CMPT_CD");
		var strCmptCode = strCmptCodes.split(",");
		var strErrMsg = "";
		
		for(var j = 0; j < strCmptCode.length;  j++){
			strCmptCode[j] = gfn_trim(strCmptCode[j]);
			nRow = ds_DMS111.findRow("CM_CD_ID",strCmptCode[j]);
			
			if(nRow == -1) {
				if(!gfn_isNull(strErrMsg)) strErrMsg += ",";
				strErrMsg += strCmptCode[j];
				
			} else {
				var nAddRow = ds_constList.addRow();
				ds_constList.setColumn(nAddRow,"PAR_NO",i);	//단말기DS와 연계번호 설정
				ds_constList.setColumn(nAddRow,"CMPT_CD",strCmptCode[j]);
				ds_constList.setColumn(nAddRow,"CHECKED","1");
				ds_constList.setColumn(nAddRow,"OUT_DTL_CD","10");
				ds_constList.setColumn(nAddRow,"CMPT_USE_QTY","1");
			}
		}
		
		if(!gfn_isNull(strErrMsg)){
			if(gfn_isNull(ds_main.getColumn(i,"ERR_YN"))) {
				ds_main.setColumn(i,"ERR_DTL","구성품코드오류["+strErrMsg+"]");
			} else {
				ds_main.setColumn(i,"ERR_DTL",ds_main.getColumn(i,"ERR_DTL") + "/" + "구성품코드오류["+strErrMsg+"]");
			}
			ds_main.setColumn(i,"ERR_YN","Y");
		} else {
			ds_main.setColumn(i,"PAR_NO",i); //구성품DS와 연계번호 설정
		}
	}
	
	ds_mainChk.clearData();
	
	for(var i=0; i<ds_main.rowcount; i++){
		nRow = ds_mainChk.addRow();
		ds_mainChk.setColumn(nRow, "EQP_SER_NO", ds_main.getColumn(i,"EQP_SER_NO"));
		ds_mainChk.setColumn(nRow, "EQP_IMEI_NO", ds_main.getColumn(i,"EQP_IMEI_NO"));
	}
	
	//sync로 실행됨
	fn_searchChk();
	
	for(var i=0; i<ds_mainChk.rowcount; i++){
		if(toNumber(ds_mainChk.getColumn(i,"EQP_SER_CNT")) > 0) {
			if(gfn_isNull(ds_main.getColumn(i,"ERR_YN"))) {
				ds_main.setColumn(i,"ERR_DTL","일련번호중복");
			} else {
				ds_main.setColumn(i,"ERR_DTL",ds_main.getColumn(i,"ERR_DTL") + "/" + "일련번호중복");
			}
			ds_main.setColumn(i,"ERR_YN","Y");
		} else if (ds_mainChk.getCaseCount("EQP_SER_NO=='"+ds_mainChk.getColumn(i,"EQP_SER_NO")+"'") > 1) {
			if(gfn_isNull(ds_main.getColumn(i,"ERR_YN"))) {
				ds_main.setColumn(i,"ERR_DTL","일련번호중복");
			} else {
				ds_main.setColumn(i,"ERR_DTL",ds_main.getColumn(i,"ERR_DTL") + "/" + "일련번호중복");
			}
			ds_main.setColumn(i,"ERR_YN","Y");
		}
		
		if(toNumber(ds_mainChk.getColumn(i,"EQP_IMEI_CNT")) > 0) {
			if(gfn_isNull(ds_main.getColumn(i,"ERR_YN"))) {
				ds_main.setColumn(i,"ERR_DTL","IMEI번호중복");
			} else {
				ds_main.setColumn(i,"ERR_DTL",ds_main.getColumn(i,"ERR_DTL") + "/" + "IMEI번호중복");
			}
			ds_main.setColumn(i,"ERR_YN","Y");
		} else if (ds_mainChk.getCaseCount("EQP_IMEI_NO=='"+ds_mainChk.getColumn(i,"EQP_IMEI_NO")+"'") > 1) {
			if(gfn_isNull(ds_main.getColumn(i,"ERR_YN"))) {
				ds_main.setColumn(i,"ERR_DTL","IMEI번호중복");
			} else {
				ds_main.setColumn(i,"ERR_DTL",ds_main.getColumn(i,"ERR_DTL") + "/" + "IMEI번호중복");
			}
			ds_main.setColumn(i,"ERR_YN","Y");
		}
	}
	
	if(ds_main.findRow("ERR_YN","Y") > -1) {
		gfn_message("A","DMS00067"); //엑셀 업로드 자료에 오류가 있습니다
		return false;
	} else if (obj.name == "btn_save"){
		return true;
	} else {
		gfn_message("A","DMS00070");	//엑셀 오류검사 결과 정상입니다.
		return true;
	}
}

