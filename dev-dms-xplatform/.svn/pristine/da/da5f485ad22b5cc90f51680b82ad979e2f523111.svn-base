				ds_main.setColumn(e.row,"IN_PLC_NM",sRtnVal["supDealCoNm"]);
			}	
			break;
		case "bind:EQP_MDL_NM":
			var sParam01 = ds_main.getColumn(e.row,"EQP_MDL_CD");
