			}	
			break;
		case "bind:CMPT_CD":
			div_main.move(e.clientX, e.clientY+20);
			
			var strCmpt = ds_main.getColumn(e.row,"CMPT_CD");
			var arrCode = strCmpt.split(",");
			var nRow;
			
			for(var i=0; i<ds_DMS111sub.rowcount; i++){
				ds_DMS111sub.setColumn(i,"CHECK","0");
			}
			
			for(var i=0; i<arrCode.length; i++){
				nRow = ds_DMS111sub.findRow("CM_CD_ID",arrCode[i]);
				if(nRow != -1) {
					ds_DMS111sub.setColumn(nRow,"CHECK","1");
				}
			}
			
			div_main.visible = true;
			break;
		default:
			break;
	}
}

function button_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj.name) 
	{	
		case "btn_confirm" :	//확인
			var strCmpt = "";
			for(var i=0; i<ds_DMS111sub.rowcount; i++){
				if(ds_DMS111sub.getColumn(i,"CHECK") == "1") {
					if(gfn_isNull(strCmpt)) {
						strCmpt = ds_DMS111sub.getColumn(i,"CM_CD_ID");
					} else {
						strCmpt += ","+ ds_DMS111sub.getColumn(i,"CM_CD_ID");
					}
				}
			}
			ds_main.setColumn(ds_main.rowposition,"CMPT_CD",strCmpt);
			div_main.visible = false;
			break;
		case "btn_cancel" : 	//취소
			div_main.visible = false;
			break;
		default:
			break;	
	}
}

