/*----------------------------------------------------------------------------------------------
 * 설명   : div drag
-----------------------------------------------------------------------------------------------*/
function div_main_ondrag(obj:Div, e:DragEventInfo)
{
	xPos = obj.position.left - e.screenX; 
	yPos = obj.position.top - e.screenY; 
	return true; 
}

/*----------------------------------------------------------------------------------------------
 * 설명   : div drag move
-----------------------------------------------------------------------------------------------*/
function div_main_ondragmove(obj:Div, e:DragEventInfo)
{
	obj.move(e.screenX + xPos, e.screenY + yPos);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : grid 콤보나 달력 닫힐때
-----------------------------------------------------------------------------------------------*/
function grd_main_oncloseup(obj:Grid, e:GridEditEventInfo)
{
	var arrBind = obj.getCellProperty("body",e.col,"text").split(":");
	if(!gfn_isNull(arrBind[1])) {
		ds_main.setColumn(e.row,arrBind[1],e.value);
	}
}

