/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {
				var Mtotal = "  총 "+"<fc v='#31a3dc'>"+ds_sum.getColumn(0,"TOTAL_CNT")
							+"</fc><b v='false'><fc v='#77787a'>건, 환급보험료 합계: </fc></b>"+"<fc v='#31a3dc'>"
							+gfn_toFixedLocaleString(ds_sum.getColumn(0,"TOTAL_SUM"), '@@@,@@@,@@@')+"</fc><b v='false'><fc v='#77787a'>원 </fc></b>";
				fn_setGridTitle(stc_subTitle,Mtotal);

				var Stotal = "  총 "+"<fc v='#31a3dc'>0"
							+"</fc><b v='false'><fc v='#77787a'>건, 환급보험료 합계: </fc></b>"+"<fc v='#31a3dc'>"
							+"0</fc><b v='false'><fc v='#77787a'>원 </fc></b>";			
				fn_setGridTitle(stc_subMTitle,Stotal);
			}
		break;
		case "searchDeail":
			if ( iErrorCode == 0 ) {			
				var Stotal = "  총 "+"<fc v='#31a3dc'>"+ds_sum_dtl.getColumn(0,"TOTAL_CNT")
							+"</fc><b v='false'><fc v='#77787a'>건, 환급보험료 합계: </fc></b>"+"<fc v='#31a3dc'>"
							+gfn_toFixedLocaleString(ds_sum_dtl.getColumn(0,"SGI_FEE_SUM"), '@@@,@@@,@@@')+"</fc><b v='false'><fc v='#77787a'>원 </fc></b>";				
				fn_setGridTitle(stc_subMTitle,Stotal);
			}		
		break;
		case "AllExcel":
			if (iErrorCode == 0) {
				var dsbind = gfn_makeGrid(ds_AllExcel);
				dsbind.binddataset = "ds_AllExcel";
				dsbind.setFormatColProperty("2","size","100");
				dsbind.setFormatColProperty("4","size","100");
				dsbind.setCellProperty("Body",0,"displaytype","text");
				dsbind.setCellProperty("Body",0,"mask","@@@@-@@");				
				dsbind.setCellProperty("Body",2,"style","align:left");
				dsbind.setCellProperty("Body",3,"displaytype","date");
				dsbind.setCellProperty("Body",4,"displaytype","text");
				dsbind.setCellProperty("Body",5,"displaytype","date");
				dsbind.setCellProperty("Body",6,"style","align:right");
				dsbind.setCellProperty("Body",7,"style","align:right");
				gfn_baseExcelExpert(dsbind,"보증보험료 해지 상세 목록","보증보험료 해지 상세 목록");
			}
			break;
		
		case "handlingSlip":
			if ( iErrorCode == 0 ) {
				ds_main.clearData();
				ds_list.clearData();
				btn_search.click();
			}
		break;		
		case "reSum" : 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//MSGI0003 :정상 처리되었습니다.
				btn_search.click();
			}
		break;	
		case "ReState" :
			if ( iErrorCode == 0 ) {
				btn_search.click();
			}		
		break;			
		default :
			break;
	}
}

