/*----------------------------------------------------------------------------------------------
 * 설명   : 전표생성
 * param   : obj - Button
 * param   : e  - ClickEventInfo
 * return  : void
-----------------------------------------------------------------------------------------------*/
function btn_slipI_onclick(obj:Button,  e:ClickEventInfo)
{
	var dt = ds_search.getColumn(0,"FEE_YM");
	var yy = dt.substring(0,4);
	var mm = dt.substring(4,6);
	var cnt = 0;
	var sum = 0;
	
	if(ds_main.findRow("CHK","1") == -1){
		//체크대상없음
		gfn_message("A","DMS00020","I"); // 선택된 정보가 없습니다.
		return;
	}

	//ds_maintemp.clearData();
	for(i=0; i<ds_main.getRowCount(); i++)
	{
		if(ds_main.getColumn(i,"CHK")=='1')
		{
			if( ds_main.getColumn(i,"MATCHED_YN") != 'Y'){
				gfn_message("A","DMS00160","E", "", "환급보험료"); //{0}가 일치하지 않은 데이타가 있습니다.
				return false;
			}
			
			if( gfn_isNull(ds_main.getColumn(i,"SLIP_ST_CD")) || ds_main.getColumn(i,"SLIP_ST_CD") == '00' || ds_main.getColumn(i,"SLIP_ST_CD") == '05')
			{
				/*
				var mrow = ds_maintemp.addRow();
				ds_maintemp.copyRow(mrow,ds_main,i);
				*/
				cnt += parseInt(ds_main.getColumn(i,"M_CNT"))
				sum += parseInt(ds_main.getColumn(i,"M_PRC"))
				ds_main.setColumn(i,"CASE_WHEN","C");
				
			}else{
			  	gfn_message("A","DMS00071","E"); //이미 처리된 건이 있습니다.
			  	//ds_sliptemp.clearData();
				return false;
			}
		}
	}	

	var st = "해당월("+yy+"년 "+mm+"월) "+cnt
		+"건, 총 금액 "+gfn_toFixedLocaleString(sum, '@@@,@@@,@@@')
		+"원에 대한  전표를 생성 하시겠습니까?" ;
						
	if(gfn_message("C",st,"I","" ,"")){
		fn_handlingSlip();
	}else {
		//ds_maintemp.clearData();
		return false;			
	}
}

