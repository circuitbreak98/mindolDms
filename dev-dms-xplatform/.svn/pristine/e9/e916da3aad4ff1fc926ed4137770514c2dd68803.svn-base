/*----------------------------------------------------------------------------------------------
 * 설명   : 조회전 유효성 검사
 * param   : 
 * return  : true/false
-----------------------------------------------------------------------------------------------*/
function fn_chkBeforeSearch()	
{
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	
	var sYm = div_search.cmc_ym.getDate();
	
	if(gfn_isNull(sYm)){
		gfn_message("A","DMS00002","E","","기준년월");  //필수입력항목 {0}이/가 누락되었습니다.
		return false;
	}
	if(ds_search.getColumn(0,"CNTRT_YM_TS") == "01") {
		ds_search.setColumn(0,"CNTRT_FR_DTM",sYm+"01");
		ds_search.setColumn(0,"CNTRT_TO_DTM",sYm+cv_sDate);
	} else if (ds_search.getColumn(0,"CNTRT_YM_TS") == "02") {
		ds_search.setColumn(0,"CNTRT_FR_DTM",gfn_addDate(sYm+cv_sDate,1));
		ds_search.setColumn(0,"CNTRT_TO_DTM",gfn_getLastDate(sYm+"01"));
	} else {
		ds_search.setColumn(0,"CNTRT_FR_DTM",sYm+"01");
		ds_search.setColumn(0,"CNTRT_TO_DTM",gfn_getLastDate(sYm+"01"));
	}
	
	return true;
}



/*----------------------------------------------------------------------------------------------
 * 설명   : Form onsetFocus 이벤트
-----------------------------------------------------------------------------------------------*/
function Form_onsetfocus(obj:Form, e:SetFocusEventInfo)
{
	
	div_search.cbo_cntrYmTs.index=0;
	
	var frmParam = gfn_getFrm("FNR025M");
	if (!gfn_isNull(frmParam)){

		var sCntrtNo1 = frmParam.ds_main.getColumn(frmParam.ds_main.rowposition,"AGN_CD");    /* 대리점코드 */
		var sCntrtNo2 = frmParam.ds_main.getColumn(frmParam.ds_main.rowposition,"AGN_NM");    /* 대리점명 */
		var sCntrtNo3 = frmParam.ds_main.getColumn(frmParam.ds_main.rowposition,"PAY_YM");    /* 지급년월 */
		var sCntrtNo4 = frmParam.ds_main.getColumn(frmParam.ds_main.rowposition,"PAY_YM_TS"); /* 지급차수 */
		var sCntrtNo5 = frmParam.ds_main.getColumn(frmParam.ds_main.rowposition,"DEALCO_BLICENS_NO");/*사업자번호*/
		
		if(!gfn_isNull(sCntrtNo1) && !gfn_isNull(sCntrtNo4) && !gfn_isNull(sCntrtNo5)) {
			ds_search.setColumn(0,"AGN_CD",sCntrtNo1);
			ds_search.setColumn(0,"AGN_NM",sCntrtNo2);
			div_search.cmc_ym.setDate(sCntrtNo3);
			ds_search.setColumn(0,"CNTRT_YM_TS",sCntrtNo4);
			ds_search.setColumn(0, "DEALCO_BLICENS_NO", frmParam.ds_main.getColumn(frmParam.ds_main.rowposition, "DEALCO_BLICENS_NO"));
	
			btn_search_onclick();
		}
	}
}


