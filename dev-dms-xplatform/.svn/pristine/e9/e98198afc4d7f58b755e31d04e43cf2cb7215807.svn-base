function fn_resetGridColSize(){
	if(fvPrchGb == div_search.cbo_prchGb.value){
		return;
	}

	fvPrchGb = div_search.cbo_prchGb.value;
	
	var numTemp = new Number(0);
	for(var i = 0; i < grd_main.getCellCount("Head"); i++){
		numTemp = i;
		vColNo = numTemp.toString();
		grd_main.setFormatColProperty(i,"size",ds_gridPro.getColumn(0,vColNo));
	}
	//출력제외대상 정리
	if(div_search.cbo_prchGb.value == "1"){ //매입대상
		grd_main.setFormatColProperty(1,"size","0"); //CNSL_DT
		grd_main.setFormatColProperty(4,"size","0"); //HQ_NM
		grd_main.setFormatColProperty(5,"size","0"); //MARKET_CD
		grd_main.setFormatColProperty(6,"size","0"); //MARKET_NM
		grd_main.setFormatColProperty(9,"size","0"); //UKEY_SUB_CD
		grd_main.setFormatColProperty(14,"size","0"); //EQP_MDL_CD
		grd_main.setFormatColProperty(15,"size","0"); //CNSL_MGMT_NO
		grd_main.setFormatColProperty(18,"size","0"); //BANK_CD
		grd_main.setFormatColProperty(19,"size","0"); //ACCO_NO
		grd_main.setFormatColProperty(21,"size","0"); //EVALCNSLR_SUGG
	}else{ //매입확인
		grd_main.setFormatColProperty(13,"size","0"); //PRCH_AMT
		grd_main.setFormatColProperty(14,"size","0"); //EQP_MDL_CD
		grd_main.setFormatColProperty(15,"size","0"); //CNSL_MGMT_NO
		grd_main.setFormatColProperty(20,"size","0"); //EQP_ST
	}
	grd_main.autofittype = "none";
	grd_main.anchor = "all";
	//grd_main.reDraw();
}
