function fn_setDetailUserInfo(bSecureYn)
{  
	//개인정보조회 콜백함수에서 호출한 경우 
	if(!gfn_isNull(bSecureYn) && bSecureYn){
		var sName = ds_prnInfoInq.lookup("PRN_INFO_INQ_ITEM_CD", DMS007_NAME,"PRN_INFO_INQ_CTT");    
		var sHpNo=  ds_prnInfoInq.lookup("PRN_INFO_INQ_ITEM_CD", DMS007_HPNO,"PRN_INFO_INQ_CTT");    
		var sAccount=  ds_prnInfoInq.lookup("PRN_INFO_INQ_ITEM_CD", DMS007_ACCOUNT,"PRN_INFO_INQ_CTT");    
		if(!gfn_isEmpty(sName)) ds_main.setColumn(ds_main.rowposition, "DPSTR",sName);
		if(!gfn_isEmpty(sHpNo)) ds_main.setColumn(ds_main.rowposition, "TEL_NO",sHpNo);
