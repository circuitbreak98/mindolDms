// 액셀전체다운로드 다음처리
function fn_nextLoop()
{
	
	if ( total_record_cnt <=  page * page_size ) {
		page_size = 50;
		if(fvPrchGb == "1"){
			gfn_baseExcelExpert(grd_mainExcelAll_01,"매입대상목록","매입대상목록");
		}else{
			gfn_baseExcelExpert(grd_mainExcelAll_02,"매입확인목록","매입확인목록");
		}
	} else {
		//다음페이지 조회
		page++;
		fn_searchExcelAll();
	} 
}

/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
				fn_resetGridColSize();
				//if(ds_main.rowcount > 0){
				//	ds_main.rowposition = 0;
				//}
			}
		break;
		case "searchExcelAll" : 
			if ( iErrorCode == 0 ) {
				ds_mainExcelAll.appendData(ds_mainTemp,true);
				fn_nextLoop();
			}
		break;
		default:
		break;
	}
}
/*----------------------------------------------------------------------------------------------
  * 설명   : 사용자암호화컬럼조회 처리
-----------------------------------------------------------------------------------------------*/
function fn_callBackPsnInfo(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg){

	switch(sSvcID) {
		
		case "searchSecureInfo" : 
			if ( iErrorCode == 0 ) {
			    //상세사용자설정
			    fn_setDetailUserInfo(true);
			}
			break;	
		default :
			break;
	}
}
