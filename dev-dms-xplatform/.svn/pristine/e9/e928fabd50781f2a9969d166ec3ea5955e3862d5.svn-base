{		
	var frYm = div_search.cmc_frYm.getDate();
	var toYm = div_search.cmc_toYm.getDate();

	if (frYm > toYm){
		div_search.cmc_frYm.setDate(frYm);
	    div_search.cmc_toYm.setDate(frYm);
	    gfn_message("A","DMS00093","E");
	}
