	var iCurrPage = ds_param.getColumn(0,"page");
	var iPageSize = ds_param.getColumn(0,"pageSize");
	var iTotalCnt = ds_param.getColumn(0,"totalRecordCnt");
	
	if ( iTotalCnt <=  iCurrPage * iPageSize ) 
	{
		gfn_message("A","모든 자료가 조회 되었습니다.","I");
