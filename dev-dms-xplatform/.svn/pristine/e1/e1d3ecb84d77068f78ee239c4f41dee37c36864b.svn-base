/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
 	switch(sSvcID) 
 	{
		case "searchMain" : 
			if ( iErrorCode == 0 ) 
			{
				if( ds_list.getRowCount() == 0 )
				{
					ds_list.addRow();
				}
			} else {
				gfn_message("A",ncMsg);
			}
			ds_upFile.addColumn("fullpath","string");
			
			/*단말기평가정보등록테이블에 존재하는지 여부 검사 존재할경우 저장버튼 enable = false처리*/
			if(ds_EqpEvalYn.getColumn(ds_EqpEvalYn.rowposition,"SVC_MGMT_NO_CNT")  > 0){
				btn_save.enable = false;
			}else {
				btn_save.enable = true;
			}
			break;
			
		case "saveMain" : 
			if ( iErrorCode == 0 ) 
			{
				gfn_message("A","DMS00000","I");	//MSGI0001 : 정상 처리되었습니다.
				fn_searchMain();
				this.close();
			}
			break;
		default :
			break;
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명   : FileUpload CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg, objRtnDs
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBackFile(sSvcID, iErrorCode, sErrorMsg, objRtnDs)
{
 	switch(sSvcID) 
 	{
		case "uploadFile":
			for(i=0;i<ds_upFile.rowcount;i++) 
			{
				ds_upFile.setColumn(i,"FILE_ID",objRtnDs.getColumn(i,"ncUploadedFileId"));
			}
			ds_upFile.filter("");
			fn_saveMain();
			break;
		default :
			break;
	}
}

