	return true;
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 변상금 전체금액 자동합계
-----------------------------------------------------------------------------------------------*/
function ds_main_onvaluechanged(obj:Dataset,  e:DSColChangeEventInfo)
{
	if ( e.columnid == "USE_YN" || e.columnid == "PEN") {
		var totAmt = 0;
		
		if(ds_main.rowposition == '0'){ //0번째 로우 클릭시 감정결과:미반납

			if(ds_main.getColumn(0,"USE_YN") == "1"){
				Div01.cbo_eqpJdgRsltCd.index = "3"; //미반납->파손 '03' 변경(2015.08.28)
				fn_sal(); //변상금계산 메소드
			} else {
				ds_list.setColumn(0,"EQP_CMP_AMT_TOT","0");
				ds_list.setColumn(0,"EQP_CMP_REVIS_AMT","0");
				Div01.cbo_eqpJdgRsltCd.index = "1"; //정상
			}
		} else if(ds_main.rowposition != '0') { //0번째 아닌 다른 로우 클릭시 감정결과:파손

				var cnt = ds_main.findRowExpr("(USE_YN == '1' || USE_YN == 'N')");

				if( cnt > 0) {

					Div01.cbo_eqpJdgRsltCd.index = "3"; //파손
				}else {
					Div01.cbo_eqpJdgRsltCd.index = "1"; //정상
				}
			var maxAmt = 0;
			var sum = 0;
			for(i=0; i<=ds_grp.getRowCount(); i++) {
			
				maxAmt = ds_main.getCaseMax("(USE_YN == '1' || USE_YN == 'N') && INSP_ITM_CD=='" + ds_grp.getColumn(i,"CD_ID")+"'","PEN",0);
				if( !gfn_isNull(maxAmt) ){
					sum += parseInt(maxAmt);
				}

				ds_main.setColumn(ds_main.rowposition,"EQP_CMP_AMT_TOT",sum);
				ds_list.setColumn(ds_list.rowposition,"EQP_CMP_AMT_TOT",sum);
				ds_list.setColumn(ds_list.rowposition,"EQP_CMP_REVIS_AMT",sum);
								
				var sal = ds_search.getColumn(0,"OUT_PRC"); //출고가
				var result = ds_usedt2.getColumn(0,"USE_DT"); //사용월수(손해배상금미반납)				
				var rsltSal = (sal * ( (36 - result)/36) ); //변상금액(max)비교로직
				var wonrsltSal2 = Math.floor(rsltSal/10) * 10;
				
				if(sum > wonrsltSal2){ //변상금액(max)가 단말기평가잔존가보다 클경우
					Div01.mae_pen.value = wonrsltSal2;
					Div01.mae_pen00.value = wonrsltSal2;
				}else {
					Div01.mae_pen.value = sum;
					Div01.mae_pen00.value = sum;
				}
			}
		}
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 감정결과 '미반납'(암호잠금) 일 경우 변상금 계산 로직
-----------------------------------------------------------------------------------------------*/
function fn_sal(){

	var sal = ds_search.getColumn(0,"OUT_PRC"); //출고가
	
	var rentStdt = ds_search.getColumn(0,"RENT_STA_DT"); //계약시작일
	
	/**************************************************
	   해지일이 공휴일시 익익영업일계산로직추가
	**************************************************/
	var ds_dt = ds_holydt.getColumn(0,"HOLY_DT"); //(대리점반납일)+31일, 택배발송일 공휴일체크

	//사용월수 
	
	var result = ds_usedt.getColumn(0,"USE_DT"); //사용월수(손해배상금미반납)
	var rsltSal = (sal * ( (36 - result)/36) ) * 1.005; //-	손해배상금(미반납) = [출고가 * (36 – 사용월수)/36] * 1.005
	var wonrsltSal2 = Math.floor(rsltSal/10) * 10;
	
	//감정한 파손금액이 출고가보다 클경우 출고가를 파손금액으로 설정
	if(ds_search.getColumn(0,"OUT_PRC") < ds_list.getColumn(0,"EQP_CMP_AMT_TOT",wonrsltSal2)){
		Div01.mae_pen.value = ds_search.getColumn(0,"OUT_PRC");
		Div01.mae_pen00.value = ds_search.getColumn(0,"OUT_PRC");
		ds_list.setColumn(0,"EQP_CMP_AMT_TOT",Div01.mae_pen.value);
		ds_list.setColumn(0,"EQP_CMP_REVIS_AMT",Div01.mae_pen00.value);
	} else {
		Div01.mae_pen.value = wonrsltSal2;
		Div01.mae_pen00.value = wonrsltSal2;
		ds_list.setColumn(0,"EQP_CMP_AMT_TOT",wonrsltSal2);
		ds_list.setColumn(0,"EQP_CMP_REVIS_AMT",wonrsltSal2);
	}

}

