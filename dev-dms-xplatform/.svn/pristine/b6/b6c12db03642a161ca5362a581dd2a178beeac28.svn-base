	ds_Bksave.setColumn(ds_main.rowposition,"EQP_MDL_CD",ds_main.getColumn(ds_main.rowposition,"EQP_MDL_CD"));
	ds_Bksave.setColumn(ds_main.rowposition,"EQP_SER_NO",ds_main.getColumn(ds_main.rowposition,"EQP_SER_NO"));
	ds_Bksave.clearData();
	ds_Bksave.addRow();
	ds_Bksave.copyRow(0,ds_main,ds_main.rowposition);
