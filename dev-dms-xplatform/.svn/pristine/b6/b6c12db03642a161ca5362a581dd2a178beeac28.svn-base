/*----------------------------------------------------------------------------------------------
 * 설명   : 교품확정버튼클릭
-----------------------------------------------------------------------------------------------*/
function btn_expartAccept_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_main.rowposition < 0){
		return;
	}
	if(ds_main.getColumn(ds_main.rowposition,"PROC_ST") != "2"){
		return;
	}
	if(!gfn_isNull(ds_main.getColumn(ds_main.rowposition,"EX_EQP_SER_NO"))){
		return;
	}

	var sParam01 = ds_main.getColumn(ds_main.rowposition,"EQP_MDL_CD"); //모델명
	if(gfn_isEmpty(sParam01)){
		return;
	}
	var sParam02 = ds_main.getColumn(ds_main.rowposition,"EQP_MDL_NM"); //모델명
	var sParam03 = ds_main.getColumn(ds_main.rowposition,"EQP_SER_NO"); //모델명
	var arrArgs = {argAutoSelect:true, argCallProgram:"FPR003M", argParam01:sParam01, argParam02:sParam02, argParam03:sParam03};
	var sRtnVal = gfn_dialog("FPR025P", arrArgs);
	//팝업에서 입력받은 값 리턴
	if ( gfn_isNull(sRtnVal) || sRtnVal == "cancel" ) {
		//취소시 값 제거
	} else {	//Object이용시
		ds_main.setColumn(ds_main.rowposition,"EX_EQP_SER_NO",sRtnVal["eqpSerNo"]);
		ds_main.setColumn(ds_main.rowposition,"EX_EQP_IMEI_NO",sRtnVal["eqpImeiNo"]);
		ds_main.setColumn(ds_main.rowposition,"EX_EQP_MDL_NM",ds_main.getColumn(ds_main.rowposition,"EQP_MDL_NM"));
		ds_main.setColumn(ds_main.rowposition,"EX_EQP_COLOR_CD",sRtnVal["eqpColorCd"]);
		ds_main.setColumn(ds_main.rowposition,"EXPART_DT",gfn_today());
		ds_main.setColumn(ds_main.rowposition,"EX_ASSET_NO",sRtnVal["assetNo"]);
		ds_save.clearData();
		ds_save.addRow();
		ds_save.copyRow(0,ds_main,ds_main.rowposition);
		fn_saveMain();
	}

}
/*----------------------------------------------------------------------------------------------
 * 설명   : 교품확정엑셀출력버튼클릭
-----------------------------------------------------------------------------------------------*/
function btn_expartAcceptExcel_onclick(obj:Button,  e:ClickEventInfo)
{
	ds_outExcel.clearData();
	ds_outExcel.copyData(ds_main);
	ds_outExcel.filter("PROC_ST == '2' && EX_EQP_SER_NO != ''"); //교품건필터
	//지점코드 6자리 넘으면 자르고 엑셀다운로드
	for(var i = 0; i < ds_outExcel.getRowCount(); i++){
		var agn = ds_outExcel.getColumn(i,"AGN_CD");
		var length = agn.length;
		if(length > 6){
			 ds_outExcel.setColumn(i,"AGN_CD", agn.substr(0,6));
		}
	}
	if(ds_outExcel.rowcount <= 0){
		gfn_message("A","DMS00021","I"); //조회된 정보가 없습니다.
		return;
	}
	gfn_baseExcelExpert(grd_expartExcel,"SKT교품확정목록","SKT교품확정목록");
}
