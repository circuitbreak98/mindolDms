function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	var arrTerm = div_search.ccp_inClctDt.getData();
	ds_search.setColumn(0,"CLCT_FR_DT",arrTerm[0]);
	ds_search.setColumn(0,"CLCT_TO_DT",arrTerm[1]);
	
	if(gfn_isNull(arrTerm[0]) || gfn_isNull(arrTerm[1])){
		gfn_message("A","DMS00002","E","","회수일자");  //필수입력항목 {0}이/가 누락되었습니다.
		return false;
	}
	
	// 입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	arrTerm = div_search.ccp_acceptDt.getData();
	if((!gfn_isNull(arrTerm[0]) && gfn_isNull(arrTerm[1])) || (!gfn_isNull(arrTerm[1]) && gfn_isNull(arrTerm[0]))){
		gfn_message("A","조회조건의 확정일자는 시작과 종료 일자를 모두 입력 하셔야 합니다.","E");  //
		return false;
	}
	ds_search.setColumn(0,"ACCEPT_FR_DT",arrTerm[0]);
	ds_search.setColumn(0,"ACCEPT_TO_DT",arrTerm[1]);
	
	//조회함수 호출 
	fn_searchMain();
}
