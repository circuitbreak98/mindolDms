-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	ds_main.clearData();		//데이타셋 clear
	//trace("\n=======================\n"+ds_search.saveXML());
	
	var sSvcID		 = "searchMain";
	var sTrID		 = "PPRSKTEqpExpartBackRgt_pInqSKTEqpExpartBackRegList";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sOutputDs	 = "ds_main=RS_EXPART_BACK_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";

	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 메인저장
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_saveMain() {
    var sSvcID		 = "saveMain";
	var sTrID		 = "PPRSKTEqpExpartBackReg_pExpartObjEqpReg";
	var sPtrn		 = "U";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RQ_EXPART_BACK_LIST=ds_save";
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";

	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc); 

} 
/*----------------------------------------------------------------------------------------------
 * 설명   : 반환확정 메인저장
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_BKsaveMain() {
    var sSvcID		 = "BKsaveMain";
	var sTrID		 = "PPRSKTEqpExpartBackReg_pBackObjEqpReg";
	var sPtrn		 = "C";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RQ_BACK_LIST=ds_Bksave";
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";

	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc); 

} 

/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
 	//trace("sSvcID ==>> "+sSvcID+" : iErrorCode ==>> "+iErrorCode+" : sErrorMsg ==>> "+sErrorMsg);
 	//trace("ncMsgId ==>> "+ncMsgId+" : ncMsg ==>> "+ncMsg);
	switch(sSvcID) {
		case "searchMain" : 
			if ( iErrorCode == 0 ) {
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
			}
			break;
			
		case "saveMain" : 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I"); //정상 처리되었습니다.
			} else {
				gfn_message("A",ncMsg);
			}
			fn_searchMain();
			break;
		
		case "BKsaveMain" : 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I"); //정상 처리되었습니다.
			} else {
				gfn_message("A",ncMsg);
			}
			fn_searchMain();
			break;	
		default :
			break;
	}
}

