function btn_confirm_onclick(obj:Button,  e:ClickEventInfo)
{
	//휴대폰번호 10자리입력시 space제거
	div_info.MaskEdit00.value = gfn_removeSpace(div_info.MaskEdit00.value); 

	if ( fn_chkBeforeSave() ) {
		// 이름,이메일,휴대폰번호 수정여부 Check
		fn_chkColumnChange();
		fn_saveMain();
	}
}

