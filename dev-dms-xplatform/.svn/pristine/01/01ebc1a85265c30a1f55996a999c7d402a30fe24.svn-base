function fn_setInitForm(obj:Form, e:LoadEventInfo)
{
	ds_chgMyInfo.updatecontrol = false;
	div_info.sta_UserInfo.text = "<fc v='#333333'><b v='true'>"+gfn_getUserInfo("USER_NM")+"</b></fc>"+"("+gfn_getUserInfo("USER_NO")+")";	
	
	ds_chgMyInfo.setColumn(0,"USER_NO",gfn_getUserInfo("USER_NO"));
	ds_chgMyInfo.setColumn(0,"LGIN_ID",gfn_getUserInfo("LGIN_ID"));
	ds_chgMyInfo.setColumn(0,"HP_NO",gfn_getUserInfo("HP_NO"));
	ds_chgMyInfo.setColumn(0,"EML_ADDR",gfn_getUserInfo("EML_ADDR"));
	div_info.edt_pwdOrg.setFocus();

	ds_chgMyInfo.setRowType(0, Dataset.ROWTYPE_NORMAL);
	ds_chgMyInfo.updatecontrol = true;

	// 휴대폰번호 형식 초기화 
	div_info_MaskEdit00_onkillfocus(div_info.MaskEdit00);
	div_info.MaskEdit00.setFocus();
	
	ds_chgMyInfo01.copyRow(0, ds_chgMyInfo,0);
}

/*===============================================================================================
 * [ PART 5 ] Transaction 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인저장
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_saveMain()
{
	var sSvcID		 = "saveMain";
	var sTrID		 = "PSCUserMgmt_pChgMyInfo";
	var sPtrn		 = ds_chgMyInfo.getRowType(ds_chgMyInfo.rowposition) == Dataset.ROWTYPE_INSERT? "I" : "U";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RS_CHG_MY_INFO=ds_chgMyInfo:U";
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);

}
/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg){

	switch(sSvcID) {
		
		case "saveMain" : 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//정상 처리되었습니다.
				// 정상 수정 후 global data 수정
				if (ds_chgMyInfo.getColumn(0,"EMAIL_CHG_YN") == "Y") {
					gds_user.setColumn(0,"EML_ADDR",ds_chgMyInfo.getColumn(0,"EML_ADDR"));
				}
				if (ds_chgMyInfo.getColumn(0,"HP_NO_CHG_YN") == "Y") {
					gds_user.setColumn(0,"HP_NO",ds_chgMyInfo.getColumn(0,"HP_NO"));
				}
				close("cancel");
			}
			break;	
		
		default :
			break;
	}
}

