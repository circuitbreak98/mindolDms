================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 저장전 유효성 검사
 * param   : 
 * return  : true/false
-----------------------------------------------------------------------------------------------*/
function fn_chkBeforeSave()	
{	
	//저장전 수정,신규입력데이터의 trim처리 
	gfn_dsTrim(ds_chgMyInfo);
	
	if(!gfn_isUpdated(ds_chgMyInfo)){       //저장할 테이타셋 ds_chgMyInfo
		gfn_message("A","DMS00022","I");	//변경된 정보가 없습니다.
		return false;
	}
	ds_chgMyInfo.setColumn(0,"PWD_ORG", div_info.edt_pwdOrg.value);
	
	if ( !gfn_getValidateCompList(div_info)) {	
		return false;
	}
	
	if (!gfn_checkPhone(div_info.MaskEdit00.value) ) {
		gfn_message("A","DMS00053","E" );	//휴대전화번호 형식이 맞지 않습니다.
		div_info.MaskEdit00.class = 'Error';
		div_info.MaskEdit00.setFocus();
		return false;
	}
	
	if (!gfn_isEmail(div_info.edt_emlAddr.value) ) {
		gfn_message("A","DMS00061","E" );	//이메일 형식이 맞지 않습니다.
		div_info.edt_emlAddr.class = 'Error';
		div_info.edt_emlAddr.setFocus();
		return false;
	}


	return true;
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 이메일,휴대폰번호 수정여부 Check
 * param   : 
 * return  : 
----------------------------------------------------------------------------------------------*/
function fn_chkColumnChange()	
{	
    // 이메일 변경 Check
	if(ds_chgMyInfo01.getColumn(0,"EML_ADDR") == div_info.edt_emlAddr.value) {
		ds_chgMyInfo.setColumn(ds_chgMyInfo.rowposition,"EMAIL_CHG_YN", "N");
		//trace( " 이메일Check " + ds_chgMyInfo.getColumn(ds_chgMyInfo.rowposition,"EMAIL_CHG_YN"));
	} else {
		ds_chgMyInfo.setColumn(ds_chgMyInfo.rowposition,"EMAIL_CHG_YN", "Y");
		//trace( " 이메일Check " + ds_chgMyInfo.getColumn(ds_chgMyInfo.rowposition,"EMAIL_CHG_YN"));
	}
	
	//trace("폰번호 : " + div_info.MaskEdit00.value);
	//trace("폰번호가져온값 : " + ds_chgMyInfo00.getColumn(0,"HP_NO"));
	// 휴대전화 변경 Check
	if(ds_chgMyInfo01.getColumn(0,"HP_NO") == div_info.MaskEdit00.value) {
		ds_chgMyInfo.setColumn(ds_chgMyInfo.rowposition,"HP_NO_CHG_YN", "N");
		//trace( " 휴대전화Check " + ds_chgMyInfo.getColumn(ds_chgMyInfo.rowposition,"HP_NO_CHG_YN"));
	} else {
		ds_chgMyInfo.setColumn(ds_chgMyInfo.rowposition,"HP_NO_CHG_YN", "Y");
		//trace( " 휴대전화Check " + ds_chgMyInfo.getColumn(ds_chgMyInfo.rowposition,"HP_NO_CHG_YN"));
	}
}

