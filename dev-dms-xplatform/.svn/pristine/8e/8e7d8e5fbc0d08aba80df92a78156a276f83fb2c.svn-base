function btn_claimCancleConfirm_onclick(obj:Button,  e:ClickEventInfo)
{
	var st = "확정 청구 취소를 하시겠습니까?";
	
	if(ds_main.findRow("CHK","1") == -1){
		//체크대상없음
		gfn_message("A","DMS00020","I"); // 선택된 정보가 없습니다.
		return;
	}
	
	ds_maintemp.clearData();
	for(i=0; i<ds_main.getRowCount(); i++)
	{
		if(ds_main.getColumn(i,"CHK")=='1')
		{					//청구취소대상 인것만
			if(ds_main.getColumn(i,"INV_ST_CD") == '06')
			{
				var mrow = ds_maintemp.addRow();
				ds_maintemp.copyRow(mrow,ds_main,i);
				
				ds_maintemp.setColumn(mrow,"CASE_WHEN","Y");
				ds_maintemp.setColumn(mrow,"INV_ST_CD","04");
				ds_maintemp.setColumn(mrow,"CONF_INV_CNCL_DT",gfn_today());
				
			}else{
			  	gfn_message("A","DMS00071","E"); //이미 처리된 건이 있습니다.
			  	ds_maintemp.clearData();
				return false;
			}
		}
	}	
	if(gfn_message("C",st,"I","" ,"")){
		fn_handling();
	}else {
		ds_maintemp.clearData();
		return false;			
	}
}

