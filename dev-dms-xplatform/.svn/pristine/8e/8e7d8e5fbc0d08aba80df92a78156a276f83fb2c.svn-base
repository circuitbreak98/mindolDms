-----------------------------------------------------------------------------------------------*/
function cal_calTo_onchanged(obj:Calendar, e:ChangeEventInfo)
{ 
	var dateSetting = null; 
	switch(obj.name) 
	{	
		case "cal_invDt" : 	    //DMS청구일자
				dateSetting = div_searcn.cal_dmsInvDt.getDate();
			break;
		case "ccp_invProcDt" : 	//UKEY수납 일자
				dateSetting = div_searcn.cal_ukeyRecvDt.getDate();
			break;
		
		default :
			break;	
		
	}
	
	
	var fr = dateSetting[0];		
	var to = dateSetting[1];	
	
 	
	if(fr > to){ 
		gfn_message("A","DMS00069","E");	//유효한 일자가 아닙니다.
		
	}

}


