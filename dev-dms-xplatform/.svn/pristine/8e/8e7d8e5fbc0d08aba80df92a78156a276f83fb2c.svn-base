================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitCode(obj:Form, e:LoadEventInfo)
{ 	
	var strCode = "DMS246=ds_DMS246=A"; 		//청구상태
		strCode += ",DMS247=ds_DMS247=A";		//수납상태
		strCode += ",DMS271=ds_DMS271=A";		//사유코드
		strCode += ",DMS272=ds_DMS272=A";		//청구취소결과코드
		
	gfn_getCodeDs( this
				  , strCode
				  ,"fn_afterCodeCallBack");
				  		  		  		  	
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 호출 후 처리
-----------------------------------------------------------------------------------------------*/
function fn_afterCodeCallBack()
{
	ds_DMS246.filter("CM_CD_ID =='03' || CM_CD_ID =='04' || CM_CD_ID =='06' || CM_CD_ID =='%' ");
	ds_DMS271.filter("MGMT_ITEM_CD3=='Y'|| CM_CD_ID =='%'");
	div_search.cbo_invStatCd.index=0;
	div_search.cbo_rsnCd.index=0;
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 그리드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitGrid(obj:Form, e:LoadEventInfo)
{

	cgp_main.setPageInit("fn_selectPageExec", false);
	cgp_mainCnt.setPageNoCnt(0, 0, 0);	//PageNo Count
	
	// 그리드 초기화
	gfn_initGrid(grd_main, false, true, true, "row", null);
	 
}

