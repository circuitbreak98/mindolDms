{
	div_search.cal_invDt.setData(gfn_getFirstDate(gfn_today()),gfn_today(), true);
	div_search.cbo_recvStatCd.index=0; 	//수납상태
}

/*===============================================================================================
 * [ PART 5 ] Transaction 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{		
	var sSvcID		 = "searchMain";
	var sTrID		 = "PNRInsuInvCnclMgmt_pInqInsuInvCnclInfoLst";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sOutputDs	 = "ds_main=RS_INSU_INV_CNCL_LIST ds_sumlist=RS_SUM_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 청구취소용
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_handling()		
{
	var sSvcID		 = "handlingMain";
	var sTrID		 = "PNRInsuInvCnclMgmt_pUpdateIncuInvCnclInfo";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RS_INSU_INV_LIST=ds_maintemp";
	var sOutputDs	 = ""; 
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
				
				div_sum.cal_invDt.cal_calFrom.value = ds_search.getColumn(0,"DMS_INV_STA_DT");
				div_sum.cal_invDt.cal_calTo.value = ds_search.getColumn(0,"DMS_INV_END_DT");
			}
		break;
		
		case "saveMain":
			if( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				btn_search_onclick();
			}
		break;
		
		
		case "handlingMain":
			if( iErrorCode == 0){
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				btn_search_onclick();
			}
		break;
		
		default :
			break;
	}
}

/*===============================================================================================
 * [ PART 7 ] Main Button Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : search
-----------------------------------------------------------------------------------------------*/
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	//입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	ds_search.setColumn(0,"DMS_INV_STA_DT",div_search.cal_invDt.cal_calFrom.value);
	ds_search.setColumn(0,"DMS_INV_END_DT",div_search.cal_invDt.cal_calTo.value);
	ds_search.setColumn(0,"INV_PROC_STA_DT",div_search.ccp_invProcDt.cal_calFrom.value);
	ds_search.setColumn(0,"INV_PROC_END_DT",div_search.ccp_invProcDt.cal_calTo.value);
	
	ds_main.clearData();
	//page = 1;
	fn_searchMain();
	
}

