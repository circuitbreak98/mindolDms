/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	//입고기간
	var arrTerm = div_search.CommCalendarPred.getData();
	ds_search.setColumn(0,"STA_CNSL_DT",arrTerm[0]);
	ds_search.setColumn(0,"END_CNSL_DT",arrTerm[1]);
	
	if(gfn_isNull(arrTerm[0]) || gfn_isNull(arrTerm[1])){
		gfn_message("A","DMS00002","E","","접수일자");  //필수입력항목 {0}이/가 누락되었습니다.
		return false;
	}
	
	//데이타셋 clear
	ds_main.clearData();
	var sSvcID		 = "searchMain";
	var sTrID		 = "PEPCnslMgmt_pInqCnslObjList";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sOutputDs	 = "ds_main=RS_CNSL_OBJ_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인저장
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_saveMain() {
  if(!fn_beforeSave()) return false; 		
    var sSvcID		 = "saveMain";
	var sTrID		 = "PPRReqpInveInfoSend_pSaveReqpOutCntrtStat";
	var sPtrn		 = "C";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RS_EQP_CNTRT_LIST=ds_save:u";
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";

	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc); 

} 
 /*----------------------------------------------------------------------------------------------
 * 설명   :  출고취소
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_cancelMain() {
  
	var sSvcID		 = "cancelMain";
	var sTrID		 = "PPRReqpInveInfoSend_pSaveReqpOutCntrtStat";
	var sPtrn		 = "U";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RS_EQP_CNTRT_LIST=ds_save:u";
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	//trace(ds_save.getRowType(0));
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain" : 
			if ( iErrorCode == 0 ) {
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);	
				gfn_message("B","DMS00001");	//MSGI0003 :정상 조회되었습니다.
				
			}
			break;
		case "saveRSN":
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I"); //정상 처리되었습니다.
				fn_searchMain();
			}
			
			break;
		case "saveInConf":
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I"); //정상 처리되었습니다.
				fn_searchMain();
			}
			
			break;	
		case "saveInExl":
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I"); //정상 처리되었습니다.
				fn_searchMain();
			}
			break;	
		case "saveInCncl":
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I"); //정상 처리되었습니다.
				fn_searchMain();
			}
			break;	
			  

	}
}
