// 									+ SumOutPrc
// 									+"</fc><b v='false'><fc v='#77787a'>원</fc></b>";
			}
			break;
		case "searchMainAll": 
			if ( iErrorCode == 0 ) {
				//엑셀출력
				if (ds_mainAll.rowcount == 0){
					gfn_message("A","DMS00021","I"); //조회된 정보가 없습니다.
				} else {
					gfn_baseExcelExpert(grd_mainAll,"가입현황목록","가입현황목록");
				}
			}
			break;
		default:
			break;
	}
}

