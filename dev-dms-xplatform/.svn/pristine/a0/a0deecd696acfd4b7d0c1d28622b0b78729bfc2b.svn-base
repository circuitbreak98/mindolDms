/*----------------------------------------------------------------------------------------------
 * 설명   : 저장전 체크
 * param   : 
 * return  : true/false
-----------------------------------------------------------------------------------------------*/
function fn_beforeSave() 
{
	var bFlag = true;

	if (FV_RMKS_USER_ID != "" && FV_RMKS_USER_ID != gds_user.getColumn(0, "USER_NO")  ){
		//alert("확인자가 상이하여 상담의견을 변경할수 없습니다.");
		gfn_message("A","DMS00133","I");
		return false;
	}
	if (div_conInfo4.cbo_custYn.index == 0 && div_conInfo4.chk_rmksOnly.value != '1'){
		gfn_message("A","DMS00023","I","","고객의사");
		return false;
	}
	if ((div_conInfo4.cbo_custYn.value == '22' || div_conInfo4.cbo_custYn.value == '26') &&
		div_conInfo4.cbo_custYn.index == 0 && div_conInfo4.chk_rmksOnly.value != '1'){
		gfn_message("A","DMS00023","I","","고객의사");
		return false;
	}
	if (gfn_isNull(div_conInfo4.txa_prchs_rmks.text)){
		gfn_message("A","DMS00023","I","","상담의견");
		return false;
	}
	
	if ( div_conInfo2.cbo_buyCl.value == 'Y' && gfn_isNull(div_conInfo3.edt_custNm.text) ) {
		gfn_message("A","DMS00023","I","","고객명");
		return false;
	}
	
	if ( div_conInfo2.cbo_buyCl.value == 'Y' && gfn_isNull(div_conInfo3.edt_telNo.text) ) {
		alert("연락처를 입력해 주십시요.");
		gfn_message("A","DMS00023","I","","상담의견");
		return false;
	}	

	if ( div_conInfo3.chk_chagDedtYn.value == '0' && 
	    (div_conInfo2.cbo_buyCl.value == 'Y' && 
		(gfn_isNull(div_conInfo3.cbo_bankCd.value)||div_conInfo3.cbo_bankCd.value=="" ))) {
		gfn_message("A","DMS00023","I","","은행");
		return false;
	}
			
	if ( div_conInfo3.chk_chagDedtYn.value == '0' && 
		(div_conInfo2.cbo_buyCl.value == 'Y' && gfn_isNull(div_conInfo3.edt_accNo.text))) {
		gfn_message("A","DMS00023","I","","계좌번호");
		return false;
	}
	
	if (div_conInfo3.chk_chagDedtYn.value == '1' && gfn_isNull(div_conInfo3.edt_svcNo.text) ) {
		gfn_message("A","DMS00023","I","","서비스번호");
		return false;
	}
	
	if ( div_conInfo3.chk_chagDedtYn.value == '1' && gfn_isNull(div_conInfo3.edt_svcMgmtNo.text) ) {
		gfn_message("A","DMS00023","I","","서비스관리번호");
		return false;
	}

	if ( div_conInfo3.chk_chagDedtYn.value == '1' && (div_conInfo3.edt_svcMgmtNo.text.length) != 10) {
		gfn_message("A","DMS00119","I","",new Array("서비스관리번호","10자리"));
		return false;
	}
	
	if (ds_prod_m.getColumn(0,"COMP_CL") != '1' && gfn_isNull(div_conInfo2.edt_imei.text)) {
		gfn_message("A","DMS00023","I","","IMEI");
		return false;
	}

	return true;
}



/*----------------------------------------------------------------------------------------------
 * 설명   : 주소검색후 우편번호,주소 셋팅 
-----------------------------------------------------------------------------------------------*/
function msie_TitleChange(obj:ActiveX, e)
{
	if(!gfn_isEmpty(gfn_replace(e.Text,"|","")) && "로그인" != e.Text && "blank" != e.Text){
		var zip1 = "";
		var zip2 = "";
		if(!gfn_isNull(e.Text.split("|")[1])){
		 zip1 = gfn_getNullToEmpty(e.Text.split("|")[1].substr(0,3));	// 우편번호 1	
		 zip2 = gfn_getNullToEmpty(e.Text.split("|")[1].substr(3,2));	// 우편번호 2	
		}
		var addr = gfn_getNullToEmpty(e.Text.split("|")[0].split(",")[0]); 	// 주소
		var addrDtl = gfn_getNullToEmpty(e.Text.split("|")[0].split(",")[1]);	// 상세주소
		/*
		trace("우편번호 1 ==> " + zip1);
		trace("우편번호 2 ==> " + zip2);	
		trace("주소 ==> " + addr);
		trace("상세주소 ==> " + addrDtl);
		*/
		/* 컴포넌트 ID 맞게 수정..*/
		div_conInfo3.edt_zipCd1.value = gfn_trim(zip1);
		div_conInfo3.edt_zipCd2.value = gfn_trim(zip2);
		div_conInfo3.edt_addr.value = gfn_trim(addr);
		div_conInfo3.edt_etcAddr.value = gfn_trim(addrDtl);							
	}
}



