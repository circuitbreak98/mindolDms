/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain" : 
			if ( iErrorCode == 0 ) {
				gfn_message("B","DMS00001");	//MSGI0003 :정상 조회되었습니다.
				
				div_conInfo2.cbo_color.value = ds_consult_m.getColumn(0,"EQP_COLOR_CD");
				
				if (ds_prod_m.getColumn(0,"SLFCO_CL") == '1') {
				div_conInfo2.sta_comp_cl.text = "<fc v='red'><b v='true'>자사</b></fc>";
				}else{
					div_conInfo2.sta_comp_cl.text = "<fc v='red'><b v='true'>타사</b></fc>";
				}
				
				
				if(!gfn_isNull(div_conInfo2.cal_oning_dt.value)){
					var diffM = gfn_diffMonth(div_conInfo2.cal_oning_dt.value, gfn_left(gfn_today(), 8));
					var sDay = div_conInfo2.cal_oning_dt.value+"";
					sDay = gfn_left(gfn_today(), 6)+sDay.substr(6,2)
					var diffD = gfn_diffDate(sDay, gfn_left(gfn_today(), 8));
					
					if ( diffD >= 0 ) {
					diffM = diffM + 1;
					}
					div_conInfo2.sta_rmk.text = "<fc v='red'><b v='true'>"+diffM+ "개월</b></fc>";	
					
				
				}
			
				//안써서 뺌 
			//div_conInfo5_chk_polYn_OnClick();
				
			div_conInfo2.edt_ProdCd.enable = false;
			div_conInfo2.btn_ProdCd.enable  = false;
			div_conInfo2.cbo_color.enable  = false;
			div_conInfo2.cal_oning_dt.enable  = false;
			div_conInfo2.edt_serNm.enable  = false;
			div_conInfo2.edt_imei.enable  = false;
//			div_conInfo2.cbo_rate.Enable = false;
			div_conInfo2.cbo_buyCl.enable  = false;
			grd_dtlList.enable  = false;		
			div_cpnt.txa_cpnt.enable  = false;
			ds_prchsCustRmks.clearData();
			ds_prchsCustRmks.insertRow(0);

			if (ds_consult_m.getColumn(0,"PROGR_ST") == '31' ||
				ds_consult_m.getColumn(0,"PROGR_ST") == '32' ||
				ds_consult_m.getColumn(0,"PROGR_ST") == '33' ||
				ds_consult_m.getColumn(0,"PROGR_ST") == '34' ){
				div_conInfo3.chk_polYn.enable = false;
				div_conInfo3.chk_chagDedtYn.enable = false;
				div_conInfo4.cbo_custYn.enable = false;
				div_conInfo4.cbo_custYnDtl.enable = false;
				div_conInfo4.chk_rmksOnly.value = "1";
				div_conInfo4.chk_rmksOnly.enable = false;
			}
				
				

				var strCode =  "";
				strCode += "DMS100=ds_DMS100=S";  //단말기색상
				strCode += ",DMS179=ds_DMS179=N";  //진행상태
				strCode += ",DMS166=ds_DMS166=S";  //진행상태
				//trace(ds_consult_m.getColumn(0,"PRCH_CL"));
				//trace(ds_consult_m.getColumn(0,"CUST_IDEA"));
				//trace(ds_consult_m.getColumn(0,"PROGR_ST_NM"));
				
				if (ds_consult_m.getColumn(0,"PRCH_CL") == 'N') {
					strCode += ",DMS231=ds_custidea=S";  //고객의사 	ZBAS_C_00540 
				} else if (ds_consult_m.getColumn(0,"POLY_OBJ_YN") == '1') {
					strCode += ",DMS229=ds_custidea=S";  //고객의사 	ZBAS_C_00520 
					strCode += ",DMS230=ds_custideadtl=S";  //고객의사상세 ZBAS_C_00530 
				} else {
					strCode += ",DMS227=ds_custidea=S";  //고객의사 	ZBAS_C_00500 
					strCode += ",DMS228=ds_custideadtl=S";  //고객의사상세 ZBAS_C_00510 
					//cf_setCommonCode("div_conInfo4.cbo_custYn"       , "ZBAS_C_00500", "선택");	
					//cf_setCommonCode("div_conInfo4.cbo_custYnDtl"    , "ZBAS_C_00510", "선택");				
				}
				//trace("strcode:"+strCode);
				gfn_getCodeDs( this
				  , strCode
				  ,"fn_afterCodeCallBack");		
				  
				div_conInfo4.cal_cust_dt.value = gfn_left(gfn_today(), 8);
				div_conInfo4.cbo_custYn.index = 0;
				div_conInfo4.cbo_custYnDtl.index = 0;
				
			}
			break;
		case "savePrchsCustRmks":
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I"); //정상 처리되었습니다.
				fn_searchMain();
			}
			
			break;

	}
}
