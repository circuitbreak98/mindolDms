		}
	}
	
		var sum = ds_main.getCaseSum("CHK == '1'","EQP_PRCH_AMT");
	    var nRowCnt = ds_main.getCaseCount("CHK == '1'");
	    var st = "총 "+nRowCnt+"건에 "+gfn_toFixedLocaleString(sum, '@@@,@@@,@@@')+" 원입니다. 전표를 삭제 하시겠습니까?" ;

		gfn_message("C",st,"I","" ,"");
		
	fn_deleteSlip();
}
