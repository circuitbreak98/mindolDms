/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	var dt = ds_search.getColumn(0,"RENTAL_CNTRT_STA_DT");
	var yymm = dt.substring(0,6);
	var yy = dt.substring(0,4);
	var mm = dt.substring(4,6);
	
	
	switch(sSvcID) {
		case "searchMain" : 
				
			cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
			cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
			gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
			break;
			
		case "AllExcel" : 
			if (iErrorCode == 0) {
				var dsbind = gfn_makeGrid(ds_AllExcel);
				dsbind.binddataset = "ds_AllExcel";
				gfn_baseExcelExpert(dsbind,"단말자산현황","단말자산현황");
			}else{
				gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
			}
			break;
			
			
		case "confirmAll" : 
			if ( iErrorCode == 0 ) {
				if(ds_confirm.getColumn(0,"TOT_CNT") != '0'){
 				var dt = ds_search.getColumn(0,"RENTAL_CNTRT_STA_DT");
 				var yy = dt.substring(0,4);
 				var mm = dt.substring(4,6);
				var st = "해당월("+yy+"년 "+mm+"월) "+ds_confirm.getColumn(0,"TOT_CNT")+"건, 총 금액 "+gfn_toFixedLocaleString(ds_confirm.getColumn(0,"TOT_PRC"), '@@@,@@@,@@@')+"원에 대한  전표를 생성 하시겠습니까?" ;
				
					if(gfn_message("A",st,"I","","")){
						fn_searchSlipAll();	
					}
								
				}else{
				var msg = "해당월("+yy+"년 "+mm+"월) 에 선택된 정보가 없습니다." ;
					gfn_message("A",msg,"C","ERROR",""); // 선택된 정보가 없습니다.
					fn_searchMain();
				}
			}			
			break;	
		
		case "searchSlipAll" : 
			if ( iErrorCode == 0 ) {
			 fn_searchMain();
			}			
			break;
			
		case "handlingMain" : 
	
			if ( iErrorCode == 0 ) {
			
			gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
			  
			  ds_main.clearData();
			  fn_searchMain();
			} 		
							
			break;
	    
	    case "delSlip" :
			if ( iErrorCode == 0 ) {
			  gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
			  ds_main.clearData();
			  fn_searchMain();
			}else{
				gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
		
			}	    
	    break;
	    
	   
		default :
			break;
	}
}

