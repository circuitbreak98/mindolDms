-----------------------------------------------------------------------------------------------*/
function grd_main_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if(ds_main.rowcount <= 0 || ds_main.rowposition < 0){
		return;
	}
	
	fvCellText = grd_main.getCellProperty( "Head", grd_main.getCellPos(), "text" );
}

