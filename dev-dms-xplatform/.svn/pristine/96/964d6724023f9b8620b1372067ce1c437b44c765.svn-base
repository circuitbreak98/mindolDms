function div_btn_reset_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj.name) 
	{	
		case "btn_resetDeal" : 	//대리점리셋
			div_search.edt_agnOrgCd.value = null;
			div_search.edt_agnOrgNm.value = null;
			div_search.edt_affilAgn.value = null;
		break;
		
