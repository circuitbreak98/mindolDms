/*----------------------------------------------------------------------------------------------
 * 설명   : 저장전 유효성 검사
 * param   : 
 * return  : true/false
-----------------------------------------------------------------------------------------------*/
function fn_chkBeforeSave()	
{	
	//저장전 수정,신규입력데이터의 trim처리 
	gfn_dsTrim(ds_main);

	if (!gfn_getValidateCompList(div_detail)) {	
		return false;
	}
			
	if(mode!='new' && !gfn_isUpdated(ds_main) && !gfn_isUpdated(ds_field))//저장할 테이타셋
	{
		gfn_message("A","DMS00022","I");	//변경된 정보가 없습니다.
		return false;
	}

	return true;
}
