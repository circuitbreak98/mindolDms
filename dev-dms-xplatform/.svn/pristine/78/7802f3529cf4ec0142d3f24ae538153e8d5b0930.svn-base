		return false;
	}
	
	// 입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	
	ds_main.clearData();		//데이타셋 clear
	//조회함수 호출 
	fn_searchMain();
}

