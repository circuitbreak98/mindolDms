	} else {	//Object이용시
	
		ds_search.setColumn(0,"EQP_MDL_CD", sRtnVal["supEqpMdlCd"]);
		ds_search.setColumn(0,"EQP_MDL_NM", sRtnVal["supEqpMdlNm"]);
	
	}
}

