	var sParam03 = "";
	if ( e == null ) {
		sParam03 = "key";
	} else {
		sParam03 = "button";
	}
	var sParam04 = "";
	var arrArgs = {argAutoSelect:true, argCallProgram:"FNR046M", argParam01:sParam01, argParam02:sParam02, argParam03:sParam03, argParam04:sParam04};
	var sRtnVal = gfn_dialog("FBI008P", arrArgs);

	//팝업에서 입력받은 값 리턴
