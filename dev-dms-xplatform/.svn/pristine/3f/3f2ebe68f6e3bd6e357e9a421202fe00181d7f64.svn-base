/*----------------------------------------------------------------------------------------------
 * 설명   : 그리드더블클릭시 검수등록호출
-----------------------------------------------------------------------------------------------*/
function grd_main_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	if(ds_main.rowcount <= 0){
		return;
	}
	
	//일반감정
	// 검수등록 화면의 Param 값						
	var sEqpMdlCd = ds_main.getColumn(obj.currentrow,"EQP_MDL_CD");		// 단말기모델코드
	var sCnslMgmtNo = ds_main.getColumn(obj.currentrow,"CNSL_MGMT_NO");	// 상담관리번호
	var sCnslDt = "";		// 상담일자
	var sScrbMthd = "";		// 가입방법	
	var sPrgGb = "P";			// 검수대상구분
	var sJdcCl = "RE";			// 검수구분
							
	var arrArgs = {sEqpMdlCd:sEqpMdlCd, sCnslMgmtNo:sCnslMgmtNo, sCnslDt:sCnslDt, sScrbMthd:sScrbMthd, sPrgGb:sPrgGb, sJdcCl:sJdcCl};
	
	gfn_tlyRgst("FEP011M",arrArgs);
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 전체엑셀다운로드
-----------------------------------------------------------------------------------------------*/
function btn_excelAll_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_main.rowcount <= 0){
		return;
	}
	if(div_search.cbo_gltyOpGb.value != fvQltyGb){
		if(div_search.cbo_gltyOpGb.value == "1"){
			gfn_message("A","현재의 조회 데이터는 품질검사등록대상 데이터가 아닙니다. 확인 바랍니다.","E","","");
		}
		if(div_search.cbo_gltyOpGb.value == "2"){
			gfn_message("A","현재의 조회 데이터는 품질검사확인 데이터가 아닙니다. 확인 바랍니다.","E","","");
		}
		return;
	}
	var arrTerm = div_search.ccp_qlty_dt.getData();
	ds_search.setColumn(0,"IN_FR_DT",arrTerm[0]);
	ds_search.setColumn(0,"IN_TO_DT",arrTerm[1]);
	
	if(gfn_isNull(arrTerm[0]) || gfn_isNull(arrTerm[1])){
		gfn_message("A","DMS00002","E","","검수일자");  //필수입력항목 {0}이/가 누락되었습니다.
		return false;
	}
	// 입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	ds_mainExcelAll.clearData();
	gfn_dsTrim(ds_search);
	page = 1;
	page_size = 10000;
	
	fn_searchExcelAll();
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 개인정보조회
