-----------------------------------------------------------------------------------------------*/
function fn_dsClear(){
	ds_main.clearData();
	stc_subTitle.text = "조회결과";
	fn_setInitGrid();
	fn_setInitForm();
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 업무구분에 따른 그리드 표시 항목 조정
-----------------------------------------------------------------------------------------------*/
function fn_resetGridColSize(){
	if(fvQltyGb == div_search.cbo_gltyOpGb.value){
		return;
	}

	var vGab = 0;
	if(fvQltyGb != "0"){
		var numTemp = new Number(0);
		for(var i = 0; i < ds_gridPro.colcount; i++){
			numTemp = i;
			vColNo = numTemp.toString();
			grd_main.setFormatColProperty(i+2,"size",ds_gridPro.getColumn(0,vColNo));
		}
		vGab = 2; //최초이후에는 그리드의 NO,체크박스 고려
	}

	//출력제외대상 정리
	if(div_search.cbo_gltyOpGb.value == "1"){ //품질검사등록대상
		grd_main.setFormatColProperty(21+vGab,"size","0"); //
	}else if(div_search.cbo_gltyOpGb.value == "2"){ //품질검사확인
		grd_main.setFormatColProperty(20+vGab,"size","0"); //
	}

	grd_main.redraw();

	fvQltyGb = div_search.cbo_gltyOpGb.value;
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 엔터키 조회 처리
-----------------------------------------------------------------------------------------------*/
function div_search_enter_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == KEYCODE_ENTER) {
		btn_search.setFocus();
		btn_search_onclick();
	}
}

