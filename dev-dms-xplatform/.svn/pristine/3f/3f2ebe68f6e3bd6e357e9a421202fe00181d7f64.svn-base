-----------------------------------------------------------------------------------------------*/
function div_search_cbo_gltyOpGb_canitemchange(obj:Combo, e:ItemChangeEventInfo)
{
	if(div_search.cbo_gltyOpGb.value == "1"){
		btn_jdgReg.enable = false;
