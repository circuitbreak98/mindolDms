// 액셀전체다운로드 다음처리
function fn_nextLoop()
{
	
	if ( total_record_cnt <=  page * page_size ) {
		page_size = 50;
		if(fvQltyGb == "1"){
			gfn_baseExcelExpert(grd_mainExcelAll_01,"품질검사등록대상목록","품질검사등록대상목록");
		}else{
			gfn_baseExcelExpert(grd_mainExcelAll_02,"품질검사확인목록","품질검사확인목록");
		}
	} else {
		//다음페이지 조회
		page++;
		fn_searchExcelAll();
	} 
}
/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
				fn_resetGridColSize();
			}
		break;
		case "saveMain": 
			if ( iErrorCode == 0 ) {
				btn_search_onclick();
			}
		break;
		case "searchExcelAll" : 
			if ( iErrorCode == 0 ) {
				ds_mainExcelAll.appendData(ds_mainTemp,true);
				fn_nextLoop();
			}
		break;
		default:
		break;
	}
}

