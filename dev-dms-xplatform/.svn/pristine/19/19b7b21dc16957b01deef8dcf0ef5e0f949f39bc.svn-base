function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
 	switch(sSvcID) {
		case "searchSub":
			if ( iErrorCode == 0 ) {
				//gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
			}
		break;
		
		case "saveMain":
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				//fn_setInitForm();
			}
		break;
				
		default:
		break;
	}
}

