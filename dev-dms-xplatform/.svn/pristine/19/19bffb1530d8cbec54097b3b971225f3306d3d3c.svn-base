{
	var sSvcID		 = "fn_saveMemo";
	var sTrID		 = "PNRCnslMngt_pSaveAgnCnslMemo";
	var sPtrn		 = "C";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RS_SUB1=ds_sub1:u";
	var sOutputDs	 = "";
