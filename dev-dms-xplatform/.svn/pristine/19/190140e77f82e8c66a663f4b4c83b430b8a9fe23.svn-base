 * [ PART 5 ] Transaction 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	var rtnVal 	= Array(2);
	rtnVal = div_search.CommCalendarPred.getData();
	
	var	sFromDt = rtnVal[0];
	var sToDt = rtnVal[1];
	ds_search.setColumn(0, "ST_DT", sFromDt);
	ds_search.setColumn(0, "ED_DT", sToDt);
	
	var rtnValIn = Array(2);	//발송기간 set
	rtnValIn = div_search.CommCalendarPredGoSend.getData();
	var	sFromDtIn = rtnValIn[0];
	var sToDtIn = rtnValIn[1];
	ds_search.setColumn(0, "ST_DT_SEND", sFromDtIn);
	ds_search.setColumn(0, "ED_DT_SEND", sToDtIn);
	
	
	ds_etmmList.clearData();
	
	var sSvcID		 = "searchMain";
	var sTrID		 = "PPREqpTransportMgmt_pEqpTransportMgmt";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sOutputDs	 = "ds_etmmList=RS_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";

	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 메인저장
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_saveMain(strObj)
{
	if(!fn_chkBeforeSave(strObj)) return false;

	var srtEqpRcvDt = ds_etmmList.getColumn(ds_etmmList.rowposition, "EQP_RCV_DT");	//수취일자(입고일자셋팅)
	
	ds_etmmList.setColumn(ds_etmmList.rowposition, "IN_SCHD_DT", srtEqpRcvDt);	//입고예정일자 (수취일자 셋)
	ds_etmmList.setColumn(ds_etmmList.rowposition, "EQP_IN_DT", gfn_today());	//입고일자 (수취일자 셋)
	
	
	
	ds_etmmListTemp.clearData();
	ds_etmmListTemp.copyData(ds_etmmList);
	
//return false;

	var sSvcID		 = "saveMain";
	var sTrID		 = "PPREqpTransportMgmt_pSaveEqpTransInfo";
	var sPtrn		 = "M";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RS_ETMM_TEMP_LIST=ds_etmmList:A";
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain" : 
			if ( iErrorCode == 0 ) {
 				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
 				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count			
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
				
				gfn_message("B","DMS00001");	//MSGI0003 :정상 조회되었습니다.
			} else {
				gfn_message("A",ncMsg);
				
			}
			fn_btnEnableSet();
			
			break;
			
		case "saveMain" : 
			if ( iErrorCode == 0 ) {
				gfn_message("B","DMS00000","I"); //정상 처리되었습니다.
				
			} else {
				//gfn_message("A",ncMsg);
 				return false;	//저장 실패시....
			}
			fn_searchMain();

	}
}

