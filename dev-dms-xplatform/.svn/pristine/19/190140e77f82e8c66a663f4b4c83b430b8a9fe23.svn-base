function div_search_btn_searchMdl_onclick(obj:Button,  e:ClickEventInfo)
{
	var sParam01 = div_search.edt_eqpMdlCd.value;
	var sParam02 = div_search.edt_eqpMdlNm.value;
	var sParam03 = "";
	
	if ( e == null ) {
		sParam03 = "key";
	} else {
		sParam03 = "button";
	}
	var arrArgs = {argAutoSelect:true, argCallProgram:"FNR064M", argParam01:sParam01, argParam02:sParam02, argParam03:sParam03};
	var sRtnVal = gfn_dialog("FBI008P", arrArgs);

	//팝업에서 입력받은 값 리턴
	if ( gfn_isNull(sRtnVal) || sRtnVal == "cancel" ) {
		//취소시 값 제거
		div_search.edt_eqpMdlCd.value = "";
		div_search.edt_eqpMdlNm.value = "";
	} else if (obj.name == "btn_searchMdl"){	//Object이용시
		div_search.edt_eqpMdlCd.value = sRtnVal["supEqpMdlCd"];
		div_search.edt_eqpMdlNm.value = sRtnVal["supEqpMdlNm"];
	} else if (obj.name == "btn_searchEqpMdl"){	//Object이용시
		div_detail.edt_eqpMdlCd.value = sRtnVal["supEqpMdlCd"];
		div_detail.edt_eqpMdlNm.value = sRtnVal["supEqpMdlNm"];
	}	
}

