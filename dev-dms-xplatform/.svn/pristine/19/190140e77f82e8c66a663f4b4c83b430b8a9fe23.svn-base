			ds_etmmList.deleteRow(nRow);	
			fn_btnEnableSet();
		}
		
	}else{  // 업데이트 글은 Temp 삭제여부만 갱신하고 더미만 삭제
		var bResult = gfn_message("C","DMS00026","I");	//데이터를 삭제합니다. 계속하시겠습니까?
		//var strInveStCd = ds_etmmList.getColumn(ds_etmmList.rowposition, "INVE_ST_CD");
		
		if ( bResult ) {
			var strEqpInNo = ds_etmmList.getColumn(ds_etmmList.rowposition, "LAST_IN_OUT_NO");
			var strEqpOutNo = ds_etmmList.getColumn(ds_etmmList.rowposition, "EQP_OUT_NO");	//출고번호(출고TB)
			
			ds_etmmList.setColumn(ds_etmmList.rowposition, "INVE_ST_CD", "20");	//입고상태(출고20)
			ds_etmmList.setColumn(ds_etmmList.rowposition, "EQP_IN_DT", "");	//입고일자
			ds_etmmList.setColumn(ds_etmmList.rowposition, "DEL_YN", "Y");
			ds_etmmList.setColumn(ds_etmmList.rowposition, "EQP_IN_NO", strEqpInNo);
			ds_etmmList.setColumn(ds_etmmList.rowposition, "LAST_IN_OUT_NO", strEqpOutNo); //입고확정취소-마지막출고일자셋팅
			fn_saveMain(); 			
		}
	}

	
}


