 * param   : 
 * return  : true/false
-----------------------------------------------------------------------------------------------*/
function fn_chkBeforeSave(obj)	
{
	var nRow = ds_etmmList.rowposition;	
	var strHomeDlvSndDt = ds_etmmList.getColumn(nRow, "HDLV_SND_DT"); //발송일자
	var strEqpRcvDt = ds_etmmList.getColumn(nRow, "EQP_RCV_DT");	//수취일자
	var sCK_ColId = "HDLV_SND_DT=발송일,EQP_MDL_CD=모델,EQP_SER_NO=일련번호,WAYBIL_NO=운송장번호,EQP_RCV_DT=수취일자,HDLVCO_CD=택배사,HDLV_AMT=택배비";		

	if (strHomeDlvSndDt > strEqpRcvDt){						//발송일 수취일 체크
	//alert("수취일이 발송일보다 이전입니다 ");
	gfn_message("A","DMS00084","E","","");  // 발송일이 수취일보다 늦습니다.
	return false;
	}	
	if(!gfn_isUpdated(ds_etmmList))//저장할 테이타셋 ds_inListMain
	{
		gfn_message("A","DMS00022","I");	//???? : 변경된 정보가 없습니다.
		return false;
	}	
 	else if (!gfn_checkValueDs(ds_etmmList,sCK_ColId))
 	{
 		return false;
 	} 
	return true;
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 조회조건에서 단말기 찾기 버튼 클릭
