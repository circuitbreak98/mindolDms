/*----------------------------------------------------------------------------------------------
 * 설명   : 저장전 유효성 검사
 * param   : 
 * return  : true/false
-----------------------------------------------------------------------------------------------*/
function fn_chkBeforeSave()	
{	
	if(ds_main.rowcount == 0)
	{
		gfn_message("A","DMS00082","I");	//해당 자산에 대한 손해배상항목정보 또는 렌탈계약이 없습니다.
		return false;
	}
	
	if(!gfn_isUpdated(ds_main) && !gfn_isUpdated(ds_mainTemp))//저장할 테이타셋 ds_Save
	{
		gfn_message("A","DMS00022","I");	//변경된 정보가 없습니다.
		return false;
	}
	if (!gfn_getValidateCompList(Div01)) {	
		return false;
	}
	return true;
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 변상금 전체금액 자동합계
-----------------------------------------------------------------------------------------------*/
function ds_main_onvaluechanged(obj:Dataset,  e:DSColChangeEventInfo)
{
	if ( e.columnid == "USE_YN" || e.columnid == "PEN" ) {
		var totAmt = 0;
		for(i=0; i<=ds_main.getRowCount(); i++) {
			if(ds_main.getColumn(i, "USE_YN") == "1") {
				totAmt += ds_main.getColumn(i, "PEN");
			}
		}
		Div01.mae_pen.value = totAmt;
	}
}

