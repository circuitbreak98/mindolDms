 * [ PART 5 ] Transaction 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	ds_main.clearData();		//데이타셋 clear
	//ds_mainTemp.clearData();	//데이타셋 clear
	
	var sSvcID		 = "searchMain";
	var sTrID		 = "PPRReqpJdgRgst_pInqReqpJdgRgstLst";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sOutputDs	 = "ds_main=RS_REQP_JDG_LIST ds_constList=RS_REQP_JDG_CLIST ds_upFile=NC_FILE_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";

	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 메인저장
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_saveMain()
{

	var sSvcID		 = "saveMain";
	var sTrID		 = "PPRReqpJdgMgmt_pSaveReqpJdg";
	var sPtrn		 = "M";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RS_REQP_JDG_LIST=ds_main RS_REQP_JDG_LIST2=ds_mainTemp NC_FILE_LIST=ds_upFile";
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
 	switch(sSvcID) {
		case "searchMain" : 
			if ( iErrorCode == 0 ) {
				
			} else {
				gfn_message("A",ncMsg);
			}
			//ds_mainTemp.addRow();
			ds_mainTemp.copyRow(0,ds_main,0);
			ds_upFile.addColumn("fullpath","string");
			break;
			
		case "saveMain" : 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//MSGI0001 : 정상 처리되었습니다.
				this.close("S");
			}
			break;
		default :
			break;
	}
}


/*----------------------------------------------------------------------------------------------
 * 설명   : FileUpload CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg, objRtnDs
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBackFile(sSvcID, iErrorCode, sErrorMsg, objRtnDs)
{
 	switch(sSvcID) {
		case "uploadFile":
			for(i=0;i<ds_upFile.rowcount;i++) {
				ds_upFile.setColumn(i,"FILE_ID",objRtnDs.getColumn(i,"ncUploadedFileId"));
			}
			ds_upFile.filter("");
			fn_saveMain();
			break;
		default :
			break;
	}
}

