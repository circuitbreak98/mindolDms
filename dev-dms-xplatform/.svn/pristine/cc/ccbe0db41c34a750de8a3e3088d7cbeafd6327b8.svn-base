/************************************************************************************************
 * Function : 구성품 원데이터와 비교
************************************************************************************************/
function fn_checkCmptEqu()
{
	ds_constList.filter("CHECKED == '1'");
	if(ds_constList.rowcount == 0 && ds_constListOrg.rowcount == 0)	{
		return true;
	}else if(ds_constList.rowcount != ds_constListOrg.rowcount)	{
		return false;
	}
	var nSCnt = 0;
	for(var i = 0; i < ds_constListOrg.rowcount; i++){
		for(var j = 0; j < ds_constList.rowcount; j++){
			if(ds_constListOrg.getColumn(i,"CMPT_CD") == ds_constList.getColumn(j,"CMPT_CD") && ds_constListOrg.getColumn(i,"CMPT_UPRC") == ds_constList.getColumn(j,"CMPT_UPRC"))
			{
				nSCnt++;
				continue;
			}
		}
	}
	ds_constList.filter("");
	if(nSCnt == ds_constListOrg.rowcount){
		return true;
	}
	return false;
}
