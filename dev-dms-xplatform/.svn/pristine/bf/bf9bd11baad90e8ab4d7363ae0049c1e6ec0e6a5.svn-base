/*----------------------------------------------------------------------------------------------
 * 설명   : insert
-----------------------------------------------------------------------------------------------*/
function btn_insert_onclick(obj:Button,  e:ClickEventInfo)
{
	var oldRow = ds_inListMain.rowposition;
	if(oldRow >= 0){
		ds_inListTemp.clearData();
		ds_inListTemp.addRow();
		ds_inListTemp.copyRow(0,ds_inListMain,oldRow);
		ds_inListTemp.setColumn(0,"ASSET_NO","");
		ds_inListTemp.setColumn(0,"EQP_SER_NO","");
		ds_inListTemp.setColumn(0,"EQP_IMEI_NO","");
	}
	var row = ds_inListMain.addRow();
	ds_inListMain.rowposition = row;
	if(ds_inListTemp.rowcount > 0){
		ds_inListMain.copyRow(ds_inListMain.rowposition,ds_inListTemp,0);
	}
	ds_inListMain.setColumn(ds_inListMain.rowposition,'DEL_YN', 'N');
	ds_inListMain.setColumn(ds_inListMain.rowposition,'OP_CL_CD', 'PR');
	ds_inListMain.setColumn(ds_inListMain.rowposition,'INVE_ST_CD', '10');
	ds_inListMain.setColumn(ds_inListMain.rowposition,'FST_IN_DT', gfn_today());
	
	//구성품
	if(ds_DMS111.rowcount > 0){
		parNo++;
		ds_inListMain.setColumn(ds_inListMain.rowposition,'PAR_NO',parNo);

		if(oldRow >= 0){
			ds_constList.filter("");
			ds_constList.filter("PAR_NO == "+ds_inListMain.getColumn(oldRow,'PAR_NO'));
		}
		for(var i = 0; i < ds_DMS111.rowcount; i++){
			ds_constListTemp.clearData();
			ds_constListTemp.addRow();
			ds_constListTemp.setColumn(0,"PAR_NO",parNo);
			ds_constListTemp.setColumn(0,"CMPT_CD",ds_DMS111.getColumn(i,"CM_CD_ID"));
			ds_constListTemp.setColumn(0,"OUT_DTL_CD","10");
			ds_constListTemp.setColumn(0,"CMPT_USE_QTY","1");
			if(oldRow >= 0){
				for(var j = 0; j < ds_constList.rowcount; j++){
					if(ds_constListTemp.getColumn(0,"CMPT_CD") == ds_constList.getColumn(j,"CMPT_CD")){
						ds_constListTemp.setColumn(0,"CHECKED",ds_constList.getColumn(j,"CHECKED"));
						break;
					}
				}
			}
			var nRow = ds_constList.addRow();
			ds_constList.copyRow(nRow,ds_constListTemp,0);
		}
		
		ds_constList.filter("PAR_NO == "+parNo);
		ds_constList.rowposition = 0;
	}

	ds_inListTemp.clearData();

	div_detail.cal_inDt.setFocus();
	if(ds_inListMain.rowposition == 0){
		fn_btnSetEnable();
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명   : delete
-----------------------------------------------------------------------------------------------*/
function btn_delete_onclick(obj:Button,  e:ClickEventInfo)
{
	// 변경된사항 바로 저장할때 delete전 Check
	if ( ds_inListMain.getRowType(ds_inListMain.rowposition) == DATASET_ROWTYPE_INSERT ) {
		var parNo = ds_inListMain.getColumn(ds_inListMain.rowposition,"PAR_NO");
		ds_constList.filter("");
		ds_constList.filter("PAR_NO == "+parNo);
		for(var i = 0; i < ds_constList.rowcount; i++){
			ds_constList.deleteRow(i);
		}
		ds_inListMain.deleteRow(ds_inListMain.rowposition);
		if(ds_inListMain.rowcount <= 0){
			parNo = 0;
			ds_constList.clearData();
			fn_btnSetUnEnable();
		}
		return;
	}
}
