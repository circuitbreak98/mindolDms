/*----------------------------------------------------------------------------------------------
 * 설명   : 단말기색상필터
-----------------------------------------------------------------------------------------------*/
function fn_eqpColorFilter()	
{	
	ds_eqpColorList.filter("");
	ds_DMS100.filter("");
	if(!gfn_isNull(ds_inListMain.getColumn(ds_inListMain.rowposition,"EQP_MDL_CD"))){
		ds_eqpColorList.filter("EQP_MDL_CD=='"+ds_inListMain.getColumn(ds_inListMain.rowposition,"EQP_MDL_CD")+"'");
		var vColorCd = "CM_CD_ID == ''";
		for(var i = 0; i < ds_eqpColorList.rowcount; i++){
			vColorCd = vColorCd + " || ";
			vColorCd = vColorCd + "CM_CD_ID == '" + ds_eqpColorList.getColumn(i,"COLOR_CD") + "'";
		}
		ds_DMS100.filter(vColorCd);
	}
	if(gfn_isNull(ds_inListMain.getColumn(ds_inListMain.rowposition,"EQP_COLOR_CD"))){
		div_detail.cbo_colorCd.index = 0;
	}
	else{
		var vChk = false;
		for(var i = 0; i < ds_DMS100.rowcount; i++){
			if(ds_DMS100.getColumn(i,"CM_CD_ID") == ds_inListMain.getColumn(ds_inListMain.rowposition,"EQP_COLOR_CD")){
				vChk = true;
				break;
			}
		}
		if(!vChk){
			ds_inListMain.setColumn(ds_inListMain.rowposition,"EQP_COLOR_CD","");
			div_detail.cbo_colorCd.index = 0;
		}
	}
}
