function fn_afterCodeCallBack()
{
	ds_DMS108.filter("MGMT_ITEM_CD2=='Y'|| CM_CD_ID == '%'");
	//ds_combo.filter("COM_CD_ID == '%'");
	div_search.cbo_rentYn.index=0;
}
/*----------------------------------------------------------------------------------------------
