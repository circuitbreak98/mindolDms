/**
버튼활성화 처리 
*/
function enableBtn(obj){


	/*
		btn_suggSave 의견저장
		btn_inConf  입고확정
		btn_inExl   입고제외
		btn_cancel 취소
	*/
	if(obj.value == "0"){
		btn_suggSave.enable = true;
		btn_inConf.enable = true;
		btn_inExl.enable = true;
		btn_cancel.enable = false;
	}else if(obj.value == "1"){
		btn_suggSave.enable = true;
		btn_inConf.enable = false;
		btn_inExl.enable = false;
		btn_cancel.enable = true;
	}else if(obj.value == "2"){
		btn_suggSave.enable = true;
		btn_inConf.enable = false;
		btn_inExl.enable = false;
		btn_cancel.enable = true;
	}

