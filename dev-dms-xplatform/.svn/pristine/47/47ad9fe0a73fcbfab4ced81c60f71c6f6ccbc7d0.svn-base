/*----------------------------------------------------------------------------------------------
 * 설명   : 저장전 체크
 * param   : 
 * return  : true/false
-----------------------------------------------------------------------------------------------*/
function fn_beforeSave() 
{
	var strChkCnt = 0;
 
	for(var i=0; i<ds_main.getRowCount(); i++){
		var strChk = ds_main.getColumn(i, "CHECKED");
		if(strChk == "1"){

		strChkCnt++;
		}
	}
 
	if(strChkCnt < 1){
		gfn_message("A","DMS00020","I"); /*선택된 정보가 없습니다.*/
		return false;
	}
	return true;
}

