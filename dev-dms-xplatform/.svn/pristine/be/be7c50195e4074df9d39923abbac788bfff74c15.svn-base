/*----------------------------------------------------------------------------------------------
 * 설명   : 단말기대금 선할인 전표발행 및 취소 버튼
-----------------------------------------------------------------------------------------------*/
function btn_slip_onclick(obj:Button,  e:ClickEventInfo)
{	
	if(ds_main.getRowCount()<=0){
		gfn_message("A","DMS00021","W");	//DMS00021 : 조회된 정보가 없습니다.
		return false;
	}else{
		switch(obj.name){			
			case "btn_slipI" :				
				var stardYm = ds_main.getColumn(ds_main.rowposition,"DCINAD_STRD_YM"); // 정산기준년월								
				// 단말기대금 선할인 전표발행 년,월,총건수,수수료 총액 Msg
				var xclYy = stardYm.substr(0,4);
				var xclMm = stardYm.substr(4,2);															
				var cmmsXclCnt = gfn_toFixedLocaleString(ds_main.getCaseCount("SLIP_ST_CD == '00' || SLIP_ST_CD == '05'"),"");	// 수수료 총 건수				
				var cmmsAmt = gfn_toFixedLocaleString(ds_main.getCaseSum("SLIP_ST_CD == '00' || SLIP_ST_CD == '05'","DCINAD_AMT"),"");	// 수수료 총 금액
				var msgArry = new Array(xclYy,xclMm,cmmsXclCnt,cmmsAmt);				
				// 체크된 건 중에 (미발행 or 전표삭제)이 아닌건은 전표 발행 제외 => 해당 행(row) 리턴
				var nRow = ds_main.findRowExpr("SLIP_ST_CD == '00' || SLIP_ST_CD == '05'");					
				if(0 > nRow){									
					gfn_message("A","DMS00071","I");// 이미 처리된 건이 있습니다.
					return false;				
				}else{
					if(gfn_message("C","DMS00132","Q","",msgArry)){	// DMS00132 : 해당월({0}년 {1}월) {2}건, 총 금액 {3}원에 대한  전표를 생성 하시겠습니까? 					
						fn_dcinadXclSlipIns(stardYm);	// 전표발행.						
					}
				}								
				break;
			case "btn_slipC" :		
				var dcinadSlipNo = ds_main.getColumn(ds_main.rowposition,"DCINAD_SLIP_NO"); // 전표번호
				var stardYm = ds_main.getColumn(ds_main.rowposition,"DCINAD_STRD_YM"); // 정산기준년월
				if("10" != ds_main.getColumn(ds_main.rowposition,"SLIP_ST_CD")){
					gfn_message("A","DMS00122","I","","해당 건은 전표취소"); // {0} 대상이 아닙니다.
					return false;
				}else{
					if(gfn_message("C","DMS00148","Q","","전표번호:"+dcinadSlipNo)){	// {0} 전표를 취소하시겠습니까?
						ds_cnclSlip.clearData();
						var sRow = ds_cnclSlip.addRow();
						ds_cnclSlip.setColumn(sRow,"SLIP_NO",dcinadSlipNo);	// 취소 전표번호
						ds_cnclSlip.setColumn(sRow,"YYYY",stardYm.substr(0,4));	// 회계연도						
						fn_adcinadXclSlipCncl(stardYm);	// 전표취소.									
					}
				}																			
				break;	
			default:
				break;				
		}	
	}
}

