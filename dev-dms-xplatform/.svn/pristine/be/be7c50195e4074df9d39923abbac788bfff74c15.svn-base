/*----------------------------------------------------------------------------------------------
 * 설명   : 그리드 내 클릭시 이벤트 발생
-----------------------------------------------------------------------------------------------*/
function grd_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	switch(obj.name) {
		case "grd_main": 				
			if("10" == e.col){										
				var sRtnVal = gfn_excelPageIndex(0,"");								
				if ( !gfn_isNull(sRtnVal) && sRtnVal["excelProcYn"] == true){
					ds_dtlExcel.clearData();
					fn_searchDtl(sRtnVal["excelFirst"],sRtnVal["excelLast"]);
				}											
			}				
			break;	
		default:
			break;
	}
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 검색 조건에서 엔터키가 눌러졌을때 검색
-----------------------------------------------------------------------------------------------*/
function div_search_onkeydown(obj:Div, e:KeyEventInfo)
{
	if (e.keycode == KEYCODE_ENTER) {
		btn_search.setFocus();
		btn_search_onclick();
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명   : Grid의 내용을 Excel로 Export 한다.
-----------------------------------------------------------------------------------------------*/
function btn_excel_onclick(obj:Button,  e:ClickEventInfo)
{
	gfn_baseExcelExpert(grd_main,"단말기 대금 선할인 목록", "단말기 대금 선할인 목록");
}

/*----------------------------------------------------------------------------------------------
 * 설명   :  form 내에서 esc버튼 클릭시 그리드 데이터셋 초기화 
 * param   : obj - Form
 * param   : e  - KeyEventInfo
 * return  : true/false
-----------------------------------------------------------------------------------------------*/
function Form_onkeyup(obj:Form, e:KeyEventInfo)
{
 	if(e.keycode ==KEYCODE_ESC) {	//ESC
 		fn_dsClear();
 		ds_search.clearData();
 		ds_search.addRow(); 		
 		div_search.cmc_toYm.setDate(gfn_left(gfn_today(), 6));	
 		div_search.cmc_toYm.setFocus();
// 		fn_afterCodeCallBack();
 	}
}

