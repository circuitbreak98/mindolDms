/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
			}
			break;
		case "dcinadXclSlipSave": 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				btn_search_onclick();
			}
			break;	
		case "saveRpetCalc": 					
			if ( iErrorCode == 0 ) {								
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				/*{0} 건의 데이터가 처리되었습니다.*/
				//gfn_message("C","DMS00143","I","",gfn_toFixedLocaleString(ds_resultCalc.getColumn(ds_resultCalc.rowposition,"INSERT_ROW"),"")); 
				//fn_searchMain();											
			}else{
				if("DMS00149" == ncMsgId){
					// 재집계 불가능 상태일 경우 재조회.
					fn_searchMain();				
				}			
			}			
			break;
		case "searchExcelDtl": 
			if ( iErrorCode == 0 ) {								
				ds_dtlExcel.appendData(ds_sub,true);
				if(ds_sub.getRowCount()>0){
					var excelTotCnt =  ds_sub.getColumn(0,"EXCEL_TOTAL_CNT");	// 데이터 총 건수																	
					var sRtnVal = gfn_excelPageIndex(excelPage,excelTotCnt);												
					if ( !gfn_isNull(sRtnVal) && sRtnVal["excelProcYn"] == true){
						return fn_searchDtl(sRtnVal["excelFirst"],sRtnVal["excelLast"]);
					}													
				}
				
				/* 엑셀다운로드 */
				var cmmsStrdYm = ds_main.getColumn(ds_main.rowposition, "DCINAD_STRD_YM");	// 정산기준연월				
				var sheetYm =  cmmsStrdYm.substr(0,4)+"년 "+cmmsStrdYm.substr(4,2)+"월";
				var sheetName = sheetYm+" 단말기 대금 선할인";
				var title = sheetYm+" 단말기 대금 선할인 목록";				
				gfn_baseExcelExpert(grd_excel,sheetName,title);																																										
			}
			break;			
		default:
			break;
	}
}

