		div_search.cmc_toYm.setFocus();
		return false;
	}
	
	//검색조건: 정산기준연월
	ds_search.setColumn(0, "DCINAD_STRD_YM",div_search.cmc_toYm.getDate());
	
	fn_searchMain();
