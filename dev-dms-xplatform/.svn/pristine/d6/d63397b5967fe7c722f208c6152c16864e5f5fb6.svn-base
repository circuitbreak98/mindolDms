/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {
				//cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
				//cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
				//gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
				if(ds_main.rowcount != 0){
					//grd_main_oncellclick();
				}
				gfn_message("B","DMS00001");	//MSGI0003 :정상 조회되었습니다.
				
				if(ds_list.rowcount != 0){
					for(i=0; i<ds_list.getRowCount(); i++){
						if(ds_list.getColumn(i,"SELEC") == '개인'){
							ds_list.setColumn(i,"GUBUN",'01');
						} else if(ds_list.getColumn(i,"SELEC") == '법인'){
							ds_list.setColumn(i,"GUBUN",'02');
						}
					}
				}
				gfn_message("B","DMS00001");	//MSGI0003 :정상 조회되었습니다.
			}
		break;
		
		case "saveMain":
			if( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				btn_search_onclick();
			}else{
				gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
				ds_main.clearData();
				ds_list.clearData();
				ds_listtemp.clearData();
			}
		break;
		
		case "saveSecondMain":
			if( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				btn_search_onclick();
			} else{
				gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
				ds_main.clearData();
				ds_list.clearData();
				ds_listtemp.clearData();
			}
		break;
		
		case "saveSlip":
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				ds_listtemp.clearData();
				ds_list.clearData();
			  fn_searchMain();
			}else{
				gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
				ds_listtemp.clearData();
				ds_list.clearData();
			}
		break;
		
		
		
		case "delSlip":

			if (iErrorCode == 0) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
			  ds_listtemp.clearData();
			  ds_list.clearData();
			  fn_searchMain();
			}else{
				gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
				ds_listtemp.clearData();
				ds_list.clearData();
			}
			
		break;
		
		case "AllExcel":
			if (iErrorCode == 0) {
				var dsbind = gfn_makeGrid(ds_AllExcel);
				dsbind.binddataset = "ds_AllExcel";
				
				dsbind.setFormatColProperty(3,"size","100");
				dsbind.setFormatColProperty(12,"size","100");
				dsbind.setFormatColProperty(13,"size","100");
				dsbind.setFormatColProperty(14,"size","100");
				
				dsbind.setCellProperty("Body",0,"style","align:left");
				dsbind.setCellProperty("Body",1,"style","align:left");
				dsbind.setCellProperty("Body",2,"style","align:left");
				dsbind.setCellProperty("Body",3,"style","align:center");
				dsbind.setCellProperty("Body",4,"style","align:left");
				dsbind.setCellProperty("Body",5,"style","align:center");
				dsbind.setCellProperty("Body",6,"style","align:left");
				dsbind.setCellProperty("Body",7,"calendardisplaynulltype","none");
				dsbind.setCellProperty("Body",8,"calendardisplaynulltype","none");
				dsbind.setCellProperty("Body",9,"style","align:left");
				dsbind.setCellProperty("Body",10,"style","align:left");
				dsbind.setCellProperty("Body",11,"style","align:right");
				dsbind.setCellProperty("Body",12,"style","align:center");
				dsbind.setCellProperty("Body",13,"style","align:center");
				dsbind.setCellProperty("Body",14,"style","align:center");
				dsbind.setCellProperty("Body",15,"style","align:left");
				
				gfn_baseExcelExpert(dsbind,"매출추정정보","매출추정정보");
			}else{
				gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
			}
		break;
			
		case "ReState" :
			if ( iErrorCode == 0 ) {
				ds_main.clearData();
				ds_list.clearData();
				ds_listtemp.clearData();
				fn_searchMain();
			}else{
				gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
				ds_main.clearData();
				ds_list.clearData();
				ds_listtemp.clearData();
			}		
		break;	
		default :
			break;
	}
}

