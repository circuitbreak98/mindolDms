/*----------------------------------------------------------------------------------------------
 * 설명   : search
-----------------------------------------------------------------------------------------------*/
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	var opDt = div_search.cal_opDt.getData();
	ds_search.setColumn(0, "OP_STA_DT",opDt[0]);
	ds_search.setColumn(0, "OP_END_DT", opDt[1]);
	
	var nmChgYn = div_search.chk_NmChgYn.value;
	var eqpChgYn = div_search.chk_EqpChgYn.value;
	var legalRepveYn = div_search.chk_LegalRepveYn.value;
	
	if( (nmChgYn != undefined && nmChgYn == true) || (eqpChgYn != undefined && eqpChgYn == true) || (legalRepveYn != undefined && legalRepveYn == true) ){
		ds_search.setColumn(0, "CHG_YN", "Y");		
	}else{
		ds_search.setColumn(0, "CHG_YN", "N");
	}
	
	ds_search.setColumn(0, "TTUL_CHG_YN", nmChgYn != undefined && nmChgYn == true ? "Y" : "");
	ds_search.setColumn(0, "EQP_CHG_YN", eqpChgYn != undefined && eqpChgYn == true ? "Y" : "");
	ds_search.setColumn(0, "LEGAL_AGENT_CHG_YN", legalRepveYn != undefined && legalRepveYn == true ? "Y" : "");
		
	ds_main.clearData();
	page = 1;
	fn_searchMain();
	
}

