	}

	var arrArgs = {argAutoSelect:true, argCallProgram:"FNR009M", argParam01:sParam01, argParam02:sParam02, argParam03:sParam03, argParam04:sParam04};
	var sRtnVal = gfn_dialog("FBI004P", arrArgs);

	//팝업에서 입력받은 값 리턴
	if ( gfn_isNull(sRtnVal) || sRtnVal == "cancel" ) {
		//취소시 값 제거
		div_search.edt_agnCd.value = "";
		div_search.edt_agnNm.value = "";
