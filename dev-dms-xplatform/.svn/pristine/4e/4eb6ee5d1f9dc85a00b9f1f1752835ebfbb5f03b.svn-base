/*----------------------------------------------------------------------------------------------
 * 설명   : 전표생성
-----------------------------------------------------------------------------------------------*/
function btn_createSlip_onclick(obj:Button,  e:ClickEventInfo)
{

	var dt = ds_search.getColumn(0,"RENTAL_CNTRT_STA_DT");
	var yy = dt.substring(0,4);
	var mm = dt.substring(4,6);
	var ym = dt.substring(0,6);
	
	if(ds_slip.findRow("CHK","1") == -1){
		//체크대상없음
		gfn_message("A","DMS00020","I"); // 선택된 정보가 없습니다.
		return;
	}

	ds_sliptemp.clearData();
	for(i=0; i<ds_slip.getRowCount(); i++)
	{
		if(ds_slip.getColumn(i,"CHK")=='1')
		{
			if( gfn_isNull(ds_slip.getColumn(i,"SLIP_ST_CD")) || ds_slip.getColumn(i,"SLIP_ST_CD") == '00' || ds_slip.getColumn(i,"SLIP_ST_CD") == '05')
			{
				var st = "해당월("+yy+"년 "+mm+"월) "+ds_slip.getColumn(i,"T_CNT")
						+"건, 총 금액 "+gfn_toFixedLocaleString(ds_slip.getColumn(i,"T_PRC"), '@@@,@@@,@@@')+"원에 대한  전표를 생성 하시겠습니까?" ;
				ds_slip.setColumn(i,"CASE_WHEN","C");
				ds_slip.setColumn(i,"YYYYMM",ym);
				ds_slip.setColumn(i,"YYYY",yy);
								
				var mrow = ds_sliptemp.addRow();
				ds_sliptemp.copyRow(mrow,ds_slip,i);
			
			}else{
			  	gfn_message("A","DMS00071","E"); //이미 처리된 건이 있습니다.
			  	ds_sliptemp.clearData();
					return false;
			}
		}
	}	
	if(gfn_message("C",st,"I","" ,"")){
			fn_handlingSlip();
	}else {
		ds_sliptemp.clearData();
		return false;			
	}

}
