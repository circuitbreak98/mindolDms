/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	var dt = ds_search.getColumn(0,"RENTAL_CNTRT_STA_DT");
	var yymm = dt.substring(0,6);
	var yy = dt.substring(0,4);
	var mm = dt.substring(4,6);
	
	
	switch(sSvcID) {
		case "searchMain" : 
	
			var Mtotal = "  총 "+"<fc v='#31a3dc'>"+ds_sum.getColumn(0,"M_CNT")
						+"</fc><b v='false'><fc v='#77787a'>건, 매입금액 합계: </fc></b>"+"<fc v='#31a3dc'>"
						+gfn_toFixedLocaleString(ds_sum.getColumn(0,"M_PRC"), '@@@,@@@,@@@')+"</fc><b v='false'><fc v='#77787a'>원 </fc></b>";
			
			var Stotal = "  총 "+"<fc v='#31a3dc'>"+ds_sum.getColumn(0,"T_CNT")
						+"</fc><b v='false'><fc v='#77787a'>건, 매입금액 합계: </fc></b>"+"<fc v='#31a3dc'>"
						+gfn_toFixedLocaleString(ds_sum.getColumn(0,"T_PRC"), '@@@,@@@,@@@')+"</fc><b v='false'><fc v='#77787a'>원 </fc></b>";
			
			fn_setGridTitle(stc_MTitle,Mtotal);
			fn_setGridTitle(stc_subTitle,Stotal);
			break;
		
		case "handlingMain" : 
	
			if ( iErrorCode == 0 ) {
			
			  gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
			  
			  ds_slip.clearData();
			  ds_main.clearData();
			  fn_searchMain();
			} 		
							
			break;
	  
	    case "AllExcel":
				
			if (iErrorCode == 0) {
				var dsbind = gfn_makeGrid(ds_AllExcel);
				dsbind.binddataset = "ds_AllExcel";
				gfn_baseExcelExpert(dsbind,"단말기취득정보목록","단말기취득정보목록");
			}else{
				gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
			}
			break;
	   
		default :
			break;
	}
	
			

}

