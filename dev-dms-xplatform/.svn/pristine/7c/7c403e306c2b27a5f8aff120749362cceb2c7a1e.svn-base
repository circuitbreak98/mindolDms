 * [ PART 4 ] Load 후 초기설정
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitCode(obj:Form, e:LoadEventInfo)
{

	var strCode =  "DMS112=ds_DMS112=A";
		strCode += ",DMS115=ds_DMS115=A"; 
		strCode += ",DMS122=ds_DMS122=A"; 
		strCode += ",DMS100=ds_DMS100=A"; 
		strCode += ",DMS123=ds_DMS123=A"; 
		strCode += ",DMS158=ds_DMS158=A"; 
		strCode += ",DMS159=ds_DMS159=A"; 
						
	gfn_getCodeDs( this
				  , strCode
				  ,"fn_afterCodeCallBack");
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 호출 후 처리
-----------------------------------------------------------------------------------------------*/
function fn_afterCodeCallBack()
{		 
	div_search.cbo_cntrtPrd.index=0;
	div_search.cbo_cntrtSt.index=0;
	div_search.cbo_eqpRtnYn.index=0;
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 그리드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitGrid(obj:Form, e:LoadEventInfo)
{
	//그리드 페이징 초기화
	cgp_main.setPageInit("fn_selectPageExec", false);
	cgp_mainCnt.setPageNoCnt(0, 0, 0);	//PageNo Count
	
	page = 1;
	
	// 그리드 초기화 
	//gfn_initGrid(objGrid:UserGrid, bReadonly, bSortable, bTooltip, sSelectType, innerDs, sPage, sPageSize, sTotCnt);
	gfn_initGrid(grd_main, false, true, true, "row", null);

}

/*----------------------------------------------------------------------------------------------
 * 설명   : 페이지 Click시 페이지조회
 * param   : iTotalRowCount - 전체 Row수
 * param   : iUnitSelRowNo  - 페이지당 Row수
 * param   : iCurrPage      - 현재 페이지
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_selectPageExec(iTotalRowCount, iUnitSelRowNo, iCurrPage)
{
	//페이징처리
	page = iCurrPage;
	fn_searchMain();
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 기본값 설정
-----------------------------------------------------------------------------------------------*/
function fn_setInitForm(obj:Form, e:LoadEventInfo)
{
	div_search.cal_cntrtDt.setData(gfn_addDate(gfn_today(), -1), gfn_today());
}

/*===============================================================================================
 * [ PART 5 ] Transaction 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	var sSvcID		 = "searchMain";
	var sTrID		 = "PNRCntrtPrstMgmt_pInqCntrtPrstLst";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sOutputDs	 = "ds_main=RS_CNTRT_PRST_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";

	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}


/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
 	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
			}
		break;
				
		default:
		break;
		
		case "searchUSCANImage": 
			if ( iErrorCode == 0 ) {
				//alert(bSucc);
				if (gfn_fileDownloadUscan(ds_img, "CUST_ACNT_SVC_MGMT_NO", "IMG_FILE_NM")){
					var iRow = gds_frm.addRow();
					gds_frm.setColumn(iRow, "FRM_ID", "CntrtPrstUscanP");
					gds_frm.setColumn(iRow, "FRM_URL", "NR::CntrtPrstUscanP.xfdl");
					
					gfn_dialog("CntrtPrstUscanP");
				}
			}
		break;
	}
}

/*===============================================================================================
 * [ PART 7 ] Main Button Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : search
-----------------------------------------------------------------------------------------------*/
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	var cntrtDt = div_search.cal_cntrtDt.getData();
	ds_search.setColumn(0, "RENTAL_CNTRT_STA_DT", cntrtDt[0]);
	ds_search.setColumn(0, "RENTAL_CNTRT_END_DT", cntrtDt[1]);
	
	var opDt = div_search.cal_opDt.getData();
	ds_search.setColumn(0, "OP_STA_DT",opDt[0]);
	ds_search.setColumn(0, "OP_END_DT", opDt[1]);
	
	ds_main.clearData();
	page = 1;
	fn_searchMain();
	
}

