}

/*----------------------------------------------------------------------------------------------
 * 설명   : grid excel
-----------------------------------------------------------------------------------------------*/
function btn_excel_onclick(obj:Button,  e:ClickEventInfo)
{
	if (grd_main.rowcount == 0){
		gfn_message("A","DMS00021","I"); //조회된 정보가 없습니다.
	} else {
		gfn_baseExcelExpert(grd_main,"계약현황목록","계약현황목록");
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명   : USCAN 조회
-----------------------------------------------------------------------------------------------*/
function btn_uscan_onclick(obj:Button,  e:ClickEventInfo)
{	
	if(ds_main.findRow("CHK","1") == -1){
		//체크대상없음
		gfn_message("A","DMS00020","I"); // 선택된 정보가 없습니다.
		return;
	}
	
	ds_svc.clearData();
	for(var i=0; i < ds_main.getRowCount(); i++){
		if(ds_main.getColumn(i, "CHK") == "1"){
			ds_svc.addRow();
			ds_svc.setColumn(i, "SVC_MGMT_NO", ds_main.getColumn(i,"SVC_MGMT_NO"));
		}
	}
	
	fn_searchUSCANImage();
	
	/*
	ds_main.filter("CHK == '1'");
	gfn_dialog("FNR008P");
	ds_main.filter("");
	*/
}

