//R센터입고일
	var inDt = div_search.cal_inDt.getData();
 	ds_search.setColumn(0, "IN_STA_DT",inDt[0]);
 	ds_search.setColumn(0, "IN_END_DT", inDt[1]);
	
	ds_main.clearData();
