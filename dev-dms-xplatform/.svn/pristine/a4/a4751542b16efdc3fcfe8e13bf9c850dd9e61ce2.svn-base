function grd_cmgrpcd_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	//최종사용자정보조회 
	if(!gfn_userInfoCall(obj,e,"FS_REG_USER_ID,LS_CHG_USER_ID")) return;

	//ds_param00.clearData();
	var cmgrpcd = ds_cmgrpcd.getColumn(ds_cmgrpcd.rowposition, 'CM_GRP_CD_ID');
	pv_cmgrpcdid = cmgrpcd;
	pv_cmgrpcdnm = ds_cmgrpcd.getColumn(ds_cmgrpcd.rowposition, 'CM_GRP_CD_NM');
	//var row = ds_param00.addRow();
	ds_param00.setColumn(0, 'CM_GRP_CD_ID', cmgrpcd);
	//관리항목이 No일때, 신규버튼 Disable 처리
	var mgmtcd = ds_cmgrpcd.getColumn(obj.rowposition, 'MGMT_CD_YN');	
	if(mgmtcd!=1){
		Div01.btn_new_pop.enable = false;
	}else{
		Div01.btn_new_pop.enable = true;
	}
	
	if(ds_param00.getRowCount()==1){
		ds_cmcd.clearData();
		cmcdpage =1;
		fn_searchMain2();
	}else{
		fn_message("A", "DMS00021","E");   // MSGI0019 : 조회된 정보가 없습니다.
	}
}


