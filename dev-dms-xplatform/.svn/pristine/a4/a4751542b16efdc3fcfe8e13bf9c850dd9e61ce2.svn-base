/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain" : 
			Div01.stc_cmGrpCdNm.visible = true; 
			//페이징 관련셋팅(공통그룹코드)
			ds_cmgrpcd.appendData(ds_cmgrpTemp, true);	
			//조회된 첫번째row로 setFocus 
			ds_cmgrpcd.rowposition = (cmgrpcdpage-1) * cmgrppgsize ;

			if(gfn_isEmpty(ds_cmgrpTemp.getColumn(ds_cmgrpTemp.rowposition, 'CM_GRP_CD_ID'))){
				gfn_setGridTitleCountPaging(Div00.stc_subTitle00, 0 );
				//공통그룹코드 조회 0건 인경우, 공통코드건수 리셋
				ds_cmcd.clear();
				gfn_setGridTitleCountPaging(Div01.stc_subTitle01, 0 ) ;
				Div01.stc_subTitle01.text = Div01.stc_subTitle01.text + "<b v='false'><fc v='#dcdcdc'> l</b></fc>" ;
				Div01.stc_cmGrpCdNm.expr = "";
			}else{

				gfn_setGridTitleCountPaging(Div00.stc_subTitle00, total_record_cnt);
				/*
				pv_cmgrpcdid = ds_cmgrpcd.getColumn(ds_cmgrpcd.rowposition,'cmGrpCdId');
				pv_cmgrpcdnm = ds_cmgrpcd.getColumn(ds_cmgrpcd.rowposition,'cmGrpCdNm');
				ds_param00.setColumn(0, 'cmGrpCdId', ds_cmgrpcd.getColumn(ds_cmgrpcd.rowposition,'cmGrpCdId'));
				if(ds_param00.getRowCount()==1){
					ds_cmcd.clearData();
					cmcdpage =1;
					fn_searchMain2();
				}
				*/
			}
			cmgrptotalcnt = total_record_cnt ;
			div_search.edt_cmGrpCd.setFocus();
			break;
		
		case "searchMainCmGrpCdSingle" : 
			//기존데이터셋에 복사
			if ( iErrorCode == 0 ) {
				ds_cmgrpcd.copyRow(ds_cmgrpcd.rowposition, ds_cmgrpTemp, 0);	
			} 			
			break;
			
		case "searchMainCmCdSingle" : 
			//기존데이터셋에 복사
			if ( iErrorCode == 0 ) {
				ds_cmcd.copyRow(ds_cmcd.rowposition, ds_cmcdTemp, 0);	
			} 			
			break;			
						
		case "searchMain2" : 
			//페이징 관련셋팅(공통코드)
			ds_cmcd.appendData(ds_cmcdTemp, true);
			//조회된 첫번째row로 setFocus 
			ds_cmcd.rowposition = (cmcdpage-1) * cmcdpgsize ; 
			
			if(gfn_isEmpty(ds_cmcdTemp.getColumn(ds_cmcdTemp.rowposition, 'CM_CD_ID'))){
				gfn_setGridTitleCountPaging(Div01.stc_subTitle01, 0 ) ;
				Div01.stc_subTitle01.text = Div01.stc_subTitle01.text + "<b v='false'><fc v='#dcdcdc'> l</b></fc>"	;			
			}else{
				gfn_setGridTitleCountPaging(Div01.stc_subTitle01, total_record_cnt );
				Div01.stc_subTitle01.text = Div01.stc_subTitle01.text + "<b v='false'><fc v='#dcdcdc'> l</b></fc>"	;			
			}
			cmcdtotalcnt = total_record_cnt;
			// stc_subTitle01 간격 맞춤
	//		var varList = (new ExtCommon()).getTextExtent(Div01.stc_subTitle01, Div01.stc_subTitle01.text);
	//		Div01.stc_cmGrpCdNm.position.left = Div01.stc_subTitle01.position.left + varList[1] - 1298;
	        // 그리드 선택값 '코드명[코드]'형식으로 표시
			Div01.stc_cmGrpCdNm.expr = "expr_result2() + expr_resultA()" ;
			Div01.stc_cmGrpCdNm.tooltiptext = expr_result() ;
			div_search.edt_cmGrpCd.setFocus();
			break;

		case "ApplyCmCd" :
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//MSGI0001 : 정상 처리되었습니다.
				btn_search_onclick();
				return true;
			}
			break;
			
		default :
			break;
	}
}
function expr_result()
{
     return pv_cmgrpcdnm;
}
function expr_resultA()  //공통그룹코드명 10자리까지 반환
{
     var grpcdnm = pv_cmgrpcdnm;
     var grpcdnm10 ;
     if(grpcdnm!=null){
		grpcdnm10 = grpcdnm.substring(0,10);
           return grpcdnm10;
     }
}
function expr_result2()
{
     return "[" + pv_cmgrpcdid + "]" ;
}


