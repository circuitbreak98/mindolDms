		//공통그룹코드
			if ( cmgrptotalcnt <=  cmgrpcdpage * cmgrppgsize ) {
				gfn_message("B","DMS00028","I"); //조회 마지막 페이지 입니다.
				return;
			} else {
				//다음페이지 조회
				cmgrpcdpage++;
				fn_searchMain();
				
			}
		break;
		case 'grd_cmCd' :
		//공통코드
			if ( cmcdtotalcnt <=  cmcdpage * cmcdpgsize ) {
				gfn_message("B","DMS00028","I"); //조회 마지막 페이지 입니다.
				return;
			} else {
				//다음페이지 조회
				cmcdpage++;
				fn_searchMain2();
			}
		break;
		default :
		break;
	}
}

