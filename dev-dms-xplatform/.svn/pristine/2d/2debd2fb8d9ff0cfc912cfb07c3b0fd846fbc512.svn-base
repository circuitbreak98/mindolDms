-----------------------------------------------------------------------------------------------*/
function test(obj:Grid, e:GridClickEventInfo)
{ 
	var checkColNm = obj._checkColumnName
	var cellIdx = obj.getBindCellIndex("body", checkColNm);
	
	if( e.col != cellIdx ){
		return false;
	}
	
	if( gfn_isNull( isCheck ) ) isCheck = "1";
		
	var objDS = eval(obj.binddataset);
	var isCheck = obj.getCellProperty("head", cellIdx, "text");
	if (gfn_isNull(isCheck)) isCheck = "0";
	
	if (isCheck == "0") {	
		isCheck = "1";
	} else {
		isCheck = "0";
	}

	obj.setCellProperty("head", cellIdx, "text", isCheck);		
	
	for(var i=0 ; i< objDS.rowcount ; i++){
		if( objDS.getColumn(i,"INV_ST_CD") == "06" ){
			continue;
		}
		objDS.setColumn( i, checkColNm, isCheck);
	}
}
