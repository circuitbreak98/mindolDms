-----------------------------------------------------------------------------------------------*/
function btn_claim_onclick(obj:Button,  e:ClickEventInfo)
{
	var st = "청구 하시겠습니까?";
	
	if(ds_main.findRow("CHK","1") == -1){
		//체크대상없음
		gfn_message("A","DMS00020","I"); // 선택된 정보가 없습니다.
		return;
	}
	
	ds_maintemp.clearData();
	for(i=0; i<ds_main.getRowCount(); i++)
	{
		if(ds_main.getColumn(i,"CHK")=='1')
		{					//청구대상, 미확정청구취소/청구확정(환급있기때문)
			if(ds_main.getColumn(i,"INV_ST_CD") == '01' ||ds_main.getColumn(i,"INV_ST_CD") == '03' ||ds_main.getColumn(i,"INV_ST_CD") == '07')
			{				
				var mrow = ds_maintemp.addRow();
				ds_maintemp.copyRow(mrow,ds_main,i);
				
				ds_maintemp.setColumn(mrow,"CASE_WHEN","Y");
				ds_maintemp.setColumn(mrow,"INV_ST_CD","02");
			}else{
			  	gfn_message("A","DMS00071","E"); //이미 처리된 건이 있습니다. 처리가 불가능합니다 필요
			  	ds_maintemp.clearData();
				return false;
			}
		}
	}	
	if(gfn_message("C",st,"I","" ,"")){
		fn_handling();
	}else {
		ds_maintemp.clearData();
		return false;			
	}
}

