}
/*----------------------------------------------------------------------------------------------
 * 설명   : 청구상태동기화
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_syncronization()		
{
	var sSvcID		 = "syncronization";
	var sTrID		 = "PNRInsuInvMgmt_pInqInsuInvStSync";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sOutputDs	 = "RS_RESTATE_LIST=ds_maintemp"; 
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain": 
			
			if ( iErrorCode == 0 ) {
			
			
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
		
				div_sum.cal_ukeyInvDt.cal_calFrom.value = ds_search.getColumn(0,"UKEY_INV_STA_DT");
				div_sum.cal_ukeyInvDt.cal_calTo.value = ds_search.getColumn(0,"UKEY_INV_END_DT");
			}
		break;
		
		case "saveMain":
			if( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				btn_search_onclick();
			}
		break;
		
		
		case "handlingMain":
			if( iErrorCode == 0){
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				btn_search_onclick();
			}
		break;
		
		case "syncronization":
			if( iErrorCode == 0){
				
				/*ds_main에 데이터 재 세팅*/
				for(var i=0; i < ds_main.getRowCount(); i++){
					for(var j=0; j < ds_maintemp.getRowCount(); j++){
						if(ds_main.getColumn(i,"INSU_INV_NO") == ds_maintemp.getColumn(j,"INSU_INV_NO")){
							ds_main.setColumn(i,"UKEY_RECV_DT",ds_maintemp.getColumn(j,"UKEY_RECV_DT"));
							ds_main.setColumn(i,"UKEY_RECV_AMT",ds_maintemp.getColumn(j,"UKEY_RECV_AMT"));
						}
					}
				}
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				
			}
		break;
		
		default :
			break;
	}
}

