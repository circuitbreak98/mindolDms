function fn_chkBeforeSave()	
{
	//저장전 수정,신규입력데이터의 trim처리 
	gfn_dsTrim(ds_main);
	
	if (!gfn_getValidateCompList(bg_detail)) {	
		return false;
	}
		
	return true;
}

