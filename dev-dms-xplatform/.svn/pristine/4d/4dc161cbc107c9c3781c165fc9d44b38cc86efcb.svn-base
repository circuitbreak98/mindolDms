function fn_setGridTitle(objStc:Static, msg)
{
	if ( gfn_isNull(objStc.userText) ) objStc.userText = objStc.text;
	
	objStc.usedecorate = true; 
	
	objStc.text = objStc.userText + msg;	
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 엑셀 그리드 조회 건수 전체 다운로드
 * param   : obj - Button
 * param   : e  - ClickEventInfo
 * return  : void
-----------------------------------------------------------------------------------------------*/
function btn_excelAll_onclick(obj:Button,  e:ClickEventInfo)
{
	// 입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	gfn_dsTrim(ds_search);
	ds_mainExcelTemp.clearData();
	page = 1;
	page_size = 10000;
	fn_searchMainAll();
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 엑셀 다운로드 다음 Roop처리
 * param   : 
 * param   :
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_nextLoop()
{
	if ( total_record_cnt <=  page * page_size ) {
		page_size = 50;
		gfn_baseExcelExpert(grd_mainExcel,"위약금정산정보","위약금정산정보");
	} else {
		//다음페이지 조회
		page++;
		fn_searchMainAll();
	} 
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 전표생성
-----------------------------------------------------------------------------------------------*/
function btn_slipI_onclick(obj:Button,  e:ClickEventInfo)
{
	var msg = "위약금정산 전표현황의 전표를 생성하시겠습니까?";
	if(ds_list.getRowCount() == '0'){
		gfn_message("A","DMS00020","I"); // 선택된 정보가 없습니다.
		return false;
	} else {
	
		if(ds_list.findRow("CHK","1") == -1){
			//체크대상없음
			gfn_message("A","DMS00020","I"); // 선택된 정보가 없습니다.
			return;
		}

		for(i=0; i<ds_list.getRowCount(); i++){
			if(ds_list.getColumn(i,"CHK")=='1'){
				if(ds_list.getColumn(i,"SLIP_ST_CD") == '10' ){
					gfn_message("A","DMS00087","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
					return false;
				}
			}
		}
		
		if(gfn_message("A",msg,"I","","")){

			if(ds_list.getColumn(ds_list.rowposition,"SELEC") == '개인')
				ds_list.setColumn(ds_list.rowposition,"GUBUN",'01');
			else if(ds_list.getColumn(ds_list.rowposition,"SELEC") == '법인'){
				ds_list.setColumn(ds_list.rowposition,"GUBUN",'02');
			}
			
			var ym = ds_search.getColumn(0,"YDATE");
			ds_list.setColumn(ds_list.rowposition,"YYYYMM",ym);
			fn_saveSlip();
		}
	}
	
}

