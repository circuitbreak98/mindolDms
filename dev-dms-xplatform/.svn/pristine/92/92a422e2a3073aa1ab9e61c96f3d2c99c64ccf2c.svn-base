/*----------------------------------------------------------------------------------------------
 * 설명   : 전표생성 버튼을 눌렀을때 
-----------------------------------------------------------------------------------------------*/
function btn_createSlip_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_main.findRow("CHK","1") == -1){
		//체크대상없음
		gfn_message("A","DMS00020","I"); // 선택된 정보가 없습니다.
		return;
	}
	for(i=0; i<ds_main.getRowCount(); i++){
		if(ds_main.getColumn(i,"CHK")=='1'){
		
		  if( gfn_isNull(ds_main.getColumn(i,"ASSET_SLIP_ST")) || ds_main.getColumn(i,"ASSET_SLIP_ST") == '05'){
		    var nRow = ds_handling.addRow();
		    ds_handling.copyRow(nRow,ds_main,i);
		  }else{
			gfn_message("A","DMS00071","E"); //이미 처리된 건이 있습니다.
			return false;
		  }
	    }
	}
fn_slip();
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 전표취소 버튼을 눌렀을때 
-----------------------------------------------------------------------------------------------*/
function btn_cancleSlip_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_main.findRow("CHK","1") == -1){
		//체크대상없음
		gfn_message("A","DMS00020","I"); // 선택된 정보가 없습니다.
		return;
	}
	for(i=0; i<ds_main.getRowCount(); i++){
		if(ds_main.getColumn(i,"CHK")=='1'){
			if(ds_main.getColumn(i,"ASSET_SLIP_NO")!= null && ds_main.getColumn(i,"ASSET_SLIP_ST") == '20' ){
			var nRow = ds_handling.addRow();
		    ds_handling.copyRow(nRow,ds_main,i);
		    }else {
		     gfn_message("A","DMS00087","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
		     return false;
		    }
		}
	}
	
	fn_deleteSlip();
}

