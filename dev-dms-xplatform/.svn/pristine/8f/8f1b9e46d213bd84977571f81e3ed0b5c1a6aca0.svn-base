/*----------------------------------------------------------------------------------------------
 * 설명   : 손해배상금정책상세 저장(insert, update)
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_saveMain()
{
	var ds_save = new Dataset;
    
    ds_save.copyData(ds_main, true);
    this.addChild("ds_save", ds_save);

	var sSvcID		 = "saveMain";
	var sTrID		 = "PNRCmpPolyMgmt_pSaveCmpPoly";
	var sPtrn		 =  mode == 'new' ? "I" : "U";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RS_CMP_POLY_DTL=ds_save ncFieldMap=ds_field";
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
