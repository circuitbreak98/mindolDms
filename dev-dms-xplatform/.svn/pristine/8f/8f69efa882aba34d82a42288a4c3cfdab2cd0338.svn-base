-----------------------------------------------------------------------------------------------*/
function fn_setInitForm(obj:Form, e:LoadEventInfo)
{
	div_search.edt_svcMgmtNo.setFocus();
	
	var setDt = gfn_today().substring(0,6);
	div_search.cal_deprStrdYm.setDate(setDt);
	
}

/*----------------------------------------------------------------------------------------------
