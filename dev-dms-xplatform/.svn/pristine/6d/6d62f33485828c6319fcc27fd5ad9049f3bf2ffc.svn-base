/*----------------------------------------------------------------------------------------------
 * 설명   : 버튼활성화 처리 
-----------------------------------------------------------------------------------------------*/
function enableBtn(obj){
	ds_main.clearData();
	/*
		btn_suggSave 의견저장
		btn_inConf  입고확정
		btn_inExl   입고제외
		btn_cancel 취소
	*/
	if(obj.value == "IO"){
		btn_suggSave.enable = true;
		btn_inConf.enable = true;
		btn_inExl.enable = true;
		btn_cancel.enable = false;
		btn_excel_upload.enable =  true;
	}else if(obj.value == "IC"){
		btn_suggSave.enable = true;
		btn_inConf.enable = false;
		btn_inExl.enable = false;
		btn_cancel.enable = true;
		btn_excel_upload.enable =  false;
	}else if(obj.value == "IE"){
		btn_suggSave.enable = true;
		btn_inConf.enable = false;
		btn_inExl.enable = false;
		btn_cancel.enable = true;
		btn_excel_upload.enable =  false;
	}
	
	var gb1 = ds_search.getColumn(0,"SR_GUBUN_1");
	var gb2 = ds_search.getColumn(0,"SR_GUBUN_2");
	var gb3 = ds_search.getColumn(0,"SR_GUBUN_3");
	
	if(gb1 == "RE" && gb2 == "IO"){
		div_search.cbo_searchtype3.visible = true;
		div_search.cbo_searchtype2.visible = false;
	}else{
		div_search.cbo_searchtype3.visible = false;
		div_search.cbo_searchtype2.visible = true;
	}

}

