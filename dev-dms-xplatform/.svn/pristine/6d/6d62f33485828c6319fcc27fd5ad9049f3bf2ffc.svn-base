function grd_main_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	var sEqpMdlCd   = "";	// 단말기모델코드
	var sCnslMgmtNo = "";	// 상담관리번호
	var sCnslDt     = "";	// 상담일자
	var sScrbMthd   = "";	// 가입방법					
	var sPrgGb      = "";  // 검수대상구분  
	var sJdcCl = "RE";														// 감정구분

	if( div_search.cbo_searchtypeM.value == "NO" ){
		sEqpMdlCd = ds_main.getColumn(obj.currentrow,"EQP_MDL_CD");			// 단말기모델코드
		sCnslMgmtNo = ds_main.getColumn(obj.currentrow,"CNSL_MGMT_NO");		// 상담관리번호
		sCnslDt = ds_main.getColumn(obj.currentrow,"CNSL_DT");				// 상담일자
		sScrbMthd = ds_main.getColumn(obj.currentrow,"SCRB_MTHD");			// 가입방법					
		sPrgGb = "N";														// 검수대상구분
		
		if(div_search.cbo_searchtype.value == "IO"){
			sPrgGb = "J";
		}else if(div_search.cbo_searchtype.value == "IC"){
			// 입고확정   sArg += " p_prc_gb=Y";
			sPrgGb = "Y";
		}else if(div_search.cbo_searchtype.value == "IE"){
			// 입고제외   sArg += " p_prc_gb=Y";
			sPrgGb = "Y";
		}				
		sJdcCl = "NO";														// 검수구분
										
		var arrArgs = {sEqpMdlCd:sEqpMdlCd, sCnslMgmtNo:sCnslMgmtNo, sCnslDt:sCnslDt, sScrbMthd:sScrbMthd, sPrgGb:sPrgGb, sJdcCl:sJdcCl};
			
		gfn_tlyRgst("FEP003M",arrArgs);
		//gfn_openFrm("FEP006D"); //일반 검수등록
	}		
	else if( div_search.cbo_searchtypeM.value == "RE" ){
			sEqpMdlCd = ds_main.getColumn(obj.currentrow,"EQP_MDL_CD");			// 단말기모델코드
			sCnslMgmtNo = ds_main.getColumn(obj.currentrow,"CNSL_MGMT_NO");		// 상담관리번호
			sCnslDt = ds_main.getColumn(obj.currentrow,"CNSL_DT");				// 상담일자
			sScrbMthd = ds_main.getColumn(obj.currentrow,"SCRB_MTHD");			// 가입방법	
			sPrgGb = "C";														// 검수대상구분
			
			if(div_search.cbo_searchtype.value == "IO"){
				sPrgGb = "J";
			}else if(div_search.cbo_searchtype.value == "IC"){
				// 입고확정   sArg += " p_prc_gb=Y";
				sPrgGb = "Y";
			}else if(div_search.cbo_searchtype.value == "IE"){
				// 입고제외   sArg += " p_prc_gb=Y";
				sPrgGb = "Y";
			}
			
						
			sJdcCl = "RE";														// 감정구분
			var arrArgs = {sEqpMdlCd:sEqpMdlCd, sCnslMgmtNo:sCnslMgmtNo, sCnslDt:sCnslDt, sScrbMthd:sScrbMthd, sPrgGb:sPrgGb, sJdcCl:sJdcCl};
			gfn_tlyRgst("FEP003M",arrArgs);
	
	}
	
	
	
}
