/*----------------------------------------------------------------------------------------------
 * 설명   : 조회전 유효성 검사
 * param   : 
 * return  : true/false
-----------------------------------------------------------------------------------------------*/
function fn_chkBeforeSearch()
{
	//입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}else if(gfn_isNull(div_search.cmc_cnslYm.getDate())){	
		gfn_message("A","DMS00002","W","",div_search.stc_frmlId05.text); /*필수입력항목 {0}이/가 누락되었습니다. */
		div_search.cmc_cnslYm.setFocus();
		return false;
	}
	
	// 검색조건: 적용년월
	ds_search.setColumn(0, "INVE_SLIP_DT",div_search.cmc_cnslYm.getDate());
	return true;
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 데이타셋 clear
-----------------------------------------------------------------------------------------------*/
function fn_dsClear(){
	ds_main.clearData();	
	fn_setInitGrid();
	fn_setInitForm();
	stc_subTitle.text = "조회결과";
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 전체엑셀다운로드
-----------------------------------------------------------------------------------------------*/
function btn_excelAll_onclick(obj:Button,  e:ClickEventInfo)
{
	if (ds_main.rowcount == 0){
		gfn_message("A","DMS00021","I"); //조회된 정보가 없습니다.
		return;
	}
	
	// 입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	curPage = page;
	gfn_dsTrim(ds_search);
	ds_mainExcel.clearData();
	page = 1;
	page_size = 10000;
	
	gfn_initGrid(grd_mainExcel, false, true, true, "row", null);
	fn_searchMainAll();
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 전체엑셀다운로드 다운로드 다음 처리
-----------------------------------------------------------------------------------------------*/
function fn_nextLoop()
{
	
	if ( total_record_cnt <=  page * page_size ) {
		page_size = 50;
		page = curPage;
		gfn_baseExcelExpert(grd_mainExcel,"매출실적목록","매출실적목록");
	} else {
		//다음페이지 조회
		page++;
		fn_searchMainAll();
	} 
}

