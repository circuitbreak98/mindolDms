================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchJdgRtnMain": 
			if ( iErrorCode == 0 ) {
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);					
					grd_main.formatid = "RtnJdg_Format";  
					grd_main.redraw();									
			}
		break;
		case "searchCustRtnMain": 
			if ( iErrorCode == 0 ) {
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
								
					grd_main.formatid = "RtnCust_Format";  
					grd_main.redraw();		
												
			}
		break;
		case "searchAgnRtnMain": 
			if ( iErrorCode == 0 ) {
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
									
					grd_main.formatid = "RtnAgn_Format";  
					grd_main.redraw();		
												
			}
		break;
		//검수반송확인저장
		case "saveJdgRtn": 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				btn_search_onclick();								
			}
		break;
		case "saveCustRtnUpdate": 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				btn_search_onclick();								
			}
		break;
		case "saveCustRtnCncl": 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				btn_search_onclick();								
			}
		break;
		case "saveAgnRtnUpdate": 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				btn_search_onclick();								
			}
		break;	
		case "saveAgnRtnCncl": 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				btn_search_onclick();								
			}
		break;

		default:
		break;
	}
}
/*===============================================================================================
 * [ PART 7 ] Main Button Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : search
-----------------------------------------------------------------------------------------------*/
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{	
    //판매기간
	var arrTerm = div_search.ccp_rtn_dt.getData();
	ds_search.setColumn(0,"FR_DT",arrTerm[0]);
	ds_search.setColumn(0,"TO_DT",arrTerm[1]);
	
	ds_main.clearData();
	page = 1;
	
	if(ds_search.getColumn(0,"RETURN_GB") == "01"  && ds_search.getColumn(0,"DT_GB") == "1" ){ 
	    fn_searchJdgRtnMain();
	    btn_RtnConfirm.enable = true;
	    btn_cancel.enable = false;
	    btn_modyfy.enable = false;
	}else if(ds_search.getColumn(0,"RETURN_GB") == "02"){ 
		fn_searchCustRtnMain();
		btn_RtnConfirm.enable = false;
		btn_cancel.enable = true;
	    btn_modyfy.enable = true;	
	}else if(ds_search.getColumn(0,"RETURN_GB") == "03"){ 
		fn_searchAgnRtnMain();
		btn_RtnConfirm.enable = false;
		btn_cancel.enable = true;
	    btn_modyfy.enable = true;
	}
	
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 검수반송확인버튼 클릭시
-----------------------------------------------------------------------------------------------*/
function btn_RtnConfirm_onclick(obj:Button,  e:ClickEventInfo)
{	
	ds_main.filter("CHEKED == '1'");
	if(ds_main.rowcount <= 0){
		//체크대상없음
		gfn_message("A","DMS00020","I"); /*선택된 정보가 없습니다.*/
		ds_main.filter("");
		return;
	}
	//저장시 필수 값 체크 
	for(var i=0; i<ds_main.rowcount; i++) {
		if (ds_main.getColumn(i,"RETUR_RSLT") == "N"  && gfn_isNull(ds_main.getColumn(i,"RETURDAY"))) {
			gfn_message("A","DMS00002","W","","반송처리일자"); /*필수입력항목 {0}이/가 누락되었습니다. */
			return false;
		}
	}
	fn_saveJdgRtn();	
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 취소버튼 클릭시
-----------------------------------------------------------------------------------------------*/
function btn_cancel_onclick(obj:Button,  e:ClickEventInfo)
{	
	ds_main.filter("CHEKED == '1'");
	if(ds_main.rowcount <= 0){
		//체크대상없음
		gfn_message("A","DMS00020","I"); /*선택된 정보가 없습니다.*/
		ds_main.filter("");
		return;
	}
	if(ds_search.getColumn(0,"RETURN_GB") == "02"){
		fn_saveCustRtnCncl();
	}else if (ds_search.getColumn(0,"RETURN_GB") == "03"){
		fn_saveAgnRtnCncl();
	}
	
}
