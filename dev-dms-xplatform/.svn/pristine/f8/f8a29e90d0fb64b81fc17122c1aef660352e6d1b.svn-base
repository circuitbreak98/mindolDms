/*----------------------------------------------------------------------------------------------
 * 설명   : grid excel
-----------------------------------------------------------------------------------------------*/
function btn_excel_onclick(obj:Button,  e:ClickEventInfo)
{
	if (ds_main.rowcount == 0){
		gfn_message("A","DMS00021","I"); //조회된 정보가 없습니다.
	} else {
		var excelTitle = "";
		if(ds_search.getColumn(0,"RETURN_GB") == "01" && ds_search.getColumn(0,"DT_GB") == "1" ){ 
			excelTitle = "검수반송대상목록";
		}else if(ds_search.getColumn(0,"RETURN_GB") == "02" && ds_search.getColumn(0,"DT_GB") == "1"){ 
			excelTitle = "고객반송대상목록_접수일자기준";
		}else if(ds_search.getColumn(0,"RETURN_GB") == "02" && ds_search.getColumn(0,"DT_GB") == "2"){ 
			excelTitle = "고객반송대상목록_출고일자기준";
		}else if(ds_search.getColumn(0,"RETURN_GB") == "03" && ds_search.getColumn(0,"DT_GB") == "1"){ 
			excelTitle = "대리점반송대상목록_접수일자기준";
		}else if(ds_search.getColumn(0,"RETURN_GB") == "03" && ds_search.getColumn(0,"DT_GB") == "2"){ 
			excelTitle = "대리점반송대상목록_출고일자기준";
		}else{
			excelTitle = "반송목록";		
		}
		gfn_baseExcelExpert(grd_main,excelTitle, excelTitle);
	}
}
