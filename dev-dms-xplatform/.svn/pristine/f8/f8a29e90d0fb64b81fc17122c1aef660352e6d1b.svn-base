function div_search_btn_searchAffil_onclick(obj:Button,  e:ClickEventInfo)
{
	var sParam01 = div_search.edt_AffilNm.value;
	var sParam02 = "";
	var sParam03 = "A1"; 
	var sParam04 = "";
	var sParam05 = "";
	var sParam06 = div_search.edt_AffilAgn.value;
	if ( e == null ) {
		sParam04 = "key";
	} else {
		sParam04 = "button";
	}
	var arrArgs = {argAutoSelect:true, argCallProgram:"FEP012M", argParam01:sParam01, argParam02:sParam02, argParam03:sParam03, argParam04:sParam04, argParam05:sParam05, argParam06:sParam06};
	var sRtnVal = gfn_dialog("FBI004P", arrArgs);

	//팝업에서 입력받은 값 리턴
	if ( gfn_isNull(sRtnVal) || sRtnVal == "cancel" ) {
		//취소시 값 제거
		div_search.edt_AffilAgn.value = "";
		div_search.edt_AffilNm.value = "";
		div_search.edt_dealCoAgnCd.value = "";
	} else {	//Object이용시
		div_search.edt_AffilAgn.value = sRtnVal["supAgnOrgCd"];
		div_search.edt_dealCoAgnCd.value = sRtnVal["supAffilAgn"];
		div_search.edt_AffilNm.value = sRtnVal["supDealCoNm"];
	}		
}
