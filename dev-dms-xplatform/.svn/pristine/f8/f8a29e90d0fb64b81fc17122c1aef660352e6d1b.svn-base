/*-----------------------------------------------------------------------------------------------------------------
 * 설명   : 콤보박스 변환시 
 * cbo_returnGb 반송구분콤보 : 01검수, 02고객, 03대리점 
 * cbo_dtGb 접수, 출고구분콤보: 1접수일자, 2출고일자
 * cbo_rtnYn 반송처리구분콤보: %전체, Y YES, N NO //검수반송 일때만 사용, 고객.대리점은 반송처리 Y만가져옴
--------------------------------------------------------------------------------------------------------------------*/
function cbo_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{	
	switch(obj.name) {
		case "cbo_returnGb": 	
			if(div_search.cbo_returnGb.value == "01"){	//검수
			   div_search.cbo_dtGb.index = 0;
			   div_search.cbo_dtGb.enable = false; // 접수일자만 검색되도록 처리
			   div_search.cbo_rtnYn.enable = true; // 반송처리구분 사용
			}else if (div_search.cbo_returnGb.value == "02"){ //고객
				div_search.cbo_dtGb.enable = true;
				div_search.cbo_rtnYn.enable = false;
			}else if (div_search.cbo_returnGb.value == "03"){ //대리점
				div_search.cbo_dtGb.enable = true;
				div_search.cbo_rtnYn.enable = false;
			}									
			break;	
		default:
			break;
	}					
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 조회조건에서 엔터키가 눌러졌을때 조회
-----------------------------------------------------------------------------------------------*/
function div_Search_cond_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == KEYCODE_ENTER) {
		btn_search_onclick();
	}	
}
function div_Search_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) {
		btn_search.setFocus();
		btn_search_onclick();
		obj.setFocus();	
	}	
}
