				dsbind.setCellProperty("Body",4,"style","align:center");
				dsbind.setCellProperty("Body",5,"style","align:center");
				dsbind.setCellProperty("Body",6,"style","align:center");
				dsbind.setCellProperty("Body",6,"calendardisplaynulltype","none");
				dsbind.setCellProperty("Body",7,"style","align:center");
				dsbind.setCellProperty("Body",7,"calendardisplaynulltype","none");
				dsbind.setCellProperty("Body",8,"style","align:center");
				dsbind.setCellProperty("Body",8,"calendardisplaynulltype","none");
				dsbind.setCellProperty("Body",9,"style","align:center");
				dsbind.setCellProperty("Body",10,"style","align:right");
				dsbind.setCellProperty("Body",11,"style","align:right");
				dsbind.setCellProperty("Body",12,"style","align:right");
				dsbind.setCellProperty("Body",13,"style","align:right");
				dsbind.setCellProperty("Body",14,"style","align:right");
				dsbind.setCellProperty("Body",15,"style","align:center");
				dsbind.setCellProperty("Body",16,"style","align:center");
				
				gfn_baseExcelExpert(dsbind,"손해배상금정산정보목록","손해배상금정산정보목록");
			}else{
				gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
			}
		break;
			
		case "taxBill_saveMain":
		if( iErrorCode == 0){
			gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
		    ds_main.clearData();
		    ds_listtemp.clearData();
			ds_list.clearData();
		    fn_searchMain();
		}else{
			gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
			ds_main.clearData();
			ds_listtemp.clearData();
			ds_list.clearData();
