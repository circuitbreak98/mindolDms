function fn_setInitCode(obj:Form, e:LoadEventInfo)
{
	var strCode =  "DMS113=ds_DMS113=A"
	           +=  ",DMS115=ds_DMS115=N"
	           +=  ",DMS116=ds_DMS116=N"
	           +=  ",DMS112=ds_DMS112=N";
						
	gfn_getCodeDs( this
				  , strCode
				  ,"fn_afterCodeCallBack");
}

/*----------------------------------------------------------------------------------------------
