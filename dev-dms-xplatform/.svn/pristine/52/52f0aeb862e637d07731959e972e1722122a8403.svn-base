-----------------------------------------------------------------------------------------------*/
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{	
	var arrTerm = div_search.ccp_inTerm.getData();
	ds_search.setColumn(0,"IN_FR_DT",arrTerm[0]);
	ds_search.setColumn(0,"IN_TO_DT",arrTerm[1]);
	
	if(gfn_isNull(arrTerm[0]) || gfn_isNull(arrTerm[1])){
		gfn_message("A","DMS00002","E","","입고기간");  //필수입력항목 {0}이/가 누락되었습니다.
		return false;
	}
	
	// 입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	
	//조회함수 호출 
	fn_searchMain();
}

/*----------------------------------------------------------------------------------------------
 * 설명   : delete
-----------------------------------------------------------------------------------------------*/
function btn_delete_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_sub.rowcount <= 0){
		return;
	}
	if(gfn_isNull(ds_sub.getColumn(ds_sub.rowposition,"ASSET_NO"))){
		//gfn_message("A","DMS00002","E","","자산번호");  //필수입력항목 {0}이/가 누락되었습니다.
		return;
	}
	if(ds_sub.getColumn(ds_sub.rowposition,"INVE_ST_CD") != "10") //재고가 매입 상태인경우만 삭제 가능
	{
		gfn_message("A","DMS00025"); //
		return;
	}
	if (gfn_message("C","DMS00026","Q")){ //데이터를 삭제합니다. 계속하시겠습니까?
		fn_delMain();
	}
}

