 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	var sSvcID		 = "searchMain";
	var sTrID		 = "PPREqpReqpInMgmt_pInqEqpReqpInLst";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sOutputDs	 = "ds_main=RS_EQP_REQP_IN_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 상세조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchDtl()
{
	var sSvcID		 = "searchDtl";
	var sTrID		 = "PPREqpReqpInMgmt_pInqEqpReqpInDtlLst";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_searchDtl";
	var sOutputDs	 = "ds_sub=RS_EQP_REQP_IN_DTL";
	sOutputDs	 += " ds_constList=CMPT_OUT_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 상세삭제
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_delMain()
{
	ds_del.clearData();
	ds_del.addRow();
	ds_del.copyRow(0,ds_sub,ds_sub.rowposition);
	
	var sSvcID		 = "delMain";
	var sTrID		 = "PPREqpReqpInMgmt_pSaveEqpReqpIn";
	var sPtrn		 = "D";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RQ_EQP_LIST=ds_del";
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
				
				//gfn_message("B","DMS00001");	//MSGI0003 :정상 조회되었습니다.
				ds_sub.clearData();
				if(ds_main.rowcount > 0){
					ds_searchDtl.copyRow(0,ds_main,0);
					fn_searchDtl();
				}
			}
		break;
		case "searchDtl":
			ds_constList.filter("");
			ds_constList.filter("ASSET_NO == '"+ds_sub.getColumn(ds_sub.rowposition,"ASSET_NO")+"'");
		break;
		case "delMain":
			if ( iErrorCode == 0 ) {
				btn_search_onclick(btn_search);
			}
		break;
		default:
		break;
	}
}

