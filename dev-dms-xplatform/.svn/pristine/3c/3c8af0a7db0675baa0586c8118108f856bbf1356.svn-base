function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	//그리드 초기 설정사항 	
	/******************************************************************************************************************
		1. 피벗그리드 속성
			1) pivotkey : 그룹이 되는 컬럼을 지정한다. (여러개 설정가능)
			2) pivotsortkey : 출력되는 순서. pivotkey수와 동일하게 맞춰준다. (순서가 필요한 경우에만 지정한다.)
	    
	    2. 셀 속성 	
			1) 두번째 피벗그리드의 경우 피벗키가 row list로 나열되는 형태이므로 
			    background,backgound2를 동일하게 설정한다. 
			    background : #ffffffff
			    backgound2 : #ffffffff
			    
			2) 각 그룹항목(pivotkey)별 소계가 필요한 경우 
			   두번째 피벗그리드의 subsumtext 속성을 참조한다.
			   
			3) 합계
			    pivotgrid의 pivotSum메서드를 사용한다.
			    text 속성에 expr:PivotGrid01.pivotSum('합계낼 컬럼id',currow) 와 같이 설정한다. 
			
	*******************************************************************************************************************/
	
	//조회후 처리사항	
	//**********************************************
	//1. 피벗그리드 데이터셋 생성 
	//**********************************************
	//피벗그리드 reCalcPivot()메서드 호출 
 	PivotGrid00.reCalcPivot();
	PivotGrid01.reCalcPivot(); 
	
	//**********************************************
	//2. 총건수 표시하기
	//**********************************************
	//첫번째 피벗그리드타이틀에 총건수 표시하기    
	var bindDs =  eval(PivotGrid00.binddataset);
	gfn_setGridTitleCountPaging(stc_subTitle00, bindDs.rowcount) ;
	
	//두번째 피벗그리드타이틀에 총건수 표시하기    
	bindDs =  eval(PivotGrid01.binddataset);
	var sumText = "소계";			
	// [ bindDs.getCaseCount("brndNm != '"+sumText+"'") 의 의미] 
	// 두번째 피벗그리드를 보면 각 회사별로 소계가 나오는데 
	// 소계는 데이터 건수로 볼 수 없으므로 제외하고 counting하겠다는 뜻임.
	gfn_setGridTitleCountPaging(stc_subTitle02, bindDs.getCaseCount("brndNm != '"+sumText+"'") ); 
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 브랜드 검색 팝업
-----------------------------------------------------------------------------------------------*/
function divLevel02_btn_00_onclick(obj:Button,  e:ClickEventInfo)
{
	//alert("브랜드 검색 팝업");
	var sParam01 = divLevel02.edt_brnCd.value;
	var sParam02 = divLevel02.edt_brnNm.value;
	var arrArgs = {argAutoSelect:true, argCallProgram:"FRM0010M", argParam01:sParam01, argParam02:sParam02};
	var sRtnVal = gfn_dialog("FRM0054P", arrArgs);

	//팝업에서 입력받은 값 리턴
	if ( gfn_isNull(sRtnVal) || sRtnVal == "cancel" ) {
		//취소시 값 제거
		divLevel02.edt_brnCd.value = ""; 
		divLevel02.edt_brnNm.value = "";
	} else {	//Object이용시
		divLevel02.edt_brnCd.value 	= sRtnVal["supBrndCd"];
		divLevel02.edt_brnNm.value  = sRtnVal["supBrndNm"];
	//	btn_search_onclick();
	}
}

