/*******************************************************************************
 * 설명    :  상담금액 구하기(최종)
*******************************************************************************/
function f_conAmt_set()
{
	var eqpGradeAmt = 0;	// 단말기등급별 금액	
	var ddctAmt = 0;
	var conAmt = 0;	
	if("Y" == ds_eqpInfo.getColumn(ds_eqpInfo.rowposition,"FIX_PRC_YN")){
		eqpGradeAmt = toNumber(div_main3.cbo_eqpGrade.value,0);		
	}	
	if( !gfn_isNull(eqpGradeAmt) && 0 != eqpGradeAmt){
		ddctAmt = 0;
		for(var i = 0 ; i < ds_sub.getRowCount() ; i++){			
			ddctAmt = toNumber(ddctAmt,0) + toNumber(ds_sub.getColumn(i,"DDCT_AMT"),0);		
		}
		if("D" == div_main3.cbo_eqpGrade.text || "E" == div_main3.cbo_eqpGrade.text){
		    conAmt = toNumber(eqpGradeAmt,0); 															
		}else{
			if( "20120419" < ds_main.getColumn(ds_main.rowposition,"CNSL_DT") && "B" == div_main3.cbo_eqpGrade.text ){
				conAmt = toNumber(eqpGradeAmt,0);
			}else{
				conAmt = toNumber(eqpGradeAmt,0) - toNumber(ddctAmt,0);
			}
		}		
		if( conAmt > toNumber(ds_eqpInfo.getColumn(ds_eqpInfo.rowposition,"HLMT_PRC"),0)){
			conAmt = toNumber(ds_eqpInfo.getColumn(ds_eqpInfo.rowposition,"HLMT_PRC"),0);
		}		
		
		if("선택" == div_main3.cbo_eqpGrade.text){		
			div_main6.div_sub1.mae_addAmt.value = 0;	// 조정한도액
			conAmt = 0;
		}else{
			div_main6.div_sub1.mae_addAmt.value = toNumber(div_main6.div_sub1.mae_cnslAmt.value,0)*2;	// 조정한도액
		}
		
		if(div_main6.div_sub1.mae_addAmt.value < div_main6.div_sub1.mae_adjAmt.value && "E" != div_main3.cbo_eqpGrade.text){			
			gfn_message("A","DMS00123","E","", new Array('조정금액','조정한도액'));	// DMS00123 : {0}이 {1}을 초과하였습니다.					
			div_main6.div_sub1.mae_adjAmt.value = 0;
		}
		
		div_main6.div_sub1.mae_conAmt.value = toNumber(Math.floor(conAmt/100)*100);	// 산출금액
		
		if( ((div_main6.div_sub1.edt_sknEqpSt.text >= div_main3.cbo_eqpGrade.text) && "N" != div_main6.div_sub1.edt_sknEqpSt.text) ||
		("N" != div_main6.div_sub1.edt_sknEqpSt.text && "N" == div_main3.cbo_eqpGrade.text) ){
			div_main6.div_sub1.mae_cnslAmt.value = div_main6.div_sub1.mae_sknJdgAmt.value;	// 매입금액 = 현장감정금액
			// 현장감정차액 = ((현장감정금액 - 매입금액)/100) * 100;
			div_main6.div_sub1.mae_sknSmplJdgDamt.value = Math.floor(((toNumber(div_main6.div_sub1.mae_sknJdgAmt.value,0)-toNumber(div_main6.div_sub1.mae_cnslAmt.value,0))/100)*100);					
		}else{
			// 매입금액 = (산출금액/100)*100+조정금액
			div_main6.div_sub1.mae_cnslAmt.value = Math.floor((toNumber(conAmt,0)/100)*100+ toNumber(div_main6.div_sub1.mae_adjAmt.value,0));
			div_main6.div_sub1.mae_sknSmplJdgDamt.value = Math.floor((toNumber(div_main6.div_sub1.mae_sknJdgAmt.value,0)-toNumber(div_main6.div_sub1.mae_cnslAmt.value,0))/100)*100;					
		}
	}	
}

