/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {				
		case "searchMain": 
			if ( iErrorCode == 0 ) {								
				if(ds_main.getRowCount()>0){														
					//등급셋팅																							
					if("N" == ds_main.getColumn(ds_main.rowposition,"EQP_ST")){
						div_main3.cbo_eqpGrade.index = 1;
					}else if("A" == ds_main.getColumn(ds_main.rowposition,"EQP_ST")){
						div_main3.cbo_eqpGrade.index = 2;
					}
					
					if("C" == gfn_getNullToEmpty(ds_param.getColumn(ds_param.rowposition,"SCRB_MTHD"))){
						if("E" == ds_main.getColumn(ds_main.rowposition,"EQP_ST")){
							div_main3.cbo_eqpGrade.index = 3;
						}					
					}else{
						if("B" == ds_main.getColumn(ds_main.rowposition,"EQP_ST")){
							div_main3.cbo_eqpGrade.index = 3;
						}	
						if("E" == ds_main.getColumn(ds_main.rowposition,"EQP_ST")){
							div_main3.cbo_eqpGrade.index = 4;
						}				
					}
					
					if("20070101" > gfn_getNullToEmpty(ds_eqpInfo.getColumn(ds_eqpInfo.rowposition,"LAUNC_DT")) 
					&& "20120307" < gfn_getNullToEmpty(ds_main.getColumn(ds_main.rowposition,"CNSL_DT")) ){
						ds_sub_Changed();	// 구성품 가격 초기화
					}
					
					// 조정한도액 = 매입금액 * 2
					div_main6.div_sub1.mae_addAmt.value = toNumber(div_main6.div_sub1.mae_cnslAmt.value,0)*2;	// 조정한도액
					
					if("1" == ds_eqpInfo.getColumn(ds_eqpInfo.rowposition,"SLFCO_CL")){						
						div_main3.st_slfcoCl.text = "자사";					
					}else{						
						div_main3.st_slfcoCl.text = "타사";
					}
					if(!gfn_isNull(div_main3.cal_oningDt.value)){
						div_main3.st_oningDtt.text = (gfn_diffMonth(div_main3.cal_oningDt.value,gfn_today())+1)+"개월";
					}												
					// 단말기등급 콤보 변경시 함수 										
					cbo_onitemchanged(div_main3.cbo_eqpGrade, null);	
					// 감정구분
					if(gfn_isNull(ds_main.getColumn(ds_main.rowposition,"SKN_JDG_CL"))){
						div_main6.Div02.cbo_sknJdgCl.index = 0;
					}
					// 대리점차감여부
					if(gfn_isNull(ds_main.getColumn(ds_main.rowposition,"AGN_DDCT_YN"))){
						div_main6.Div02.chk_agnDdctYn.value = "N";
					}
					
					// 단말기 상세 등급 
					if("A" == div_main3.cbo_eqpGrade.text){
						div_main3.cbo_eqpGradeDtl.visible = true;
						ds_grdeDtl.deleteRow(3);
						div_main3.cbo_eqpGradeDtl.index = 0;						
					}else if("B" == div_main3.cbo_eqpGrade.text){
						div_main3.cbo_eqpGradeDtl.visible = true;
						ds_grdeDtl.deleteRow(3);
						ds_grdeDtl.deleteRow(1);
						ds_grdeDtl.deleteRow(0);
						div_main3.cbo_eqpGradeDtl.index = 0;
					}else{
						div_main3.cbo_eqpGradeDtl.visible = false;
					}
																												
					// Sub 감정내역 콤보 초기화										
					cbo_onitemchanged(div_main6.Div02.cbo_sknJdgCl,null);												
				}																			
			}
			break;			
		case "rpetSearchMain": 
			if ( iErrorCode == 0 ) {				
				//trace(ds_eqpInfo.saveXML());
				//trace(ds_eqpGrade.saveXML());
				if(ds_main.getRowCount()>0){														
					//등급셋팅
					div_main3.cbo_eqpGrade.text = ds_main.getColumn(0,"EQP_ST");
					
					if("20070101" > gfn_getNullToEmpty(ds_eqpInfo.getColumn(ds_eqpInfo.rowposition,"LAUNC_DT")) 
					&& "20120307" < gfn_getNullToEmpty(ds_main.getColumn(ds_main.rowposition,"CNSL_DT")) ){
						ds_sub_Changed();	// 구성품 가격 초기화
					}
					
					// 조정한도액 = 매입금액 * 2
					div_main6.div_sub1.mae_addAmt.value = toNumber(div_main6.div_sub1.mae_cnslAmt.value,0)*2;	// 조정한도액				
					
					if("1" == ds_eqpInfo.getColumn(ds_eqpInfo.rowposition,"SLFCO_CL")){						
						div_main3.st_slfcoCl.text = "자사";					
					}else{						
						div_main3.st_slfcoCl.text = "타사";
					}
					
					if(!gfn_isNull(div_main3.cal_oningDt.value)){
						div_main3.st_oningDtt.text = (gfn_diffMonth(div_main3.cal_oningDt.value,gfn_today())+1)+"개월";	
					}																				
					// 단말기등급 콤보 변경시 함수 										
					cbo_onitemchanged(div_main3.cbo_eqpGrade, null);	
					
					// 감정구분
					if(gfn_isNull(ds_main.getColumn(ds_main.rowposition,"SKN_JDG_CL"))){
						div_main6.Div02.cbo_sknJdgCl.index = 0;
					}
					// 대리점차감여부
					if(gfn_isNull(ds_main.getColumn(ds_main.rowposition,"AGN_DDCT_YN"))){
						div_main6.Div02.chk_agnDdctYn.value = "N";
					}
					
					// 단말기 상세 등급 
					if("A" == div_main3.cbo_eqpGrade.text){
						div_main3.cbo_eqpGradeDtl.visible = true;
						ds_grdeDtl.deleteRow(3);
						div_main3.cbo_eqpGradeDtl.index = 0;						
					}else if("B" == div_main3.cbo_eqpGrade.text){
						div_main3.cbo_eqpGradeDtl.visible = true;
						ds_grdeDtl.deleteRow(3);
						ds_grdeDtl.deleteRow(1);
						ds_grdeDtl.deleteRow(0);
						div_main3.cbo_eqpGradeDtl.index = 0;
					}else{
						div_main3.cbo_eqpGradeDtl.visible = false;
					}
					
					// Sub 감정내역 콤보 초기화										
					cbo_onitemchanged(div_main6.Div02.cbo_sknJdgCl,null);				
				}								
			}
			break;	
		case "eqpMdlSearch": 
			if ( iErrorCode == 0 ) {
				if(ds_eqpInfo.getRowCount()>0){
					
					if("20070101" > gfn_getNullToEmpty(ds_eqpInfo.getColumn(ds_eqpInfo.rowposition,"LAUNC_DT")) 
					&& "20120307" < gfn_getNullToEmpty(ds_main.getColumn(ds_main.rowposition,"CNSL_DT")) ){
						ds_sub_Changed();	// 구성품 가격 초기화
					}
					
					if("20080101" > gfn_getNullToEmpty(ds_eqpInfo.getColumn(ds_eqpInfo.rowposition,"LAUNC_DT"))){
						div_main6.div_sub1.mae_conAmt.value = gfn_getNullToEmpty(ds_eqpInfo.getColumn(ds_eqpInfo.rowposition,"LLMT_PRC"),"0");						
						div_main6.div_sub1.mae_addAmt.value = toNumber(div_main6.div_sub1.mae_cnslAmt.value,0)*2;	// 조정한도액
						
						if(div_main6.div_sub1.mae_addAmt.value < div_main6.div_sub1.mae_adjAmt.value && "E" != div_main3.cbo_eqpGrade.text){					
							gfn_message("A","DMS00123","E","", new Array('조정금액','조정한도액'));	// DMS00123 : {0}이 {1}을 초과하였습니다.					
							div_main6.div_sub1.mae_adjAmt.value = 0;			
						}
						
						div_main6.div_sub1.mae_cnslAmt.value = 	toNumber(div_main6.div_sub1.mae_conAmt.value,0)+toNumber(div_main6.div_sub1.mae_adjAmt.value,0);   // 매입금액						
					}
					
					if("1" == ds_eqpInfo.getColumn(ds_eqpInfo.rowposition,"SLFCO_CL")){						
						div_main3.st_slfcoCl.text = "자사";					
					}else{						
						div_main3.st_slfcoCl.text = "타사";
					}
					
					div_main3.cbo_color.index = 0;					
				}																
			} 
			break;	
		case "saveRpetTly": 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.				
								
				var vForm = gfn_getFrm(FV_PRE_FORM_ID);
				if (!gfn_isNull(vForm)){	
					vForm.btn_search_onclick();					
					vForm.setFocus();							
					gfn_closeFrm("FEP041D");
				}								
				
			} 
			break;	
		default :
			break;
	}
}

