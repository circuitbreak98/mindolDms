/*----------------------------------------------------------------------------------------------
 * 설명   : 콤보박스 변견된 값이 적용된후 발생.
-----------------------------------------------------------------------------------------------*/
function cbo_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{	
	switch(obj.name) {					
		case "cbo_sknJdgCl":
			if("01" == obj.value){
				div_main6.Div02.stc_msgInDate02.visible = true;
				div_main6.Div02.cbo_sknJdgClSub.visible = true;
			}else{
				div_main6.Div02.stc_msgInDate02.visible = false;
				div_main6.Div02.cbo_sknJdgClSub.visible = false;			
			}			
			if(gfn_isNull(ds_main.getColumn(ds_main.rowposition,"SKN_JDG_CL_SUB"))){
				div_main6.Div02.cbo_sknJdgClSub.index = 0;
			}			
			break;
		case "cbo_eqpGrade": 
			//trace(obj.text);
			//trace(obj.value);			
			var strdIncen ="";						 			
			if("선택" == obj.text){
				div_main6.div_sub1.mae_conAmt.value=0;
			}else{
				div_main6.div_sub1.mae_conAmt.value = obj.value;
			}			
			div_main6.div_sub1.mae_addAmt.value = toNumber(div_main6.div_sub1.mae_cnslAmt.value,0)*2;	// 조정한도액
			
			if(div_main6.div_sub1.mae_addAmt.value < div_main6.div_sub1.mae_adjAmt.value && "E" != div_main3.cbo_eqpGrade.text){			
				gfn_message("A","DMS00123","E","", new Array('조정금액','조정한도액'));	// DMS00123 : {0}이 {1}을 초과하였습니다.					
				div_main6.div_sub1.mae_adjAmt.value = 0;
			}			
									
			for(var i = 0 ; i < ds_eqpGrade.getRowCount() ; i++){
				if(obj.text == ds_eqpGrade.getColumn(i,"PRC_GRADE")){
					strdIncen = ds_eqpGrade.getColumn(i,"STRD_INCEN");
				}
			}						
			div_main6.div_sub1.mae_strdIncen.value = strdIncen;	// 기준인센티브 셋팅			
			div_main6.div_sub1.mae_cnslAmt.value = 	toNumber(div_main6.div_sub1.mae_conAmt.value,0)+toNumber(div_main6.div_sub1.mae_adjAmt.value,0);   // 매입금액			
			div_main6.div_sub1.edt_EqpSt.value = obj.text;	// 에코감정등급
			div_main6.div_sub1.mae_sknSmplJdgDamt.value = 0;	// 샘플감정차액
			
			// 단말기 상세 등급 
			ds_grdeDtl.reset();
			if("A" == div_main3.cbo_eqpGrade.text){
				div_main3.cbo_eqpGradeDtl.visible = true;
				ds_grdeDtl.deleteRow(3);
				div_main3.cbo_eqpGradeDtl.index = 0;						
			}else if("B" == div_main3.cbo_eqpGrade.text){
				div_main3.cbo_eqpGradeDtl.visible = true;
				ds_grdeDtl.deleteRow(3);
				ds_grdeDtl.deleteRow(1);
				ds_grdeDtl.deleteRow(0);
				div_main3.cbo_eqpGradeDtl.index = 0;
			}else{
				div_main3.cbo_eqpGradeDtl.visible = false;
			}
					
			f_conAmt_set();	// 상담금액 구하기
			
			break;						
		default :
			break;
	}
}

