================================================================================================*/
include "LIB::common.xjs"; //공통함수 호출
include "LIB::comFile.xjs"; //공통함수 호출
include "LIB::comEpBiz.xjs";	// 중고폰 공통 업무 호출
/*===============================================================================================
 * [ PART 2 ] Form에서 사용될 전역변수를 선언
================================================================================================*/

 var FV_PRE_FORM_ID = "";	// 전 화면 form Id
 var FV_EQP_E = "";
 var fvCellText = "";
/*===============================================================================================
 * [ PART 3 ] Form Event 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Form 활성화 시 재 초기화
-----------------------------------------------------------------------------------------------*/
function fn_reOnload(obj:Form)
{
	//공통 Form 초기화
	gfn_setInitPop(obj, null, "FEP041D");	
 	//공통코드 초기화
 	fn_setInitCode(obj, null);
 	//그리드 초기화
 	fn_setInitGrid(obj, null);
 	//기본값 초기화
 	fn_setInitForm(obj, null);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : Form Load Event
-----------------------------------------------------------------------------------------------*/
function Form_onload(obj:Form, e:LoadEventInfo)
{
	
	//공통 Form 초기화
	gfn_setInitPop(obj, e, "FEP041D");
	//공통코드 초기화
 	fn_setInitCode(obj, e);
 	//그리드 초기화
 	fn_setInitGrid(obj, e);
 	//기본값 초기화
 	fn_setInitForm(obj, e);
 	 	 	 	
 	// 조회
 	//fn_searchMain();
}

/*----------------------------------------------------------------------------------------------
 * 설명   : Form 종료전 Check 함수(공통)
-----------------------------------------------------------------------------------------------*/
function Form_checkBeforeClose()
{
	//변경된 Dataset검증
	if( gfn_formBeforeOnClose(this, "ds_main") && gfn_formBeforeOnClose(this, "ds_sub")){
		return true;
	} else {
		return false;
	}
}

/*===============================================================================================
 * [ PART 4 ] Load 후 초기설정
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitCode(obj:Form, e:LoadEventInfo)
{
	// 추후 사용시 추가	
	var strCode =  " ";	
	var strCode =  "DMS156=ds_DMS156=N";		 
		strCode +=  ",DMS166=ds_DMS166=S";
		strCode +=  ",DMS100=ds_DMS100=S";
		strCode +=  ",DMS183=ds_DMS183=A";
		strCode +=  ",DMS164=ds_DMS164=A";
		strCode +=  ",DMS178=ds_DMS178=A";
		strCode +=  ",DMS226=ds_DMS226=S";
		strCode +=  ",DMS180=ds_DMS180=S";
		strCode +=  ",DMS181=ds_DMS181=S";
						
	gfn_getCodeDs( this
				  , strCode
				  ,"fn_afterCodeCallBack");			  
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 호출 후 처리
-----------------------------------------------------------------------------------------------*/
function fn_afterCodeCallBack()
{
	ds_DMS180.filter("MGMT_ITEM_CD1=='Y'|| CM_CD_NM=='선택'");
	ds_DMS181.filter("MGMT_ITEM_CD1=='Y'|| CM_CD_NM=='선택'");	
	if(!gfn_isNull(gv_preFormId)){	// 글로벌 변수 gv_preFormId 초기화 안되었을시
		FV_PRE_FORM_ID = gv_preFormId;	// 글로벌 변수 값을 전역변수에 셋팅
		gv_preFormId = "";				// 글로벌 변수 값 초기화				
	}else{
		gfn_message("A","DMS00009","E");	// DMS00009 : 시스템 오류가 발생하였습니다. 운영팀에 문의하세요.		
	}
				
	var paramList = gfn_getFrm(FV_PRE_FORM_ID); //데이터셋을 가져오고자 하는 화면ID
	if (!gfn_isNull(paramList)){			
		ds_param.clearData();
		var sRow = ds_param.addRow();
		ds_param.copyRow(sRow, paramList.ds_param, paramList.ds_param.rowposition);			
	}
		
	if("C" == gfn_getNullToEmpty(ds_param.getColumn(ds_param.rowposition,"SCRB_MTHD"))){
		div_main.stc_scrbMthd.text = "기변";	
	}else{
		div_main.stc_scrbMthd.text = "";
	}
	
	// 화면구성 초기화
	fn_formInit(ds_param.getColumn(ds_param.rowposition,"PRG_GB"));
			
	// 고객의사 결정변
	if("P" == gfn_getNullToEmpty(ds_param.getColumn(ds_param.rowposition,"PRG_GB")) 
		|| "J" == gfn_getNullToEmpty(ds_param.getColumn(ds_param.rowposition,"PRG_GB"))){
		btn_save.enable = false;
	}else{
		btn_save.enable = true;
	}
		
	// 공통코드 콜백에서 처리.	
	div_main.cbo_jdcCl.value = ds_param.getColumn(0,"JDC_CL");					
	
	div_main5.cbo_bnkCd.index = 0;	// 은행구분
	div_main3.cbo_color.index = 0;  // 색상구분				
			
	if(!gfn_isNull(ds_param.getColumn(0,"CNSL_MGMT_NO"))){
	 	ds_param.setColumn(0,"NEW_YN","N");
		fn_searchMain();	// 상담관리번호가 존재시에 조회			
	}		
}

