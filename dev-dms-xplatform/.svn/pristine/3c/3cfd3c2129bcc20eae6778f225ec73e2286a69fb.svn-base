/*----------------------------------------------------------------------------------------------
 * 설명   : 주소검색후 우편번호,주소 셋팅 
-----------------------------------------------------------------------------------------------*/
function msie_TitleChange(obj:ActiveX, e)
{
	if(!gfn_isEmpty(gfn_replace(e.Text,"|","")) && "로그인" != e.Text && "blank" != e.Text ){
		var zip1 = gfn_getNullToEmpty(e.Text.split("|")[1].substr(0,3));	// 우편번호 1	
		var zip2 = gfn_getNullToEmpty(e.Text.split("|")[1].substr(3,2));	// 우편번호 2	
		var addr = gfn_getNullToEmpty(e.Text.split("|")[0].split(",")[0]); 	// 주소
		var addrDtl = gfn_getNullToEmpty(e.Text.split("|")[0].split(",")[1]);	// 상세주소
		/*
		trace("우편번호 1 ==> " + zip1);
		trace("우편번호 2 ==> " + zip2);	
		trace("주소 ==> " + addr);
		trace("상세주소 ==> " + addrDtl);
		*/
		/* 컴포넌트 ID 맞게 수정..*/
		div_main5.edt_zipCd1.value = gfn_trim(zip1);
		div_main5.edt_zipCd2.value = gfn_trim(zip2);
		div_main5.edt_addr.value = gfn_trim(addr);
