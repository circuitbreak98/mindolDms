/*******************************************************************************
 * 설명    :  저장시 validation check
*******************************************************************************/
function fn_saveValidationCheck()
{
    var bFlag = true;
	
    if ( gfn_isNull(div_main2.edt_dealCoCd2.text) || gfn_isNull(div_main2.edt_dealCoNm2.text) ) {
		gfn_message("A","DMS00002","W","",div_main2.stc_msgNm.text); /*상담처: 필수입력항목 {0}이/가 누락되었습니다. */
		div_main2.edt_dealCoNm2.setFocus();
		return false;
	}
	
	if ( gfn_isNull(div_main2.edt_userId.text) )	{
		gfn_message("A","DMS00002","W","",div_main2.stc_msgNm00.text); /*처리자ID: 필수입력항목 {0}이/가 누락되었습니다. */
		div_main2.edt_userId.setFocus();
		return false;
	}
	
	if ( gfn_isNull(div_main3.edt_eqpMdlCd.text) || gfn_isNull(div_main3.edt_eqpMdlNm.text) ) {
		gfn_message("A","DMS00002","W","",div_main3.stc_msgNm.text); /*모델: 필수입력항목 {0}이/가 누락되었습니다. */
		div_main3.edt_eqpMdlNm.setFocus();
		return false;
	}
	
	if ( gfn_isNull(div_main3.cbo_cmctCo.value) ) {
		gfn_message("A","DMS00002","W","",div_main3.stc_msgNm07.text); /*통신사: 필수입력항목 {0}이/가 누락되었습니다. */
		div_main3.cbo_cmctCo.setFocus();
		return false;
	}
	
	if ( gfn_isNull(div_main3.edt_eqpSerNo.text) ) {
		gfn_message("A","DMS00002","W","",div_main3.stc_msgNm01.text); /*일련번호: 필수입력항목 {0}이/가 누락되었습니다. */
		div_main3.edt_eqpSerNo.setFocus();
		return false;
	}
	
	if ( gfn_getLength(div_main3.edt_eqpSerNo.text) != 6 && 
	     gfn_getLength(div_main3.edt_eqpSerNo.text) != 7 && 
		 gfn_getLength(div_main3.edt_eqpSerNo.text) != 11 &&
		 gfn_getLength(div_main3.edt_eqpSerNo.text) != 12) {
		gfn_message("A","DMS00120","E","",new Array(div_main3.stc_msgNm01.text,'7자리','11자리 또는 12자리')); /* 일련번호: {0}는 {1} 또는 {2}만 입력가능합니다. */ 
		div_main3.edt_eqpSerNo.setFocus();
		return false;
	}
	
	if ( gfn_isNull(div_main3.cbo_color.value) || div_main3.cbo_color.text == "") {
		gfn_message("A","DMS00002","W","",div_main3.stc_msgNm02.text); /*색상: 필수입력항목 {0}이/가 누락되었습니다. */
		div_main3.cbo_color.setFocus();
	}
	
	if ( gfn_isNull(div_main3.edt_imei.text)) {
		gfn_message("A","DMS00002","W","",div_main3.stc_msgNm04.text); /*IMEI: 필수입력항목 {0}이/가 누락되었습니다. */
		div_main3.edt_imei.setFocus();
		return false;
	}

	if ( gfn_isNull(div_main3.cbo_eqpGrade.value) || div_main3.cbo_eqpGrade.value == "100" ) {
		gfn_message("A","DMS00002","W","",div_main3.stc_msgNm05.text); /*단말기등급: 필수입력항목 {0}이/가 누락되었습니다. */
		div_main3.cbo_eqpGrade.setFocus();
		return false;
	}
	
	if ( gfn_isNull(div_main4.edt_sellGrade.text)) {
		gfn_message("A","DMS00002","W","",div_main4.stc_05.text); /*판매등급: 필수입력항목 {0}이/가 누락되었습니다. */
		div_main4.edt_sellGrade.setFocus();
		return false;
	}
	
	if ( gfn_isNull(div_main5.edt_svcNo.text) ) {
		gfn_message("A","DMS00002","W","",div_main5.stc_msgNm05.text); /*서비스번호: 필수입력항목 {0}이/가 누락되었습니다. */
		div_main5.edt_svcNo.setFocus();
		return false;
	}
	
	if ( gfn_isNull(div_main5.edt_svc_mgmt_no.text) ) {
		gfn_message("A","DMS00002","W","",div_main5.stc_msgNm01.text); /*서비스관리번호: 필수입력항목 {0}이/가 누락되었습니다. */
		div_main5.edt_svc_mgmt_no.setFocus();
		return false;
	}
	
	if ( gfn_getLength(div_main5.edt_svc_mgmt_no.text) != 10) {
		gfn_message("A","DMS00119","E","",new Array(div_main5.stc_msgNm01.text,'10자리')); /* 서비스관리번호: {0}는 {1}만 입력가능합니다. */
		div_main5.edt_svc_mgmt_no.setFocus();
		return false;
	}	
	
	if ( (div_main6.div_sub1.edt_sknEqpSt.text < div_main6.div_sub1.edt_EqpSt.text) && div_main6.Div02.chk_agnDdctYn.value == 'N' && FV_EQP_E == 'Y' ){
          gfn_message("A","DMS00121","E"); /* 대리점차감여부를 확인해 주시길 바랍니다. */
		  return false;
    }  
	
	return bFlag;
}

