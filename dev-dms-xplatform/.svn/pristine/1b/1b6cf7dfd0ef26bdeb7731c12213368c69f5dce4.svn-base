/*----------------------------------------------------------------------------------------------
 * 설명   : 재집계 버튼 클릭시
-----------------------------------------------------------------------------------------------*/
function btn_slipOnline_onclick(obj:Button,  e:ClickEventInfo)
{
	var arrTerm = div_search.cmc_StrdDt.getData();
	// 필수 입력 항목 Validate Check
	if(gfn_isNull(arrTerm[0]) || gfn_isNull(arrTerm[1])){
		gfn_message("A","DMS00002","E","",div_search.stc_msgInDate.text);  //필수입력항목 {0}이/가 누락되었습니다.
		div_search.cmc_StrdDt.setFocus();
		return false;
	}else{			
		var xclYm = arrTerm[0].substr(0,4)+"년 "+arrTerm[0].substr(4,2)+"월"+arrTerm[0].substr(6,2)+"일 ~";		// 정산기준연월
			xclYm += arrTerm[1].substr(0,4)+"년 "+arrTerm[1].substr(4,2)+"월"+arrTerm[1].substr(6,2)+"일 ";
	    var xclGb = div_search.cbo_gb.text;
		if(gfn_message("C","DMS00144","Q","",xclYm+xclGb+" 정산")){
			// 조건: 정산기준연월
			ds_search.setColumn(0, "STRD_FR", arrTerm[0]);
			ds_search.setColumn(0, "STRD_TO", arrTerm[1]);
			fn_saveAmtRmtXcl();		
		}		
	}

}
/*----------------------------------------------------------------------------------------------
 * 설명   : 삭제 버튼 클릭시
-----------------------------------------------------------------------------------------------*/
function btn_delete_onclick(obj:Button,  e:ClickEventInfo)
{
	ds_main.filter("CHK=='1'");
	if(ds_main.getRowCount() == 0){
		ds_main.filter("");
		gfn_message("A","DMS00004","I");
		return;
	}
	ds_main.filter("");
	
	if(gfn_message("C","DMS00026","I")){//데이터를 삭제합니다. 계속하시겠습니까?
		ds_main.filter("CHK=='1'");
		for(var i=0; i<ds_main.getRowCount(); i++){
			ds_xclDel.addRow();
			ds_xclDel.copyRow(i,ds_main,i);
			ds_xclDel.setColumn(i, "STRD_FR", ds_main.getColumn(i,'XCL_STRD_STA_DT'));
			ds_xclDel.setColumn(i, "STRD_TO", ds_main.getColumn(i,'XCL_STRD_END_DT'));
		}
		ds_main.filter("");
		fn_delAmtRmtXcl();
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명   : search
-----------------------------------------------------------------------------------------------*/
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{	
	var arrTerm = div_search.cmc_StrdDt.getData();
	// 필수 입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	if(gfn_isNull(arrTerm[0]) || gfn_isNull(arrTerm[1])){
		gfn_message("A","DMS00002","E","",div_search.stc_msgInDate.text);  //필수입력항목 {0}이/가 누락되었습니다.
		div_search.cmc_StrdDt.setFocus();
		return false;
	}
	
	// 검색조건: 적용일자
	ds_search.setColumn(0, "STRD_FR", arrTerm[0]);
	ds_search.setColumn(0, "STRD_TO", arrTerm[1]);
	
	fn_searchMain();
}

