/*----------------------------------------------------------------------------------------------
 * 설명   : 그리드 이벤트
-----------------------------------------------------------------------------------------------*/
function grd_main_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if("9" == e.col){
		var sRtnVal = gfn_excelPageIndex(0,"");								
		if ( !gfn_isNull(sRtnVal) && sRtnVal["excelProcYn"] == true){
			ds_excel.clearData();
			ds_search_dtl.setColumn(0, "ACCO_RMT_XCL_NO", ds_main.getColumn(ds_main.rowposition, "ACCO_RMT_XCL_NO"));
			ds_search_dtl.setColumn(0, "EXCEL_FIRST" ,sRtnVal["excelFirst"]);
	        ds_search_dtl.setColumn(0, "EXCEL_LAST" ,sRtnVal["excelLast"]);	
			fn_searchDtl();
		}		
	}
	if("1" == e.col){
		var chk = ds_main.getColumn(e.row,'CHK');
		for(i=0; i<ds_main.getRowCount(); i++){
			if(ds_main.getColumn(e.row,'CNCL_SLIP_YN') != 'Y'){
				if(ds_main.getColumn(i,'INVE_TYP_CD') !=  30 && ds_main.getColumn(e.row,'INVE_TYP_CD') !=  30){
					if(ds_main.getColumn(e.row,'XCL_STRD_STA_DT') == ds_main.getColumn(i, "XCL_STRD_STA_DT") ){
						ds_main.setColumn(i, "CHK", chk);
					}else{
						ds_main.setColumn(i, "CHK", 0);
					}
				}else{
					if("1" == chk){
						if(e.row != i){
							ds_main.setColumn(i, "CHK", 0);
						}
					}
				}
			}else{
				if(e.row != i){
					ds_main.setColumn(i, "CHK", 0);
				}
			}
			
		}
	}
}
