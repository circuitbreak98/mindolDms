/*----------------------------------------------------------------------------------------------
 * 설명   : 전표발행 버튼 클릭시
-----------------------------------------------------------------------------------------------*/
function btn_slipI_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_main.getRowCount() == 0){
		gfn_message("A","DMS00004","I");
		return;
	}
	
	ds_main.filter("CHK=='1'");
	if(ds_main.getColumn(0,"INVE_TYP_CD") == '30'){
		if(ds_main.getSum("ACCO_RMT_AMT",0,ds_main.getRowCount()) == 0){
		ds_main.filter("");
		gfn_message("A","DMS00157","I");
		return false;
		}
	}else{
		ds_main.filter("CHK=='1' && INVE_TYP_CD !='30'");
		if(ds_main.getSum("ACCO_RMT_AMT",0,ds_main.getRowCount()) == 0){
		ds_main.filter("");
		gfn_message("A","DMS00157","I");
		return false;
		}
	}
	if(ds_main.getSum("ACCO_RMT_AMT",0,ds_main.getRowCount()) == 0){
		ds_main.filter("");
		gfn_message("A","DMS00157","I");
		return false;
	}
	
	for(i=0; i<ds_main.getRowCount(); i++){
		if(ds_main.getColumn(i,"SLIP_ST") == '10' || ds_main.getColumn(i,"SLIP_ST") == '20' ){
			ds_main.filter("");
			gfn_message("A","DMS00087","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
			return false;
		}
	}

	var msg = ds_main.getColumn(0,'XCL_STRD_STA_DT').substr(0,4)+"년 "+ds_main.getColumn(0,'XCL_STRD_STA_DT').substr(4,2)+"월"+ds_main.getColumn(0,'XCL_STRD_STA_DT').substr(6,2)+"일 ~";		// 정산기준연월
		msg += ds_main.getColumn(0,'XCL_STRD_END_DT').substr(0,4)+"년 "+ds_main.getColumn(0,'XCL_STRD_END_DT').substr(4,2)+"월"+ds_main.getColumn(0,'XCL_STRD_END_DT').substr(6,2)+"일 ";
		msg += ds_main.getSum("ACCO_RMT_XCL_CNT",0,ds_main.getRowCount())+"건, 총 금액 "+gfn_toFixedLocaleString(ds_main.getSum("ACCO_RMT_AMT",0,ds_main.getRowCount())) +"원에 대한  전표를 생성 하시겠습니까?";  
	
	ds_mainTmp.clearData();	
	for(var i=0; i<ds_main.getRowCount(); i++){
		ds_mainTmp.addRow();
		ds_mainTmp.copyRow(i,ds_main,i);
	}
	
	ds_main.filter("");
	if(gfn_message("A",msg,"I","","")){
		fn_saveSlip();
	}
	
	
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 전표취소 버튼 클릭시
-----------------------------------------------------------------------------------------------*/
function btn_slipC_onclick(obj:Button,  e:ClickEventInfo)
{
	/*var xclDtFr = ds_main.getColumn(ds_main.rowposition,'XCL_STRD_STA_DT');
	var xclDtTo = ds_main.getColumn(ds_main.rowposition,'XCL_STRD_END_DT');*/
	
	if(ds_main.getRowCount() == 0){
		gfn_message("A","DMS00004","I");
		return;
	}
	ds_main.filter("CHK=='1'");
	if(ds_main.getRowCount() == 0){
		ds_main.filter("");
		gfn_message("A","DMS00157","I");
		return;
	}
	for(i=0; i<ds_main.getRowCount(); i++){
		if(ds_main.getColumn(i,"SLIP_ST") != '10' ){
			ds_main.filter("");
			gfn_message("A","DMS00087","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
			return false;
		}  
	}
	
	ds_mainTmp.clearData();		
	for(var i=0; i<ds_main.getRowCount(); i++){
		ds_mainTmp.addRow();
		ds_mainTmp.copyRow(i,ds_main,i);
	}
	ds_main.filter("");		
	if(gfn_message("A","해당전표를 취소하시겠습니까?","I","","")){
		fn_deleteSlip();	
	}	
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 전표상태재조회
-----------------------------------------------------------------------------------------------*/
function btn_slipRe_onclick(obj:Button,  e:ClickEventInfo)
{
	var arrTerm = div_search.cmc_StrdDt.getData();
	// 필수 입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	if(gfn_isNull(arrTerm[0]) || gfn_isNull(arrTerm[1])){
		gfn_message("A","DMS00002","E","",div_search.stc_msgInDate.text);  //필수입력항목 {0}이/가 누락되었습니다.
		div_search.cmc_StrdDt.setFocus();
		return false;
	}
	
	
	
	if(gfn_message("A","전표상태재조회를 진행하시겠습니까?","I","","")){
		// 검색조건: 적용일자
		ds_search.setColumn(0, "STA_DT", arrTerm[0]);
		ds_search.setColumn(0, "END_DT", arrTerm[1]);
		fn_ReState();
	}
}

