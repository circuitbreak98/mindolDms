/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {										
				//ds_main.keystring = 'G:+XCL_STRD_STA_DT-XCL_STRD_END_DT)';
				if( !gfn_isEmpty(ds_main.getColumn(0,"TOTAL_CNT")) ){
					gfn_setGridTitleCountPaging(stc_subTitle, ds_main.getColumn(0,"TOTAL_CNT"));
				}else{
					stc_subTitle.text="조회결과";
				}
				grd_main.setCellProperty("Head", 1, "displaytype",  'none');
			}
			break;
		case "searchDtl": 
			if ( iErrorCode == 0 ) {
				
				ds_excel.appendData(ds_tmpExcel,true);
				if(ds_tmpExcel.getRowCount()>0){
					var excelTotCnt =  ds_tmpExcel.getColumn(0,"EXCEL_TOTAL_CNT");	// 데이터 총 건수		
															
					var sRtnVal = gfn_excelPageIndex(excelPage,excelTotCnt);												
					if ( !gfn_isNull(sRtnVal) && sRtnVal["excelProcYn"] == true){
						ds_search_dtl.setColumn(0, "ACCO_RMT_XCL_NO", ds_main.getColumn(ds_main.rowposition, "ACCO_RMT_XCL_NO"));
						ds_search_dtl.setColumn(0, "EXCEL_FIRST" ,sRtnVal["excelFirst"]);
						ds_search_dtl.setColumn(0, "EXCEL_LAST" ,sRtnVal["excelLast"]);	
						fn_searchDtl();
					}													
				}
				
				if(ds_main.getColumn(ds_main.rowposition, "INVE_TYP_CD") == "20" && ds_main.getColumn(ds_main.rowposition, "CNCL_SLIP_YN") != "Y"){
					grd_excelNotArriv.redraw();						
					gfn_baseExcelExpert(grd_excelNotArriv,"단말기대금계좌송금상세목록","단말기대금계좌송금상세목록");	
				}else if(ds_main.getColumn(ds_main.rowposition, "INVE_TYP_CD") == "10" && ds_main.getColumn(ds_main.rowposition, "CNCL_SLIP_YN") != "Y"){
					grd_excelArriv.redraw();						
					gfn_baseExcelExpert(grd_excelArriv,"단말기대금계좌송금상세목록","단말기대금계좌송금상세목록");	
				}else if(ds_main.getColumn(ds_main.rowposition, "INVE_TYP_CD") == "30" && ds_main.getColumn(ds_main.rowposition, "CNCL_SLIP_YN") != "Y"){
					grd_excelE4u.redraw();						
					gfn_baseExcelExpert(grd_excelE4u,"단말기대금계좌송금상세목록","단말기대금계좌송금상세목록");	
				}else{
					grd_excelSlipDel.redraw();						
					gfn_baseExcelExpert(grd_excelSlipDel,"단말기대금계좌송금상세목록","단말기대금계좌송금상세목록");	
				}
			}
			break;	
		case "saveAmtRmtXcl": 
			if ( iErrorCode == 0 ) {		
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				ds_main.clearData();
				ds_mainTmp.clearData();
				btn_search_onclick();										
			}			
			break;
		case "delAmtRmtXcl": 					
			if ( iErrorCode == 0 ) {								
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				ds_main.clearData();
				ds_xclDel.clearData();
				btn_search_onclick();										
			}			
			break;
			
		case "saveSlip":
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				ds_main.clearData();
				btn_search_onclick();
			}else{
				gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
			}
		break;
		
		case "delSlip":
			if (iErrorCode == 0) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				ds_main.clearData();
				btn_search_onclick();
			}else{
				gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
			}
		break;
		
		case "ReState":
			if (iErrorCode == 0) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				btn_search_onclick();
			}
		break;	
		default:
			break;
	}
}

