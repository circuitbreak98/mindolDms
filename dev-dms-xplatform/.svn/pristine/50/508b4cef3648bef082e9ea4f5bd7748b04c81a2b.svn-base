    </Bind>
    <Script type="xscript4.0"><![CDATA[/*
 * 01. 업무구분 : 공통
 * 02. 화면 : DealCoSrchP.xfdl
 * 03. 화면 설명 : 거래처검색팝업
 * 04. 관련 화면/서비스 : 
 * 05. 작성자 : 이영진
 * 06. 작성일 : 2015.07.06
 * 07. 수정이력 :
 ************************************************************************************************
 *     수정일        이  름    사유
 ************************************************************************************************
 *     2015.07.06    이영진    최초 작성
 ************************************************************************************************
 */
 
/*===============================================================================================
 * [ PART 1 ] Form에서 Include 할 라이브러리(xjs)파일 정의
================================================================================================*/
include "LIB::common.xjs"; //공통함수 호출

/*===============================================================================================
 * [ PART 2 ] Form에서 사용될 전역변수를 선언
================================================================================================*/
var inputType;
/*===============================================================================================
 * [ PART 3 ] Form Event 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Form Load Event
-----------------------------------------------------------------------------------------------*/
function Form_onload(obj:Form, e:LoadEventInfo)
{
	//공통 Form 초기화
	gfn_setInitPop(obj, e, "FBI004P");
	
 	//Form 초기 개발자 셋팅
 	//공통코드 초기화
 	fn_setInitCode(obj, e);
 	//그리드 초기화
 	fn_setInitGrid(obj, e);
 	//기본값 초기화
 	fn_setInitForm(obj, e);
 	
 	if( ds_dealCo.getRowCount() == 1){
		fn_selectValue(0);
	}
}

/*===============================================================================================
 * [ PART 4 ] Load 후 초기설정
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitCode(obj:Form, e:LoadEventInfo)
{
	var strCode =  "DMS103=ds_DMS103=A";
	   strCode += ",DMS104=ds_DMS104=A";
	   strCode += ",DMS103=ds_DMS1031=N";
	   strCode += ",DMS104=ds_DMS1041=N";
						
	gfn_getCodeDs( this
				  , strCode
				  ,"fn_afterCodeCallBack");
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 호출 후 처리
-----------------------------------------------------------------------------------------------*/
function fn_afterCodeCallBack()
{
	div_search.cbo_dealCoCl1.index=0;		  	
	div_search.cbo_dealCoStCd.index=0;
	
	//자동조회시 파라메타 전달예제
	if ( !gfn_isNull(argParam01) ) {
		ds_ncFieldMap.setColumn(0,'DEALCO_NM',argParam01);
	}
	if ( !gfn_isNull(argParam02) ) {
		ds_ncFieldMap.setColumn(0,'DEALCO_CD',argParam02);
	}
	if ( !gfn_isNull(argParam03) ) {
		ds_ncFieldMap.setColumn(0,'DEALCO_CL_1',argParam03);
	}	
	try {
		if ( !gfn_isNull(argParam04) ) {
			inputType = argParam04;
		} else {
			inputType = "button";
		}
	}
	catch(e) {

	}
	try {
		if ( !gfn_isNull(argParam05) ) {
			ds_ncFieldMap.setColumn(0,'AFFIL_AGN',argParam05);
		}
	}
	catch(e) {

	}
	try {
		if ( !gfn_isNull(argParam06) ) {
			ds_ncFieldMap.setColumn(0,'AGN_ORG_CD',argParam06);
		}
	}
	catch(e) {

	}
	
	btn_search.click();
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 그리드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitGrid(obj:Form, e:LoadEventInfo)
{
	//기본페이지 설정
	page = 1;   
	page_size = 13;
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 기본값 설정
-----------------------------------------------------------------------------------------------*/
function fn_setInitForm(obj:Form, e:LoadEventInfo)
{
	div_search.edt_dealCoNm.setFocus();
	//alert(argParam01+"/"+argParam02);

}

/*===============================================================================================
 * [ PART 5 ] Transaction 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	var sSvcID		 = "searchMain";
	var sTrID		 = "PBIDealCoMgmt_pInqDealCoLst";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_ncFieldMap";
	var sOutputDs	 = "ds_dealCoTemp=RS_DEALCO_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";

	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{

	switch(sSvcID) {
		case "searchMain" : 
			if ( iErrorCode == 0 ) {
				//페이징 관련셋팅
				ds_dealCo.appendData(ds_dealCoTemp,true);	
				//조회된 첫번째row로 setFocus 
				ds_dealCo.rowposition = (page-1) * page_size ;

				gfn_setGridTitleCountPaging(stc_subTitle01, total_record_cnt);
				div_search.edt_dealCoNm.setFocus();
				//Data 1건일 경우 close
				if( ds_dealCo.getRowCount() == 1 && inputType == "key"){
					fn_selectValue(0);
				}
			}
			break;	
		
		default :
			break;
	}
}

/*===============================================================================================
 * [ PART 7 ] Main Button Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 버튼 Event
-----------------------------------------------------------------------------------------------*/
function button_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj.name) 
	{	
		case "btn_search" :		//조회
		    // 입력 항목 Validate Check
			if (!gfn_getValidateCompList(div_search)) {	
				return false;
			}
			ds_dealCo.clearData();
			page = 1;
			fn_searchMain();
			break;
			
		case "btn_confirm" :	//확인
			fn_selectValue(ds_dealCo.rowposition);
			break;
			
		case "btn_cancel" : 	//취소
			close("cancel");
			break;
			
		case "btn_close" : 		//버튼X
			close("cancel");
			break;	
	}
}

/*===============================================================================================
 * [ PART 8 ] Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Grid Double Click Event
-----------------------------------------------------------------------------------------------*/
function grd_dealCo_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	fn_selectValue(e.row);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 조회조건에서 엔터키가 눌러졌을때 조회
-----------------------------------------------------------------------------------------------*/
function div_Search_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == KEYCODE_ENTER) {
		btn_search.setFocus();
		ds_dealCo.clearData();
		page = 1;
		fn_searchMain();
	}	
}

/*===============================================================================================
 * [ PART 9 ] User Defined
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 결과값 리턴후 Close
-----------------------------------------------------------------------------------------------*/
function fn_selectValue(nRow)
{
	//단건(Object사용)
	if ( nRow == -1 ) return;
	
	var objRtn = new Object;
		objRtn["supDealCoCd"] = ds_dealCo.getColumn(nRow,"DEALCO_CD");
		objRtn["supDealCoNm"] = ds_dealCo.getColumn(nRow,"DEALCO_NM");
		objRtn["supAgnOrgCd"] = ds_dealCo.getColumn(nRow,"AGN_ORG_CD");
		objRtn["supOrgNm"] = ds_dealCo.getColumn(nRow,"ORG_NM");
		objRtn["supAffilAgn"] = ds_dealCo.getColumn(nRow,"AFFIL_AGN");

	this.close(objRtn);		
	
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 수직 스크롤바가 마지막에 도달했을때 페이지 조회 이벤트
 * param   : obj - Grid Object
 * param   : e  - ScrollEventInfo
 * return  : void
-----------------------------------------------------------------------------------------------*/
function grd_dealCo_onvtracklast(obj:Grid, e:ScrollEventInfo)
{
	
	if ( total_record_cnt <=  page * page_size ) {
	//	gfn_message("A","DMS00028","I"); //조회 마지막 페이지 입니다.   
		return;
	} else {
		//다음페이지 조회
		page++;
		fn_searchMain();
	} 
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 조회조건에서 엔터키가 눌러졌을때 조회
-----------------------------------------------------------------------------------------------*/
function div_search_msk_bizNum_onkeydown(obj:MaskEdit, e:KeyEventInfo)
{
	if (e.keycode == KEYCODE_ENTER) {
		btn_search.setFocus();
		ds_dealCo.clearData();
		page = 1;
		fn_searchMain();
	}
}
]]></Script>
