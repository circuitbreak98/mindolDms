function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	ds_search.setColumn(0,"RTN_STA_DT",div_search.cal_eqpRtnDt.cal_calFrom.value);
	ds_search.setColumn(0,"RTN_END_DT",div_search.cal_eqpRtnDt.cal_calTo.value);
	
	ds_search.setColumn(0,"SKN_SDT",div_search.cal_sknClctDt.cal_calFrom.value);
	ds_search.setColumn(0,"SKN_EDT",div_search.cal_sknClctDt.cal_calTo.value);
	
	ds_search.setColumn(0,"IN_STA_DT",div_search.cal_inDt.cal_calFrom.value);
	ds_search.setColumn(0,"IN_END_DT",div_search.cal_inDt.cal_calTo.value);
	
	ds_main.clearData();
	page = 1;
	fn_searchMain();
	
}

