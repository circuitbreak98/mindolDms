function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_btnClick(obj.name);
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 그리드 더블 클릭
-----------------------------------------------------------------------------------------------*/
function grd_list_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	if(ds_main.rowposition != -1){
		fn_btnClick("btnConfirm");
	}
}
/*===============================================================================================
 * [ PART 8 ] Event
================================================================================================*/
/*===============================================================================================
 * [ PART 9 ] User Defined
================================================================================================*/
function fn_btnClick(bName){
	switch(bName) 
	{	
		case "btnConfirm" :	//확인
			if(ds_main.rowposition < 0){
				return;
			}
			
			var objRtn = new Object;
				objRtn["eqpSerNo"] = ds_main.getColumn(ds_main.rowposition,"EQP_SER_NO");
				objRtn["eqpImeiNo"] = ds_main.getColumn(ds_main.rowposition,"EQP_IMEI_NO");
				objRtn["eqpColorCd"] = ds_main.getColumn(ds_main.rowposition,"EQP_COLOR_CD");
				objRtn["assetNo"] = ds_main.getColumn(ds_main.rowposition,"ASSET_NO");

			this.close(objRtn);		
		break;
		case "btn_close" : 		//버튼X
		case "btnClose" : 		//취소버튼
			close("cancel");
			break;	
		default:
		break;
	}
}
