/*----------------------------------------------------------------------------------------------
 * 설명   : 전표발행요청
-----------------------------------------------------------------------------------------------*/
function btn_slipRgst_onclick(obj:Button,  e:ClickEventInfo)
{
	var nRow = 0;
	ds_save.clearData();
	for(var i = 0; i < ds_main.rowcount; i++){
		if(ds_main.getColumn(i,"CHECKED") == "1"){
			if(ds_main.getColumn(i,"INVE_SLIP_ST") != "00" || ds_main.getColumn(i,"INVE_SLIP_ST") != "05"){
				gfn_message("A","선택 데이터 중에 전표 상태가 미발행 또는 취소가 아닌 데이터가 있습니다. 확인 바랍니다.","E","","");
				return;
			}
			nRow = ds_save.addRow();
			ds_save.copyRow(nRow,ds_main,i);
		}
	}
	if(ds_save.rowcount <= 0){
		gfn_message("A","DMS00020","E","",""); //선택된 정보가 없습니다.
		return;
	}
	
	fn_saveMain("R");
}
