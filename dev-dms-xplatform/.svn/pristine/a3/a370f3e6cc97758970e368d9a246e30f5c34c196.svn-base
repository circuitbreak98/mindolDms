-----------------------------------------------------------------------------------------------*/
function fn_setInitCode(obj:Form, e:LoadEventInfo)
{
	var strCode = "DMS244=ds_DMS244=N";
		 strCode += ",DMS245=ds_DMS245=N";
		 strCode += ",DMS249=ds_DMS249=N";
		 strCode += ",DMS153=ds_DMS153=N";

	gfn_getCodeDs( this
				  , strCode
