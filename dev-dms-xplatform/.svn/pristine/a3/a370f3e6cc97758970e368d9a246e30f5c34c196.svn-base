/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {
			}
		break;
		case "searchDtl": 
		case "searchDtlChk": 
			if ( iErrorCode == 0 ) {
				ds_excel.appendData(ds_excelTemp,true);
				if(ds_excelTemp.getRowCount()>0){
					var excelTotCnt =  ds_excelTemp.getColumn(0,"EXCEL_TOTAL_CNT");	// 데이터 총 건수		
															
					var sRtnVal = gfn_excelPageIndex(excelPage,excelTotCnt);												
					if ( !gfn_isNull(sRtnVal) && sRtnVal["excelProcYn"] == true){
						return fn_searchDtl(sRtnVal["excelFirst"],sRtnVal["excelLast"]);
					}													
				}
				if(ds_excel.getRowCount()>0){
					if(sSvcID == "searchDtl"){
						grd_notArrivExcel.redraw();
						if(ds_excel.getColumn(0,"INVE_TYP_CD") == "20"){
							gfn_baseExcelExpert(grd_notArrivExcel,"미착재고목록","미착재고목록");
						}else if(ds_excel.getColumn(0,"INVE_TYP_CD") == "10"){
							gfn_baseExcelExpert(grd_arrivExcel,"확정재고목록","확정재고목록");
						}else{
							gfn_baseExcelExpert(grd_slipDelExcel,"반제재고목록","반제재고목록");
						}
					}else if(sSvcID == "searchDtlChk"){
						fn_ExcelUpload();
					}
				}
			}
		break;
		case "xclSum": //정산집계
			if ( iErrorCode == 0 ) {
				btn_search_onclick();
			}
		break;
		case "saveMain": 
			if ( iErrorCode == 0 ) {
				btn_search_onclick();
			}
		break;
		default:
		break;
	}
}

