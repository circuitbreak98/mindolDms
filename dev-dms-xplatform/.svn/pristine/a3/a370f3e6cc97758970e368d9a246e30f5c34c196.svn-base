/*----------------------------------------------------------------------------------------------
 * 설명   : 그리드 내 클릭시 이벤트 발생
             -. 업로드시에는 원본을 조회후 업로드 팝업에서 대상과 비교
-----------------------------------------------------------------------------------------------*/
function grd_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	switch(obj.name) {
		case "grd_notArriv":
			if("11" == e.col && !gfn_isNull(ds_main.getColumn(ds_main.rowposition,"INVE_SLIP_ST"))){
				//gfn_message("A","DMS00071","E","","");	//이미 처리된 건이 있습니다.
				gfn_message("A","업로드 완료건 입니다.\n재집계후 업로드 하여 주시기 바랍니다.","E","","");
				return;
			}
			if("10" == e.col || "11" == e.col){
				var sRtnVal = gfn_excelPageIndex(0,"");						
				if ( !gfn_isNull(sRtnVal) && sRtnVal["excelProcYn"] == true){
					ds_excel.clearData();
					if("10" == e.col){
						fn_searchDtl(sRtnVal["excelFirst"],sRtnVal["excelLast"]);
					}else if("11" == e.col){
						fn_searchDtlChk(sRtnVal["excelFirst"],sRtnVal["excelLast"]);
					}
				}											
			}
			break;	
		case "grd_arriv":
			if("12" == e.col && !gfn_isNull(ds_main.getColumn(ds_main.rowposition,"INVE_SLIP_ST"))){
				//gfn_message("A","DMS00071","E","","");	//이미 처리된 건이 있습니다.
				gfn_message("A","업로드 완료건 입니다.\n재집계후 업로드 하여 주시기 바랍니다.","E","","");
				return;
			}
			if("11" == e.col || "12" == e.col){
				var sRtnVal = gfn_excelPageIndex(0,"");						
				if ( !gfn_isNull(sRtnVal) && sRtnVal["excelProcYn"] == true){
					ds_excel.clearData();
					if("11" == e.col){
						fn_searchDtl(sRtnVal["excelFirst"],sRtnVal["excelLast"]);
					}else if("12" == e.col){
						fn_searchDtlChk(sRtnVal["excelFirst"],sRtnVal["excelLast"]);
					}
				}											
			}
			break;	
		case "grd_slipDel":
			if("8" == e.col){
				var sRtnVal = gfn_excelPageIndex(0,"");						
				if ( !gfn_isNull(sRtnVal) && sRtnVal["excelProcYn"] == true){
					ds_excel.clearData();
					fn_searchDtl(sRtnVal["excelFirst"],sRtnVal["excelLast"]);
				}											
			}
			break;	
		default:
			break;
	}
}
