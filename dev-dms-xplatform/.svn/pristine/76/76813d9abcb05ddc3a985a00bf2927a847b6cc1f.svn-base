function fn_chkBeforeSave()	
{
	//저장전 수정,신규입력데이터의 trim처리 
	gfn_dsTrim(ds_authRol);

	if(!gfn_isUpdated(ds_authRol))//저장할 테이타셋 ds_Save
	{
		gfn_message("A","DMS00022","I");	//변경된 정보가 없습니다.
		return false;
	}
	
	if (!gfn_getValidateCompList(Div01)) {	
