/*===============================================================================================
 * [ PART 1 ] Form에서 Include 할 라이브러리(xjs)파일 정의
================================================================================================*/
include "LIB::common.xjs"; //공통함수 호출

/*===============================================================================================
 * [ PART 2 ] Form에서 사용될 전역변수를 선언
================================================================================================*/
 
/*===============================================================================================
 * [ PART 3 ] Form Event 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Form Load Event
-----------------------------------------------------------------------------------------------*/
function Form_onload(obj:Form, e:LoadEventInfo)
{
	//공통 Form 초기화
	gfn_setInitPop(obj, e, "FNR043P");
	//Form 초기 개발자 셋팅
 	//공통코드 초기화
 	fn_setInitCode(obj, e);
 	//기본값 초기화
 	fn_setInitForm(obj, e); 	
 	//그리드 초기화
 	fn_setInitGrid(obj, e);
}
/*===============================================================================================
 * [ PART 4 ] Load 후 초기설정
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitCode(obj:Form, e:LoadEventInfo)
{
	
	var strCode =  "DMS100=ds_DMS100=N"
						
	gfn_getCodeDs( this
				  , strCode
				  ,"fn_afterCodeCallBack");			  
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 호출 후 처리
-----------------------------------------------------------------------------------------------*/
function fn_afterCodeCallBack()
{
	if ( !gfn_isNull(argParam01) ) {

		ds_search.setColumn(0,'PRCHCO_CD',argParam01);
	}
	
	if ( !gfn_isNull(argParam02) ) {

		ds_search.setColumn(0,'XCL_PROC_DT',argParam02);
	}
	
	fn_searchMain();
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 그리드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitGrid(obj:Form, e:LoadEventInfo)
{
	
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 기본값 설정
-----------------------------------------------------------------------------------------------*/
function fn_setInitForm(obj:Form, e:LoadEventInfo)
{
	ds_search.copyRow(0, opener.ds_main, opener.ds_main.rowposition);
	if(!gfn_isNull(ds_search.getColumn(0, "XCL_PROC_DT"))){
		ds_search.setColumn(0,"XCL_PROC_DT", ds_search.getColumn(0, "XCL_PROC_DT").substr(0,6));
	}
	fn_searchMain();
}

/*===============================================================================================
 * [ PART 5 ] Transaction 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	var sSvcID		 = "searchMain";
	var sTrID		 = "PNRAgnEqpStlMgmt_pInqAgnEqpStlDtl";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sOutputDs	 = "ds_main=RS_AGN_EQP_STL_DTL ds_sumcnt=RS_SUM_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";

	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain" : 
			if ( iErrorCode == 0 ) {
				var Ttotal = "조회결과  총 "+"<fc v='#31a3dc'>"+ds_sumcnt.getColumn(0,"M_CNT")
						+"</fc><b v='false'><fc v='#77787a'>건, 합계: </fc></b>"+"<fc v='#31a3dc'>"
						+gfn_toFixedLocaleString(ds_sumcnt.getColumn(0,"M_PRC"), '@@@,@@@,@@@')+"</fc><b v='false'><fc v='#77787a'>원 </fc></b>";
			    fn_setGridTitle(stc_subTitle,Ttotal);   
			}
			break;	
		
		default :
			break;
	}
}

/*===============================================================================================
 * [ PART 7 ] Main Button Event
================================================================================================*/
