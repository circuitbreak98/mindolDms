/*===============================================================================================
 * [ PART 1 ] Form에서 Include 할 라이브러리(xjs)파일 정의
================================================================================================*/
include "LIB::common.xjs"; //공통함수 호출

/*===============================================================================================
 * [ PART 2 ] Form에서 사용될 전역변수를 선언
================================================================================================*/
  var inputType; 
/*===============================================================================================
 * [ PART 3 ] Form Event 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Form Load Event
-----------------------------------------------------------------------------------------------*/
function Form_onload(obj:Form, e:LoadEventInfo)
{
	//공통 Form 초기화
	gfn_setInitPop(obj, e, "FSC015P");
	
 	//Form 초기 개발자 셋팅
 	//공통코드 초기화
 	fn_setInitCode(obj, e);
 	//그리드 초기화
 	fn_setInitGrid(obj, e);
 	//기본값 초기화
 	fn_setInitForm(obj, e);
  	
	if( ds_frm.getRowCount() == 1){
		fn_selectValue(0);
	}
}

/*===============================================================================================
 * [ PART 4 ] Load 후 초기설정
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitCode(obj:Form, e:LoadEventInfo)
{

}

/*----------------------------------------------------------------------------------------------
 * 설명   : 그리드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitGrid(obj:Form, e:LoadEventInfo)
{

}

/*----------------------------------------------------------------------------------------------
 * 설명   : 기본값 설정
-----------------------------------------------------------------------------------------------*/
function fn_setInitForm(obj:Form, e:LoadEventInfo)
{
	//자동조회시 파라메타 전달예제
	if ( !gfn_isNull(argParam01)){
		Div02.Edit00.value = argParam01;
	}
	if ( !gfn_isNull(argParam02) ){
		Div02.Edit01.value = argParam02;
	}
	
	try {
		if ( !gfn_isNull(argParam03) ) {
			inputType = argParam03;
		} else {
			inputType = "button";
		}
	}
	catch(e) {

	}
	
	if (!gfn_isNull(argParam01) || !gfn_isNull(argParam02)) {
		btn_search.click();
		Div02.Edit00.setFocus();
	}
}

/*===============================================================================================
 * [ PART 5 ] Transaction 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	var sSvcID		 = "searchMain";
	var sTrID		 = "PSCFrmMgmt_pInqFrmLst";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_ncFieldMap";
	var sOutputDs	 = "ds_frmTempPage=RS_FRM_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain" : 	
			if ( iErrorCode == 0 ) {
				ds_frm.appendData(ds_frmTempPage,true);	//조회 페이지 추가
				//조회된 첫번째row로 setFocus 
				ds_frm.rowposition = (page-1) * page_size ;

				gfn_setGridTitleCountPaging(stc_subTitle01, total_record_cnt);
								
				//trace(ds_frmTempPage.getRowCount());
				if( ds_frm.getRowCount() == 1 && inputType == "key"){
					fn_selectValue(0);
				}
			//	gfn_message("B","DMS00001");	//MSGI0003 :조회하였습니다
			} 
			break;
			
		default :
			break;
	}
}

/*===============================================================================================
 * [ PART 7 ] Main Button Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 버튼 Event
-----------------------------------------------------------------------------------------------*/
function button_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj.name) 
	{	
		case "btn_search" :		//조회
			// 입력 항목 Validate Check
			if (!gfn_getValidateCompList(Div02)) {	
				return false;
			}
			ds_frm.clearData();
			page = 1;
			fn_searchMain();	
			break;
			
		case "btn_confirm" :	//확인
			fn_selectValue(ds_frm.rowposition);
			break;
			
		case "btn_cancel" : 	//취소
			close("cancel");
			break;
			
		case "btn_close" : 		//버튼X
			close("cancel");
			break;	
	}
}

/*===============================================================================================
 * [ PART 8 ] Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Grid Double Click Event
-----------------------------------------------------------------------------------------------*/
function grd_list_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	fn_selectValue(e.row);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 조회조건에서 엔터키가 눌러졌을때 조회
-----------------------------------------------------------------------------------------------*/
function Div02_Edit00_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == KEYCODE_ENTER) {
		btn_search.setFocus();
		ds_frm.clearData();
		page = 1;
		fn_searchMain();
	}	
}
/*===============================================================================================
 * [ PART 9 ] User Defined
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 결과값 리턴후 Close
-----------------------------------------------------------------------------------------------*/
function fn_selectValue(nRow)
{
	//단건(Object사용)
	if ( nRow == -1 ) return;
	
	var objRtn = new Object;
		objRtn["FRM_ID"] = ds_frm.getColumn(nRow,"FRM_ID");
		objRtn["FRM_NM"] = ds_frm.getColumn(nRow,"FRM_NM");
		objRtn["FRM_URL"] = ds_frm.getColumn(nRow,"FRM_URL");
	this.close(objRtn);		
	
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 수직 스크롤바가 마지막에 도달했을때 페이지 조회 이벤트
 * param   : obj - Grid Object
 * param   : e  - ScrollEventInfo
 * return  : void
-----------------------------------------------------------------------------------------------*/
function grd_main_onvtracklast(obj:Grid, e:ScrollEventInfo)
{
	if ( total_record_cnt <=  page * page_size ) {
	//	gfn_message("A","DMS00028","I"); //조회 마지막 페이지 입니다.
		return;
	} else {
		//다음페이지 조회
		page++;
		fn_searchMain();
	}
}]]></Script>
