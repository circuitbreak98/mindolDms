function btn_2_onclick(obj:Button,  e:ClickEventInfo)
{
	var arg1,arg2;
	
	//---------------------------------------------
	// arg1 : 차트속성
	// caption		- 차트제목
	// xAxisName	- X축의 명
	// yAxisName	- Y축의 명
	// showValues	- 0 : 차트에 값 표시 안함
	//---------------------------------------------
	arg1 = ["caption","Monthly Revenue","xAxisName","Month","yAxisName","Revenue","showValues","0"];
	
	//---------------------------------------------
	// arg2 : 그룹속성(3개씩 셋트)
	// [그룹명,Y축값컬럼명,그룹속성, ...]
	// 다중차트는 그룹속성을 설정안함.
	//---------------------------------------------
	arg2 = ["Product A","value_1","","Product B","value_2",""];
	
	gfn_MultiChart(atx_chart2,cbo_type2.value,ds_data2,"label",arg2,arg1,"",["code","code"]);
}

 /******************************************************************************************
 * 함  수  명	: btn_3_onclick(obj:Button,  e:ClickEventInfo)
 * 기      능	: 혼합차트 생성
 ******************************************************************************************
 */
function btn_3_onclick(obj:Button,  e:ClickEventInfo)
{
	var arg1,arg2,arg3;
	
	switch(cbo_type3.value)
    {
        case "MSCombi2D" :			// Y축 1개사용
        case "MSCombi3D" :
        case "MSColumnLine3D" :
			//---------------------------------------------
			// arg1 : 차트속성
			// caption		- 차트제목
			// xAxisName	- X축의 명
			// yAxisName	- Y축의 명
			// showValues	- 0 : 차트에 값 표시 안함
			//---------------------------------------------
			arg1 = ["caption","Monthly Revenue","xAxisName","Month","yAxisName","Revenue","showValues","0"];
			
			//---------------------------------------------
			// arg2 : 그룹속성(3개씩 셋트)
			// [그룹명,Y축값컬럼명,그룹속성, ...]
			// 그룹속성	- renderAs='Area' : 표시형식을 Area로, renderAs='Line' : 표시형식을 선으로
			//---------------------------------------------
			arg2 = ["2004","value_1","","2005","value_2","renderAs='Area'","2006","value_3","renderAs='Line'"];
			
			//--------------------------------------------
			// arg3 : (차트클릭 시) 리턴값 컬럼 배열
			// [그룹1 리턴컬럼, 그룹2 리턴컬럼, 그룹3 리턴컬럼,...]
			//--------------------------------------------
			arg3 = ["code_1","code_2","code_3"];
            break;
		default :					// Y축 2개사용
			//---------------------------------------------
			// arg1 : 차트속성
			// caption		- 차트제목
			// PYAxisName	- 왼쪽 Y축의 명
			// yAxisName	- 오늘쪽쪽 Y축의 명
			// showValues	- 0 : 차트에 값 표시 안함
			//---------------------------------------------
			arg1 = ["caption","Sales Volume","PYAxisName","Revenue","SYAxisName","Quantity","showValues","0"];
			
			//---------------------------------------------
			// arg2 : 그룹속성(3개씩 셋트)
			// [그룹명,Y축값컬럼명,그룹속성, ...]
			// 그룹속성	- parentYAxis='S' : 오른쪽 Y축을 사용
			//---------------------------------------------
			arg2 = ["2004","value_1","","2005","value_2","","2006","value_3","parentYAxis='S'"];
			break;	
    }
    gfn_MultiChart(atx_chart3,cbo_type3.value,ds_data3,"label",arg2,arg1,"",arg3);
}

function cbo_type_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	var nIndex = obj.name.substr(obj.name.length -1);
	var sObjName = "atx_chart" + nIndex;
	var objActiveX = eval(sObjName);
	
	var nLeft = objActiveX.position.left;
	var nTop = objActiveX.position.top;
	var nRight = objActiveX.position.right;
	var nBottom = objActiveX.position.bottom;
	
	objActiveX.destroy();
	objActiveXComp = new ActiveX(sObjName, nLeft, nTop, nRight, nBottom);
	
	this.addChild(sObjName, objActiveXComp);
	
	var objNewActiveX = eval(sObjName);
	objNewActiveX.progid = "{D27CDB6E-AE6D-11CF-96B8-444553540000}";
	objNewActiveX.codebase	= "http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab";
	objNewActiveX.popupstyle = false;
	objNewActiveX.usepersistdata = false;
	objNewActiveX.windowed = true;
 	objNewActiveX.show();
	
	var sBtnName = "btn_" + nIndex;
	var objBtn = eval(sBtnName);
	objBtn.click();
}

function atx_chart_FSCommand(obj:ActiveX, e)
{
	trace(e.args);
}
