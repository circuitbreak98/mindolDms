//=========================================================================================
// [ PART 1 ]
// Form에서 Include 할 내용을 정의
//
//=========================================================================================
/************************************************************************************************
 * 공통 라이브러리 INCLUDE 영역
************************************************************************************************/
include "LIB::comChart.xjs"; //차트함수 호출

var fv_ChartColor = ["FF006E", "3845FF", "FFFF07", "9EFFDE", "8C3FFF","D3D3D3","FFFF07","FF8B1E"];	// 공통으로 사용하는 차트색배열
//=========================================================================================
// [ PART 3 ]
// Form Event 정의
//
//=========================================================================================
 /******************************************************************************************
 * 함  수  명	: SampleChart_onload(obj:Form, e:LoadEventInfo)
 * 기      능	: 화면로드시 실행되는 작업
 ******************************************************************************************
 */
function SampleChart_onload(obj:Form, e:LoadEventInfo)
{
	// Chart1,2,3 실행
	btn_1.click();
	btn_2.click();
	btn_3.click();
}

//=========================================================================================
// [ PART 6 ]
// component Event 정의 ( User Define function )
// --> component Event의 기요,기능,동작 등 상세내용은 MiPlatform 메뉴얼을 참조하시기 바랍니다.
//=========================================================================================
 /******************************************************************************************
 * 함  수  명	: btn_1_onclick(obj:Button,  e:ClickEventInfo)
 * 기      능	: 단순차트 생성
 ******************************************************************************************
 */
function btn_1_onclick(obj:Button,  e:ClickEventInfo)
{
	var arg1, arg2; 
	
    switch(cbo_type1.value)
    {
        case "Pie2D" :	// 원차트(데이터의 합계가 100이여야함)
        case "Pie3D" :
			//---------------------------------------------
			// arg1 : 차트속성
			// caption				- 차트제목
			// showPercentageValues	- 1 : 차트에 값 표시
			//---------------------------------------------
            arg1 = ["caption","Company Revenue","showPercentageValues","1"];
            
            // arg2 : 차트색상 지정
			arg2 = ["0094FF","FFFFFF","FF0C41","82CF27","AE19FF","FF7F87"];
            
            // gfn_SingleChart(차트Obj,차트종류,차트데이터,X축값컬럼명,Y축값컬럼명,차트속성,차트색상,리턴컬럼명)
			gfn_SingleChart(atx_chart1,cbo_type1.value,ds_data11,"label","value",arg1,arg2,"code");
            break;
        case "Column2D":
			//---------------------------------------------
			// arg1 : 차트속성
			// caption			- 차트제목
			// xAxisName		- X축의 명
			// yAxisName		- Y축의 명
			// showValues		- 0 : 차트에 값 표시 안함
			// useRoundEdges	- 1 : 막대를 라운드처리
			//---------------------------------------------
			arg1 = ["caption","Monthly Revenue","xAxisName","Month","yAxisName","Revenue","showValues","0","useRoundEdges","1"];
			
			// gfn_SingleChart(차트Obj,차트종류,차트데이터,X축값컬럼명,Y축값컬럼명,차트속성,차트색상,리턴컬럼명)
			// fv_ChartColor : 차트색배열
			gfn_SingleChart(atx_chart1,cbo_type1.value,ds_data1,"label","value",arg1,fv_ChartColor);
			break;	
		default :		// 그외의 차트
			//---------------------------------------------
			// arg1 : 차트속성
			// caption				- 차트제목
			// xAxisName			- X축의 명
			// yAxisName			- Y축의 명
			// showValues			- 0 : 차트에 값 표시 안함
			// formatNumberScale	- 0 : 숫자형식인 경우, 단위표시여부(천(1000)인경우, K로 표시 등등)
			//---------------------------------------------
			arg1 = ["caption","Monthly Revenue","xAxisName","Month","yAxisName","Revenue","showValues","0","formatNumberScale","0"];
			
			// gfn_SingleChart(차트Obj,차트종류,차트데이터,X축값컬럼명,Y축값컬럼명,차트속성,차트색상,리턴컬럼명)
			// 색상지정 안하면, 기본 팔레트색으로...
			gfn_SingleChart(atx_chart1,cbo_type1.value,ds_data1,"label","value",arg1);
			break;	
    }
}

 /******************************************************************************************
 * 함  수  명	: btn_2_onclick(obj:Button,  e:ClickEventInfo)
 * 기      능	: 다중차트 생성
 ******************************************************************************************
