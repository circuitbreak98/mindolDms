function fn_chkBeforeSave()	
{	
	//필수항목 Check
	if ( !grd_msglist.ufn_isRequiredColumn() ) return false;

	if(!gfn_isUpdated(ds_main))//저장할 테이타셋 ds_Save
	{
		gfn_message("A","변경된 정보가 없습니다.","I");	//???? : 변경된 정보가 없습니다.
		return false;
	}	
	return true;
}

