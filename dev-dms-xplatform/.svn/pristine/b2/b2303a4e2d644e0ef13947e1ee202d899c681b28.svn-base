-----------------------------------------------------------------------------------------------*/
function div_search_cbo_returnGb_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	//데이타셋 clear
	ds_main.clearData();
	if(obj.value == "01"){
		grd_main.visible = true; 
		grd_main2.visible = false; 
	}else{
		grd_main2.visible = true; 
		grd_main.visible = false; 
	}
}

