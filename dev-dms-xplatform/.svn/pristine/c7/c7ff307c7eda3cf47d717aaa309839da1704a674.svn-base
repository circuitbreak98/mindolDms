/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain" : 
				
			cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
			cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
			gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
			
			if ( ds_main.rowposition != -1 && ds_main.getRowType(ds_main.rowposition) != Dataset.ROWTYPE_NORMAL){
					cal_calFrom_onchanged();
				}
			var subMsg = "  총 "+"<fc v='#31a3dc'>"+ds_searchSum.getColumn(0,"TOTAL_CNT")
						+"</fc><b v='false'><fc v='#77787a'>건, 감가상각누계액 합계: </fc></b>"
						+"<fc v='#31a3dc'>"+gfn_toFixedLocaleString(ds_searchSum.getColumn(0,"DEPR_PRC"), '@@@,@@@,@@@')
						+"</fc><b v='false'><fc v='#77787a'>원 </fc></b>"
						+"<b v='false'><fc v='#77787a'>, 충당부채누계액 합계: </fc></b>"
						+"<fc v='#31a3dc'>"+gfn_toFixedLocaleString(ds_searchSum.getColumn(0,"PROV_PRC"), '@@@,@@@,@@@')
						+"</fc><b v='false'><fc v='#77787a'>원 </fc></b>"
						+"<b v='false'><fc v='#77787a'>, 매입금액 합계: </fc></b>"
						+"<fc v='#31a3dc'>"+gfn_toFixedLocaleString(ds_searchSum.getColumn(0,"EQP_PRCH_AMT"), '@@@,@@@,@@@')
						+"</fc><b v='false'><fc v='#77787a'>원 </fc></b>"
						+"<b v='false'><fc v='#77787a'>, 잔존가 합계: </fc></b>"
						+"<fc v='#31a3dc'>"+gfn_toFixedLocaleString(ds_searchSum.getColumn(0,"REM_PRC"), '@@@,@@@,@@@')
						+"</fc><b v='false'><fc v='#77787a'>원 </fc></b>"
						+"<b v='false'><fc v='#77787a'>, 단말변상액 합계: </fc></b>"
						+"<fc v='#31a3dc'>"+gfn_toFixedLocaleString(ds_searchSum.getColumn(0,"EQP_CMP_AMT_TOT"), '@@@,@@@,@@@')
						+"</fc><b v='false'><fc v='#77787a'>원 </fc></b>";
						
				fn_setGridTitle(stc_subTitle,subMsg);	
					
			break;
		
		case "callOnline" :
		 	if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//MSGI0003 :정상 처리되었습니다.
				fn_searchMain();
			}	
		
			break;
			
		case "powerDspsl" :
		 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//MSGI0003 :정상 처리되었습니다.
				fn_searchMain();
			}	
		
			break;
				
	    case "AllExcel":
				
			if (iErrorCode == 0) {
				var dsbind = gfn_makeGrid(ds_AllExcel);
				dsbind.binddataset = "ds_AllExcel";
				dsbind.setCellProperty("Body",0,"displaytype","text");	 //서비스관리번호
				dsbind.setCellProperty("Body",1,"displaytype","text");	 //일련번호
				dsbind.setCellProperty("Body",4,"style","align:right");
				dsbind.setCellProperty("Body",6,"style","align:right");
				dsbind.setCellProperty("Body",7,"style","align:right");
				dsbind.setCellProperty("Body",8,"style","align:right");
				
				dsbind.setCellProperty("Body",10,"style","align:right");
				dsbind.setCellProperty("Body",11,"style","align:right");
				
				gfn_baseExcelExpert(dsbind,"단말매각목록","단말매각목록");
			}else{
				gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
			}
			break;
		
	   case "ReState":
				
			if (iErrorCode == 0) {
				 gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				  fn_searchMain();
			}
			break;		
		default :
			break;
	}
}


