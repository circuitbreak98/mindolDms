/*===============================================================================================
 * [ PART 6 ] CallBack 처리  
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain" : 
			if ( iErrorCode == 0 ) {
			
				ds_search.setColumn(0,"LST_DEPR_DT",ds_searchSum.getColumn(ds_searchSum.rowposition, 'DEPR_DT'));
				ds_search.setColumn(0,"LST_INVE_ST_CD",ds_searchSum.getColumn(ds_searchSum.rowposition, 'INVE_ST_CD'));
				ds_search.setColumn(0,"LST_ASSET_SLIP_NO",ds_searchSum.getColumn(ds_searchSum.rowposition, 'ASSET_SLIP_NO'));
				
				if (gfn_isNull(ds_searchSum.getColumn(ds_searchSum.rowposition, 'ASSET_SLIP_ST'))){
					ds_search.setColumn(0,"LST_ASSET_SLIP_ST","%");
				}else{
					ds_search.setColumn(0,"LST_ASSET_SLIP_ST",ds_searchSum.getColumn(ds_searchSum.rowposition, 'ASSET_SLIP_ST'));
				}
				ds_search.setColumn(0,"LST_DISPO_DT",ds_searchSum.getColumn(ds_searchSum.rowposition, 'DISPO_DT'));
				ds_search.setColumn(0,"YYYY",ds_searchSum.getColumn(ds_searchSum.rowposition, 'YYYY'));

				ds_main.clearData();
				fn_searchEqpExcLst();
			}

			break;
			
		case "searchEqpExcLst" :
		 
			if ( iErrorCode == 0 ) {
				//gfn_message("A","DMS00000","I");	//MSGI0003 :정상 처리되었습니다.
			}	
		
			break;
				
		case "callOnline" :
		 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//MSGI0003 :정상 처리되었습니다.
				fn_searchMain();
			}	
		
			break;
		
		case "powerExc" :
		 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//MSGI0003 :정상 처리되었습니다.
				fn_searchMain();
			}	
		
			break;		
		case "pSlipExcClsAssetDispo" :
		 
			if ( iErrorCode == 0 ) {
				//전표처리
				//ds_search.setColumn(0,"CASE_WHEN","02");
				//fn_onlineCall();
			    gfn_message("A","DMS00000","I");	//MSGI0003 :정상 처리되었습니다.
				fn_searchMain();
			}	
			break;

	    case "AllExcel":
				
			if (iErrorCode == 0) {
				var dsbind = gfn_makeGrid(ds_AllExcel);
				dsbind.binddataset = "ds_AllExcel";
				dsbind.setCellProperty("Body",1,"displaytype","text");	 //서비스관리번호
				dsbind.setCellProperty("Body",2,"displaytype","text");	 //일련번호
				dsbind.setCellProperty("Body",3,"displaytype","text");	 //자산번호
				dsbind.setCellProperty("Body",7,"displaytype","date");	 //계약일
				dsbind.setCellProperty("Body",11,"displaytype","date");	 //분실일자
				dsbind.setCellProperty("Body",12,"displaytype","date");  //제각일자
				dsbind.setCellProperty("Body",11,"calendardisplaynulltype","none");	 //분실일자 nulltext
				dsbind.setCellProperty("Body",12,"calendardisplaynulltype","none");	 //제각일자 nulltext
				
				dsbind.setCellProperty("Body",5,"style","align:left");
				dsbind.setCellProperty("Body",6,"style","align:right");
				dsbind.setCellProperty("Body",8,"style","align:right");
				dsbind.setCellProperty("Body",9,"style","align:right");
				dsbind.setCellProperty("Body",10,"style","align:right");
				
				dsbind.setFormatColProperty(0,"size",30);
				dsbind.setFormatColProperty(1,"size",100);
				dsbind.setFormatColProperty(2,"size",100);
				dsbind.setFormatColProperty(3,"size",100);
				dsbind.setFormatColProperty(5,"size",100);
				dsbind.setFormatColProperty(7,"size",100);
				dsbind.setFormatColProperty(8,"size",100);
				dsbind.setFormatColProperty(9,"size",100);
				dsbind.setFormatColProperty(10,"size",100);
				
				gfn_baseExcelExpert(dsbind,"단말제각목록","단말제각목록");
			}else{
				gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
			}
			break;
		
	   case "ReState":
				
			if (iErrorCode == 0) {
				 gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				  fn_searchMain();
			}
			break;
			
		default :
			break;
	}
}

