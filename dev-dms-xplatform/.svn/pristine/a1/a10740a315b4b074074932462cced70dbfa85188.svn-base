function fn_setGridTitle(objStc:Static, msg)
{
	if ( gfn_isNull(objStc.userText) ) objStc.userText = objStc.text;
	
	objStc.usedecorate = true; 
	
	objStc.text = objStc.userText 
	         
	         + msg;
}
