/*----------------------------------------------------------------------------------------------
 * 설명   : 단말기계약목록 추가
-----------------------------------------------------------------------------------------------*/
function btn_insert_rentList_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_mainRent.rowposition < 0){
		return;
	}
	if(ds_mainRentList.getDeletedRowCount() > 0 ||
		(ds_mainRentList.rowcount > 0 && ds_mainRentList.getRowType(ds_mainRentList.rowposition) != Dataset.ROWTYPE_NORMAL)){
		gfn_message("A","DMS00078","I")
		return;
	}
	ds_mainRentList.addRow();
	ds_mainRentList.setColumn(ds_mainRentList.rowposition,"OP_CL_CD","PR");
	ds_mainRentList.setColumn(ds_mainRentList.rowposition,"CMPHS_CNTRT_NO",ds_mainRent.getColumn(ds_mainRent.rowposition,"CMPHS_CNTRT_NO"));
	ds_mainRentList.setColumn(ds_mainRentList.rowposition,"CNTRT_STA_DT",ds_mainRent.getColumn(ds_mainRent.rowposition,"CMPHS_CNTRT_STA_DT"));
	ds_mainRentList.setColumn(ds_mainRentList.rowposition,"CNTRT_END_DT",ds_mainRent.getColumn(ds_mainRent.rowposition,"CMPHS_CNTRT_END_DT"));
	div_sub.mae_rentPrd.setFocus();
	fn_btnSetEnable();

	//구성품
	ds_constList.clearData();
	if(ds_DMS111.rowcount > 0){
		for(var i = 0; i < ds_DMS111.rowcount; i++){
			ds_constList.addRow();
			ds_constList.setColumn(i,"CMPT_CD",ds_DMS111.getColumn(i,"CM_CD_ID"));
		}
		ds_constList.rowposition = 0;
	}
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 단말기계약목록 삭제
-----------------------------------------------------------------------------------------------*/
function btn_delete_rentList_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_mainRentList.getDeletedRowCount() > 0){
		gfn_message("A","DMS00078","I")
		return;
	}
	if(ds_mainRentList.getRowType(ds_mainRentList.rowposition) != Dataset.ROWTYPE_NORMAL){
		if (gfn_message("C","DMS00024","Q")){ //편집중인 데이터가 있습니다. 계속하시겠습니까?
			//삭제전 단말기 저장용 데이터 복사
			ds_saveRentList.clearData();
			ds_saveRentList.addRow();
			ds_saveRentList.copyRow(0,ds_mainRentList,ds_mainRentList.rowposition);
			
			ds_mainRentList.deleteRow(ds_mainRentList.rowposition);
			ds_constList.clearData();	//구성품 초기화
			if(ds_mainRentList.rowcount > 0){
				ds_mainRentList.rowposition = 0;
			}
		}
	}
	else{
		//삭제전 단말기 저장용 데이터 복사
		ds_saveRentList.clearData();
		ds_saveRentList.addRow();
		ds_saveRentList.copyRow(0,ds_mainRentList,ds_mainRentList.rowposition);
		
		ds_mainRentList.deleteRow(ds_mainRentList.rowposition);
		ds_constList.clearData();	//구성품 초기화
		if(ds_mainRentList.rowcount > 0){
			ds_mainRentList.rowposition = 0;
		}
	}
	if(ds_mainRentList.rowcount > 0){
		fn_btnSetEnable();
		ds_searchCmpt.setColumn(0,"CNTRT_NO",ds_mainRentList.getColumn(ds_mainRentList.rowposition,"CNTRT_NO"));
		ds_searchCmpt.setColumn(0,"OP_CL_CD","PR");
		fn_searchCmptList();
	}else{
		fn_btnSetUnEnable();
	}
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 단말기계약목록 저장
-----------------------------------------------------------------------------------------------*/
function btn_save_rentList_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_mainRentList.getRowType(ds_mainRentList.rowposition) == Dataset.ROWTYPE_INSERT
		|| ds_mainRentList.getRowType(ds_mainRentList.rowposition) == Dataset.ROWTYPE_UPDATE
		|| ds_mainRentList.getDeletedRowCount() > 0
		)//저장할 테이타셋 
	{
		if(ds_mainRentList.getDeletedRowCount() <= 0){
			//동일단말기체크
			var nRow = ds_mainRentList.rowposition;
			var strEqpMdlCdTemp = ds_mainRentList.getColumn(nRow,"EQP_MDL_CD");
			ds_mainRentList.filter("EQP_MDL_CD == '"+strEqpMdlCdTemp+"'");
			if(ds_mainRentList.rowcount > 1){
				ds_mainRentList.filter("");
				gfn_message("A","DMS00079","I")
				return;
			}
			ds_mainRentList.filter("");
			ds_mainRentList.rowposition = nRow;
			//단말기 저장용 데이터 복사
			ds_saveRentList.clearData();
			ds_saveRentList.addRow();
			ds_saveRentList.copyRow(0,ds_mainRentList,ds_mainRentList.rowposition);
			ds_saveRentList.updatecontrol = false;
			ds_saveRentList.setRowType(0,ds_mainRentList.getRowType(ds_mainRentList.rowposition));
			
			if(!fn_chkBeforeSave()){
				ds_saveRentList.clearData();
				return;
			}
		}
		fn_saveSub();
	}
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 단말기계약ROW 변경전에 변경 여부 체크
-----------------------------------------------------------------------------------------------*/
function ds_mainRentList_canrowposchange(obj:Dataset, e:DSRowPosChangeEventInfo)
{
	if(ds_mainRentList.getRowType(ds_mainRentList.rowposition) == Dataset.ROWTYPE_EMPTY){
