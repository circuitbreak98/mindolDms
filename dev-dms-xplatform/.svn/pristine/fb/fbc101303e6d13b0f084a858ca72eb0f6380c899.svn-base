/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain" : 
			if(ds_mainRent.rowcount > 0){ //단말기상세조회
				ds_mainRent.rowposition = 0;
				fn_searchEqpDtl();
			}
			break;
		case "searchEqpMain" : 
			if(ds_mainRentList.rowcount > 0){
				fn_btnSetEnable();
				//구성품조회 ds_searchCmpt
				ds_searchCmpt.setColumn(0,"CNTRT_NO",ds_mainRentList.getColumn(ds_mainRentList.rowposition,"CNTRT_NO"));
				ds_searchCmpt.setColumn(0,"OP_CL_CD","PR");
				fn_searchCmptList();
			}else{
				fn_btnSetUnEnable();
			}
			ds_mainRentList.updatecontrol = true;
			break;
		case "searchCmptList" : 
		trace(ds_constList.saveXML());
			if(ds_DMS111.rowcount > 0){
				for(var i = 0; i < ds_DMS111.rowcount; i++){
					if(ds_constList.rowcount <= 0){
						ds_constList.addRow();
						ds_constList.setColumn(ds_constList.rowposition,"CMPT_CD",ds_DMS111.getColumn(i,"CM_CD_ID"));
						continue;
					}
					for(var j = 0; j < ds_constList.rowcount; j++){
						if(ds_DMS111.getColumn(i,"CM_CD_ID") == ds_constList.getColumn(j,"CMPT_CD")){
							break;
						} else if(j == (ds_constList.rowcount - 1)){
							ds_constList.addRow();
							ds_constList.setColumn(ds_constList.rowposition,"CMPT_CD",ds_DMS111.getColumn(i,"CM_CD_ID"));
						}
					}
				}
				ds_constList.rowposition = 0;
			}
			break;
		case "saveMain" : 
		case "cancelMain" : 
		case "saveSub" : 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				btn_search_onclick(btn_search);
			}
			break;
		default :
			break;
	}
}
