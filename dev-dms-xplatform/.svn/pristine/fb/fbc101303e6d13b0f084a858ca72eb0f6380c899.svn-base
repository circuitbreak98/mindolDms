-----------------------------------------------------------------------------------------------*/
function btn_insert_rent_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_mainRentList.getDeletedRowCount() > 0 ||
		(ds_mainRentList.rowcount > 0 && ds_mainRentList.getRowType(ds_mainRentList.rowposition) != Dataset.ROWTYPE_NORMAL)){
		gfn_message("A","DMS00078","I")
		return;
	}

	var sParam01 = "I";
	var sParam02 = "";

	var arrArgs = {argAutoSelect:true, argCallProgram:"FPR013M", argParam01:sParam01, argParam02:sParam02};
	var sRtnVal = gfn_dialog("FPR020P", arrArgs);

	//팝업에서 입력받은 값 리턴
	if ( gfn_isNull(sRtnVal) || sRtnVal == "cancel" ) {
		//취소
	} else {	//등록
		ds_saveCmphs.clearData();
		ds_saveCmphs.addRow();
		ds_saveCmphs.setColumn(0,"CMPHS_CNTRT_NM",sRtnVal["cmphsCntrtNm"]);
		ds_saveCmphs.setColumn(0,"CMPHS_CNTRT_STA_DT",sRtnVal["cmphsCntrtStaDt"]);
		ds_saveCmphs.setColumn(0,"CMPHS_CNTRT_END_DT",sRtnVal["cmphsCntrtEndDt"]);
		
		ds_mainRent.addRow();
		ds_mainRent.copyRow(ds_mainRent.rowposition,ds_saveCmphs,0);
		
		fn_saveMain();
	}	

}
