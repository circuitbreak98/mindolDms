		case "btn_confirm" :	//확인
			fn_selectValue(ds_userLst.rowposition);
			break;
		case "btn_cancel" : 	//취소
			close("cancel");
			break;
			
		case "btn_close" : 		//버튼X
			close("cancel");
			break;	
	}
}

