/*******************************************************************************
 * 오류검증- 이벤트
*******************************************************************************/
function btn_errCheck_onclick(obj:Button,  e:ClickEventInfo)
{
	var bCf = true;
	if(ds_upLoadXlsList.getRowCount() == 0 
		|| (ds_upLoadXlsList.getRowCount() == 1 && gfn_isNull(ds_upLoadXlsList.getRowCount(0,"PROD_NM")) ) ){
		//업로드 문서에 처리 할 데이터가 없습니다.\n업로드 문서를 확인하시기 바랍니다.
		//alert("업로드 문서에 처리 할 데이터가 없습니다.\n업로드 문서를 확인하시기 바랍니다.");
		gfn_message("A","DMS00067");
		return;
	}	
	
	for(var i = 0 ; i < ds_upLoadXlsList.rowcount ; i++) {
		
		//trace(ds_upLoadXlsList.getColumn(i,"EQP_COLOR_CD")+":"+isNaN(ds_upLoadXlsList.getColumn(i,"EQP_COLOR_CD")));
		if (  gfn_isNull(ds_upLoadXlsList.getColumn(i,"EQP_MDL_CD"))  ){
			ds_upLoadXlsList.setColumn(i, "ERR_DESC", "모델코드 공백 오류");
		    bCf = false;
		}else if (  gfn_isNull(ds_upLoadXlsList.getColumn(i,"EQP_SER_NO"))  ){
			ds_upLoadXlsList.setColumn(i, "ERR_DESC", "일련번호 공백 오류");
			bCf = false;
		}else if (  gfn_isNull(ds_upLoadXlsList.getColumn(i,"CNSL_DEALCO_CD"))  )  {
			ds_upLoadXlsList.setColumn(i, "ERR_DESC", "상담처코드 공백 오류");
			bCf = false;
		}else if (  gfn_isNull(ds_upLoadXlsList.getColumn(i,"EQP_COLOR_CD"))  )  {
			ds_upLoadXlsList.setColumn(i, "ERR_DESC", "색상코드 공백 오류");
			bCf = false;
			
		}else if ( isNaN(ds_upLoadXlsList.getColumn(i,"EQP_COLOR_CD"))  )  {
			ds_upLoadXlsList.setColumn(i, "ERR_DESC", "색상코드 숫자예외 오류");
			bCf = false;
			
		}else if ( gfn_isNull(ds_upLoadXlsList.getColumn(i,"PRC_GB")) 
		       ||(ds_upLoadXlsList.getColumn(i,"PRC_GB") == 'T' || ds_upLoadXlsList.getColumn(i,"PRC_GB") == 'UC' 
		       || ds_upLoadXlsList.getColumn(i,"PRC_GB") == 'UN') &&	gfn_isNull(ds_upLoadXlsList.getColumn(i,"T_MGMT_NO"))) {
		       trace("aaaa"+ds_upLoadXlsList.getColumn(i,"PRC_GB")+"/"+gfn_isNull(ds_upLoadXlsList.getColumn(i,"PRC_GB")));
		       
				ds_upLoadXlsList.setColumn(i, "ERR_DESC", "T_관리번호 항목 공백 오류");
				bCf = false;
		}else if ( (  ds_upLoadXlsList.getColumn(i,"CNSL_DEALCO_CD") == "11030" || ds_upLoadXlsList.getColumn(i,"CNSL_DEALCO_CD") == "11050" )&&	gfn_isNull(ds_upLoadXlsList.getColumn(i,"T_MGMT_NO")) ) {
				trace("bbb");
				ds_upLoadXlsList.setColumn(i, "ERR_DESC", "T_관리번호 항목 공백 오류");
				bCf = false;
		}
		
		
		if(!gfn_isEmpty(ds_upLoadXlsList.getColumn(i,"CNSL_DT")))ds_upLoadXlsList.setColumn(i, "CNSL_DT", ds_upLoadXlsList.getColumn(i,"CNSL_DT").replace("-","").substr(0,8));
		if(!gfn_isEmpty(ds_upLoadXlsList.getColumn(i,"ONING_DT")))ds_upLoadXlsList.setColumn(i, "ONING_DT", ds_upLoadXlsList.getColumn(i,"ONING_DT").replace("-",""));
		if(!gfn_isEmpty(ds_upLoadXlsList.getColumn(i,"CHECK_DT")))ds_upLoadXlsList.setColumn(i, "CHECK_DT", ds_upLoadXlsList.getColumn(i,"CHECK_DT").replace("-",""));
		if(!gfn_isEmpty(ds_upLoadXlsList.getColumn(i,"CUST_DT")))ds_upLoadXlsList.setColumn(i, "CUST_DT", ds_upLoadXlsList.getColumn(i,"CUST_DT").replace("-",""));
		if(!gfn_isEmpty(ds_upLoadXlsList.getColumn(i,"TEL_NO")))ds_upLoadXlsList.setColumn(i, "TEL_NO", ds_upLoadXlsList.getColumn(i,"TEL_NO").replace("-",""));
	}
	if(bCf){
	//엑셀반영 버튼 활성화.
	Div00.btn_excelSave.enable = true;
	
	} 
	
}



