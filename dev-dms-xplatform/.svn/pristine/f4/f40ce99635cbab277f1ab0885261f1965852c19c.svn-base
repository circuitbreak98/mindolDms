-----------------------------------------------------------------------------------------------*/
function div_search_onkeydown(obj:Div, e:KeyEventInfo)
{	
	if (e.keycode == KEYCODE_ENTER) {
		div_search.btn_search.setFocus();
		btn_search_onclick();
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명   : Grid의 내용을 Excel로 Export 한다.
-----------------------------------------------------------------------------------------------*/
function btn_excel_onclick(obj:Button,  e:ClickEventInfo)
{
	
	var excelTitle = "";		
	if( "NO" == ds_search.getColumn(0,"JDC_CL") && "JO" == ds_search.getColumn(0,"TLY_CL") ){
		excelTitle = "일반감정 검수대상 목록";
	}else if( "RE" == ds_search.getColumn(0,"JDC_CL") && "JO" == ds_search.getColumn(0,"TLY_CL") ){
		excelTitle = "재감정 검수대상 목록";
	}else if( "NO" == ds_search.getColumn(0,"JDC_CL") && "JR" == ds_search.getColumn(0,"TLY_CL") ){
		excelTitle = "일반감정 검수등록 목록";
	}else if( "RE" == ds_search.getColumn(0,"JDC_CL") && "JR" == ds_search.getColumn(0,"TLY_CL") ){
		excelTitle = "재감정 검수등록 목록";
	}else{
		excelTitle = "검수관리 목록";		
	}
	gfn_baseExcelExpert(grd_main,excelTitle, excelTitle);			
				
}

/*----------------------------------------------------------------------------------------------
 * 설명   :  form 내에서 esc버튼 클릭시 그리드 데이터셋 초기화 
 * param   : obj - Form
 * param   : e  - KeyEventInfo
 * return  : true/false
-----------------------------------------------------------------------------------------------*/
function Form_onkeyup(obj:Form, e:KeyEventInfo)
{
 	if(e.keycode ==KEYCODE_ESC) {	//ESC
 		fn_dsClear();
 		ds_search.clearData();
 		ds_search.addRow(); 		 	
 		fn_afterCodeCallBack();
 	}
}

