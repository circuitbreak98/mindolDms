================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{	
	var sSvcID		 = "searchMain";
	var sTrID		 = "PEPTlyMgmt_pSJdgTlyObjMgmtLst";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sOutputDs	 = "ds_main=RS_JDG_TLY_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 검수취소
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_cnclSave()
{	
	var sSvcID		 = "cnclSave";
	var sTrID		 = "PEPTlyMgmt_pURpetJdgTlyCncl";
	var sPtrn		 = "U";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RS_CNCL_LIST=ds_main:U";
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}


/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
				
				var sRowNumEpr = "("+page_size+"*("+page+"-1))+(currow+1)";	// 순번 이어서 작성
				/*
				if("NO" == ds_search.getColumn(0,"JDC_CL") && "JO" == ds_search.getColumn(0,"TLY_CL") ){
					//일반감정 이고 검수대상인건
					btn_cancel.enable = false;							
					grd_main.formatid = "gnrlJdg_format";
					grd_main.redraw();							
				}else if(("NO" == ds_search.getColumn(0,"JDC_CL") || "RE" == ds_search.getColumn(0,"JDC_CL") )&& "JR" == ds_search.getColumn(0,"TLY_CL") ){
					//일반감정 이고 검수등록인건(검수등록취소목적) => 신규
					btn_cancel.enable = true;
					grd_main.formatid = "rpetJdgCncl_format";					
					grd_main.redraw();						
					//grd_main.redraw();					
				}else if("RE" == ds_search.getColumn(0,"JDC_CL") && "JO" == ds_search.getColumn(0,"TLY_CL") ){
					//재감정 이고 검수대상인건										
					btn_cancel.enable = false;
					grd_main.formatid = "rpetJdg_format";
					grd_main.redraw();					
				}
				*/				
				if("JO" == ds_search.getColumn(0,"TLY_CL")){
					//일반감정 이고 검수대상인건
					btn_cancel.enable = false;							
					grd_main.formatid = "rpetJdg_format";															
				}else if("JR" == ds_search.getColumn(0,"TLY_CL")){
					//일반감정 이고 검수등록인건(검수등록취소목적) => 신규
					btn_cancel.enable = true;
					grd_main.formatid = "rpetJdgCncl_format";															
				}
				grd_main.setCellProperty("Body", 0, "expr",  sRowNumEpr);
				grd_main.redraw();								
			}
			break;
		case "cnclSave": 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				btn_search_onclick();								
			}
			break;	
		default:
			break;
	}
}

