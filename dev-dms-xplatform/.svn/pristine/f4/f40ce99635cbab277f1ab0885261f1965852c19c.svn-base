/*----------------------------------------------------------------------------------------------
 * 설명   : search
-----------------------------------------------------------------------------------------------*/
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{	

	// 필수 입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}else if(gfn_isNull(div_search.cbo_typ156.value)){
		gfn_message("A","DMS00002","W","","감정구분"); /*필수입력항목 {0}이/가 누락되었습니다. */
		div_search.cbo_typ156.setFocus();
		return false;
	}else if(gfn_isNull(div_search.cbo_typ157.value)){
		gfn_message("A","DMS00002","W","","검수구분"); /*필수입력항목 {0}이/가 누락되었습니다. */
		div_search.cbo_typ157.setFocus();
		return false;
	}else if(gfn_isNull(div_search.cbo_strdDt.value)){
		gfn_message("A","DMS00002","W","","기준일자"); /*필수입력항목 {0}이/가 누락되었습니다. */
		div_search.cbo_strdDt.setFocus();
		return false;
	}
	
	// 검색조건: 적용년월
	var arrTerm = div_search.ccp_tly_dt.getData();
	ds_search.setColumn(0,"STDR_SDT",arrTerm[0]);
	ds_search.setColumn(0,"STDR_EDT",arrTerm[1]);
	
	page = 1;
	//trace("ds_search.saveXML ==> "+ds_search.saveXML());
	
	
	fn_searchMain();
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 취소버튼 클릭시
-----------------------------------------------------------------------------------------------*/
function btn_cancel_onclick(obj:Button,  e:ClickEventInfo)
{	
	if(ds_main.findRow("CHECKED","1")<0){	
		gfn_message("A","DMS00020","W");	//DMS00020 : 선택된 정보가 없습니다.
		return false;
	}else if(gfn_message("C","DMS00104","Q","","")){	// 취소하시겠습니까?
		fn_cnclSave();	
	}
	
}

