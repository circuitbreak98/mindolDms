	switch(obj.name) {
		case "cbo_typ156": 	
			if(!gfn_isNull(e.postvalue) && "JO" == div_search.cbo_typ157.value ){				
				div_search.cbo_strdDt.index = 0;
			}else if(!gfn_isNull(e.postvalue) && "JR" == div_search.cbo_typ157.value ){
				// 검수등록일경우				
				div_search.cbo_strdDt.index = 1;
			}						
			break;
		case "cbo_typ157": 									
			if("JO" == e.postvalue && !gfn_isNull(div_search.cbo_typ156.value)){								
				div_search.cbo_strdDt.index = 0;					
			}else if ("JO" != e.postvalue && !gfn_isNull(div_search.cbo_typ156.value)){								
				// 검수등록일경우																			
				div_search.cbo_strdDt.index = 1;
			}			
			break;	
		default:
			break;
	}					
}
