/*----------------------------------------------------------------------------------------------
 * 설명   : 기본값 설정
-----------------------------------------------------------------------------------------------*/
function fn_setInitForm(obj:Form, e:LoadEventInfo)
{
	if(!gfn_isNull(argParam01)){
		div_search.edt_BfeqpMdlCd.value = argParam01; //이전모델코드
	}
	if(!gfn_isNull(argParam02)){
		div_search.edt_BfSerNo.value = argParam02; //이전단말기일련번호
	}
    if(!gfn_isNull(argParam03)){
		ds_search.setColumn(0, "EQP_COLOR_CD", argParam03)  //단말기 색상코드
	}
	if(!gfn_isNull(argParam04)){
		ds_search.setColumn(0, "CNSL_MGMT_NO", argParam04) //상담관리번호
	}
	if(!gfn_isNull(argParam05)){
		ds_search.setColumn(0, "EQP_SER_NO", argParam05) //상품일련번호
	}
		
	 //fn_searchMain();
	 div_search.edt_AfeqpMdlCd.setFocus();
}

/*===============================================================================================
 * [ PART 5 ] Transaction 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 비밀번호저장
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_saveMain()
{
	var sSvcID		 = "saveMain";
	var sTrID		 = "PSCUserMgmt_pChgPwd";
	var sPtrn		 = ds_pwd.getRowType(ds_pwd.rowposition) == Dataset.ROWTYPE_INSERT? "I" : "U";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RS_CHG_PWD=ds_pwd:U";
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "saveMain" : 
			if ( iErrorCode == 0 ) {
				gfn_message("A",ncMsg,"I");	//MSGI0005 : 수정하였습니다
				close("cancel");
			}
			break;
		default :
			break;
	}
}

/*===============================================================================================
 * [ PART 7 ] Main Button Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 버튼 Event
-----------------------------------------------------------------------------------------------*/
function button_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj.name) 
	{				
		case "btn_confirm" :	//확인
			btn_save_onclick();
			break;
			
		case "btn_cancel" : 	//취소
			close("cancel");
			break;
			
		case "btn_close" : 		//버튼X
			close("cancel");
			break;	
	}
}

function btn_save_onclick(obj:Button,  e:ClickEventInfo)
{

	fn_saveMain();
	
}


/*===============================================================================================
 * [ PART 8 ] Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 조회조건에서 단말기 찾기 버튼 클릭
-----------------------------------------------------------------------------------------------*/
function div_search_btn_searchMdl_onclick(obj:Button,  e:ClickEventInfo)
{
	var sParam01 = div_search.edt_AfeqpMdlCd.value;
	var sParam02 = div_search.edt_AfeqpMdlNm.value;
	var sParam03 = "";
	if ( e == null ) {
		sParam03 = "key";
	} else {
		sParam03 = "button";
	}
	var arrArgs = {argAutoSelect:true, argCallProgram:"FEP038P", argParam01:sParam01, argParam02:sParam02, argParam03:sParam03};
	var sRtnVal = gfn_dialog("FBI008P", arrArgs);

	//팝업에서 입력받은 값 리턴
	if ( gfn_isNull(sRtnVal) || sRtnVal == "cancel" ) {
		//취소시 값 제거
		div_search.edt_AfeqpMdlCd.value = "";
		div_search.edt_AfeqpMdlNm.value = "";
	} else {	//Object이용시
		div_search.edt_AfeqpMdlCd.value = sRtnVal["supEqpMdlCd"];
		div_search.edt_AfeqpMdlNm.value = sRtnVal["supEqpMdlNm"];
	}	
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 모델명 reset버튼 클릭시
-----------------------------------------------------------------------------------------------*/
function div_search_btn_resetMdl_onclick(obj:Button,  e:ClickEventInfo)
{
	ds_search.setColumn(0,"AF_EQP_MDL_CD","");
	ds_search.setColumn(0,"AF_EQP_MDL_NM","");
}

