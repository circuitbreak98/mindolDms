 * 설명   : 업무콤보변경에 따른 OBJ ENABLE 상태변경
-----------------------------------------------------------------------------------------------*/
function fn_chgStsEnable(){
	btn_excel_upload.enable = false;
	btn_sndAmt.enable = false;
	btn_feeDedc.enable = false;
	btn_forcingPrch.enable = false;
	btn_sndAmtCncl.enable = false;
	btn_feeDedcCncl.enable = false;
	btn_forcingPrchCncl.enable = false;
	
	div_search.cbo_eco4UYn.enable = false;
	div_search.cbo_preDcYn.enable = false;
	div_search.cbo_clubTYn.enable = false;
	div_search.cbo_inAmtYn.enable = false;
	div_search.cbo_dirPrchYn.enable = false;

	if(div_search.cbo_opGb.value == "NO"){ //일반감정
		div_search.edt_BoxNo.enable = true;

		if(div_search.cbo_subOpGb.value == "31"){ //송금
			ds_dtGb.filter("DT_GB_CD == '1' || DT_GB_CD == '2'");
			btn_excel_upload.enable = true;
			btn_sndAmt.enable = true;
			btn_sndAmtCncl.enable = true;
			div_search.cbo_eco4UYn.enable = true;
			div_search.cbo_preDcYn.enable = true;
			div_search.cbo_inAmtYn.enable = true;
			div_search.cbo_dirPrchYn.enable = true;
		}else if(div_search.cbo_subOpGb.value == "32"){ //요금공제
			ds_dtGb.filter("DT_GB_CD == '1' || DT_GB_CD == '6'");
			btn_feeDedc.enable = true;
			btn_feeDedcCncl.enable = true;
			div_search.cbo_inAmtYn.enable = true;
		}else if(div_search.cbo_subOpGb.value == "34"){ //강제매입
			btn_forcingPrch.enable = true;
			btn_forcingPrchCncl.enable = true;
			ds_dtGb.filter("DT_GB_CD == '3'");
		}
	}else if(div_search.cbo_opGb.value == "RE"){ //재감정
		div_search.edt_BoxNo.value = "";
		div_search.edt_BoxNo.enable = false;

		if(div_search.cbo_subOpGb.value == "31"){ //송금
			ds_dtGb.filter("DT_GB_CD == '2' || DT_GB_CD == '4' || DT_GB_CD == '5'");
			btn_sndAmt.enable = true;
			btn_sndAmtCncl.enable = true;
			div_search.cbo_eco4UYn.enable = true;
			div_search.cbo_preDcYn.enable = true;
			div_search.cbo_inAmtYn.enable = true;
		}else if(div_search.cbo_subOpGb.value == "32"){ //요금공제
			ds_dtGb.filter("DT_GB_CD == '4' || DT_GB_CD == '5' || DT_GB_CD == '6'");
			btn_feeDedc.enable = true;
			btn_feeDedcCncl.enable = true;
			div_search.cbo_preDcYn.enable = true;
			div_search.cbo_clubTYn.enable = true;
			div_search.cbo_inAmtYn.enable = true;
		}
	}
}
