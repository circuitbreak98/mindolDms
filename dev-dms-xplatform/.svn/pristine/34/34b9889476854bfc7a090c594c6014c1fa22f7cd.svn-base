/*===============================================================================================
 * [ PART 1 ] Form에서 Include 할 라이브러리(xjs)파일 정의
================================================================================================*/
include "LIB::common.xjs"; //공통함수 호출
include "LIB::comEpBiz.xjs";	// 중고폰 공통 업무 호출

/*===============================================================================================
 * [ PART 2 ] Form에서 사용될 전역변수를 선언
================================================================================================*/
var fvOpGb = "0";
var fvSubOpGb = "0";
var fvDtGb = "0";
var fvCellText = "";
var fvOldPage = 1;
var g_row_type = "";
var g_row = -1;
/*===============================================================================================
 * [ PART 3 ] Form Event 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Form Load Event
-----------------------------------------------------------------------------------------------*/
function Form_onload(obj:Form, e:LoadEventInfo)
{
	//공통 Form 초기화
	gfn_setInitForm(obj, e, "FEP014M")
 	//Form 초기 개발자 셋팅
 	//공통코드 초기화
 	fn_setInitCode(obj, e);
 	//그리드 초기화
 	fn_setInitGrid(obj, e);
 	//기본값 초기화
 	fn_setInitForm(obj, e);
}
/*----------------------------------------------------------------------------------------------
 * 설명   : Form 종료전 Check 함수(공통)
-----------------------------------------------------------------------------------------------*/
function Form_checkBeforeClose()
{
	//변경된 Dataset검증
	var bResult = gfn_isUpdated(ds_main);
	//변경여부에 따라 Confirm창을 호출하고 결과를 리턴
	return gfn_beforeFormCloseConfirm(bResult);
}
/*===============================================================================================
 * [ PART 4 ] Load 후 초기설정
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitCode(obj:Form, e:LoadEventInfo)
{
	var strCode =  "DMS100=ds_DMS100=N";	//단말기색상
 		strCode += ",DMS179=ds_DMS179=A";	//중고폰G.Key 진행상태
 		strCode += ",DMS166=ds_DMS166=A";	//중고폰은행코드
 		strCode += ",DMS241=ds_DMS241=N";	//중고폰가입방법
 		strCode += ",DMS244=ds_DMS244=N";	//중고폰공제유형구분

	gfn_getCodeDs( this
				  , strCode
				  ,"fn_afterCodeCallBack");	
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 호출 후 처리
-----------------------------------------------------------------------------------------------*/
function fn_afterCodeCallBack()
{
 	ds_dtGb.filter("DT_GB_CD == '1' || DT_GB_CD == '2'");
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 그리드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitGrid(obj:Form, e:LoadEventInfo)
{
	//그리드 페이징 초기화
	cgp_main.setPageInit("fn_selectPageExec", false);
	cgp_mainCnt.setPageNoCnt(0, 0, 0);	//PageNo Count
	
	page = 1;
	page_size = 50;
	
	fn_onGridReSize(grd_main);
	fn_onGridReSize(grd_feeDedc);
	fn_onGridReSize(grd_forcingPrch);
	fn_onGridReSize(grd_rJdgSendAmt);
	fn_onGridReSize(grd_rJdgFeeDedc);
	// 그리드 초기화 
	//gfn_initGrid(objGrid:UserGrid, bReadonly, bSortable, bTooltip, sSelectType, innerDs, sPage, sPageSize, sTotCnt);
	gfn_initGrid(grd_main, false, true, true, "row", null);
	gfn_initGrid(grd_feeDedc, false, true, true, "row", null);
	gfn_initGrid(grd_forcingPrch, false, true, true, "row", null);
	gfn_initGrid(grd_rJdgSendAmt, false, true, true, "row", null);
	gfn_initGrid(grd_rJdgFeeDedc, false, true, true, "row", null);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 페이지 Click시 페이지조회
 * param   : iTotalRowCount - 전체 Row수
 * param   : iUnitSelRowNo  - 페이지당 Row수
 * param   : iCurrPage      - 현재 페이지
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_selectPageExec(iTotalRowCount, iUnitSelRowNo, iCurrPage)
{
	//trace("페이징처리::total row="+iTotalRowCount+"::unit row="+iUnitSelRowNo+"::current page="+iCurrPage);
	//페이징처리
	page = iCurrPage;
	fn_searchMain();
}

