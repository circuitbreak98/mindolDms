		var dsNm = ds_main;
		if(fvOpGb == "RE"){
			dsNm = ds_rmain;
		}
		var sName = ds_prnInfoInq.lookup("PRN_INFO_INQ_ITEM_CD", DMS007_NAME,"PRN_INFO_INQ_CTT");    
		var sHpNo=  ds_prnInfoInq.lookup("PRN_INFO_INQ_ITEM_CD", DMS007_HPNO,"PRN_INFO_INQ_CTT");    
		var sAccount=  ds_prnInfoInq.lookup("PRN_INFO_INQ_ITEM_CD", DMS007_ACCOUNT,"PRN_INFO_INQ_CTT");    
		var sSvcNo=  ds_prnInfoInq.lookup("PRN_INFO_INQ_ITEM_CD", "13","PRN_INFO_INQ_CTT");    
		if(!gfn_isEmpty(sName)) dsNm.setColumn(dsNm.rowposition, "DPSTR",sName);
		if(!gfn_isEmpty(sHpNo)) dsNm.setColumn(dsNm.rowposition, "TEL_NO",sHpNo);
		if(!gfn_isEmpty(sAccount)) dsNm.setColumn(dsNm.rowposition, "ACCO_NO",sAccount);
		if(!gfn_isEmpty(sSvcNo)) dsNm.setColumn(dsNm.rowposition, "SVC_NO",sSvcNo);
	}
}	
/*----------------------------------------------------------------------------------------------
 * 설명   : 일반감정 송금 취소
-----------------------------------------------------------------------------------------------*/
function fn_sndAmtCncl()
{
	if(fvOpGb != "NO" || fvSubOpGb != "31"){ //일반감정 송금확인
		gfn_message("A","현재의 조회 데이터는 송금취소대상 데이터가 아닙니다. 확인 바랍니다.","E","","");
		return;
	}
	//저장함수 호출
	ds_save.clearData();
	ds_dtlSave.clearData();
	
	var nRow = -1;
	var nRowSub = -1;
	for(var i = 0; i < ds_main.rowcount; i++){
		if(ds_main.getColumn(i,"CHECKED") == "1"){
			if(gfn_isNull(ds_main.getColumn(i,"RMT_DT")) || ds_main.getColumn(i,"RMT_DT") == "" || !gfn_isDate(ds_main.getColumn(i,"RMT_DT"))){
				gfn_message("A","송금일자가 없거나 부정확 합니다. 확인하여 주십시요.","E","","");
				return;
			}
			if(ds_main.getColumn(i,"AMT_RMT_YN") == "N" ){
				gfn_message("A","선택데이터중에 미지급건이 있습니다. 확인하여 주십시요.","E","","");
				return;
			}
			ds_main.setColumn(i,"RMT_DT",ds_main.getColumn(i,"RMT_DT").replace("-",""));
			nRow = ds_save.addRow();
			ds_save.copyRow(nRow,ds_main,i);
			//ds_save.setColumn(nRow,"PRCH_MGMT_NO",ds_main.getColumn(i,"PRCH_MGMT_NO"));
			for(var j = 0; j < ds_dtlList.rowcount; j++){
				if(ds_main.getColumn(i,"PRCH_MGMT_NO") == ds_dtlList.getColumn(j,"PRCH_MGMT_NO")){
					nRowSub = ds_dtlSave.addRow();
					ds_dtlSave.copyRow(nRowSub,ds_dtlList,j);
				}
			}
			ds_save.setColumn(nRow,"OP_GB",fvOpGb);
			ds_save.setColumn(nRow,"OP_SUB_GB",fvSubOpGb);
		}
	}
	if(ds_save.rowcount <= 0){
		gfn_message("A","DMS00020","E","",""); //선택된 정보가 없습니다.
		return;
	}
	//trace(ds_save.saveXML());
	//trace(ds_dtlSave.saveXML());
	fn_saveMain();
}
