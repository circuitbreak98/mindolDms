function grd_main_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	
	// 검수등록 화면의 Param 값						
	var sEqpMdlCd = ""
	var sCnslMgmtNo = ""
	var sCnslDt = "";		// 상담일자
	var sScrbMthd = "";		// 가입방법	
	var sPrgGb = "C";			// 검수대상구분
	var sJdcCl = "NO";			// 검수구분
	if("NO" == fvOpGb && ("31" == fvSubOpGb || "32" == fvSubOpGb || "34" == fvSubOpGb)){
		if(ds_main.rowcount <= 0){
			return;
		}
		sEqpMdlCd = ds_main.getColumn(obj.currentrow,"EQP_MDL_CD");		// 단말기모델코드
		sCnslMgmtNo = ds_main.getColumn(obj.currentrow,"CNSL_MGMT_NO");	// 상담관리번호
		sPrgGb = "C";			// 검수대상구분
		sJdcCl = "NO";			// 검수구분 일반감정
	}
	else if("RE" == fvOpGb && ("31" == fvSubOpGb || "32" == fvSubOpGb)){
		if(ds_rmain.rowcount <= 0){
			return;
		}
		sEqpMdlCd = ds_rmain.getColumn(obj.currentrow,"EQP_MDL_CD");		// 단말기모델코드
		sCnslMgmtNo = ds_rmain.getColumn(obj.currentrow,"CNSL_MGMT_NO");	// 상담관리번호
		sPrgGb = "P";			// 검수대상구분
		sJdcCl = "RE";			// 검수구분 재감정
	}else{
		return;
	}
	
	var arrArgs = {sEqpMdlCd:sEqpMdlCd, sCnslMgmtNo:sCnslMgmtNo, sCnslDt:sCnslDt, sScrbMthd:sScrbMthd, sPrgGb:sPrgGb, sJdcCl:sJdcCl};
		
	gfn_tlyRgst("FEP014M",arrArgs);
}
