// 액셀전체다운로드 다음처리
function fn_nextLoop()
{
	var nTotCnt = gfn_toFixedLocaleString(total_record_cnt,0);
	var nNowCnt = gfn_toFixedLocaleString(page * page_size,0);
	div_downInfoBase.div_downPbr.sta_totDownCnt.text = nTotCnt+" 건";
	div_downInfoBase.div_downPbr.sta_nowDownCnt.text = nNowCnt+" 건";
	if ( total_record_cnt <=  page * page_size ) {
		div_downInfoBase.visible = false;
		page = fvOldPage;
		page_size = 50;
		if("NO" == fvOpGb && "31" == fvSubOpGb){
			gfn_baseExcelExpert(grd_mainExcelAll,"입금목록","입금목록");
		} else if("NO" == fvOpGb && "32" == fvSubOpGb){
			gfn_baseExcelExpert(grd_feeDedcExcelAll,"요금공제목록","요금공제목록");
		} else if("NO" == fvOpGb && "34" == fvSubOpGb){
			gfn_baseExcelExpert(grd_forcingPrchExcelAll,"강제매입목록","강제매입목록");
		} else if("RE" == fvOpGb && "31" == fvSubOpGb){
			gfn_baseExcelExpert(grd_rJdgSendAmtExcelAll,"재감정송금목록","재감정송금목록");
		} else if("RE" == fvOpGb && "32" == fvSubOpGb){
			gfn_baseExcelExpert(grd_rJdgFeeDedcExcelAll,"재감정요금공제목록","재감정요금공제목록");
		} else {
			return;
		}
	} else {
		//다음페이지 조회
		page++;
		fn_searchExcelAll();
	} 
}
/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
				//fn_resetGridColSize();
			}
		break;
		case "saveMain": 
			if ( iErrorCode == 0 ) {
				btn_search_onclick();
			}
		break;
		case "searchExcelAll" : 
			if ( iErrorCode == 0 ) {
				if("NO" == fvOpGb){
					ds_mainExcelAll.appendData(ds_mainExcelAllTemp,true);
				}else{
					ds_rmainExcelAll.appendData(ds_rmainExcelAllTemp,true);
				}
				if(page == 1){
					div_downInfoBase.visible = true;
				}
				fn_nextLoop();
			}else{
				div_downInfoBase.visible = false;
				page = fvOldPage;
				page_size = 50;
			}
		break;
		default:
		break;
	}
}
