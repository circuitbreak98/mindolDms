-----------------------------------------------------------------------------------------------*/
function Div01_Div02_btn_reset_onclick(obj:Button,  e:ClickEventInfo)
{
	Div01.Div02.edt_posAgency.value = "";
	Div01.Div02.edt_posAgencyNm.value = "";
}
