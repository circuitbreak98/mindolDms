// 		var WAYBIL_NO //운송장번호
// 		var ASSET_NO //자산번호
	
		if(strChk == "1" && ds_etmmList.getRowType(i) == "4" ){	// 업데이트 글은 Temp 삭제여부만 갱신하고 더미만 삭제
			ds_etmmList.deleteRow(i);
			ds_etmmListTemp.setColumn(i, "DEL_YN", "Y");
			fn_saveMain();
			
		}else if (strChk == "1" && ds_etmmList.getRowType(i) == "2"){  //insert 글은 바로 삭제
		ds_etmmList.deleteRow(i);
		ds_etmmListTemp.deleteRow(i);
// 		i++;
// 		alert(i+"번 글이 작성중 입니다.");
		}
	
	}	
	
	
	
	if(!gfn_isUpdated(ds_etmmList))//저장할 테이타셋 
	{
		gfn_message("A","변경된 정보가 없습니다.","I");	//???? : 변경된 정보가 없습니다.
		return false;
	}	
	return true;
	
	
}
