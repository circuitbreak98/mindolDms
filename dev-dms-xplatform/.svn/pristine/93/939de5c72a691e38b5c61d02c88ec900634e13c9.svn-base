function btn_reset_onclick(obj:Button,  e:ClickEventInfo)
{
	if (gfn_isUpdated(ds_inListMain)) {		
		if (gfn_message("C","DMS00024","Q")){ //편집중인 데이터가 있습니다. 계속하시겠습니까?
			ds_inListMain.reset();
			ds_constList.clearData();
		}
	} else {
		ds_inListMain.clearData();
		ds_constList.clearData();
	}
}

