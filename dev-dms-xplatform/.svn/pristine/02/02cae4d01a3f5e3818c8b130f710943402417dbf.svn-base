/*----------------------------------------------------------------------------------------------
 * 설명   : 저장전 유효성 검사
 * param   : 
 * return  : true/false
-----------------------------------------------------------------------------------------------*/
function fn_chkBeforeSave()	
{

	if(ds_main.getRowCount() == 0 ){
		gfn_message("A","DMS00022","I");	//변경된 정보가 없습니다.
		return;
	}
	
	//저장전 수정,신규입력데이터의 trim처리 
	gfn_dsTrim(ds_remprc);
	
	/*
	if (!gfn_getValidateCompList(div_detail)) {	
		return false;
	}
	*/

	if (!gfn_getValidateCompList(div_detail)) {	
		gfn_message("A","DMS00022","I");	//변경된 정보가 없습니다.
		return false;
	}

	//1건이상 금액존재하면 입력
	var icnt = 0;		
	for(var i = 0 ; i < ds_remprc.getRowCount() ; i++ ){
		for(var j = 1 ; j < ds_remprc.getColCount() ; j++ ){
			if(!gfn_isNull(ds_remprc.getColumn(i,j))){
				icnt++;
			}	  			
		}
	}
	
	if(icnt==0){							
		gfn_message("A","DMS00022","I");	//변경된 정보가 없습니다.
		return false;
	}
	return true;
}

