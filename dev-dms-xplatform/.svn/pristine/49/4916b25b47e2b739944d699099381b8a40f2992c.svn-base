			ds_mainList.addRow();
			ds_mainList.setColumn(j, "SVC_MGMT_NO", opener.ds_main.getColumn(i,"SVC_MGMT_NO"));
			ds_mainList.setColumn(j, "CNTRT_NO", opener.ds_main.getColumn(i,"CNTRT_NO"));
			//trace(ds_mainList.getColumn(j, "SVC_MGMT_NO"));
			j++;
