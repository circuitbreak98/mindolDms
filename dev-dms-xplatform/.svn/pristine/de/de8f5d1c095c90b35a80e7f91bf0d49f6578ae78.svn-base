function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	// 입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	
	var yn =  div_search.cal_deprStrdYm.getDate();
	ds_search.setColumn(0,"DEPR_STRD_YM",yn);
	
	ds_main.clearData();
	page = 1;
	fn_searchMain();
	
}

