/*******************************************************************************
 * 설명    :  데이터셋 변경시 
*******************************************************************************/
function ds_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{	
	switch(obj.name) {				
		case "ds_sub": 
			if("HLD_QTY" == e.columnid){								
				if(obj.getColumn(e.row,"QTY") <=  obj.getColumn(e.row,"HLD_QTY")){
					obj.setColumn(e.row,"DDCT_AMT","0");
				}else if("0" == obj.getColumn(e.row,"HLD_QTY")){					
					obj.setColumn(e.row,"DDCT_AMT",toNumber(obj.getColumn(e.row,"UPRC"),0)*toNumber(obj.getColumn(e.row,"QTY"),0));							
				}else{
					obj.setColumn(e.row,"DDCT_AMT",toNumber(obj.getColumn(e.row,"UPRC"),0)*toNumber(obj.getColumn(e.row,"HLD_QTY"),0));
				}	
				f_conAmt_set();	// 매입(상담) 금액 계산
				f_conDdctAmt_set();	// 오차 금액 구하기(2015.11.12 추가)
			}
			break;
		case "ds_main": 
			if("ADJ_AMT" == e.columnid){												
				if(div_main6.div_sub1.mae_addAmt.value < e.newvalue && "E" != div_main3.cbo_eqpGrade.text){					
					gfn_message("A","DMS00123","E","", new Array('조정금액','조정한도액'));	// DMS00123 : {0}이 {1}을 초과하였습니다.					
					obj.setColumn(e.row,"ADJ_AMT","0");
					div_main6.div_sub1.mae_adjAmt.setFocus();
					return;
				}								
															
				div_main6.div_sub1.mae_cnslAmt.value = 	toNumber(div_main6.div_sub1.mae_conAmt.value,0)+toNumber(div_main6.div_sub1.mae_adjAmt.value,0);   // 매입금액
				f_conDdctAmt_set(); // 오차 금액 구하기(2015.11.12 추가)
			}
			break;	
		
		default :
			break;
	}		
}

