		var sEtcAddr = ds_prnInfoInq.lookup("PRN_INFO_INQ_ITEM_CD", DMS007_ADDR_DTL,"PRN_INFO_INQ_CTT");    
		if(!gfn_isEmpty(sEtcAddr)) ds_main.setColumn(ds_main.rowposition, "ETC_ADDR",sEtcAddr);
		//계좌번호
		var sAccoNo = ds_prnInfoInq.lookup("PRN_INFO_INQ_ITEM_CD", DMS007_ACCOUNT,"PRN_INFO_INQ_CTT");    
		if(!gfn_isEmpty(sAccoNo)) ds_main.setColumn(ds_main.rowposition, "ACCO_NO",sAccoNo);
		//서비스번호
		var sSvcNo = ds_prnInfoInq.lookup("PRN_INFO_INQ_ITEM_CD", "13","PRN_INFO_INQ_CTT");    
		if(!gfn_isEmpty(sSvcNo)) ds_main.setColumn(ds_main.rowposition, "SVC_NO",sSvcNo);
		
