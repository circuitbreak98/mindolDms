	if(!gfn_isNull(bSecureYn) && bSecureYn){
	    //고객명
		var sDpstr = ds_prnInfoInq.lookup("PRN_INFO_INQ_ITEM_CD", DMS007_NAME,"PRN_INFO_INQ_CTT"); 
		if(!gfn_isEmpty(sDpstr)) ds_main.setColumn(ds_main.rowposition, "DPSTR",sDpstr);
		//연락처
		var sTel = ds_prnInfoInq.lookup("PRN_INFO_INQ_ITEM_CD", DMS007_TEL,"PRN_INFO_INQ_CTT");    
		if(!gfn_isEmpty(sTel)) ds_main.setColumn(ds_main.rowposition, "TEL_NO",sTel);
