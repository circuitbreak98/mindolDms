/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {				
		case "searchMain": 
			if ( iErrorCode == 0 ) {								
				if(ds_main.getRowCount()>0){	
					// 검수일자 셋팅
					div_main3.cal_tlyDt.value = gfn_today();	
										
					/*																																									
					trace("PRG_GB => " + gfn_getNullToEmpty(ds_param.getColumn(ds_param.rowposition,"PRG_GB"))); 					
					trace("출시일자 => "+ ds_eqpInfo.getColumn(ds_eqpInfo.rowposition,"LAUNC_DT"));
					trace("상담일자 => "+ ds_main.getColumn(ds_main.rowposition,"CNSL_DT"));
					trace("상품 고정가경 여부 => "+ ds_eqpInfo.getColumn(ds_eqpInfo.rowposition,"FIX_PRC_YN"));
					trace("매입구분 => "+ gfn_getNullToEmpty(ds_main.getColumn(ds_main.rowposition,"PRCH_CL")));
					trace("처리상태 => "+ gfn_getNullToEmpty(ds_main.getColumn(ds_main.rowposition,"PROGR_ST")));
					trace("매입금액 => "+toNumber(div_main6.div_sub1.mae_cnslAmt.value)); 
					trace("조정금액 => "+toNumber(div_main6.div_sub1.mae_adjAmt.value));
					trace("자사여부 => "+ ds_eqpInfo.getColumn(ds_eqpInfo.rowposition,"SLFCO_CL"));
					*/
					
					//등급셋팅
					div_main3.cbo_eqpGrade.text = ds_main.getColumn(0,"EQP_ST");
					
					if("20070101" > gfn_getNullToEmpty(ds_eqpInfo.getColumn(ds_eqpInfo.rowposition,"LAUNC_DT")) 
					&& "20120307" < gfn_getNullToEmpty(ds_main.getColumn(ds_main.rowposition,"CNSL_DT")) ){
						ds_sub_Changed();	// 구성품 가격 초기화
					}															
					if("N" == gfn_getNullToEmpty(ds_eqpInfo.getColumn(ds_eqpInfo.rowposition,"FIX_PRC_YN"))){
						if("S" != gfn_getNullToEmpty(ds_param.getColumn(ds_param.rowposition,"PRG_GB"))){
								f_conAmt_set();	// 매입(상담)금액 구하기
						}						
						if("N" != gfn_getNullToEmpty(ds_param.getColumn(ds_param.rowposition,"PRG_GB")) &&
						!("P" == gfn_getNullToEmpty(ds_param.getColumn(ds_param.rowposition,"PRG_GB")) &&  "N" == gfn_getNullToEmpty(ds_main.getColumn(ds_main.rowposition,"PRCH_CL")))
						){
							// 산출출금액 = 매입금액 - 조정금액 
							div_main6.div_sub1.mae_conAmt.value = toNumber(div_main6.div_sub1.mae_cnslAmt.value,0) - toNumber(div_main6.div_sub1.mae_adjAmt.value,0);	
							
							fn_formInitSearch(ds_param.getColumn(ds_param.rowposition,"PRG_GB"));	// 화면구성 초기화																				
						}												
					}else{						
						// 고객의사 저장
						// 매입미확정 || 매입구분이 'N'인경우											
						if("N" == gfn_getNullToEmpty(ds_param.getColumn(ds_param.rowposition,"PRG_GB")) || 
						("P" == gfn_getNullToEmpty(ds_param.getColumn(ds_param.rowposition,"PRG_GB")) &&  "N" == gfn_getNullToEmpty(ds_main.getColumn(ds_main.rowposition,"PRCH_CL"))) ||
						"13" == gfn_getNullToEmpty(ds_main.getColumn(ds_main.rowposition,"PROGR_ST")) ){
							if("13" == gfn_getNullToEmpty(ds_main.getColumn(ds_main.rowposition,"PROGR_ST"))){
								btn_save.enable = true;		
								btn_save2.enable = true;
							}
							// 조정한도액 셋팅 
							div_main6.div_sub1.mae_addAmt.value = toNumber(div_main6.div_sub1.mae_cnslAmt.value,0)*2;	// 조정한도액
							div_main6.div_sub1.mae_conAmt.value = div_main3.cbo_eqpGrade.value;	// 산출금액 																												
						// 고객의사 미저장		
						}else{							
							// 산출출금액 = 매입금액 - 조정금액 
							div_main6.div_sub1.mae_conAmt.value = toNumber(div_main6.div_sub1.mae_cnslAmt.value,0) - toNumber(div_main6.div_sub1.mae_adjAmt.value,0);	
							
							fn_formInitSearch(ds_param.getColumn(ds_param.rowposition,"PRG_GB")); // 화면구성 초기화																																																																														
						}
						if("1" == ds_eqpInfo.getColumn(ds_eqpInfo.rowposition,"SLFCO_CL")){						
							div_main3.st_slfcoCl.text = "자사";					
						}else{						
							div_main3.st_slfcoCl.text = "타사";
						}
						
						if(!gfn_isNull(div_main3.cal_oningDt.value)){
							div_main3.st_oningDtt.text = (gfn_diffMonth(div_main3.cal_oningDt.value,gfn_today())+1)+"개월";												
						}
						
						// 조정한도액 셋팅 
						div_main6.div_sub1.mae_addAmt.value = toNumber(div_main6.div_sub1.mae_cnslAmt.value,0)*2;	// 조정한도액
						// 정책대사 콤보박스 초기화
						div_main5_chk_polyObjYn_onclick(div_main5.chk_polyObjYn,null);											
					}
					// 오차 금액 구하기(2015.11.12 추가)
					f_conDdctAmt_set();	
					
					// 개인정보 초기화
					div_main5.edt_dpstr.enable = false;
					div_main5.edt_telNo.enable = false;
					div_main5.edt_accoNo.enable = false;
					div_main5.edt_svcNo.enable = false;
					div_main5.cbo_bnkCd.enable = false;
					div_main5.btn_addr.enableevent = false;
					fvPsnlInfoFlag = false;
				}										
			}
			break;			
		case "rpetSearchMain": 
			if ( iErrorCode == 0 ) {												
				if(ds_main.getRowCount()>0){
					// 검수일자 셋팅
					div_main3.cal_tlyDt.value = gfn_today();	
					/*
					trace("PRG_GB => " + gfn_getNullToEmpty(ds_param.getColumn(ds_param.rowposition,"PRG_GB"))); 					
					trace("출시일자 => "+ ds_eqpInfo.getColumn(ds_eqpInfo.rowposition,"LAUNC_DT"));
