/*----------------------------------------------------------------------------------------------
 * 설명   : 저장버튼 클릭시
-----------------------------------------------------------------------------------------------*/
function btn_save_onclick(obj:Button,  e:ClickEventInfo)
{		
	if (!fn_saveValidationCheck() ) {
		return;
	}
	if ( fvPsnlInfoFlag == false ){
          btn_psnInfoPopup_onclick();
    }
	//trace("EQP_COLOR_CD ==> "+ ds_main.getColumn(0,"EQP_COLOR_CD"));	
	var dRow = ds_main.rowposition;	// 검수등록 정보 현재 ROW.
	ds_main.setColumn(dRow,"EQP_ST_TXT",div_main3.cbo_eqpGrade.text);	// 단말기등급
	ds_main.setColumn(dRow,"PRC_GB",ds_param.getColumn(0,"PRG_GB"));	// 검수등록대상구분
	/*				
	trace(" NEW_YN ==>> " +gfn_getNullToEmpty(ds_param.getColumn(0,"NEW_YN")));
	trace(" PRC_GB ==>> " +gfn_getNullToEmpty(ds_main.getColumn(dRow,"PRC_GB")));
	trace(" PROGR_ST ==>> " +gfn_getNullToEmpty(ds_main.getColumn(dRow,"PROGR_ST")));
	*/
	
	if("N" == gfn_getNullToEmpty(ds_param.getColumn(0,"NEW_YN"))){
		//신규 등록이 아닌 경우.
		if("S" == gfn_getNullToEmpty(ds_param.getColumn(0,"PRG_GB"))){	// 검수등록대상구분: 삭제 인경우
			// 매입관리정보 수정.
			fn_saveTlyFnsh();	// 검수완료대상 정보 저장
		}else{
			// 매입관리정보 등록.
			fn_saveTlyProgr();	// 검수진행대상 정보 저장						
		}	
	}else if(gfn_isNull(ds_param.getColumn(0,"PRG_GB"))){
		// 검수등록 대상 구분이 NULL 인 경우.
		if("11" ==  gfn_getNullToEmpty(ds_main.getColumn(dRow,"PROGR_ST"))){
			// 11: 입고확정.
			// 매입관리정보 등록.
			fn_saveTlyProgr();	// 검수진행대상 정보 저장						
		}else if("13" ==  gfn_getNullToEmpty(ds_main.getColumn(dRow,"PROGR_ST")) 
				|| "14" ==  gfn_getNullToEmpty(ds_main.getColumn(dRow,"PROGR_ST"))
				|| "23" ==  gfn_getNullToEmpty(ds_main.getColumn(dRow,"PROGR_ST"))
				|| "2C" ==  gfn_getNullToEmpty(ds_main.getColumn(dRow,"PROGR_ST"))){
			// 13: 검수완료, 14: 검수제외, 23: 고객보류, 2C: 검수제외 고객보류
			// 매입관리정보 수정.	
			fn_saveTlyFnsh();	// 검수완료대상 정보 저장										
		}else{			
			gfn_message("A","DMS00122","E","", "검수");	// DMS00122 : {0} 대상이 아닙니다.											
		}
	}else{		
		gfn_message("A","DMS00009","E","", "");	// DMS00009 : 시스템 오류가 발생하였습니다. 운영팀에 문의하세요.
	}
	
}

