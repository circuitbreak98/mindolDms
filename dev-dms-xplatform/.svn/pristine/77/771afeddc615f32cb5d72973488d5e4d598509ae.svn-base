						f_conDdctAmt_set(); // 오차 금액 구하기(2015.11.12 추가)
					}
					
					if("1" == ds_eqpInfo.getColumn(ds_eqpInfo.rowposition,"SLFCO_CL")){						
						div_main3.st_slfcoCl.text = "자사";					
					}else{						
						div_main3.st_slfcoCl.text = "타사";
					}
					
					div_main3.cbo_color.index = 0;					
				}																
			} 
			break;	
		case "saveTlyProgr": 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.				
								
				var vForm = gfn_getFrm(FV_PRE_FORM_ID);
				if (!gfn_isNull(vForm)){	
					vForm.btn_search_onclick();					
					vForm.setFocus();							
					gfn_closeFrm("FEP006D");
				}								
				
			} 
			break;
		case "saveTlyFnsh": 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				
				var vForm = gfn_getFrm(FV_PRE_FORM_ID);
				if (!gfn_isNull(vForm)){	
					vForm.btn_search_onclick();					
					vForm.setFocus();								
					gfn_closeFrm("FEP006D");
				}
								
			} 
			break;	
			
		case "searchSecureInfo" : 
			if ( iErrorCode == 0 ) {
			    //상세사용자설정
			    fn_setDetailUserInfo(true);
			}
		    break;
		
		default :
			break;
	}
}

