/*----------------------------------------------------------------------------------------------
 * 설명   : 박스번호 키업  
-----------------------------------------------------------------------------------------------*/
function div_search_edt_boxNo_onkeyup(obj:Edit, e:KeyEventInfo)
{
	
	if(!gfn_isNull(obj.value)){
		if(obj.value.length == 9){
			btn_search.setFocus();
			btn_search_onclick();
			obj.setFocus();	
		}
		
	}
	
	if (e.keycode == 13) {
		btn_search.setFocus();
		btn_search_onclick();
		obj.setFocus();	
	}	
}
