 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	//입고기간
	/*
	if(gfn_isNull(arrTerm[0]) || gfn_isNull(arrTerm[1])){
		gfn_message("A","DMS00002","E","","접수일자");  //필수입력항목 {0}이/가 누락되었습니다.
		return false;
	}
	*/
	//데이타셋 clear
	ds_consult_m.clearData();
	ds_consult_d.clearData();
	ds_color.clearData();
	ds_prod_m.clearData();
	ds_rate.clearData();
	ds_rmk_list.clearData();
	var sSvcID		 = "searchMain";
	var sTrID		 = "PEPCnslMgmt_pInqPrchsCustRmksNo";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sOutputDs	 = "ds_consult_m=RS_CONSULT_M ds_consult_d=RS_CONSULT_D ds_color=RS_COLOR ds_prod_m=RS_PROD_M ds_rate=RS_RATE ds_rmk_list=RS_RMK_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인저장
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_saveMain() {
  if(!fn_beforeSave()) return false; 		
    var sSvcID		 = "saveMain";
	var sTrID		 = "PPRReqpInveInfoSend_pSaveReqpOutCntrtStat";
	var sPtrn		 = "C";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RS_EQP_CNTRT_LIST=ds_save:u";
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";

	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc); 

} 
 
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain" : 
			if ( iErrorCode == 0 ) {
				gfn_message("B","DMS00001");	//MSGI0003 :정상 조회되었습니다.
				//trace(ds_consult_m.getColumn(0,"EQP_COLOR_CD"));
				div_conInfo2.cbo_color.value = ds_consult_m.getColumn(0,"EQP_COLOR_CD");
				
				
			}
			break;
		case "saveRSN":
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I"); //정상 처리되었습니다.
				fn_searchMain();
			}
			
			break;

	}
}
