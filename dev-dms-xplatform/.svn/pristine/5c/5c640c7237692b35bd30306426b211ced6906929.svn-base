/*===============================================================================================
 * [ PART 7 ] Main Button Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 버튼 Event
-----------------------------------------------------------------------------------------------*/
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj.name) 
	{	
			
		case "btn_confirm" :	//확인
			var exEqpSerNum = "";
			var exEqpImeiNum = "";
			var exEqpColorCd = "";
			gfn_dsTrim(ds_new);
			exEqpSerNum = ds_new.getColumn(0,"EQP_SER_NUM");
			exEqpImeiNum = ds_new.getColumn(0,"EQP_IMEI_NUM");
			exEqpColorCd = ds_new.getColumn(0,"EQP_COLOR_CD");
			if ( gfn_isNull(ds_new.getColumn(0,"EQP_SER_NUM")) ) {
				gfn_message("A","DMS00023","I","","일련번호 "); 
				return;
			}
			if ( gfn_isNull(ds_new.getColumn(0,"EQP_IMEI_NUM")) ) {
				gfn_message("A","DMS00023","I","","IMEI번호 "); 
				return;
			}

			var objRtn = new Object;
				objRtn["exEqpSerNum"] = exEqpSerNum;
				objRtn["exEqpImeiNum"] = exEqpImeiNum;
				objRtn["exEqpColorCd"] = exEqpColorCd;

			this.close(objRtn);		
			break;
			
		case "btn_cancel" : 	//취소
			close("cancel");
			break;
			
		case "btn_close" : 		//버튼X
		
			close("cancel");
			break;	
	}
}

