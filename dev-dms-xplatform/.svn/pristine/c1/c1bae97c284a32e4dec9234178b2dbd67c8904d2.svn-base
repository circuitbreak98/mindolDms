-----------------------------------------------------------------------------------------------*/
function Form_onload(obj:Form, e:LoadEventInfo)
{
	//공통 Form 초기화
	gfn_setInitPop(obj, e, "FNR024P");
	
	//Form 초기 개발자 셋팅
 	//공통코드 초기화
