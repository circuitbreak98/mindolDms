/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
 	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {
			
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);	
					
				if(!ds_sumcnt.getColumn(0,"M_CNT") == "" || ds_sumcnt.getColumn(0,"M_PRC") == ""){

					var Mtotal = "  총 "+"<fc v='#31a3dc'>"+ds_sumcnt.getColumn(0,"M_CNT")
						+"</fc><b v='false'><fc v='#77787a'>건, 합계: </fc></b>"+"<fc v='#31a3dc'>"
						+gfn_toFixedLocaleString(ds_sumcnt.getColumn(0,"M_PRC"), '@@@,@@@,@@@')+"</fc><b v='false'><fc v='#77787a'>원 </fc></b>";
					fn_setGridTitle(stc_subTitle,Mtotal);
				
				} else {

					var Mtotal = "  총 "+"<fc v='#31a3dc'>"+ds_sumcnt.setColumn("M_CNT","0")
						+"</fc><b v='false'><fc v='#77787a'>건, 합계: </fc></b>"+"<fc v='#31a3dc'>"
						+gfn_toFixedLocaleString(ds_sumcnt.setColumn("M_PRC","0"),'@@@,@@@,@@@')+"</fc><b v='false'><fc v='#77787a'>원 </fc></b>";
					fn_setGridTitle(stc_subTitle,Mtotal);
				}
				
				
			
			}
		break;
		
		case "saveSlip":
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				ds_main.clearData();
			  fn_searchMain();
			}else{
				gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
			}
		break;
			
		case "delSlip":

			if (iErrorCode == 0) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
			  ds_main.clearData();
			  fn_searchMain();
			}else{
				gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
			}
			
		break;
		
		case "saveMain":
			if (iErrorCode == 0) {
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
			  ds_main.clearData();
			  fn_searchMain();
			}else{
				gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
			}
			break;
		
		case "AllExcel":
		//alert(ds_AllExcel.saveXML());
		
			if (iErrorCode == 0) {
				var dsbind = gfn_makeGrid(ds_AllExcel);
				dsbind.binddataset = "ds_AllExcel";
				gfn_baseExcelExpert(dsbind,"단말기정산목록","단말기정산목록");
			}else{
				gfn_message("A","XYZE0000","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
			}
			break;	
			
		default:
		break;
	}
}

