function btn_slipCreate_onclick(obj:Button,  e:ClickEventInfo)
{
	/* 1.전표마감처분 */
	
	if(ds_searchSum.getColumn(ds_searchSum.rowposition, 'LST_INVE_ST_CD') == '60'){
		gfn_message("A","DMS00071","E"); //이미 처리된 건이 있습니다.	???
		return false;
	}
	
	if(ds_main.findRow("CHK","1") == -1){
