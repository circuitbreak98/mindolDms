					ds_search.setColumn(0,"LST_ASSET_SLIP_ST","%");
				}else{
					ds_search.setColumn(0,"LST_ASSET_SLIP_ST",ds_searchSum.getColumn(ds_searchSum.rowposition, 'ASSET_SLIP_ST'));
				}
				ds_search.setColumn(0,"LST_DISPO_DT",ds_searchSum.getColumn(ds_searchSum.rowposition, 'DISPO_DT'));
				ds_search.setColumn(0,"YYYY",ds_searchSum.getColumn(ds_searchSum.rowposition, 'YYYY'));
				
				ds_main.clearData();
				fn_searchEqpDspslLst()

			}
				
				
// 			cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
// 			cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
// 			gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);

/*			
			if ( ds_main.rowposition != -1 && ds_main.getRowType(ds_main.rowposition) != Dataset.ROWTYPE_NORMAL){
					cal_calFrom_onchanged();
				}
			var subMsg = "  총 "+"<fc v='#31a3dc'>"+ds_searchSum.getColumn(0,"TOTAL_CNT")
