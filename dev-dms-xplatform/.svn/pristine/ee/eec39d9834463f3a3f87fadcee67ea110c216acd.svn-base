				gfn_message("A","DMS00087","E","",new Array(intLine+"라인 마감자산진행  처리"));	//{0} 건은 처리가 불가능 합니다.
				return false;
			}
			
		}
	}
	
	if(gfn_message("A",st,"I","","")){
		if (gfn_isNull(ds_search.getColumn(0, 'LST_DEPR_DT'))){
			ds_search.setColumn(0,"LST_DEPR_DT",ds_search.getColumn(0, 'DEPR_DT'));
		}
		//
		fn_slipClsAssetDispo();
	}else{
		return false;			
	}


	/* 2전표발행 */
