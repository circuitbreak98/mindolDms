/*----------------------------------------------------------------------------------------------
 * 설명   : 전표취소
-----------------------------------------------------------------------------------------------*/
function btn_slipCancle_onclick(obj:Button,  e:ClickEventInfo)
{
	// 1.
	if(ds_searchSum.getRowCount() == 0 ){
		gfn_message("A","DMS00020","E"); 	//선택된 정보가 없습니다.
		return false;
	}
	// 2.
	gfn_message("A","DMS00153","I"); // 전표상태재조회 후 처리를 권장합니다.

	var dt = ds_search.getColumn(0,"DEPR_DT");
	var yy = dt.substring(0,4);
	var mm = dt.substring(4,6);
	//var st = "해당월("+yy+"년 "+mm+"월) 전표를 삭제합니다. 계속하시겠습니까?"
	var st = "전표["+ds_search.getColumn(0, "LST_ASSET_SLIP_NO")+"] "+ds_searchSum.getColumn(ds_searchSum.rowposition,"TOTAL_CNT")+"건의 매각처리 ";
	
	/* 3.전표마감처분 (매각완료)*/
	if(ds_search.getColumn(ds_search.rowposition, 'LST_INVE_ST_CD') == '60'){
		gfn_message("A","DMS00071","E"); //이미 처리된 건이 있습니다.
		return false;
	}
	
	/*	4.전표상태가 미발행, 전표삭제 경우 실행*/
	if (ds_search.getColumn(0, 'LST_ASSET_SLIP_ST') == '00' || ds_search.getColumn(0, 'LST_ASSET_SLIP_ST') == '%' || ds_search.getColumn(0, 'LST_ASSET_SLIP_ST') == '05' )
	{
		if(gfn_message("A","DMS00148","I","",st)){ 		//{} 전표발행하시겠습니까??
			ds_search.setColumn(0,"CASE_WHEN","03");
			if (gfn_isNull(ds_search.getColumn(0, 'LST_DEPR_DT'))){
				ds_search.setColumn(0,"LST_DEPR_DT",ds_search.getColumn(0, 'DEPR_DT'));
			}	
			fn_onlineCall();
		}
	}else{
		gfn_message("A","DMS00071","E"); //이미 처리된 건이 있습니다.
		return false;
	}
}

