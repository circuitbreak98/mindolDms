	for(i=0; i<ds_main.getRowCount(); i++)
	{
		if(ds_main.getColumn(i,"CHK")=='1')
		{
			if(ds_main.getColumn(i,"INVE_ST_CD") == '61'){
				var mrow = ds_handling.addRow();
				ds_handling.copyRow(mrow,ds_main,i);
			}else{
				gfn_message("A","DMS00071","E"); //이미 처리된 건이 있습니다.
