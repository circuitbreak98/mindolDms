function grd_main_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	//파라미터 셋팅 구현
	ds_searchExcel.setColumn(0, "IN_DT", ds_main.getColumn(ds_main.rowposition, "XCL_YM") );
	ds_searchExcel.setColumn(0, "XCL_GUBUN", ds_main.getColumn(ds_main.rowposition, "XCL_CD") );
	ds_searchExcel.setColumn(0, "XCL_NM", ds_main.getColumn(ds_main.rowposition, "XCL_NM") );
	ds_searchExcel.setColumn(0, "TOT_CNT", ds_main.getColumn(ds_main.rowposition, "XCL_QTY") );
	ds_searchExcel.setColumn(0, "TOT_AMT", ds_main.getColumn(ds_main.rowposition, "XCL_AMT") );
	
	if(ds_main.rowposition >= 0 && grd_main.currentcol == 10){
		
		
		ds_mainExcel.clear;
		var sSvcID		 = "searchMainExcel";
		var sTrID		 = "PPRAssetSlipRgst_pInqAssetDeprExcelList";
		var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
		var sInputDs	 = "ncFieldMap=ds_searchExcel";
		var sOutputDs	 = "ds_mainExcel=RS_SALE_XCL_LIST";
		var sArgs		 = "";
		var sCallBackFnc = "fn_callBack";

		gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
	}
	else if(ds_main.rowposition >= 0 && grd_main.currentcol == 9){
		//취소
		var slip_st_cd =  ds_main.getColumn(ds_main.rowposition, "SLIP_ST_CD");
		if( slip_st_cd == '00' ){
			gfn_message("A","DMS00131","I","","미발행건은");//"미발행건은 취소할수 없습니다."
			
			return;
		}else if( slip_st_cd  == '05'){
			gfn_message("A","DMS00071","I");// 이미 처리된 건이 있습니다.
			return;
		}else if( slip_st_cd == '20'   || slip_st_cd  == '30'){
			gfn_message("A","DMS00131","I","","승인완료 또는 반대전표는");//"승인완료 또는 반대전표는 취소할수 없습니다."
			return;
		}else{
			var slip_no =  ds_main.getColumn(ds_main.rowposition, "SLIP_NO");
			ds_searchExcel.setColumn(0,"SLIP_NO", slip_no);
			var sSvcID		 = "delXclSlipNo";
			var sTrID		 = "PPRSaleXclSlipRgst_pDelSaleXclSlipNo";
			var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
			var sInputDs	 = "ncFieldMap=ds_searchExcel";
			var sOutputDs	 = "ds_mainExcel=RS_SALE_XCL_LIST";
			var sArgs		 = "";
			var sCallBackFnc = "fn_callBack";
			gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
		}
	}
	else if(ds_main.rowposition >= 0 && grd_main.currentcol == 8){
		//등록
		var slip_st_cd =  ds_main.getColumn(ds_main.rowposition, "SLIP_ST_CD");
		if( slip_st_cd == '00'   || slip_st_cd  == '05'){
			
			var dt = ds_main.getColumn(ds_main.rowposition, "XCL_YM");
			
			var yy = dt.substring(0,4);
			var mm = dt.substring(4,6);
			var ym = dt.substring(0,6);

			var array = new Array(yy ,mm ,ds_main.getColumn(ds_main.rowposition, "XCL_QTY")
			                      ,gfn_toFixedLocaleString(ds_main.getColumn(ds_main.rowposition,"XCL_AMT"), '@@@,@@@,@@@'));
			if( gfn_message("C","DMS00132","Q","",array)	){
				var sSvcID		 = "saveXclSlipNo";
				var sTrID		 = "PPRSaleXclSlipRgst_pSaveSaleXclSlipNo";
				var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
				var sInputDs	 = "ncFieldMap=ds_searchExcel";
				var sOutputDs	 = "";
				var sArgs		 = "";
				var sCallBackFnc = "fn_callBack";
				gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
			}
			
		}
		else{
			gfn_message("A","DMS00071","I");// 이미 처리된 건이 있습니다.
			return;
		}
		
	}
}


