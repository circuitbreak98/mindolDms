-----------------------------------------------------------------------------*/
function fn_searchMain()
{
	ds_main.clear;
	var sSvcID		 = "searchMain";
	var sTrID		 = "PPRAssetSlipRgst_pInqAssetSlipList";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sOutputDs	 = "ds_main=RS_SALE_XCL_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";

	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*-----------------------------------------------------------------------------
 * 설명   : 메인저장 (insert, update)
 * param   : 
 * return  : void
-----------------------------------------------------------------------------*/
function fn_saveMain()
{	
	var sSvcID		 = "saveMain";
	var sTrID		 = "PBSSCMsgMgmt_pSaveMsg";
	var sPtrn		 = "I";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RS_MSG_LIST=ds_msg:u";
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	pv_sPtrn         = sPtrn;  // 저장Type을 전역변수로 저장
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*=============================================================================
 * [ PART 6 ] CallBack 처리
=============================================================================*/
/*-----------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
 	switch(sSvcID) {
		case "searchMain" : 
			//페이징 관련셋팅
			//ds_msg.appendData(ds_msgTempPage, true);	
			//조회된 첫번째row로 setFocus 
			//ds_msg.rowposition = (page-1) * page_size ;
			break;
			
		case "saveXclSlipNo" : 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//MSGI0001 : 정상 처리되었습니다.
			}
			break;
		case "xclCreate" : 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");
				fn_searchMain();
			}
			break;
		case "searchMainExcel" : 
			if ( iErrorCode == 0 ) {
				//gfn_message("A","DMS00000","I");	//MSGI0001 : 정상 처리되었습니다.
				// excel ds 로 바꿔야함..
				if (ds_mainExcel.rowcount == 0){
					gfn_message("A","DMS00021","I"); //조회된 정보가 없습니다.
				}else{
				
				var xcl_nm = ds_searchExcel.getColumn(0,"XCL_NM");
				
					gfn_baseExcelExpert(grd_mainExcelEtc, xcl_nm, xcl_nm);
				}			
			}
			break;
			
		default :
			break;
	}
}

