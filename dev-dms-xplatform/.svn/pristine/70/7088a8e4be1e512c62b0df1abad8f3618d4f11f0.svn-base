{

	 //입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	
	var yn =  div_search.cal_Ydate.getDate();
	ds_search.setColumn(0,"YDATE",yn);
	
	ds_main.clearData();
	page = 1;
	fn_searchMain();
	
}

