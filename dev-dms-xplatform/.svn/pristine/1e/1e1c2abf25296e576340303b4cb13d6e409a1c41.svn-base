{
	
	if ( total_record_cnt <=  page * page_size ) {
		page_size = 50;
		page = curPage;
		gfn_baseExcelExpert(grd_mainExcel,"가용재고목록","가용재고목록");
	} else {
		//다음페이지 조회
		page++;
		fn_searchMainAll();
	} 
}
