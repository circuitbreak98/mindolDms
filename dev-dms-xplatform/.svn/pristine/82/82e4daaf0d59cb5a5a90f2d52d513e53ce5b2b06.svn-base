/*----------------------------------------------------------------------------------------------
 * 설명   : 회수입고 확정 버튼을 클릭했을때
-----------------------------------------------------------------------------------------------*/
function btn_clct_in_fix_onclick(obj:Button,  e:ClickEventInfo)
{
	var msg = "단말기 입고처리를 합니다.계속하시겠습니까?"
	if(ds_list.findRow("CHECKED","1") == -1){
		//체크대상없음
		gfn_message("A","DMS00020","I"); // 선택된 정보가 없습니다.
		return;
	}
	
	for(var i=0; i<ds_list.getRowCount(); i++){
		if(ds_list.getColumn(i,"CHECKED")=='1')
		{
			if(ds_list.getColumn(i, "INVE_ST_CD") == "10" || ds_list.getColumn(i, "INVE_ST_CD") == "40"){
			gfn_message("A","DMS00071","E"); //이미 처리된 건이 있습니다.
			return false;
			}else {
					var mrow = ds_handling.addRow();
					ds_handling.copyRow(mrow,ds_list,i);
					ds_handling.setColumn(mrow,"IN_OBJ_DTL_CD",'04'); //R센터입고 - 회수구분코드
					ds_handling.setColumn(mrow,"INVE_ST_CD",'10');    //입고 - 재고상태코드
					ds_handling.setColumn(mrow,"EQP_IN_DT",gfn_today());
					
				  }
		}
	
	}
	
	if(gfn_message("C",msg,"I","" ,"")){
		fn_handling();
	}else {
		ds_handling.clearData();
		return false;			
	}
	
	
	
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 회수입고 취소 버튼을 클릭했을때
-----------------------------------------------------------------------------------------------*/
function btn_clct_in_cncl_onclick(obj:Button,  e:ClickEventInfo)
{
	var msg = "입고단말기 취소처리를 합니다.계속하시겠습니까?"
	
	if(ds_list.findRow("CHECKED","1") == -1){  //체크대상없음
		gfn_message("A","DMS00020","I"); // 선택된 정보가 없습니다.
		return;
	}
	
	for(var i=0; i<ds_list.getRowCount(); i++){
		if(ds_list.getColumn(i,"CHECKED")=='1')
		{
			if(ds_list.getColumn(i, "EQP_IN_DT") == gfn_today()) /* 입고일자 */			
			{ 
					var mrow = ds_handling.addRow();
					ds_handling.copyRow(mrow,ds_list,i);
				if(ds_list.getColumn(i, "INVE_ST_CD") == "40") /* 감정완료 */
				{ 
					ds_handling.setColumn(mrow,"INVE_DEL_YN",'Y');
					ds_handling.setColumn(mrow,"IN_OBJ_DTL_CD",'03'); //R센터입고 - 회수구분코드
					ds_handling.setColumn(mrow,"INVE_ST_CD",'20');    //입고 - 재고상태코드
					ds_handling.setColumn(mrow,"EQP_IN_DT",null);
				
				}else if(ds_list.getColumn(i, "INVE_ST_CD") == "10" ) {
					ds_handling.setColumn(mrow,"IN_OBJ_DTL_CD",'03'); //R센터입고 - 회수구분코드
					ds_handling.setColumn(mrow,"INVE_ST_CD",'20');    //입고 - 재고상태코드
					ds_handling.setColumn(mrow,"EQP_IN_DT",null);
				}else if(ds_list.getColumn(i, "INVE_ST_CD") == "20"){
					gfn_message("A","DMS00087","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
					return false;
				}
			}else {
				gfn_message("A","DMS00069","E"); //유효한 일자가 아닙니다.
				return false;
		     }
				
		}
	
	}
	
	if(gfn_message("C",msg,"I","" ,"")){
		fn_handling();
	}else {
		ds_handling.clearData();
		return false;			
	}
}

