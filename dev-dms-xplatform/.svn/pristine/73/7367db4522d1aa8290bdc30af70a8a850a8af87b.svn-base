		gfn_message("A","DMS00002","E","",div_search.cbo_dtGb.text);  //필수입력항목 {0}이/가 누락되었습니다.
		grd_main.setFocus(i);
	return false;
	}
	ds_search.setColumn(0, "FROM_DT", arrTerm[0]);
	ds_search.setColumn(0, "TO_DT", arrTerm[1]);
	
	return true;
}
