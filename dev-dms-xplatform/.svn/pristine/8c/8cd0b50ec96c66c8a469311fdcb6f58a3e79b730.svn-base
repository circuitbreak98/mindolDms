function btn_slipOnline_onclick(obj:Button,  e:ClickEventInfo)
{
	// 필수 입력 항목 Validate Check
	if(gfn_isNull(div_search.cmc_toYm.getDate())){	
		gfn_message("A","DMS00002","W","",div_search.stc_msgInDate.text); /*필수입력항목 {0}이/가 누락되었습니다. */
		div_search.cmc_toYm.setFocus();
		return false;
	}else{			
		var xclYm = div_search.cmc_toYm.getDate().substr(0,4)+"년 "+div_search.cmc_toYm.getDate().substr(4,2)+"월";		// 정산기준연월		
		if(gfn_message("C","DMS00144","Q","",xclYm+" 정산")){
			// 조건: 정산기준연월
			ds_search.setColumn(0, "CMMS_DT",div_search.cmc_toYm.getDate());	
			fn_saveRpetCalc();		
		}		
	}

}

/*----------------------------------------------------------------------------------------------
 * 설명   : search
-----------------------------------------------------------------------------------------------*/
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{	

	// 필수 입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}else if(gfn_isNull(div_search.cmc_toYm.getDate())){	
		gfn_message("A","DMS00002","W","",div_search.stc_msgInDate.text); /*필수입력항목 {0}이/가 누락되었습니다. */
		div_search.cmc_toYm.setFocus();
		return false;
	}
	
	// 검색조건: 적용년월
	ds_search.setColumn(0, "CMMS_DT",div_search.cmc_toYm.getDate());
	
	fn_searchMain();
}

