/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {				
				gfn_setGridTitleCountPaging(stc_subTitle, ds_sub.getRowCount());	// 조회결과 건수								
				/*
				// 총 건수
				div_detail.mae_grossCount.value = ds_sub.getSum("CMMS_COUNT");	
				// 업무처리 수수료 = 총건수 * 870(고정데이터)
				div_detail.mae_opProcCmms.value = ds_sub.getSum("CMMS_COUNT") * 870 ;	
				// 고객관리시스템 사용료 : 4220000 (고정데이터)
				if("0"==ds_sub.getSum("CMMS_COUNT")){
					div_detail.mae_custMgmtCmms.value = 0;
				}else{
					div_detail.mae_custMgmtCmms.value = 4220000;
				}						
				// 수수료 총액 : 업무처리 수수료 + 고객관리시스템 사용료
				div_detail.mae_cmmsGrossAmt.value = (ds_sub.getSum("CMMS_COUNT") * 870) + toNumber(div_detail.mae_custMgmtCmms.value,0);						
				*/
			}
			break;
		case "searchExcelDtl": 
			if ( iErrorCode == 0 ) {								
				ds_excel.appendData(ds_tmpExcel,true);
				if(ds_tmpExcel.getRowCount()>0){
					var excelTotCnt =  ds_tmpExcel.getColumn(0,"EXCEL_TOTAL_CNT");	// 데이터 총 건수		
															
					var sRtnVal = gfn_excelPageIndex(excelPage,excelTotCnt);												
					if ( !gfn_isNull(sRtnVal) && sRtnVal["excelProcYn"] == true){
						return fn_searchDtl(sRtnVal["excelFirst"],sRtnVal["excelLast"]);
					}													
				}
				
				/* 엑셀다운로드 */
				var cmmsStrdYm = ds_search.getColumn(ds_search.rowposition, "CMMS_STRD_YM");	// 정산기준연월
				var dedcTtpCd = ds_search.getColumn(ds_search.rowposition, "DEDC_TYP_CD");	// 공제유형 구분
				var inveTypCd = ds_search.getColumn(ds_search.rowposition, "INVE_TYP_CD");	// 재고유형 구분
				var sheetYm =  cmmsStrdYm.substr(0,4)+"년 "+cmmsStrdYm.substr(4,2)+"월";
				var sheetName = "";
				var title = "";
				if("01" == dedcTtpCd && "10" == inveTypCd){
					// 계좌송금 && 확정재고
					grd_excel.formatid = "accoRmtConf";
					sheetName = sheetYm+" 송금_비현장";
					title = sheetYm+" 송급 비현장 목록";
				}else if("01" == dedcTtpCd && "20" == inveTypCd){
					// 계좌송금 && 미착재고
					grd_excel.formatid = "accoRmtNonConf";
					sheetName = sheetYm+" 송금_현장";
					title = sheetYm+" 송급 현장 목록";
				}else if("02" == dedcTtpCd && "20" == inveTypCd){
					// 선할인 && 미착재고
					grd_excel.formatid = "preDcDtl";
					sheetName = sheetYm+" 선할인";
					title = sheetYm+" 선할인 목록";						
				}else if("03" == dedcTtpCd && "10" == inveTypCd){
					// 요금선납 && 확정재고
					grd_excel.formatid = "feePpayConf";
					sheetName = sheetYm+" 선납_비현장";
					title = sheetYm+" 선납 비현장 목록";
				}else if("03" == dedcTtpCd && "20" == inveTypCd){
					// 요금선납 && 미착재고
					grd_excel.formatid = "feePpayNonConf";
					sheetName = sheetYm+" 선납_현장";
					title = sheetYm+" 선납 현장 목록";
				}	
				grd_excel.redraw();						
				gfn_baseExcelExpert(grd_excel,sheetName,title);																																										
			}
			break;	
		case "saveRpetCalc": 					
			if ( iErrorCode == 0 ) {								
				/*{0} 건의 데이터가 처리되었습니다.*/
				gfn_message("C","DMS00143","I","",gfn_toFixedLocaleString(ds_resultCalc.getColumn(ds_resultCalc.rowposition,"INSERT_ROW"),"")); 
				fn_searchMain();											
			}else{
				if("DMS00149" == ncMsgId){
					// 재집계 불가능 상태일 경우 재조회.
					fn_searchMain();				
				}			
			}			
			break;	
		default:
			break;
	}
}

