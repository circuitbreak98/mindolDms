function div_sum_edt_BfDdMthRentalFee_onkeydown(obj:Object, e:KeyEventInfo)
{
	dmg_amt_setting();
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 재계산
-----------------------------------------------------------------------------------------------*/
function dmg_amt_setting()
{
	var amt = div_sum.edt_BfDdMthRentalFee.value;
	for(var i=0; i<ds_main.getRowCount(); i++){
		ds_main.setColumn(i,"DMG_ALLWN_AMT",0);
		ds_main.setColumn(i,"DMG_DEPR_AMT",0);
		if(ds_main.getColumn(i,"CHK")==1){ 				//체크된애들중에서
			if(ds_main.getColumn(i,"UNPAID")<=amt){		//미납금<= 충당금 이면
				ds_main.setColumn(i,"DMG_ALLWN_AMT",ds_main.getColumn(i,"UNPAID"));		//충당금에 미납금을 넣고
				ds_main.setColumn(i,"DMG_DEPR_AMT",0);									//상각비에 0을 넣고
				amt -= ds_main.getColumn(i,"UNPAID");									//총금액에서 충당금 빼고
			}else{										//미납금이 > 충당금 이면
				ds_main.setColumn(i,"DMG_ALLWN_AMT",amt);								//충당금이 잔액이고
				ds_main.setColumn(i,"DMG_DEPR_AMT",ds_main.getColumn(i,"UNPAID")-amt);  //상각액이 미납금-충당금
				amt -= amt;															//총금액 털고
			}
		}
	}
}
