-----------------------------------------------------------------------------------------------*/
function btn_slipI_onclick(obj:Button,  e:ClickEventInfo)
{
	var msg = "미납채권 전표를 생성하시겠습니까?";
	ds_sum.setColumn(0,"CASE_WHEN",'01');
	if(ds_main.findRow("CHK","1") == -1){
		//체크대상없음
		gfn_message("A","DMS00020","I"); // 선택된 정보가 없습니다.
		return;
	}
	
	if(gfn_message("A",msg,"I","",""))
	{	
		for(var i=0; i<ds_main.getRowCount(); i++){
			if(ds_main.getColumn(i,"CHK")==1){ 
				if(gfn_isNull(ds_main.getColumn(i,"SLIP_ST_CD"))||ds_main.getColumn(i,"SLIP_ST_CD")=='00'||ds_main.getColumn(i,"SLIP_ST_CD")=='05'){//전표요청가능상태
					var nRow = ds_handling.addRow();
					ds_handling.copyRow(nRow,ds_main,i);
				}else{
					gfn_message("A","DMS00071","E"); //이미 처리된 건이 있습니다. 처리가 불가능합니다 필요
					ds_handling.clearData();
					return false;
				}	
			}
		}
		fn_saveSlip();
	}else{
		return;
	}
}

