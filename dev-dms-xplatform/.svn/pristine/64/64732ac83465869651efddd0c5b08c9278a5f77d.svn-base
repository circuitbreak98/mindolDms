================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{		
	var sSvcID		 = "searchMain";
	var sTrID		 = "PNRColsReqsMgmt_pInqColsReqsLst";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sOutputDs	 = "ds_main=RS_COLS_LIST ds_sumlist=RS_SUM_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 재추심요청
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_handling()		
{
	var sSvcID		 = "handlingMain";
	var sTrID		 = "PNRColsReqsMgmt_pUpdateColsReqs";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RS_COLS_LIST=ds_maintemp";
	var sOutputDs	 = ""; 
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
				
				div_sum.cal_claimCancleDt.cal_calFrom.value = ds_search.getColumn(0,"COLS_REQS_STA_DT");
				div_sum.cal_claimCancleDt.cal_calTo.value = ds_search.getColumn(0,"COLS_REQS_END_DT");
			}
		break;
		
		case "handlingMain":
			if( iErrorCode == 0){
				gfn_message("A","DMS00000","I");	//DMS00000 : 정상 처리되었습니다.
				btn_search_onclick();
			}
		break;
		
		default :
			break;
	}
}

