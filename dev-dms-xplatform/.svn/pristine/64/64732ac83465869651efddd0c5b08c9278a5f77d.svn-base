 * 설명   : search
-----------------------------------------------------------------------------------------------*/
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	//입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	//확정청구일CONF_INV_CNCL_DT
	ds_search.setColumn(0,"CONF_INV_CNCL_STA_DT",div_search.cal_claimCancleDt.cal_calFrom.value);
	ds_search.setColumn(0,"CONF_INV_CNCL_END_DT",div_search.cal_claimCancleDt.cal_calTo.value);
	//처리일자COLS_REQS_DT
	ds_search.setColumn(0,"COLS_REQS_STA_DT",div_search.cal_colsReqsDt.cal_calFrom.value);
	ds_search.setColumn(0,"COLS_REQS_END_DT",div_search.cal_colsReqsDt.cal_calTo.value);
		
	ds_main.clearData();
	//page = 1;
	fn_searchMain();
	
}

