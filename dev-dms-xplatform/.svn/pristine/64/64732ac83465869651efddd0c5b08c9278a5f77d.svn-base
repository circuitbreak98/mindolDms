/*----------------------------------------------------------------------------------------------
 * 설명   :  재추심요청버튼 클릭이벤트
-----------------------------------------------------------------------------------------------*/
function btn_colsReqs_onclick(obj:Button,  e:ClickEventInfo)
{
	var st = "재추심요청을 하시겠습니까?";
	
	if(ds_main.findRow("CHK","1") == -1){
		//체크대상없음
		gfn_message("A","DMS00020","I"); // 선택된 정보가 없습니다.
		return;
	}
	
	for(i=0; i<ds_main.getRowCount(); i++)
	{
		if(ds_main.getColumn(i,"CHK")=='1')
		{	//확정청구취소 상태인지확인
			if(ds_main.getColumn(i,"INV_ST_CD") == '04')
			{
				var mrow = ds_maintemp.addRow();
				ds_maintemp.copyRow(mrow,ds_main,i);
				
				ds_maintemp.setColumn(mrow,"INV_ST_CD","08");
			}else if(ds_main.getColumn(i,"INV_ST_CD") == '08'||ds_main.getColumn(i,"INV_ST_CD") == '09'){
			  	gfn_message("A","DMS00087","E","추심요청중/재추심요청 "); //처리가 불가능합니다 
			  	ds_maintemp.clearData();
				return false;
			}
		}
	}	
	if(gfn_message("C",st,"I","" ,"")){
		fn_handling();
	}else {
		ds_maintemp.clearData();
		return false;			
	}
}

