-----------------------------------------------------------------------------------------------*/
function Form_checkBeforeClose()
{
	//변경된 Dataset검증
	var bResult = gfn_isUpdated(ds_main);
	//변경여부에 따라 Confirm창을 호출하고 결과를 리턴
	return gfn_beforeFormCloseConfirm(bResult);
}
