/*----------------------------------------------------------------------------------------------
 * 설명   : search
-----------------------------------------------------------------------------------------------*/
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{	
	var arrTerm = div_search.ccp_dspt_dt.getData();
	ds_search.setColumn(0,"IN_FR_DT",arrTerm[0]);
	ds_search.setColumn(0,"IN_TO_DT",arrTerm[1]);
	
	if(gfn_isNull(arrTerm[0]) || gfn_isNull(arrTerm[1])){
		gfn_message("A","DMS00002","E","",div_search.cbo_dtGb.text+" ");  //필수입력항목 {0}이/가 누락되었습니다.
		return false;
	}
	if(ds_search.getColumn(0,"BOX_NO") != null && ds_search.getColumn(0,"BOX_NO") != ""){
		var strBoxNo = ds_search.getColumn(0,"BOX_NO").substr(0,2);
		if(ds_search.getColumn(0,"OP_GB_CD") == "NO" && ds_search.getColumn(0,"SUB_OP_GB_CD") == "31"){ //일반감정 송금목록
			if(strBoxNo != "SM" && strBoxNo != "IN"){
				gfn_message("A","송금목록 대상 BOX가 아닙니다.","E","");
				return;
			}
		}else if(ds_search.getColumn(0,"OP_GB_CD") == "NO" && ds_search.getColumn(0,"SUB_OP_GB_CD") == "32"){ //일반감정 요금공제
			if(strBoxNo != "DE" && strBoxNo != "IN"){
				gfn_message("A","요금공제 대상 BOX가 아닙니다.","E","");
				return;
			}
		}else if(ds_search.getColumn(0,"OP_GB_CD") == "NO" && ds_search.getColumn(0,"SUB_OP_GB_CD") == "34"){ //일반감정 강제매입
			if(strBoxNo != "CO" && strBoxNo != "IN"){
				gfn_message("A","강제매입 대상 BOX가 아닙니다.","E","");
				return;
			}
		}
	}
	
	// 입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	gfn_dsTrim(ds_search);

	page = 1;

	if(ds_search.getColumn(0,"OP_GB_CD") == "NO" && ds_search.getColumn(0,"SUB_OP_GB_CD") == "31" && grd_main.visible == false){ //일반감정 송금목록
		grd_main.position.left = 0;
		grd_main.position.top = 145;
		grd_main.position.right = 1018;
		grd_main.position.bottom = 489;
		grd_main.autofittype = "none";
		grd_main.visible = true;
		
		grd_feeDedc.visible = false;
		grd_forcingPrch.visible = false;
	} else if(ds_search.getColumn(0,"OP_GB_CD") == "NO" && ds_search.getColumn(0,"SUB_OP_GB_CD") == "32" && grd_feeDedc.visible == false){ //일반감정 요금공제
		grd_feeDedc.position.left = 0;
		grd_feeDedc.position.top = 145;
		grd_feeDedc.position.right = 1018;
		grd_feeDedc.position.bottom = 489;
		grd_feeDedc.autofittype = "none";
		grd_feeDedc.visible = true;

		grd_main.visible = false;
		grd_forcingPrch.visible = false;
	} else if(ds_search.getColumn(0,"OP_GB_CD") == "NO" && ds_search.getColumn(0,"SUB_OP_GB_CD") == "34" && grd_forcingPrch.visible == false){ //일반감정 강제매입
		grd_forcingPrch.position.left = 0;
		grd_forcingPrch.position.top = 145;
		grd_forcingPrch.position.right = 1018;
		grd_forcingPrch.position.bottom = 489;
		grd_forcingPrch.autofittype = "none";
		grd_forcingPrch.visible = true;

		grd_main.visible = false;
		grd_feeDedc.visible = false;
	} else {
	}
	//조회함수 호출 
	fn_searchMain();
}

