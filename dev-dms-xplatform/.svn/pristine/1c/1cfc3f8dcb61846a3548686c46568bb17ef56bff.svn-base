/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	fvOpGb = div_search.cbo_opGb.value;
	fvSubOpGb = div_search.cbo_subOpGb.value;

	ds_main.clearData();
	ds_dtlList.clearData();
	
	var sSvcID		 = "searchMain";
	var sTrID		 = "";
	var sOutputDs	 = "";
	if(ds_search.getColumn(0,"OP_GB_CD") == "NO" && ds_search.getColumn(0,"SUB_OP_GB_CD") == "31"){ //일반감정 송금목록
		sTrID		 = "PEPDpstMgmt_pInqSendAmtObjList";
	    sOutputDs	 = "ds_main=SEND_AMT_OBJ_LIST";
	    sOutputDs	 += " ds_dtlList=SEND_AMT_OBJ_DTL_LIST";
	}else if(ds_search.getColumn(0,"OP_GB_CD") == "NO" && ds_search.getColumn(0,"SUB_OP_GB_CD") == "32"){ //일반감정 요금공제
		sTrID		 = "PEPDpstMgmt_pInqFeeDedcObjList";
	    sOutputDs	 = "ds_main=FEE_DEDC_OBJ_LIST";
	    sOutputDs	 += " ds_dtlList=FEE_DEDC_OBJ_DTL_LIST";
	}else if(ds_search.getColumn(0,"OP_GB_CD") == "NO" && ds_search.getColumn(0,"SUB_OP_GB_CD") == "34"){ //일반감정 강제매입
		sTrID		 = "PEPDpstMgmt_pInqForcingPrchObjList";
	    sOutputDs	 = "ds_main=FORCING_PRCH_OBJ_LIST";
	    sOutputDs	 += " ds_dtlList=FORCING_PRCH_OBJ_DTL_LIST";
	}else{
		return;
	}
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";

	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 저장
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_saveMain()
{
	var sSvcID		 = "saveMain";
	var sTrID		 = "";
	var sOutputDs	 = "";
	var sInputDs	 = "";
	if(fvOpGb == "NO" && (fvSubOpGb == "31" || fvSubOpGb == "32" || fvSubOpGb == "34")){ //일반감정 송금확인,요금공제,강제매입
		sTrID		 = "PEPDpstMgmt_pUDpstMgmtProcRegUpd";
	    sInputDs	 = "REG_OBJ_LIST=ds_save";
	    sInputDs	 += " REG_OBJ_DTL_LIST=ds_dtlSave";
	}else{
		return;
	}
	var sPtrn		 = "U";  //입력C,조회R,수정U,삭제D,복합M
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	//trace(ds_save.saveXML());
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
				//fn_resetGridColSize();
			}
		break;
		case "saveMain": 
			if ( iErrorCode == 0 ) {
				btn_search_onclick();
			}
		break;
		case "searchExcelAll" : 
			if ( iErrorCode == 0 ) {
				ds_mainExcelAll.appendData(ds_mainTemp,true);
				fn_nextLoop();
			}
		break;
		default:
		break;
	}
}

