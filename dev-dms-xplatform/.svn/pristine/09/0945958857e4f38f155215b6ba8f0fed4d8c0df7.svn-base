function fn_chkBeforeSearch()
{
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	
	// 저장전 기간 셋팅
	var arrTerm = div_search.ccp_term.getData();
	ds_search.setColumn(0,"CNTRT_FR_DTM",arrTerm[0]);
	ds_search.setColumn(0,"CNTRT_TO_DTM",arrTerm[1]);
	
	if(gfn_isNull(arrTerm[0]) || gfn_isNull(arrTerm[1])){
		gfn_message("A","DMS00002","E","","계약일");  //필수입력항목 {0}이/가 누락되었습니다.
		return false;
	}
	
	return true;
}

