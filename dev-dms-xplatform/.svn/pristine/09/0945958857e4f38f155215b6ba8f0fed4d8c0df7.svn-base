/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);

				var SumOutPrc;
				
				if(ds_main.rowcount > 0){
					SumOutPrc = toNumber(ds_main.getColumn(0,"SUM_PRC")).toLocaleString("ko_KR");
				} else {
					SumOutPrc = 0;
				}
				stc_subTitle.text = stc_subTitle.text + " <b v='false'><fc v='#dcdcdc'>l </fc><fc v='#77787a'> 합계금액 : </fc> </b><fc v='#31a3dc'>" 
									+ SumOutPrc
									+"</fc><b v='false'><fc v='#77787a'>원</fc></b>";
									
				gfn_message("B","DMS00001");	//MSGI0003 :정상 조회되었습니다.
			}
		break;
		case "searchMainAll": 
			if ( iErrorCode == 0 ) {
				//엑셀출력
				if (ds_mainAll.rowcount == 0){
					gfn_message("A","DMS00021","I"); //조회된 정보가 없습니다.
				} else {
					gfn_baseExcelExpert(grd_mainAll,"매입정보조회목록","매입정보조회목록");
				}
			}
		break;
		default:
		break;
	}
}

