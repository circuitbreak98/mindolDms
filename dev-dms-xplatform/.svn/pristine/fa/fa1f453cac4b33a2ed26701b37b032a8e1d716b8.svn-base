/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {

 				
				ds_search.setColumn(0,"PROD_QTY", ds_boxInfo.getColumn(0,"PROD_QTY"));
				ds_search.setColumn(0,"QTY", ds_boxInfo.getColumn(0,"QTY"));

// 				if(ds_sub.rowcount == 0) {
// 					gfn_message("A","DMS00004");	//데이터가 존재하지 않습니다.\n데이터를 확인해 주세요.
// 					edt_boxBarCd.setFocus();
// 					fn_dsClear();
// 					return;
//				}
				//fn_setGridTitleCount(stc_subTitle, ds_main.rowcount, toNumber(ds_sub.getColumn(0,"MAX_QTY")));
				
				//gfn_message("B","DMS00001");	//MSGI0003 :정상 조회되었습니다.
				fn_searchDtl();
				
			}

		break;
		
		case "searchDtl": 
			if ( iErrorCode == 0 ) {
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
 				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
 				
 				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
 				
 				ds_mainTemp.clearData();
 				ds_mainTemp.copyData(ds_main);
 				
 				//var ds_boxInfo.getRowCount
 				if (ds_boxInfo.getRowCount() == 0){
					ds_boxInfo.addRow();
				}

				if(gfn_isNull(ds_search.getColumn(0, "PROD_QTY"))){
				ds_search.setColumn(0, "PROD_QTY", 0);
				}
			}
		break;
		
		case "saveMain": 
			if ( iErrorCode == 0 ) {
				//gfn_message("A","DMS00000","I");	//MSGI0001 : 정상 처리되었습니다.
				//var boxCd = ds_main.getColumn(0, "BOX_BAR_CD");
				
				
				//ds_search.setColumn(0,"BOX_BAR_CD", boxCd);
				fn_searchMain();
			}
		break;
	}
}

