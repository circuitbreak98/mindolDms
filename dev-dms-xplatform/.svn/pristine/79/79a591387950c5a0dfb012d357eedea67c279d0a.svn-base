function btn_delete_onclick(obj:Button,  e:ClickEventInfo)
{
	var nRow = 0;
	ds_save.clearData();
	for(var i = 0; i < ds_main.rowcount; i++){
		if(ds_main.getColumn(i,"CHECKED") == "1"){
		//trace(ds_main.getColumn(i,"INCEN_SLIP_ST"));
			if(ds_main.getColumn(i,"SLIP_ST_CD") != "" && ds_main.getColumn(i,"SLIP_ST_CD") != "00" && ds_main.getColumn(i,"SLIP_ST_CD") != "05"){
				gfn_message("A","삭제는 업로드 이전 이거나 전표상태가 미발행 또는 삭제인 건만 가능 합니다. 확인 바랍니다.","E","","");
				return;
			}
			nRow = ds_save.addRow();
			ds_save.copyRow(nRow,ds_main,i);
		}
	}
	
	if(ds_save.rowcount <= 0){
		gfn_message("A","DMS00020","E","",""); //선택된 정보가 없습니다.
		return;
	}
	
	//trace(ds_save.saveXML());
	if(gfn_message("A","DMS00026","I","","")){
		fn_saveMain("D");
	}
}

