/*===============================================================================================
 * [ PART 1 ] Form에서 Include 할 라이브러리(xjs)파일 정의
================================================================================================*/
include "LIB::common.xjs"; //공통함수 호출

/*===============================================================================================
 * [ PART 2 ] Form에서 사용될 전역변수를 선언
================================================================================================*/
var curPage = 1;
/*===============================================================================================
 * [ PART 3 ] Form Event 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Form Load Event
-----------------------------------------------------------------------------------------------*/
function Form_onload(obj:Form, e:LoadEventInfo)
{
	//공통 Form 초기화
	gfn_setInitForm(obj, e, "FEP065M");
 	//Form 초기 개발자 셋팅
 	//공통코드 초기화
 	fn_setInitCode(obj, e);
 	//그리드 초기화
 	fn_setInitGrid(obj, e);
 	//기본값 초기화
 	fn_setInitForm(obj, e);
 	
}

/*----------------------------------------------------------------------------------------------
 * 설명   : Form 종료전 Check 함수(공통)
-----------------------------------------------------------------------------------------------*/
function Form_checkBeforeClose()
{
	//변경된 Dataset검증
	if ( gfn_formBeforeOnClose(this, "ds_main") ) {
		return true;
	} else {
		return false;
	}
}
/*===============================================================================================
 * [ PART 4 ] Load 후 초기설정
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitCode(obj:Form, e:LoadEventInfo)
{

	var strCode = "DMS244=ds_DMS244=N";//중고폰공제유형구분
						
	gfn_getCodeDs( this
				  , strCode
				  ,"fn_afterCodeCallBack");
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 호출 후 처리
-----------------------------------------------------------------------------------------------*/
function fn_afterCodeCallBack()
{		 
	
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 그리드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitGrid(obj:Form, e:LoadEventInfo)
{
    //그리드 페이징 초기화
	cgp_main.setPageInit("fn_selectPageExec", false);
	cgp_mainCnt.setPageNoCnt(0, 0, 0);	//PageNo Count
	
	page = 1;
	page_size = 50;
	// 그리드 초기화 
	//gfn_initGrid(objGrid:UserGrid, bReadonly, bSortable, bTooltip, sSelectType, innerDs, sPage, sPageSize, sTotCnt);
	gfn_initGrid(grd_main, false, true, true, "row", null);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 페이지 Click시 페이지조회
 * param   : iTotalRowCount - 전체 Row수
 * param   : iUnitSelRowNo  - 페이지당 Row수
 * param   : iCurrPage      - 현재 페이지
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_selectPageExec(iTotalRowCount, iUnitSelRowNo, iCurrPage)
{
	//trace("페이징처리::total row="+iTotalRowCount+"::unit row="+iUnitSelRowNo+"::current page="+iCurrPage);
	//페이징처리
	page = iCurrPage;
	fn_searchMain();
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 기본값 설정
-----------------------------------------------------------------------------------------------*/
function fn_setInitForm(obj:Form, e:LoadEventInfo)
{
	div_search.cmc_toYm.setDate(gfn_left(gfn_today(), 6));
	
	//개인정보 조회권한 및 수정권한여부 파라미터 설정 (조회,저장,삭제,출력,엑셀,개인정보)
	gfn_setAutrChild(this);
}

/*===============================================================================================
 * [ PART 5 ] Transaction 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	var sSvcID		 = "searchMain";
	var sTrID		 = "PEPTransitMgmt_pInqTransitList";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sOutputDs	 = "ds_main=RS_TRANSIT_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";

	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 전체엑셀다운
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMainAll()
{
	var sSvcID		 = "searchMainExcel";
	var sTrID		 = "PEPTransitMgmt_pInqTransitList";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_search";
	var sOutputDs	 = "ds_mainExcelTemp=RS_TRANSIT_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
// 액셀전체다운로드 다음처리
function fn_nextLoop()
{	
	if ( total_record_cnt <=  page * page_size ) {
		page_size = 50;
		page = curPage;
		gfn_baseExcelExpert(grd_mainExcel,"미착내역목록","미착내역목록");
	} else {
		//다음페이지 조회
		page++;
		fn_searchMainAll();
	} 
}
/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
 	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {
				cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
				cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
				gfn_setGridTitleCountPaging(stc_subTitle, total_record_cnt);
				gfn_message("B","DMS00001");	//MSGI0003 :정상 조회되었습니다.
			}
		break;
		case "searchMainExcel" : 
			if ( iErrorCode == 0 ) {
				ds_mainExcel.appendData(ds_mainExcelTemp,true);
				fn_nextLoop();
			}else{
				page_size = 50;
				page = curPage;
			}
		break;		
		default:
		break;
		
	}
}

/*===============================================================================================
 * [ PART 7 ] Main Button Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : search
-----------------------------------------------------------------------------------------------*/
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	var sellDt = div_search.cmc_toYm.getDate();
	ds_search.setColumn(0,"SELL_DTM",sellDt);
	
	if(gfn_isNull(sellDt)){
		gfn_message("A","DMS00002","E","","기준년월");  //필수입력항목 {0}이/가 누락되었습니다.
		return false;
	}
	
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
		
		
	ds_main.clearData();
	page = 1;
	fn_searchMain();
	
}

/*----------------------------------------------------------------------------------------------
 * 설명   : grid filter
-----------------------------------------------------------------------------------------------*/
function btn_filter_onclick(obj:Button,  e:ClickEventInfo)
{
	gfn_toggleGridFilter(grd_main);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : grid excel
-----------------------------------------------------------------------------------------------*/
function btn_excel_onclick(obj:Button,  e:ClickEventInfo)
{
	if (grd_main.rowcount == 0){
		gfn_message("A","DMS00021","I"); //조회된 정보가 없습니다.
	} else {
		gfn_baseExcelExpert(grd_main,"미착내역목록","미착내역목록");
	}
}
//전체액셀다운로드
function btn_excelAll_onclick(obj:Button,  e:ClickEventInfo)
{
	if (ds_main.rowcount == 0){
		gfn_message("A","DMS00021","I"); //조회된 정보가 없습니다.
		return;
	}
	
	// 입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	curPage = page;
	gfn_dsTrim(ds_search);
	ds_mainExcel.clearData();
	page = 1;
	page_size = 10000;
	
	gfn_initGrid(grd_mainExcel, false, true, true, "row", null);
	fn_searchMainAll();
}

/*===============================================================================================
 * [ PART 8 ] Event
================================================================================================*/

/*----------------------------------------------------------------------------------------------
 * 설명   : 검색 조건에서 엔터키가 눌러졌을때 검색
-----------------------------------------------------------------------------------------------*/
function div_search_onkeydown(obj:Div, e:KeyEventInfo)
{
	if (e.keycode == KEYCODE_ENTER) {
		div_search.btn_search.setFocus();
		btn_search_onclick();
	}
}

