/*----------------------------------------------------------------------------------------------
 * 설명   : Form Load Event
-----------------------------------------------------------------------------------------------*/
function Form_onload(obj:Form, e:LoadEventInfo)
{
	//공통 Form 초기화
	gfn_setInitForm(obj, e, "FPR049P")

 	//Form 초기 개발자 셋팅
 	//공통코드 초기화
 	fn_setInitCode(obj, e);
 	//그리드 초기화
 	fn_setInitGrid(obj, e);
 	//기본값 초기화
 	fn_setInitForm(obj, e);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : Form 종료전 Check 함수(공통)
-----------------------------------------------------------------------------------------------*/
function Form_checkBeforeClose()
{
}

/*===============================================================================================
 * [ PART 4 ] Load 후 초기설정
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitCode(obj:Form, e:LoadEventInfo)
{
	var strCode = "DMS244=ds_DMS244=N";
		 strCode += ",DMS245=ds_DMS245=N";			 		 

	gfn_getCodeDs( this
				  , strCode
				  ,"fn_afterCodeCallBack");	
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 호출 후 처리
-----------------------------------------------------------------------------------------------*/
function fn_afterCodeCallBack()
{
	//div_search.cbo_opGb.index = 1;
	div_search.cbo_opGb.value = argParam01;
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 그리드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitGrid(obj:Form, e:LoadEventInfo)
{
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 기본값 설정
-----------------------------------------------------------------------------------------------*/
function fn_setInitForm(obj:Form, e:LoadEventInfo)
{
	if(argParam01 == "10"){
		div_search.stc_cntrtPrd.text = "입금일자";
	}
	div_search.CommCalendarPred00.setData(gfn_getFirstDate(gfn_today()),gfn_today(),true);
}

/*===============================================================================================
 * [ PART 7 ] Main Button Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 버튼 Event
-----------------------------------------------------------------------------------------------*/
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj.name) 
	{	
			
		case "btn_confirm" :	//확인
			var arrTerm = div_search.CommCalendarPred00.getData();
			if(gfn_isNull(arrTerm[0]) || gfn_isNull(arrTerm[1])){
				gfn_message("A","DMS00023","E","","회수일자 ");
				return;
			}
			if(!gfn_isDate(arrTerm[0]) || !gfn_isDate(arrTerm[1])){
				gfn_message("A","DMS00069","E");
				return;
			}
			//if(arrTerm[0].substr(0,6) != arrTerm[1].substr(0,6)){
			//	gfn_message("A","회수일자의 기간은 동일 년월에서만 집계 가능합니다.","E");
			//	return;
			//}

			var objRtn = new Object;
				objRtn["opGbCd"] = div_search.cbo_opGb.value;
				objRtn["clctFrDt"] = arrTerm[0];
				objRtn["clctToDt"] = arrTerm[1];

			this.close(objRtn);		
			break;
			
		case "btn_cancel" : 	//취소
			close("cancel");
			break;
			
		case "btn_close" : 		//버튼X
		
			close("cancel");
			break;	
	}
}

