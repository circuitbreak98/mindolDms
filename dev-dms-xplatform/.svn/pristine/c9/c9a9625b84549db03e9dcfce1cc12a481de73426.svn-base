{
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 기본값 설정
-----------------------------------------------------------------------------------------------*/
function fn_setInitForm(obj:Form, e:LoadEventInfo)
{
	ds_org.setColumn(0,"EQP_MDL_NM",argParam01);
	ds_org.setColumn(0,"EQP_SER_NO",argParam02);
	ds_org.setColumn(0,"EQP_IMEI_NO",argParam03);
	ds_org.setColumn(0,"IN_PLC_NM",argParam04);
	ds_org.setColumn(0,"EQP_COLOR_CD",argParam05);
	ds_org.setColumn(0,"EQP_MDL_CD",argParam06);
	ds_new.setColumn(0,"EQP_COLOR_CD",argParam05);
	
	fn_searchColorList();
}
/*===============================================================================================
 * [ PART 5 ] Transaction 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 단말기 색상조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchColorList()
{
	var sSvcID		 = "searchColorList";
	var sTrID		 = "PBIEqpMdlMgmt_pInqEqpColorLst";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_ncFieldMap";
	var sOutputDs	 = "ds_eqpColorList=RS_EQP_COLOR_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";

	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchColorList":
			fn_eqpColorFilter();
			break;
		default :
			break;
	}
}

/*===============================================================================================
 * [ PART 7 ] Main Button Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 버튼 Event
-----------------------------------------------------------------------------------------------*/
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj.name) 
	{	
			
		case "btn_confirm" :	//확인
			var exEqpSerNum = "";
			var exEqpImeiNum = "";
			var exEqpColorCd = "";
			gfn_dsTrim(ds_new);
			exEqpSerNum = ds_new.getColumn(0,"EQP_SER_NO");
			exEqpImeiNum = ds_new.getColumn(0,"EQP_IMEI_NO");
			exEqpColorCd = ds_new.getColumn(0,"EQP_COLOR_CD");
			if ( gfn_isNull(ds_new.getColumn(0,"EQP_SER_NO")) ) {
				gfn_message("A","DMS00023","I","","일련번호 "); 
				return;
			}
			if ( gfn_isNull(ds_new.getColumn(0,"EQP_IMEI_NO")) ) {
				gfn_message("A","DMS00023","I","","IMEI번호 "); 
				return;
			}

			var objRtn = new Object;
				objRtn["exEqpSerNum"] = exEqpSerNum;
				objRtn["exEqpImeiNum"] = exEqpImeiNum;
				objRtn["exEqpColorCd"] = exEqpColorCd;

			this.close(objRtn);		
			break;
			
		case "btn_cancel" : 	//취소
			close("cancel");
			break;
			
		case "btn_close" : 		//버튼X
		
			close("cancel");
			break;	
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 단말기색상필터
-----------------------------------------------------------------------------------------------*/
function fn_eqpColorFilter()	
{	
	ds_eqpColorList.filter("");
	ds_DMS100.filter("");
	if(!gfn_isNull(ds_org.getColumn(0,"EQP_MDL_CD"))){
		ds_eqpColorList.filter("EQP_MDL_CD=='"+ds_org.getColumn(0,"EQP_MDL_CD")+"'");
		var vColorCd = "CM_CD_ID == ''";
		for(var i = 0; i < ds_eqpColorList.rowcount; i++){
			vColorCd = vColorCd + " || ";
			vColorCd = vColorCd + "CM_CD_ID == '" + ds_eqpColorList.getColumn(i,"COLOR_CD") + "'";
		}
		ds_DMS100.filter(vColorCd);
	}
}]]></Script>
