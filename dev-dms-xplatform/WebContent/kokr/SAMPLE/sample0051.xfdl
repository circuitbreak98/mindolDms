function fn_setInitForm(obj:Form, e:LoadEventInfo)
{
	//자동조회시 파라메타 전달예제
	if ( !gfn_isNull(argParam01) ) div_search.edt_jumpo.value = argParam01;
	if ( !gfn_isNull(argParam02) ) div_search.edt_jumpoName.value = argParam02;
	
	if ( argAutoSelect ) {
		btn_search.click();
	} else {
		div_search.edt_jumpo.setFocus();
	}
}

