/*----------------------------------------------------------------------------------------------
 * 설명   :  전체선택 체크박스 이벤트 처리(정산포함인것만 체크)
-----------------------------------------------------------------------------------------------*/
function checkAll(obj:Grid, e:GridClickEventInfo)
{ 
	var checkColNm = obj._checkColumnName
	var cellIdx = obj.getBindCellIndex("body", checkColNm);
	
	if( e.col != cellIdx ){
		return false;
	}
	
	if( gfn_isNull( isCheck ) ) isCheck = "1";
		
	var objDS = eval(obj.binddataset);
	var isCheck = obj.getCellProperty("head", cellIdx, "text");
	if (gfn_isNull(isCheck)) isCheck = "0";
	
	if (isCheck == "0") {	
		isCheck = "1";
	} else {
		isCheck = "0";
	}

	obj.setCellProperty("head", cellIdx, "text", isCheck);		
	
	for(var i=0 ; i< objDS.rowcount ; i++){
		if( objDS.getColumn(i,"XCL_DEDC_YN") == "Y" ){
			continue;			
		}
		objDS.setColumn( i, checkColNm, isCheck);
	}
}
/*----------------------------------------------------------------------------------------------
 * 설명   :  정산제외일 경우 checkbox를 disalbed 처리
-----------------------------------------------------------------------------------------------*/
function grd_main_onformatchanged(obj:Grid, e:GridFormatChangedEventInfo)
{	
	var checkColNm = obj._checkColumnName;
	var cellIdx = obj.getBindCellIndex("body", checkColNm);
	var oCell = obj.body.getCell(cellIdx);
	oCell.edittype  = "expr:XCL_DEDC_YN=='Y'?'none':'checkbox'";
