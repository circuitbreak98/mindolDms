			}else if(gv_RunMode == "Dev"){
				loc = "http://hpwapv.dskcc.net/rfcconnect/Default.aspx?";
			}else if(gv_RunMode == "Staging"){
				loc = "http://hpwapv.dskcc.net/rfcconnect/Default.aspx?";				
			}else{
				loc = "http://hpwapv.skcc.com/rfcconnect/default.aspx?"; 
	//			doc_type = "FI01";
			}
			
			var emp_id = gds_user.getColumn(0,"EMP_NO"); //gfn_getUserInfo("LGIN_ID");
			var doc_id = ds_list.getColumn(ds_list.rowposition,"SLIP_NO");
