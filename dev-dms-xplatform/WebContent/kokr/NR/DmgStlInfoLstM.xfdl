							gfn_message("A","DMS00087","E","ERROR" ,"전표 진행중인"); //{해당} 건은 처리가 불가능 합니다.
							return false;
						}
						
				} else {	
					gfn_message("A","DMS00087","E","ERROR" ,"해당"); //{해당} 건은 처리가 불가능 합니다.
					return false;
				}
			}
		}
		if(gfn_message("A",msg,"I","",""))
		{
			fn_saveSlip();
		}
}

