-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	fvCellText = "";
	//page_size = Math.floor(grd_main.position.height / 24) - 1;
	
	ds_search.setColumn(0,"SVC_NO",svcNo);
