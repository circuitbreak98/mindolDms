//전체액셀다운로드
function fn_searchMainAll()
{
	var sSvcID		 = "searchMainAll";
	var sTrID		 = "PNRCnslMngt_pInqRentalCnslLst";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
