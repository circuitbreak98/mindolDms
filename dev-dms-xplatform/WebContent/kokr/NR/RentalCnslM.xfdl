/*----------------------------------------------------------------------------------------------
 * 설명   :  복호화 처리
-----------------------------------------------------------------------------------------------*/
function fn_setDecodeInfo(bSecureYn)
{  
	//개인정보조회 콜백함수에서 호출한 경우 
	if(!gfn_isNull(bSecureYn) && bSecureYn){
		var sTel = ds_prnInfoInq.lookup("PRN_INFO_INQ_ITEM_CD", DMS007_TEL,"PRN_INFO_INQ_CTT");    
		var sName =  ds_prnInfoInq.lookup("PRN_INFO_INQ_ITEM_CD", DMS007_NAME,"PRN_INFO_INQ_CTT");
		//var sAccount = ds_prnInfoInq.lookup("PRN_INFO_INQ_ITEM_CD", DMS007_ACCOUNT,"PRN_INFO_INQ_CTT");
		ds_mainAll.setColumn(g_rNb, "SVC_NO", "");
		ds_mainAll.setColumn(g_rNb, "NEW_CNTRTR_NM", "");
		//ds_mainAll.setColumn(g_rNb, "OK_RWD_ACCO", "");
		if(!gfn_isEmpty(sTel)) ds_mainAll.setColumn(g_rNb, "SVC_NO", sTel);
		if(!gfn_isEmpty(sName)) ds_mainAll.setColumn(g_rNb, "NEW_CNTRTR_NM", sName);
		//if(!gfn_isEmpty(sAccount)) ds_mainAll.setColumn(g_rNb, "OK_RWD_ACCO",sAccount);
		//trace(g_rNb+"/"+sTel+"/"+sName+"/"+sAccount);
	}
	g_rNb++;
	if(ds_mainAll.rowcount == g_rNb) {
		gfn_baseExcelExpert(grd_mainAll,"렌탈상담목록","렌탈상담목록");
	} else {
		fn_dept(g_rNb);
	}
}

