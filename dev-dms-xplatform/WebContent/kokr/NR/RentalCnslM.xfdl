//개인정보 조회
function btn_psnInfoPopup_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_main.rowcount <= 0 || ds_main.rowposition < 0){
		return;
	}
	
	g_row = ds_main.rowposition;
	ds_prnInfoInq.clearData();

	//고객명
	if(!gfn_isNull(ds_main.getColumn(g_row,"NEW_CNTRTR_NM_ENPT"))) {
		nRow = ds_prnInfoInq.addRow();
		ds_prnInfoInq.setColumn(nRow ,"PRN_INFO_INQ_ITEM_CD",DMS007_NAME);
		ds_prnInfoInq.setColumn(nRow ,"PRN_INFO_INQ_CTT", ds_main.getColumn(g_row,"NEW_CNTRTR_NM_ENPT"));
	}

	//서비스번호
	if(!gfn_isNull(ds_main.getColumn(g_row,"SVC_NO_ENPT"))) {
		nRow = ds_prnInfoInq.addRow();
		ds_prnInfoInq.setColumn(nRow ,"PRN_INFO_INQ_ITEM_CD",DMS007_HPNO);
		ds_prnInfoInq.setColumn(nRow ,"PRN_INFO_INQ_CTT", ds_main.getColumn(g_row,"SVC_NO_ENPT"));
	}


	if(ds_prnInfoInq.getRowCount()>0){
	
		ds_searchSecureInfo.clearData();
		ds_searchSecureInfo.insertRow(0);
		ds_searchSecureInfo.setColumn(0,'FRM_ID',objID);
		ds_searchSecureInfo.setColumn(0,'INQ_OBJ_NO',ds_main.getColumn(g_row,"CNSL_NO"));  //조회대상번호 
		ds_searchSecureInfo.setColumn(0,'REG_HST_YN','Y'); //이력여부
		
		gfn_searchSecureInfo("searchSecureInfoNEW", "fn_callBack", "ds_prnInfoInq", "ds_searchSecureInfo" ); //개인정보 조회 
	}
}

