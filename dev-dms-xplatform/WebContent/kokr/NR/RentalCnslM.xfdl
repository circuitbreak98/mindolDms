		ds_searchSecureInfo.setColumn(0,'REG_HST_YN','Y'); //이력여부
		
		gfn_searchSecureInfo("searchSecureInfo", "fn_callBack", "ds_prnInfoInq", "ds_searchSecureInfo"); //개인정보 조회 
	} else {
		fn_setDecodeInfo(false);
