function div_search_btn_search02_onclick(obj:Button,  e:ClickEventInfo)
{
	var sParam01 = div_search.edt_agnNm.value;
	var sParam02 = "";
	var sParam03 = "";
	var sParam04 = "";
	var sParam06 = div_search.edt_agnCd.value;
	if ( e == null ) {
		sParam04 = "key";
	} else {
		sParam04 = "button";
	}

	var arrArgs = {argAutoSelect:true, argCallProgram:"FNR071M", argParam01:sParam01, argParam02:sParam02, argParam03:sParam03, argParam04:sParam04, argParam06:sParam06};
	var sRtnVal = gfn_dialog("FBI004P", arrArgs);

	//팝업에서 입력받은 값 리턴
	if ( gfn_isNull(sRtnVal) || sRtnVal == "cancel" ) {
		//취소시 값 제거
		div_search.edt_agnCd.value = "";
		div_search.edt_agnNm.value = "";
	} else {	//Object이용시
		div_search.edt_agnCd.value  = sRtnVal["supAgnOrgCd"];
		div_search.edt_agnNm.value  = sRtnVal["supDealCoNm"];		
	}
}

