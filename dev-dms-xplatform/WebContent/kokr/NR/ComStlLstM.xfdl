/************************************************************************************************
 * Function : 서브타이틀(Static)에 Dataset row count표시
 * @param   : objStc - Static Object
 * @param   : objDs - count용 Dataset Object
 * Return   : objStc 에 objDs Row Count표시
************************************************************************************************/
function fn_setGridTitle(objStc:Static, msg)
{
	if ( gfn_isNull(objStc.userText) ) objStc.userText = objStc.text;
	objStc.usedecorate = true; 
	objStc.text = objStc.userText 
	         + msg;
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 재집계 button click
-----------------------------------------------------------------------------------------------*/
function btn_slipOnline_onclick(obj:Button,  e:ClickEventInfo)
{
	var yn =  div_search.cal_deprStrdYm.getDate();
	
	var dt = ds_search.getColumn(0,"DEPR_STRD_YM");
	var yy = dt.substring(0,4);
	var mm = dt.substring(4,6);
	var st = "해당월("+yy+"년 "+mm+"월) 단말보험료수수료를 재집계합니다. 계속하시겠습니까?"
	
		if(gfn_message("A",st,"I","","")){
			ds_search.setColumn(0,"DEPR_STRD_YM",yn);
			fn_onlineCallNEW();	
		}
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 전표발행 
-----------------------------------------------------------------------------------------------*/
function btn_slipCreate_onclick(obj:Button,  e:ClickEventInfo)
{
	var dt = ds_search.getColumn(0,"DEPR_STRD_YM");
	var yy = dt.substring(0,4);
	var mm = dt.substring(4,6);
	var st = "해당월("+yy+"년 "+mm+"월) 전표를 발행합니다. 계속하시겠습니까?"
	
	if( gfn_isNull(ds_main.getColumn(0,"SLIP_ST_CD")) || ds_main.getColumn(0,"SLIP_ST_CD") == '00' || ds_main.getColumn(0,"SLIP_ST_CD") == '05'){
		if(gfn_message("A",st,"I","","")){
			ds_search.setColumn(0,"IS_RECALL","01");
			fn_onlineCall();
		}
	}else{
		gfn_message("A","DMS00071","E"); //이미 처리된 건이 있습니다.
	}
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 전표취소
-----------------------------------------------------------------------------------------------*/
function btn_slipCancle_onclick(obj:Button,  e:ClickEventInfo)
{
	var dt = ds_search.getColumn(0,"DEPR_STRD_YM");
	var yy = dt.substring(0,4);
	var mm = dt.substring(4,6);
	var st = "해당월("+yy+"년 "+mm+"월) 전표를 삭제합니다. 계속하시겠습니까?"
	
	if(ds_main.getColumn(0,"SLIP_ST_CD") == '10'){
		if(gfn_message("A",st,"I","","")){
			ds_search.setColumn(0,"IS_RECALL","02");
			ds_search.setColumn(0,"YYYY",ds_main.getColumn(0,"SLIP_DT").substring(0,4));
			ds_search.setColumn(0,"SLIP_NO",ds_main.getColumn(0,"SLIP_NO"));
			
			fn_onlineCall();
		}
	}else{
			gfn_message("A","DMS00087","E","ERROR","전표미발행"); //{0} 건은 처리가 불가능 합니다.	
	}		
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 전체엑셀다운로드
-----------------------------------------------------------------------------------------------*/
function btn_excelAll_onclick(obj:Button,  e:ClickEventInfo)
{
	// 입력 항목 Validate Check
	if (!gfn_getValidateCompList(div_search)) {	
		return false;
	}
	gfn_dsTrim(ds_search);
	ds_mainAll.clearData();
	curPage = page;
	page = 1;
	page_size = 10000;
	
	fn_searchMainAll();
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 페이지 이동하면서 추가하는 로직
-----------------------------------------------------------------------------------------------*/
function fn_nextLoop()
{
	var yymm = ds_search.getColumn(0,"DEPR_STRD_YM");
	var yy = yymm.substring(0,4);
	var mm = yymm.substring(4,6);
	if ( total_record_cnt <=  page * page_size ) {
		page_size = 50;
		page = curPage;
		gfn_baseExcelExpert(grd_mainAll,yy+"년도 "+mm+"월 "+"수수료정산목록","수수료정산목록");
	} else {
		//다음페이지 조회
		page++;
		fn_searchMainAll();
	} 
}
