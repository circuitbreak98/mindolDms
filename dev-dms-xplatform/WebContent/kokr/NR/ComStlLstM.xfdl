/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "searchMain" : 
				//setPageNo(iPageSize, iTotalCount, iCurrPage, sFunction)
 			cgp_main.setPageNo(page_size, total_record_cnt, page);	//PageNo
 			cgp_mainCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
 			gfn_setGridTitleCountPaging(stc_subTitle,total_record_cnt);

			var subMsg = "  총 "+"<fc v='#31a3dc'>"+ds_searchSum.getColumn(0,"CNT")
						+"</fc><b v='false'><fc v='#77787a'>건, 합계금액 </fc></b>"
						+"<fc v='#31a3dc'>"+gfn_toFixedLocaleString(ds_searchSum.getColumn(0,"AMT"), '@@@,@@@,@@@')
						+"</fc><b v='false'><fc v='#77787a'>원 </fc></b>";
			
						
			fn_setGridTitle(stc_subTitle,subMsg);
						
			break;
		
		case "fn_onlineCall" : 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//MSGI0003 :정상 처리되었습니다.
				fn_searchMain();
			}
			
		
			break;
			
		case "fn_onlineCallNEW" : 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//MSGI0003 :정상 처리되었습니다.
				//fn_onlineCallNEWnew();
				fn_searchMain();
			}
			
		
			break;
			
		case "fn_onlineCallNEWnew" : 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//MSGI0003 :정상 처리되었습니다.
				fn_searchMain();
			}
			
			break;
			
		case "searchMainAll" : 
			ds_mainAll.appendData(ds_mainTemp,true);
			fn_nextLoop();
			break;
		
		default :
			break;
	}
}

