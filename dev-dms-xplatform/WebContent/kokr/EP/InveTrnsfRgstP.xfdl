================================================================================================*/

/*----------------------------------------------------------------------------------------------
 * 설명   : 메세지 추가
-----------------------------------------------------------------------------------------------*/
function fn_msgAdd(orgMsg,strAddMsg)
{
	if(strAddMsg == null || strAddMsg == ""){
		return orgMsg;
	}
	if(orgMsg != null && orgMsg != ""){
		orgMsg += ",";
	}
	orgMsg += strAddMsg;
	return orgMsg;
}
