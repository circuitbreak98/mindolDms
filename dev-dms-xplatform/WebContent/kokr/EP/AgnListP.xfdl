/*===============================================================================================
 * [ PART 1 ] Form에서 Include 할 라이브러리(xjs)파일 정의
================================================================================================*/
include "LIB::common.xjs"; //공통함수 호출

/*===============================================================================================
 * [ PART 2 ] Form에서 사용될 전역변수를 선언
================================================================================================*/
var inputType;
/*===============================================================================================
 * [ PART 3 ] Form Event 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Form Load Event
-----------------------------------------------------------------------------------------------*/
function Form_onload(obj:Form, e:LoadEventInfo)
{
	//공통 Form 초기화
	gfn_setInitPop(obj, e, "FBI008P");
	
 	//Form 초기 개발자 셋팅
 	//공통코드 초기화
 	fn_setInitCode(obj, e);
 	//그리드 초기화
 	fn_setInitGrid(obj, e);
 	//기본값 초기화
 	fn_setInitForm(obj, e);
 	
 	if( ds_eqp.getRowCount() == 1){
			fn_selectValue(0);
	}
}

/*===============================================================================================
 * [ PART 4 ] Load 후 초기설정
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitCode(obj:Form, e:LoadEventInfo)
{

}

/*----------------------------------------------------------------------------------------------
 * 설명   : 그리드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitGrid(obj:Form, e:LoadEventInfo)
{

}

/*----------------------------------------------------------------------------------------------
 * 설명   : 기본값 설정
-----------------------------------------------------------------------------------------------*/
function fn_setInitForm(obj:Form, e:LoadEventInfo)
{
	div_search.edt_mdlNm.setFocus();
	//alert(argParam01+"/"+argParam02);
	//자동조회시 파라메타 전달예제
	if ( !gfn_isNull(argParam01) ) {
		div_search.edt_mdlCd.value = argParam01;
	}
	if ( !gfn_isNull(argParam02) ) {
	 div_search.edt_mdlNm.value = argParam02;
	}
	//신규R 단말기모델 여부(Y)
	try {
		if ( !gfn_isNull(argParam04) ) {
			ds_ncFieldMap.setColumn(0,'NR_EQP_MDL_YN',argParam04);
		}
	}
	catch(e) {

	}
	
	try {
		if ( !gfn_isNull(argParam03) ) {
			inputType = argParam03;
		} else {
			inputType = "button";
		}
	}
	catch(e) {

	}
	
	btn_search.click();
}

/*===============================================================================================
 * [ PART 5 ] Transaction 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	var sSvcID		 = "searchMain";
	var sTrID		 = "PBIEqpMdlMgmt_pInqEqpMdlLst";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_ncFieldMap";
	var sOutputDs	 = "ds_eqpTemp=RS_EQP_MDL_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{

	switch(sSvcID) {
		case "searchMain" : 
			if ( iErrorCode == 0 ) {
				//페이징 관련셋팅
				ds_eqp.appendData(ds_eqpTemp,true);	
				//조회된 첫번째row로 setFocus 
				ds_eqp.rowposition = (page-1) * page_size ;

				gfn_setGridTitleCountPaging(stc_subTitle01, total_record_cnt);
				div_search.edt_mdlNm.setFocus();
				//Data 1건일 경우 close
				if( ds_eqp.getRowCount() == 1 && inputType == "key"){
					fn_selectValue(0);
				}
			}
			break;	
		
		default :
			break;
	}
}

/*===============================================================================================
 * [ PART 7 ] Main Button Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 버튼 Event
-----------------------------------------------------------------------------------------------*/
function button_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj.name) 
	{	
		case "btn_search" :		//조회
		    // 입력 항목 Validate Check
			if (!gfn_getValidateCompList(div_search)) {	
				return false;
			}
			ds_eqp.clearData();
			page = 1;
			fn_searchMain();
			break;
			
		case "btn_confirm" :	//확인
			fn_selectValue(ds_eqp.rowposition);
			break;
			
		case "btn_cancel" : 	//취소
			close("cancel");
			break;
			
		case "btn_close" : 		//버튼X
			close("cancel");
			break;	
	}
}

/*===============================================================================================
 * [ PART 8 ] Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Grid Double Click Event
-----------------------------------------------------------------------------------------------*/
function grd_eqplist_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	fn_selectValue(e.row);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 조회조건에서 엔터키가 눌러졌을때 조회
-----------------------------------------------------------------------------------------------*/
function div_Search_edt_mdlCd_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == KEYCODE_ENTER) {
		btn_search.setFocus();
		ds_eqp.clearData();
		page = 1;
		fn_searchMain();
	}	
}

/*===============================================================================================
 * [ PART 9 ] User Defined
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 결과값 리턴후 Close
-----------------------------------------------------------------------------------------------*/
function fn_selectValue(nRow)
{
	//단건(Object사용)
	if ( nRow == -1 ) return;
	
	var objRtn = new Object;
		objRtn["supEqpMdlCd"] 	= ds_eqp.getColumn(nRow,"EQP_MDL_CD");	//단말기모델코드
		objRtn["supEqpMdlNm"] 	= ds_eqp.getColumn(nRow,"EQP_MDL_NM");	//단말기모델명
		objRtn["supMfactCd"]   	= ds_eqp.getColumn(nRow,"MFACT_CD");	//제조사코드
		objRtn["supFstRgstDt"] 	= ds_eqp.getColumn(nRow,"FST_RGST_DT");	//출시일
		objRtn["supCnsgDt"] 	= ds_eqp.getColumn(nRow,"CNSG_DT");		//단종일

	this.close(objRtn);		
	
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 수직 스크롤바가 마지막에 도달했을때 페이지 조회 이벤트
 * param   : obj - Grid Object
 * param   : e  - ScrollEventInfo
 * return  : void
-----------------------------------------------------------------------------------------------*/
function grd_eqplist_onvtracklast(obj:Grid, e:ScrollEventInfo)
{
	
	if ( total_record_cnt <=  page * page_size ) {
	//	gfn_message("A","DMS00028","I"); //조회 마지막 페이지 입니다.   
		return;
	} else {
		//다음페이지 조회
		page++;
		fn_searchMain();
	} 
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 제조사 검색
-----------------------------------------------------------------------------------------------*/
function div_search_btn_cmCdSearch_onclick(obj:Button,  e:ClickEventInfo)
{
	var sParam01 = "DMS101";
	var sParam02 = "";
	var sParam03 = "";
	var sParam04 = "Y";  //선택버튼 활성화여부
	var sParam05 = div_search.edt_mfactCd.value;	//공통코드
	var sParam06 = div_search.edt_mfactNm.value;	//공통코드명
	if ( e == null ) {
		sParam03 = "key";
	} else {
		sParam03 = "button";
	}
	
	var arrArgs = {argAutoSelect:true, argCallProgram:"FBI001M",argParam01:sParam01, argParam02:sParam02, argParam03:sParam03
					, argParam04:sParam04, argParam05:sParam05, argParam06:sParam06};
	var sRtnVal = gfn_dialog("FSC007P", arrArgs);

	//팝업에서 입력받은 값 리턴
	if ( gfn_isNull(sRtnVal) || sRtnVal == "cancel" ) {
		//취소시 값 제거
		div_search.edt_mfactCd.value = "";
		div_search.edt_mfactNm.value = "";
	} else {	//Object이용시
		div_search.edt_mfactCd.value  = sRtnVal["supCmCd"];
		div_search.edt_mfactNm.value = sRtnVal["supCmCdNm"];
		//btn_search_onclick();
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 제조사 검색 리셋
-----------------------------------------------------------------------------------------------*/
function div_search_btn_reset_onclick(obj:Button,  e:ClickEventInfo)
{
	div_search.edt_mfactCd.value  = "";
	div_search.edt_mfactNm.value = "";
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 제조사 ENTER키 검색
-----------------------------------------------------------------------------------------------*/
function div_search_edt_mfactCd_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == KEYCODE_ENTER) {
		div_search_btn_cmCdSearch_onclick();
	}
}
]]></Script>
