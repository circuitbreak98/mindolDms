/*----------------------------------------------------------------------------------------------
 * 설명   : 회수율의 경우 평균 소계
-----------------------------------------------------------------------------------------------*/
function fn_subAvg(nRow, nCol)
{
	var rtnVal = 0;
	var dsorg = eval(PivotGrid01.bindds);
	var dspivot = eval(PivotGrid01.binddataset);

	var sSum = "(DATA_GB=='" + dspivot.getColumn(nRow-1,"DATA_GB") + "')";
		sSum += " && (CNSL_YM=='" + PivotGrid01.getCellProperty("head",nCol,"text") + "')";
					
	if (dspivot.getColumn(nRow-1,"DATA_GB_SORT") == '41') {
		//rtnVal = dsorg.getCaseAvg(sSum,"CNSL_CNT") == Infinity ? "" : dsorg.getCaseAvg(sSum,"CNSL_CNT");	
		sSum = "(DATA_GB_SORT=='32')";
		sSum += " && (CNSL_YM=='" + PivotGrid01.getCellProperty("head",nCol,"text") + "')";
		var clctSum = dsorg.getCaseSum(sSum,"CNSL_CNT");	
		
		sSum = "(DATA_GB_SORT=='3')";
		sSum += " && (CNSL_YM=='" + PivotGrid01.getCellProperty("head",nCol,"text") + "')";
		var cnslSum = dsorg.getCaseSum(sSum,"CNSL_CNT");	
		
		rtnVal = (clctSum/cnslSum )*100;
	} else {
		rtnVal = dsorg.getCaseSum(sSum,"CNSL_CNT");	
	}
	
	if (rtnVal > 0) {
		return Math.round(rtnVal,1);	
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명   :  월평균 소계
-----------------------------------------------------------------------------------------------*/
function fn_sumAvg(nRow, data_section_sum)
{
	var rtnVal = 0;
	
	//평균구할 월수
    var mth_cnt = ds_search.getColumn(0,'MTH_CNT');
	rtnVal =  data_section_sum/mth_cnt;
	if (rtnVal > 0) {
		return Math.round(rtnVal,1);	
	}
	
	
