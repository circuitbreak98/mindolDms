-----------------------------------------------------------------------------------------------*/
function fn_dsClear(){
	ds_main.clearData();
	ds_search.clearData();
 	ds_search.addRow();
 	stc_subTitle.text = "조회결과";
	fn_setInitGrid();
	fn_setInitForm();
}
