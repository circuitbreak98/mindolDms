}
/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
 	switch(sSvcID) {
		case "searchMain": 
			if ( iErrorCode == 0 ) {
				gfn_message("B","DMS00001");	   //정상 조회되었습니다.																				
			}
		break;			
		case "saveMain" : 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//정상 처리되었습니다.
			} else {
				gfn_message("A",ncMsg);
			}
			fn_searchMain();
			break;
		case "cnlMain" : 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//정상 처리되었습니다.
			} else {
				gfn_message("A",ncMsg);
			}
			fn_searchMain();
			break;
		default :
			break;
	}
}
