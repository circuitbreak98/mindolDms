================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	var sSvcID		 = "searchMain";
	var sTrID		 = "PSCUserMgmt_pInqUserLst";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_ncFieldMap";
	var sOutputDs	 = "ds_userLst_temp=RS_USER_LIST";
	//var sOutputDs	 = "ds_userLst=rsUserList";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	ds_userLst.clearData();
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인저장/수정
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_saveMain()
{

	var sSvcID		 = "saveMain";
	var sTrID		 = "PSCUserMgmt_pSaveUser";
	var sPtrn		 = ds_userLst.getRowType(ds_userLst.rowposition) == Dataset.ROWTYPE_INSERT? "I" : "U";  //입력C,조회R,수정U,삭제D,복합M
	//var sInputDs	 = "rsUserList=ds_userLst:U rsUserHis=ds_userLst_his:U";
	var sInputDs	 = "RS_USER_LIST=ds_userLst:U";
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
	
}

/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{

	switch(sSvcID) {
		
			case "searchMain" : 
			//페이징 관련셋팅
			ds_userLst.appendData(ds_userLst_temp,true);
			//조회된 첫번째row로 setFocus 
			//ds_userLst.rowposition = (page-1) * page_size ;
			ds_userLst.rowposition = 0;
			cgp_pageNo.setPageNo(page_size, total_record_cnt, page);	//PageNo
			cgp_pageNoCnt.setPageNoCnt(page_size, total_record_cnt, page);	//PageNo Count
			gfn_setGridTitleCountPaging(stc_subTitle01, total_record_cnt);
			divLevel02.edt_lgid.setFocus();
			
			break;
			
		case "saveMain" : 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	// MSGI0001 : 정상 처리되었습니다.
				if (pv_mode == "new") {				// 신규일 경우 해당 건 재조회
					btn_search_onclick();
				} 
				gfn_closePop("FSC010P");
				return true;
			}
			
			break;
			
		case "chkLoginId" : 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//MSGI0001 : 정상 처리되었습니다.
			} 
			break;
			
		default :
			break;
	}
}
