-----------------------------------------------------------------------------------------------*/
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	ds_iflog.clearData();
	ds_ncFieldMap.setColumn(0,"SLIP_TYP_CD",div_search.cbo_clCd00.value);
	page = 1;
	if(fn_chkBeforeSearch())
	{
		fn_searchMain();
	}
}

