{
	//공통 Form 초기화
	gfn_setInitForm(obj, e, "FRM0024M")

 	//Form 초기 개발자 셋팅
 	//공통코드 초기화
 	fn_setInitCode(obj, e);
 	//그리드 초기화
 	fn_setInitGrid(obj, e);
 	//기본값 초기화
 	fn_setInitForm(obj, e);
  	
}

