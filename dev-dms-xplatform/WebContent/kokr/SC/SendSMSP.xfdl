-----------------------------------------------------------------------------------------------*/
function Form_onload(obj:Form, e:LoadEventInfo)
{
	//공통 Form 초기화
	gfn_setInitPop(obj, e, "FSC037P");
	
 	//Form 초기 개발자 셋팅
 	//공통코드 초기화
 	fn_setInitCode(obj, e);
 	//그리드 초기화
 	fn_setInitGrid(grd_smsParam, e);
 	//기본값 초기화 - 화면 오픈시 수신전화번호 파라미터 수신번호목록 데이터셋에 추가 
 	fn_setInitForm(obj, e);  
}

