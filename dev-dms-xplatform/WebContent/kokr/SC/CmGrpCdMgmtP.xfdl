-----------------------------------------------------------------------------------------------*/
function DivBtn_btn_confirm_onclick(obj:Button,  e:ClickEventInfo)
{
	if ( fn_chkBeforeSave() ){  
		if ( fn_saveMain() )	{
			close(true);
		} else {
			div_search.edt_cmGrpCd.class = 'Error';
			div_search.edt_cmGrpCd.setFocus();
		}
	}
}

