/*----------------------------------------------------------------------------------------------
 * 설명   : 공통그룹코드 메인저장 (insert, update)
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_saveMain()
{
	var sSvcID		 = "saveMain";
	var sTrID		 = "PSCCmCdMgmt_pSaveCmGrpCd";
	var sPtrn		 = mode == 'new' ? "I" : "U";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "RS_CM_GRP_CD_LIST=ds_cmgrpcd_pop" ;
	var sOutputDs	 = "";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : Transaction CallBack
 * param   : sSvcID, iErrorCode, sErrorMsg(ncMsgId,ncMsg)
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	switch(sSvcID) {
		case "saveMain" : 
			if ( iErrorCode == 0 ) {
				gfn_message("A","DMS00000","I");	//MSGI0001 : 정상 처리되었습니다.
				if(mode=='new'){
					//opener.ds_cmgrpcd.insertRow(0);
					opener.ds_ncFieldMap.setColumn(0, "CM_GRP_CD_ID", ds_cmgrpcd_pop.getColumn(0, "CM_GRP_CD_ID"));
					opener.ds_ncFieldMap.setColumn(0, "CM_GRP_CD_NM", ds_cmgrpcd_pop.getColumn(0, "CM_GRP_CD_NM"));
					if (ds_cmgrpcd_pop.getColumn(0, "USE_YN") == "1") {
						opener.ds_ncFieldMap.setColumn(0, "USE_YN", "Y");
					} else if (ds_cmgrpcd_pop.getColumn(0, "USE_YN") == "0") {
						opener.ds_ncFieldMap.setColumn(0, "USE_YN", "N");
					} else {
						opener.ds_ncFieldMap.setColumn(0, "USE_YN", "%");
					}
					if (ds_cmgrpcd_pop.getColumn(0, "MGMT_CD_YN") == "1") {
						opener.ds_ncFieldMap.setColumn(0, "MGMT_CD_YN", "Y");
					} else if (ds_cmgrpcd_pop.getColumn(0, "MGMT_CD_YN") == "0") {
						opener.ds_ncFieldMap.setColumn(0, "MGMT_CD_YN", "N");
					} else {
						opener.ds_ncFieldMap.setColumn(0, "MGMT_CD_YN", "%");
					}
					
				}
				opener.ds_ncFieldMapSingle.setColumn(0,"CM_GRP_CD_ID",ds_cmgrpcd_pop.getColumn(0,"CM_GRP_CD_ID"));
				close(true);
			} 
			break;
		default :
			break;
	}
}

