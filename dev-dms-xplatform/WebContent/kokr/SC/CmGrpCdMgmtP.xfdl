-----------------------------------------------------------------------------------------------*/
function fn_setInitForm(obj:Form, e:LoadEventInfo)
{
	//신규등록시, 관리여부 Yes로 픽스
	if(mode=='new'){
		div_search.rdo_mgmtYn.value = '1';
		div_search.rdo_mgmtYn.enable = false;
		div_search.edt_cmGrpCd.readonly = false;
		//div_search.edt_cmGrpCd.setFocus();
	}else{
		//부모창의 데이터 복사
		ds_cmgrpcd_pop.updatecontrol = false;
		ds_cmgrpcd_pop.copyRow(0, opener.ds_cmgrpcd,opener.ds_cmgrpcd.rowposition);	
		ds_cmgrpcd_pop.setRowType(0, Dataset.ROWTYPE_NORMAL);
		ds_cmgrpcd_pop.updatecontrol = true;
		div_search.edt_cmGrpCd.readonly = true; //수정일때 PK컬럼 disable
		//div_search.edt_cmGrpCdNm.setFocus();
	}

}

