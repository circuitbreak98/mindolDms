/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitCode(obj:Form, e:LoadEventInfo)
{
	var strCode =  "DMS005=ds_DMS005=S";
			
							
	gfn_getCodeDs( this
				  , strCode
				  ,"fn_afterCodeCallBack");				//결과 받을 function	
				  
}

