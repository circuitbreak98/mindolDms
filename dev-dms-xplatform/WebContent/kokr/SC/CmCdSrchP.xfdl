{

}

/*----------------------------------------------------------------------------------------------
 * 설명   : 기본값 설정
-----------------------------------------------------------------------------------------------*/
function fn_setInitForm(obj:Form, e:LoadEventInfo)
{
	//기본페이지 설정
	page = 1;   
	page_size = 30;
	
	//선택버튼 비활성화여부 
	if( !gfn_isNull(argParam04) && argParam04 =="Y"){
		Div00.btn_confirm.enable = true;
	} else {
		Div00.btn_confirm.enable = false;
	}
	
	//자동조회시 파라메타 전달
	if ( !gfn_isNull(argParam01) ){
		Div02.edt_cmGrpCd.value = argParam01;
		Div02.edt_cmGrpCd.readonly = true;
	}
	if ( !gfn_isNull(argParam02) ){
		Div02.edt_cmGrpCdNm.value = argParam02;
	}
	try {
		if ( !gfn_isNull(argParam05) ){
			Div02.edt_cmCd.value = argParam05;
		}
		if ( !gfn_isNull(argParam06) ){
			Div02.edt_cmCdNm.value = argParam06;
		}
	}
    catch(e) {
    }
	
	try {
		if ( !gfn_isNull(argParam03) ) {
			inputType = argParam03;
		} else {
			inputType = "button";
		}
    }
    catch(e) {
    }
      
	 if (!gfn_isNull(argParam01) || !gfn_isNull(argParam02)) {   
		btn_search.click();
	} else {
		Div02.edt_cmGrpCd.setFocus();
		
	}
}

/*===============================================================================================
 * [ PART 5 ] Transaction 처리
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 메인조회
 * param   : 
 * return  : void
-----------------------------------------------------------------------------------------------*/
function fn_searchMain()
{
	var sSvcID		 = "searchMain";
	var sTrID		 = "PSCCmCdMgmt_pSrchCmCd";
	var sPtrn		 = "R";  //입력C,조회R,수정U,삭제D,복합M
	var sInputDs	 = "ncFieldMap=ds_ncFieldMap";
	var sOutputDs	 = "ds_cmcdTemp=RS_SUP_CM_CD_LIST";
	var sArgs		 = "";
	var sCallBackFnc = "fn_callBack";
	 
	gfn_transaction(sSvcID, sTrID, sPtrn, sInputDs, sOutputDs, sArgs, sCallBackFnc);
}
/*===============================================================================================
 * [ PART 6 ] CallBack 처리
================================================================================================*/
function fn_callBack(sSvcID, iErrorCode, sErrorMsg, ncMsgId, ncMsg)
{
	if(sSvcID = "searchMain"){
			ds_cmcd.appendData(ds_cmcdTemp, true);	
			gfn_setGridTitleCountPaging(stc_subTitle01, total_record_cnt);
			//Data 1건일 경우 close
			if( ds_cmcd.getRowCount() == 1 && inputType == "key"){
				Div00.btn_confirm.click();
			}
			Div02.edt_cmGrpCd.setFocus();
	}
}

/*===============================================================================================
 * [ PART 7 ] Main Button Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 버튼 Event
-----------------------------------------------------------------------------------------------*/
function button_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj.name) 
	{	
		case "btn_search" :		//조회
		    // 입력 항목 Validate Check
			if (!gfn_getValidateCompList(Div02)) {	
				return false;
			}
			ds_cmcd.clearData();
			page = 1;
			fn_searchMain();	
			break;
			
		case "btn_confirm" :	//확인
			if(!gfn_isEmpty(ds_cmcd.rowposition)){
				fn_selectValue(ds_cmcd.rowposition);
			}else{
				gfn_message("A","DMS00021","I") //조회된 정보가 없습니다.
			}
			break;
			
		case "btn_cancel" : 	//취소
			close("cancel");
			break;
			
		case "btn_close" : 		//버튼X
			close("cancel");
			break;	
	}
}

/*===============================================================================================
 * [ PART 8 ] Event
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 조회조건에서 엔터키가 눌러졌을때 조회
-----------------------------------------------------------------------------------------------*/
function Div02_edt_cmGrpCd_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == KEYCODE_ENTER) {
		btn_search.setFocus();
		ds_cmcd.clearData();
		page = 1;
		fn_searchMain();
	}	
}
/*----------------------------------------------------------------------------------------------
 * 설명   : Grid Double Click Event
-----------------------------------------------------------------------------------------------*/
function UserGrid00_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	if (Div00.btn_confirm.enable == true ) fn_selectValue(e.row);
}

/*===============================================================================================
 * [ PART 9 ] User Defined
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 수직 스크롤바가 마지막에 도달했을때 페이지 조회 이벤트
 * param   : obj - Grid Object
 * param   : e  - ScrollEventInfo
 * return  : void
-----------------------------------------------------------------------------------------------*/
function Div00_grd_cmGrpCd_Convtracklast(obj:Grid, e:ScrollEventInfo)
{
	if ( total_record_cnt <=  page * page_size ) {
		gfn_message("A","DMS00028","I"); //조회 마지막 페이지 입니다.
		return;		
	} else {
		//다음페이지 조회
		page++;
		fn_searchMain();
	} 
}
/*----------------------------------------------------------------------------------------------
 * 설명   : 결과값 리턴후 Close
-----------------------------------------------------------------------------------------------*/
function fn_selectValue(nRow)
{
	//단건(Object사용)
	if ( nRow == -1 ) return;
	
	var objRtn = new Object;
		objRtn["supGrpCd"] = ds_cmcd.getColumn(nRow,"CM_GRP_CD_ID");
		objRtn["supGrpCdNm"] = ds_cmcd.getColumn(nRow,"CM_GRP_CD_NM");		
		objRtn["supCmCd"] = ds_cmcd.getColumn(nRow,"CM_CD_ID");
		objRtn["supCmCdNm"] = ds_cmcd.getColumn(nRow,"CM_CD_NM");		

	this.close(objRtn);		
}]]></Script>
