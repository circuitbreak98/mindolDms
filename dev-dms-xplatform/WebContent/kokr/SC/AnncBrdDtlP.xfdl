/*----------------------------------------------------------------------------------------------
 * 설명   : Form Load Event
-----------------------------------------------------------------------------------------------*/
function Form_onload(obj:Form, e:LoadEventInfo)
{
	//공통 Form 초기화
	gfn_setInitPop(obj, e, "FSC022P");
	//Form 초기 개발자 셋팅
 	//공통코드 초기화
 	fn_setInitCode(obj, e);
 	//그리드 초기화
 	fn_setInitGrid(obj, e);
 	//기본값 초기화
 	fn_setInitForm(obj, e);
}
/*===============================================================================================
 * [ PART 4 ] Load 후 초기설정
================================================================================================*/
/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitCode(obj:Form, e:LoadEventInfo)
{
	var strCode =  "DMS010=ds_DMS010=N";
						
	gfn_getCodeDs( this
				  , strCode
				  ,"fn_afterCodeCallBack");	
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 공통코드 호출 후 처리
-----------------------------------------------------------------------------------------------*/
function fn_afterCodeCallBack()
{
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 그리드 초기화
-----------------------------------------------------------------------------------------------*/
function fn_setInitGrid(obj:Form, e:LoadEventInfo)
{
	// 그리드 초기화 
	//gfn_initGrid(objGrid:UserGrid, bReadonly, bSortable, bTooltip, sSelectType, innerDs, sPage, sPageSize, sTotCnt);
	gfn_initGrid(grd_main, true, false, true, "row", null);
}

/*----------------------------------------------------------------------------------------------
 * 설명   : 기본값 설정
-----------------------------------------------------------------------------------------------*/
function fn_setInitForm(obj:Form, e:LoadEventInfo)
{	
	//웹에디터 초기화 BackGroundColor 세팅
	div_main.cwe_main.setInit("#F7F7F7");
	
	if(mode=='new'){
		//개인정보 조회권한 및 수정권한여부 파라미터 설정 (조회,저장,삭제,출력,엑셀,개인정보)
		//gfn_setAutrChild(this);
		
		div_main.ccp_anncTerm.setData(gfn_today(),gfn_addDate(gfn_today(),+30),true);
		div_main.cbo_anncType.index=0;
		ds_main.setColumn(0,"ANNCE_TYPE","20");
		ds_main.setColumn(0,"USE_YN","Y");
		ds_main.setColumn(0,"POPUP_YN","N");
	}else{
		//부모창의 데이터 복사
		ds_main.updatecontrol = false;
		ds_main.copyRow(0, opener.ds_main,opener.ds_main.rowposition);
		ds_main.setRowType(0, Dataset.ROWTYPE_NORMAL);
		ds_main.updatecontrol = true;
		
		
		
		//권한
		if(gds_user.getColumn(0,"USER_NO") != ds_main.getColumn(0,"FS_REG_USER_ID")){
			btn_addFile.enable = false;
			btn_deleteFile.enable = false;
			DivBtn.btn_save.enable = false;
		}
		
		//dms 홈 공지사항 조회인 경우 
		if(mode=='notice'){
			btn_addFile.visible = false;
			btn_deleteFile.visible = false;
			div_main.visible = false;
			
			DivBtn.btn_confirm.visible = true;			
			div_mainNotice.visible = true;
			div_mainNotice.cwe_main.setData(ds_main.getColumn(0,"CTT"));
			
		} else {
			div_mainNotice.visible = false;
			div_main.visible = true;
			DivBtn.btn_confirm.visible = false;
			
			div_main.ccp_anncTerm.setData(ds_main.getColumn(0,"BLTN_FR_DT"),ds_main.getColumn(0,"BLTN_TO_DT"),true);
			div_main.cwe_main.setData(ds_main.getColumn(0,"CTT"));
		}
		
		
		//조회수 증가
		fn_saveIncreBrwsCnt();
	}
	div_main.edt_titl.setFocus();
}

