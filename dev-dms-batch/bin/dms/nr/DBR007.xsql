	<select id="SREqpClctInfo" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.nr.DBR007.SREqpClctInfo R센터입고확인정보조회 */
/* IO: 장미진,2015-08-18T15:20:50(장미진,2015-08-18T14:42:57) */
SELECT
        A.EQP_SER_NO
       ,A.EQP_MDL_CD
       ,A.EQP_IN_DT 
       ,B.SVC_MGMT_NO
       ,B.RENTAL_SER_NO
  FROM  TB_EQP_CLCT_OBJ A
       ,TB_NR_CNTRT B
