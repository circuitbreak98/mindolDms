<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="dms.ep.DBE004">
	<select id="SIfConsultDtl" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.pr.DBP012.SIfConsultDtl IF상담관리상세정보조회(현장) */
/* IO: 이준우,2015-10-02T10:33:54(김윤환,2015-08-11T14:15:42) */
  SELECT  IF_PROC_DT
         ,IF_FILE_NM
         ,IF_SEQ
         ,CNSL_MGMT_NO
         ,CNSL_MGMT_SEQ               /*상담관리순번 CNSL_SER_NO*/
         ,DTL_GUBUN                    /*상세구분 DTL_CL*/
         ,GUBUN_CD                     /*구분코드 CL_CD*/
         ,EQP_MDL_CD
         ,CMPT_PROD_CD
         ,QTY
         ,NORM_QTY
         ,HLD_QTY
         ,DDCT_AMT
         ,DEL_YN
         ,UPDATE_CNT 
         ,UKEYREG_DTM
         ,UKEYREG_USER_ID
         ,UKEYCHG_DTM
         ,UKEYCHG_USER_ID
    FROM  TF_CNRL_MGMT_SCN_DTL
   WHERE  IF_PROC_DT = #PROC_DT#		/*전문송신일자*/
     AND  PROC_ST_CD = '09'
ORDER BY  IF_SEQ]]>
	</select>
	<insert id="IConsultDtl" parameterClass="map" ><![CDATA[/* Biz: dms.pr.DBP012.IConsultDtl 상담관리상세등록 */
/* IO: 이준우,2015-10-02T09:28:09(이준우,2015-07-29T10:19:57) */
INSERT INTO  TB_EP_CST_CONSULT_DTL (
             CNSL_MGMT_NO
            ,CNSL_SER_NO
            ,DTL_CL
            ,CL_CD
            ,EQP_MDL_CD
            ,CMPT_PROD_CD
            ,QTY
            ,NORM_QTY
            ,HLD_QTY
            ,DDCT_AMT
            ,DEL_YN
            ,UPDATE_COUNT 
            ,FS_REG_DTM
            ,FS_REG_USER_ID
            ,LS_CHG_DTM
            ,LS_CHG_USER_ID
            ,SKN_SMPL_QTY
             )
     VALUES  (
             #CNSL_MGMT_NO#
            ,#CNSL_SER_NO#
            ,TO_CHAR(TO_NUMBER(#DTL_CL#))
            ,#CL_CD#
            ,#EQP_MDL_CD#
            ,#CMPT_PROD_CD#
            ,#QTY#
            ,#NORM_QTY#
            ,#HLD_QTY#
            ,#DDCT_AMT#
            ,#DEL_YN#
            ,NVL(#UPDATE_COUNT#,0)
            ,SYSDATE
            ,'BAT'
            ,SYSDATE
            ,'BAT'
            ,#HLD_QTY#
             )]]>
	</insert>
	<select id="SConsultDtlCnt" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.pr.DBP012.SConsultDtlCnt 상담관리상세카운트 */
/* IO: 이준우,2015-10-02T09:40:54(이준우,2015-09-30T13:09:13) */
  SELECT  COUNT(*) AS CNT
    FROM  TB_EP_CST_CONSULT_DTL
   WHERE  CNSL_MGMT_NO = #CNSL_MGMT_NO#
     AND  CNSL_SER_NO  = #CNSL_SER_NO#]]>
	</select>
	<select id="SPrchsMgmtCnt" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.pr.DBP011.SPrchsMgmtCnt 매입관리카운트 */
/* IO: 이준우,2015-09-30T13:17:19(이준우,2015-09-30T13:15:09) */
  SELECT  COUNT(*) AS CNT
    FROM  TB_EP_CST_PRCHS_MGMT
   WHERE  CNSL_MGMT_NO = #CNSL_MGMT_NO#
     AND  DEL_YN = 'N']]>
	</select>
	<update id="UIfConsultDtl" parameterClass="map" ><![CDATA[/* Biz: dms.pr.DBP012.UIfConsultDtl IF상담관리상세수정(현장) */
/* IO: 이준우,2015-10-02T09:51:57(이준우,2015-09-30T13:24:28) */
UPDATE  TF_CNRL_MGMT_SCN_DTL
   SET  PROC_ST_CD = '01'
       ,UKEYCHG_DTM     = TO_CHAR(SYSDATE, 'YYYYMMDDHH24MISS')
 WHERE  IF_PROC_DT = #SPECI_PROC_DT#
   AND  IF_FILE_NM = #SPECI_PROC_TIME#
   AND  IF_SEQ   = #SPECI_SER_NO#]]>
	</update>
	<select id="SConsultMgmtCnt" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.pr.DBP012.SConsultMgmtCnt 상담관리카운트 */
/* IO: 이준우,2015-09-30T19:24:38(이준우,2015-09-30T13:26:32) */
SELECT  COUNT(*) AS CNT
  FROM  TB_EP_CST_CONSULT_MGMT
 WHERE  CNSL_MGMT_NO = #CNSL_MGMT_NO#
   AND  del_yn      = 'N']]>
	</select>
	<delete id="DConsultDtl" parameterClass="map" ><![CDATA[/* Biz: dms.pr.DBP012.DConsultDtl 상담관리상세삭제 */
/* IO: 이준우,2015-09-30T19:26:09(이준우,2015-09-30T19:26:09) */
DELETE  FROM TB_EP_CST_CONSULT_DTL
 WHERE  CNSL_MGMT_NO = #CNSL_MGMT_NO#]]>
	</delete>
</sqlMap>