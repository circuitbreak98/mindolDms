  FROM  TB_XCL_BOND
 WHERE  OP_CL_CD = #OP_CL_CD# 
   AND  BOND_XCL_YM = #DEBT_XCL_YM#
   AND  BOND_BIZ_REG_NO = #DEBT_BIZ_REG_NO#
   AND  BOND_DEALCO_CD = #XCL_DEALCO_CD#
   AND  XCL_CL = #XCL_CL#
   AND  XCL_ITM_CL = #XCL_ITM_CL#
   AND  BOND_SALE_TS = #DEBT_PRCH_TS#]]>
	</select>
