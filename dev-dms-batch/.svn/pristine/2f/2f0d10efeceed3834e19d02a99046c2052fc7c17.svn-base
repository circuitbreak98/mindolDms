]]></selectKey><![CDATA[
INSERT  INTO TB_PRCH
(
  PRCH_NO
 ,OP_CL_CD
 ,EQP_MDL_CD
 ,EQP_SER_NUM
 ,PRCH_DT
 ,PRCH_AMT
 ,PRCH_ASSET_CL
 ,PRCH_ST_CD
 ,REVS_DEPT_CD
 ,PRCHCO_CD
 ,PRCHCO_CHRGR
 ,PRCHCO_TEL_NO
 ,SPLY_PRC
 ,SURTAX_AMT
 ,SURTAX_CL
 ,FS_REG_USER_ID
 ,FS_REG_DTM
 ,LS_CHG_USER_ID
 ,LS_CHG_DTM
)
SELECT  #PRCH_NO#
       ,#OP_CL_CD#
       ,#EQP_MDL_CD#
       ,#EQP_SER_NUM#
       ,#RENTAL_CNTRT_STA_DT#
       ,#OUT_PRC#
       ,#PRCH_ASSET_CL#
       ,'01'
       ,'501802'
       ,#AGN_CD#
       ,(SELECT DEAL_REP_USER_NM FROM TB_DEAL_CO WHERE DEAL_CO_CD = #AGN_CD#) 
       ,(SELECT DEAL_TEL_NO FROM TB_DEAL_CO WHERE DEAL_CO_CD = #AGN_CD#) 
       ,TO_NUMBER(#OUT_PRC#) - TO_NUMBER(#OUT_PRC#)*0.1
       ,TO_NUMBER(#OUT_PRC#)*0.1
       ,#SURTAX_CL#
       ,'SYSTEM'
       ,SYSDATE
       ,'SYSTEM'
       ,SYSDATE
  FROM  DUAL]]>
