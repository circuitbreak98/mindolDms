	<insert id="IEqpCntrtDtl" parameterClass="map" ><![CDATA[/* Biz: dms.nr.BNR001.IEqpCntrtDtl 단말계약상세등록 */
/* IO: 안진갑,2015-08-05T14:46:51(안진갑,2015-08-05T12:37:52) */
INSERT  INTO TB_EQP_CNTRT_DTL
(
  ASSET_NO
 ,CNTRT_NO
 ,RENT_MM_DT
 ,RENTAL_FEE
 ,DEL_YN
 ,FS_REG_USER_ID
 ,FS_REG_DTM
 ,LS_CHG_USER_ID
 ,LS_CHG_DTM
)
SELECT  #ASSET_NO#
       ,#CNTRT_NO#
       ,TO_CHAR(ADD_MONTHS(TO_DATE(#RENTAL_CNTRT_STA_DT#,'YYYYMM'), LEVEL - 1), 'YYYYMM')
	   ,'0'
       ,'N'
       ,#FS_REG_USER_ID#
       ,#FS_REG_DTM#
       ,#LS_CHG_USER_ID#
       ,#LS_CHG_DTM#       
  FROM  DUAL
CONNECT BY LEVEL <= MONTHS_BETWEEN(TO_DATE(#RENTAL_CNTRT_END_DT#,'YYYYMM'), TO_DATE(#RENTAL_CNTRT_STA_DT#,'YYYYMM'))+1]]>
	</insert>
