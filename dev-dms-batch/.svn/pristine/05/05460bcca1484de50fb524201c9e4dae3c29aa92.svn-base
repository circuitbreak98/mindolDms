    ,XCL_ITM_CD   
    ,XCL_DEALCO_CD
    ,XCL_GUBUN_CD 
    ,ACNT_CD
    ,XCL_ST_CD
    ,XCL_AMT
    ,ASSET_NO
    ,SPLY_PRC
    ,SURTAX_AMT
    ,FS_REG_USER_ID
  )
  VALUES
  (
     SQ01_TB_ETC_XCL.NEXTVAL         
    ,#CNTRT_NO#
    ,#SALE_DEPT_CD#   
    ,#SALE_DT#        
    ,#SALE_ITM_CD#     
    ,#SALE_DEALCO_CD# 
    ,#SALE_GUBUN_CD#  
    ,#ACNT_CD#      
    ,#SALE_ST_CD#   
    ,#SALE_AMT#     
    ,#ASSET_NO#   
    ,#SPLY_PRC#
    ,#SURTAX_AMT#
    ,#USER_ID#  
    )]]>
	</insert>
	<update id="UEtcXclDel" parameterClass="map" ><![CDATA[/* Biz: dms.pr.DBP017.UEtcXclDel 기타정산삭제플래그 */
/* IO: 김상오,2015-08-17T17:06:32(김상오,2015-08-13T16:21:27) */
UPDATE TB_ETC_XCL
SET DEL_YN = 'Y'
WHERE XCL_DT like #IN_DT#||'%'
and XCL_ITM_CD = #SALE_ITM_CD#
and DEL_YN = 'N']]>
	</update>
</sqlMap>