	,#EQP_SER_NO#			/* 단말기일련번호 */
	,#CMPT_INDF_BACK_AMT#	/* 구성품변상반환금액 */
	,#EQP_BKAG_INDF_AMT#		/* 단말기파손변상금액 */
	,#EQP_BKAG_RSN#			/* 단말기파손사유 */
	,#CMPT_CD#				/* 구성품코드 */
	,#CMPT_RTN_YN#			/* 구성품반납여부 */
	,#CMPT_INV_AMT#		/* 구성품청구금액 */
	,'09'		
	,'BAT'	
	,SYSDATE		
	,'BAT'	
	,SYSDATE  
  )]]>
	</insert>
</sqlMap>