	<insert id="ISlipData" parameterClass="map" ><![CDATA[/* Biz: dms.inf.EPR010.ISlipData 전표데이터등록 */
/* IO: 진병수,2015-10-29T16:49:18(진병수,2015-08-20T14:46:28) */
INSERT INTO TB_SLIP
            (
               SLIP_NO
             , PRPSL_DEPT_CD
             , SLIP_DT
             , PRPSR
             , PRPSL_DT
             , DEAL_TYP_CD
             , SLIP_APRV_DEPT_CD
             , SLIP_APRVR
             , SLIP_APRV_DT
             , DEBIT_TOT
             , CR_TOT
             , PRF_CD
             , SLIP_APLY
             , SLIP_TRMS_YN
             , ORGL_SLIP_NO
             , SLIP_NORM_CNCL_CL
             , SLIP_RTNR
             , SLIP_RETUR_DT
             , TAX_BILL_NO
             , SLIP_CL_CD
             , FISCL_SYS_SEQ     
             , SLIP_ST_CD 
             , FS_REG_USER_ID
             , FS_REG_DTM    
             , LS_CHG_USER_ID
             , LS_CHG_DTM    
            )   
     VALUES 
            (
               #SLIP_NO#
             , #PRPSL_DEPT_CD#
             , NVL(#SLIP_DT#, TO_CHAR(SYSDATE, 'YYYYMMDD'))
             , NVL(#PRPSR#,#USER_NO#)
             , NVL(#PRPSL_DT#, TO_CHAR(SYSDATE, 'YYYYMMDD'))
             , NVL(#DEAL_TYP_CD#, ' ')
             , #SLIP_APRV_DEPT_CD#
             , #SLIP_APRVR#
             , #SLIP_APRV_DT#
             , NVL(#DEBIT_TOT# , 0)
             , NVL(#CR_TOT#    , 0)
             , #PRF_CD#
             , #SLIP_APLY#
             , #SLIP_TRMS_YN#
             , #ORGL_SLIP_NO#
             , #SLIP_NORM_CNCL_CL#
             , #SLIP_RTNR#
             , #SLIP_RETUR_DT#
             , #TAX_BILL_NO#
             , #SLIP_CL_CD#
             , #FISCL_SYS_SEQ#
             , NVL(#SLIP_ST_CD#,'10')
             , NVL(#USER_NO#,'BAT')
             , SYSDATE
             , NVL(#USER_NO#,'BAT')
             , SYSDATE
            )]]>
	</insert>
