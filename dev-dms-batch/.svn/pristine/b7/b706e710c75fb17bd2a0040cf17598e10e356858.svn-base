<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="dms.nr.DBT001">
	<insert id="ISKNInfo" parameterClass="map" ><![CDATA[/* Biz: dms.nr.DBT001.ISKNInfo SKN여신등록 */
/* IO: 장미진,2015-09-24T15:10:58(박홍서,2015-08-05T09:44:24) */
MERGE INTO  TB_NR_SKN_CRD A
     USING  (SELECT  IF_SEQ
                    ,XCL_YM
                    ,TS
                    ,SKN_AGN_CD
                    ,USE_AMT
                    ,FS_REG_USER_ID
                    ,FS_REG_DTM
                    ,LS_CHG_USER_ID
                    ,LS_CHG_DTM
               FROM  TF_SKN_CRD_INFO
              WHERE  PROC_ST_CD = '09') B
        ON  (
            A.AGN_CD = B.SKN_AGN_CD
       AND  A.PAY_YM = B.XCL_YM
       AND  A.PAY_YM_TS = B.TS
            )
WHEN MATCHED THEN
    UPDATE
       SET  AGN_CRD_AMT = B.USE_AMT
           ,LS_CHG_USER_ID = #OPER_ID#
           ,LS_CHG_DTM = SYSDATE
WHEN NOT MATCHED THEN
    INSERT (
            AGN_CD
           ,PAY_YM
           ,PAY_YM_TS
           ,AGN_CRD_AMT
           ,AGN_SALE_AMT
           ,CRD_SALE_DAMT
           ,FS_REG_USER_ID
           ,FS_REG_DTM
           ,LS_CHG_USER_ID
           ,LS_CHG_DTM
            )
    VALUES
           (
            B.SKN_AGN_CD
           ,B.XCL_YM
           ,B.TS
           ,B.USE_AMT
           ,(SELECT NVL(SUM(PRCH_AMT),0) FROM TB_PRCH WHERE SUBSTRB(PRCH_DT, 0, 6) = B.XCL_YM AND SUBSTR(PRCHCO_CD,0,6)=B.SKN_AGN_CD)
           ,(SELECT NVL(SUM(PRCH_AMT),0) FROM TB_PRCH WHERE SUBSTRB(PRCH_DT, 0, 6) = B.XCL_YM AND SUBSTR(PRCHCO_CD,0,6)=B.SKN_AGN_CD)-B.USE_AMT
           ,#OPER_ID#
           ,SYSDATE
           ,#OPER_ID#
           ,SYSDATE
            )]]>
	</insert>
	<select id="SSKNInfoTotCnt" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.nr.BNR004.SSKNInfoTotCnt SKN여신 작업건수 조회 */
/* IO: 박홍서,2015-08-05T14:37:17(박홍서,2015-08-05T14:35:43) */
SELECT  COUNT(1) AS TOT_CNT
  FROM  TF_SKN_CRD_INFO
 WHERE  PROC_ST_CD = '09']]>
	</select>
	<update id="USKNInfo" parameterClass="map" ><![CDATA[/* Biz: dms.nr.BNR004.USKNInfo SKN여신처리상태업데이트 */
/* IO: 박홍서,2015-08-05T14:39:31(박홍서,2015-08-05T14:39:31) */
UPDATE  TF_SKN_CRD_INFO
   SET  PROC_ST_CD = '01'
 WHERE  PROC_ST_CD = '09']]>
	</update>
</sqlMap>