	<update id="UXclSale" parameterClass="map" ><![CDATA[/* Biz: dms.pr.DBP101.UXclSale 정산데이터삭제플래그 */
/* IO: 김상오,2015-10-15T16:16:07(김상오,2015-08-12T18:11:19) */
update TB_SALE
   SET DEL_YN = 'Y'
 WHERE SALE_DT LIKE #IN_DT#||'%'
   and SALE_ITM_CD = #SALE_ITM_CD#
   AND SALE_DEPT_CD = 'PR'  
   and DEL_YN = 'N']]>
	</update>
