       ,CUST_INV_DTL_ADDR      AS NEW_CNTRTR_INV_ADDR_DTL
       ,CUST_INV_DTL_ADDR_ENPT AS NEW_CNTRTR_INV_ADDR_DTL_ENPT
       ,AGENT_NM               AS LEGAL_AGENT_NM
