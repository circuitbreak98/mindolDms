	*/
	private  long fInqSlipLogHstSeq()
	{
		Long slipLogHstNo = Calendar.getInstance().getTimeInMillis();
		
		IRecord rec  = dbSelectSingle("SSlipLogHstSeq", new HashMap());
		
		slipLogHstNo = rec.getLong(0);
		
