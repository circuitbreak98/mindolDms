# Batch EJB Facade
nexcore.bat.agent.ejb.facade.jndi.RentDevBatchSvr1=ejb/nexcore/batch/BatchFacadeLocal
# (Websphere)
#nexcore.bat.agent.ejb.facade.jndi.RentDevBatchSvr1=ejblocal:ejb/nexcore/batch/BatchFacade

# Old Job Log Clean
nexcore.bat.agent.oldjoblogclean.enable.RentDevBatchSvr1=true
nexcore.bat.agent.oldjoblogclean.time.RentDevBatchSvr1=2100
nexcore.bat.agent.oldjoblogclean.keep.RentDevBatchSvr1=D7

