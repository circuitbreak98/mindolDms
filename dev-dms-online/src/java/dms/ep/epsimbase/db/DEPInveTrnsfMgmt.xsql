        ,IN_DTL_CD                      /* 입고상세코드 */
        ,IN_DEALCO_CD                   /* 입고거래처코드 */
        ,DEL_YN                         /* 삭제여부 */
        ,FS_REG_USER_ID                 /* 최초등록사용자ID */
