        ( 
         #EQP_IN_NO#                    /* 입고번호 */
        ,#ASSET_NO#                     /* 자산번호 */
        ,#EQP_MDL_CD#                   /* 모델코드 */ 
        ,#EQP_SER_NO#                   /* 일련번호 */
        ,TO_CHAR(SYSDATE,'YYYYMMDD')    /* 입고일자 */
        ,'50'                           /* 입고상세코드 */
        ,'11010'                        /* 입고거래처코드 */ 
        ,'N'                            /* 삭제여부 */
        ,#USERNO#                       /* 최초등록사용자ID */
        ,SYSDATE                        /* 최종등록일시 */
        ,#USERNO#                       /* 최종변경사용자ID */
        ,SYSDATE                        /* 최종변경일시 */ 
        )]]>
	</insert>
	<select id="SInveTrnsfEqpAssetSeq" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.ep.epsimbase.db.DEPInveTrnsfMgmt.SInveTrnsfEqpAssetSeq 단말기자산번호채번 */
