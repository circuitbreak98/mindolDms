<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="DNRInveInfoMgmt">
	<select id="SInveInfoLstPaging" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.nr.nrseabase.db.DNRInveInfoMgmt.SInveInfoLstPaging 단말자산현황조회 */
/* IO: 장미진,2015-07-28T10:12:05(이민재,2015-07-17T16:00:30) */
SELECT
        *
  FROM (
        SELECT 
                ROW_NUMBER() OVER (ORDER BY SVC_MGMT_NO ASC) AS ROWNO 
                ,B.SVC_MGMT_NO    				 	 /* 서비스관리번호 */
                ,J.EQP_MDL_NM   					 /* 모델명 */
                ,A.EQP_MDL_CD   					 /* 모델코드 */
                ,B.COLOR_CD							 /* 색상코드 */
                ,E.CM_CD_NM AS COLOR_NM		 		 /* 색상 */					 			
                ,B.PET_NM       					 /* 펫네임 */ 
                ,K.CNTRT_PRD     					 /* 계약기간코드 */
                ,H.CM_CD_NM AS CNTRT_PRD_NM			 /* 계약기간 */
                ,B.CNTRT_DT 						 /* 계약일 */
                ,A.CNTRT_STA_DT 					 /* 계약시작일 */ 
                ,A.CNTRT_END_DT 					 /* 계약종료일 */ 
                ,A.PRCH_PRC     					 /* 매입가 */ 
                ,A.REM_PRC      					 /* 잔존가 */  
                ,B.CNTRT_ST     					 /* 계약상태코드 */
                ,I.CM_CD_NM AS CNTRT_ST_NM			 /* 계약상태 */ 
                ,B.CTT_PRD							 /* 내용년수 */
                ,K.CAPA_CD                 			 /* 용량코드 */
                ,F.CM_CD_NM AS CAPA_NM				 /* 용량 */
         FROM  TB_RENTAL_CNTRT A
               JOIN TB_NR_CNTRT B ON (A.CNTRT_NO = B.CNTRT_NO)
               JOIN TB_EQP_CNTRT C ON (A.CNTRT_NO = C.CNTRT_NO)
               JOIN TB_EQP_ASSET D ON (C.ASSET_NO = D.ASSET_NO)
               JOIN  TB_RENTAL_SALE_POLY K ON (A.RENTAL_POLY_NO = K.RENTAL_POLY_NO)
               LEFT JOIN TB_CM_CD E ON (B.COLOR_CD = E.CM_CD_ID AND E.CM_GRP_CD_ID = 'DMS100') /* 색상 */
               LEFT JOIN TB_CM_CD F ON (K.CAPA_CD = F.CM_CD_ID AND F.CM_GRP_CD_ID = 'DMS110')  /* 용량 */
               LEFT JOIN TB_CM_CD H ON (K.CNTRT_PRD = H.CM_CD_ID AND H.CM_GRP_CD_ID = 'DMS112') /* 계약기간코드 */
               LEFT JOIN TB_CM_CD I ON (B.CNTRT_ST = I.CM_CD_ID AND I.CM_GRP_CD_ID = 'DMS115') /* 계약상태코드 */
               LEFT JOIN TB_EQP_MDL J ON (A.EQP_MDL_CD = J.EQP_MDL_CD)
        WHERE  A.OP_CL_CD = 'NR'
]]><isNotEmpty prepend="AND" property="SVC_MGMT_NO"><![CDATA[
       B.SVC_MGMT_NO = #SVC_MGMT_NO#
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="EQP_MDL_NM"><![CDATA[
       J.EQP_MDL_NM LIKE '%'||#EQP_MDL_NM#||'%'
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="EQP_MDL_CD"><![CDATA[
       A.EQP_MDL_CD = #EQP_MDL_CD#
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="CNTRT_STA_DT"><![CDATA[
       B.CNTRT_DT  BETWEEN #CNTRT_STA_DT# AND #CNTRT_END_DT#
]]></isNotEmpty><![CDATA[
]]><isNotEqual property="CNTRT_ST" prepend="AND" compareValue="%"><![CDATA[
        B.CNTRT_ST = #CNTRT_ST#
]]></isNotEqual><![CDATA[
        ) PA
 WHERE  ROWNO BETWEEN #nc_firstRowIndex# AND #nc_lastRowIndex#]]>
	</select>
	<select id="SInveInfoTotCnt" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.nr.nrseabase.db.DNRInveInfoMgmt.SInveInfoTotCnt 단말자산현황총건수 */
/* IO: 장미진,2015-07-28T10:13:13(이민재,2015-07-17T16:00:46) */
SELECT    
        COUNT(1) AS TOTAL_CNT
  FROM  TB_RENTAL_CNTRT A
        JOIN TB_NR_CNTRT B ON (A.CNTRT_NO = B.CNTRT_NO)
        JOIN TB_EQP_CNTRT C ON (A.CNTRT_NO = C.CNTRT_NO)
        JOIN TB_EQP_ASSET D ON (C.ASSET_NO = D.ASSET_NO)
        JOIN  TB_RENTAL_SALE_POLY K ON (A.RENTAL_POLY_NO = K.RENTAL_POLY_NO)
        LEFT JOIN TB_EQP_MDL J ON (A.EQP_MDL_CD = J.EQP_MDL_CD)
 WHERE  A.OP_CL_CD = 'NR'        
]]><isNotEmpty prepend="AND" property="SVC_MGMT_NO"><![CDATA[
       B.SVC_MGMT_NO = #SVC_MGMT_NO#
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="EQP_MDL_NM"><![CDATA[
       J.EQP_MDL_NM LIKE '%'||#EQP_MDL_NM#||'%'
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="EQP_MDL_CD"><![CDATA[
       A.EQP_MDL_CD = #EQP_MDL_CD#
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="CNTRT_STA_DT"><![CDATA[
       B.CNTRT_DT  BETWEEN #CNTRT_STA_DT# AND #CNTRT_END_DT#
]]></isNotEmpty><![CDATA[
]]><isNotEqual property="CNTRT_ST" prepend="AND" compareValue="%"><![CDATA[
        B.CNTRT_ST = #CNTRT_ST#
]]></isNotEqual><![CDATA[]]>
	</select>
	<select id="SInveInfoDtl" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.nr.nrseabase.db.DNRInveInfoMgmt.SInveInfoDtl 단말자산현황상세조회 */
/* IO: 장미진,2015-07-28T09:43:18(이민재,2015-07-17T16:01:03) */
SELECT 
        ROW_NUMBER() OVER (ORDER BY SVC_MGMT_NO ASC) AS ROWNO 
        ,B.SVC_MGMT_NO    					 /* 서비스관리번호 */
        ,J.EQP_MDL_NM   					 /* 모델명 */
        ,A.EQP_MDL_CD   					 /* 모델코드 */
        ,B.COLOR_CD							 /* 색상코드 */
        ,E.CM_CD_NM AS COLOR_NM		 		 /* 색상 */					 			
        ,B.PET_NM       					 /* 펫네임 */ 
        ,K.CNTRT_PRD     					 /* 계약기간코드 */
        ,H.CM_CD_NM AS CNTRT_PRD_NM			 /* 계약기간 */
        ,B.CNTRT_DT							 /* 계약일 */
        ,A.CNTRT_STA_DT 					 /* 계약시작일 */ 
        ,A.CNTRT_END_DT 					 /* 계약종료일 */ 
        ,A.PRCH_PRC     					 /* 매입가 */ 
        ,A.REM_PRC      					 /* 잔존가 */  
        ,B.CNTRT_ST     					 /* 계약상태코드 */
        ,I.CM_CD_NM AS CNTRT_ST_NM			 /* 계약상태 */ 
        ,B.CTT_PRD							 /* 내용년수 */
        ,K.CAPA_CD                 		 	 /* 용량코드 */
        ,F.CM_CD_NM AS CAPA_NM				 /* 용량 */ 
  FROM  TB_RENTAL_CNTRT A
        JOIN TB_NR_CNTRT B ON (A.CNTRT_NO = B.CNTRT_NO)
        JOIN TB_EQP_CNTRT C ON (A.CNTRT_NO = C.CNTRT_NO)
        JOIN TB_EQP_ASSET D ON (C.ASSET_NO = D.ASSET_NO)
        JOIN  TB_RENTAL_SALE_POLY K ON (A.RENTAL_POLY_NO = K.RENTAL_POLY_NO)
        LEFT JOIN TB_CM_CD E ON (B.COLOR_CD = E.CM_CD_ID AND E.CM_GRP_CD_ID = 'DMS100') /* 색상 */
        LEFT JOIN TB_CM_CD F ON (K.CAPA_CD = F.CM_CD_ID AND F.CM_GRP_CD_ID = 'DMS110')  /* 용량 */
        LEFT JOIN TB_CM_CD H ON (K.CNTRT_PRD = H.CM_CD_ID AND H.CM_GRP_CD_ID = 'DMS112') /* 계약기간코드 */
        LEFT JOIN TB_CM_CD I ON (B.CNTRT_ST = I.CM_CD_ID AND I.CM_GRP_CD_ID = 'DMS115') /* 계약상태코드 */
        LEFT JOIN TB_EQP_MDL J ON (A.EQP_MDL_CD = J.EQP_MDL_CD)
 WHERE  A.OP_CL_CD = 'NR'
   AND  B.SVC_MGMT_NO = #SVC_MGMT_NO#]]>
	</select>
</sqlMap>