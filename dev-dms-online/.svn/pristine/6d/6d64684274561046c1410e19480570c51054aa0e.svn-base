                       ,NVL(SUM(DECODE(SUBSTR(TMP_A.CNSL_YM,5,6) ,'11',TLY_CNT )),0) AS MTH_11
                       ,NVL(SUM(DECODE(SUBSTR(TMP_A.CNSL_YM,5,6) ,'12',TLY_CNT )),0) AS MTH_12
                 FROM  TMP_A  
                 GROUP BY #CNSL_YR#, ROLLUP(TMP_A.DATA_SECTION)
                 ORDER BY TMP_A.DATA_SECTION) D1
        UNION ALL
        SELECT  D2.*
          FROM (SELECT  #CNSL_YR#  AS  CNSL_YR
                       ,'08'  AS  DATA_GB /* 감정(건) */
                       , 0 AS G_ID
                       ,DATA_SECTION /*마케팅본부분류*/
                       ,NVL(SUM(TMP_C.TLY_CNT),0) AS RSLT_SUM 
                       ,NVL(ROUND(SUM(TMP_C.TLY_CNT)/12),0) AS MTH_AVG 
                       ,NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'01',TLY_CNT )),0) AS MTH_1 
                       ,NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'02',TLY_CNT )),0) AS MTH_2 
                       ,NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'03',TLY_CNT )),0) AS MTH_3 
                       ,NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'04',TLY_CNT )),0) AS MTH_4 
