                    ,ROUND(NVL(SUM(TMP_C.BAD_JDG_AMT_SUM)/1000,0)) AS RSLT_SUM 
                    ,NVL(ROUND((SUM(TMP_C.BAD_JDG_AMT_SUM)/12)/1000),0) AS MTH_AVG 
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'01',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_1 
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'02',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_2 
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'03',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_3 
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'04',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_4 
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'05',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_5 
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'06',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_6 
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'07',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_7 
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'08',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_8 
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'09',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_9 
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'10',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_10
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'11',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_11
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'12',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_12
              FROM  TMP_C
              GROUP BY #CNSL_YR#,ROLLUP(TMP_C.DATA_SECTION)
              ORDER BY TMP_C.DATA_SECTION ) K2            
)T
ORDER BY T.DATA_GB,T.G_ID DESC ,T.DATA_SECTION]]>
