                              ,NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'05',CLCT_RT )),0) AS MTH_5
                              ,NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'06',CLCT_RT )),0) AS MTH_6
                              ,NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'07',CLCT_RT )),0) AS MTH_7 
