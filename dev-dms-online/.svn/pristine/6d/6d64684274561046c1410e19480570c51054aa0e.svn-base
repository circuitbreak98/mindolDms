                    ,NVL(TRUNC(SUM(TMP_C.BAD_JDG_CNT)/12),0) AS MTH_AVG 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'01',BAD_JDG_CNT )),0) AS MTH_1 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'02',BAD_JDG_CNT )),0) AS MTH_2 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'03',BAD_JDG_CNT )),0) AS MTH_3 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'04',BAD_JDG_CNT )),0) AS MTH_4 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'05',BAD_JDG_CNT )),0) AS MTH_5 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'06',BAD_JDG_CNT )),0) AS MTH_6 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'07',BAD_JDG_CNT )),0) AS MTH_7 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'08',BAD_JDG_CNT )),0) AS MTH_8 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'09',BAD_JDG_CNT )),0) AS MTH_9 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'10',BAD_JDG_CNT )),0) AS MTH_10
                    ,NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'11',BAD_JDG_CNT )),0) AS MTH_11 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_C.CNSL_YM,5,6) ,'12',BAD_JDG_CNT )),0) AS MTH_12
              FROM  TMP_C
              GROUP BY #CNSL_YR#,ROLLUP(TMP_C.DATA_SECTION)
              ORDER BY TMP_C.DATA_SECTION) K1
      UNION ALL
      SELECT K2.*
        FROM (SELECT #CNSL_YR# AS CNSL_YR
                    ,'13' AS DATA_GB /* 오감정금액(천원) */
                    ,GROUPING(TMP_C.DATA_SECTION) AS G_ID
