	<insert id="IEqpInve" parameterClass="map" ><![CDATA[/* Biz: dms.pr.prspmbase.db.DPREqpReqpInMgmt.IEqpInve 단말기자산입력 */
/* IO: 양재석,2015-08-24T19:55:46(김상오,2015-07-13T11:13:32) */
INSERT
  INTO  TB_EQP_ASSET                           /* 단말기자산 */ 
       (
        ASSET_NO                              /* 자산번호 */
        ,EQP_MDL_CD                            /* 단말기모델코드 */
        ,EQP_SER_NO                           /* 일련번호 */
        ,EQP_IMEI_NO                          /* IMEI */
        ,EQP_COLOR_CD                          /* 색상코드 */
        ,EQP_PRCH_AMT                          /* 매입단가 */
        ,FST_IN_DT                             /* 입고일자 */
        ,DEL_YN                                /* 삭제여부 */
        ,FS_REG_USER_ID                        /* 최초등록사용자 */
        ,FS_REG_DTM                            /* 최종등록일시 */
        ,LS_CHG_USER_ID                        /* 최종변경사용자ID */
        ,LS_CHG_DTM                            /* 최종변경일시 */
        ,BOX_NO                                /* BOX NO */
        ,OP_CL_CD                              /* 업무구분코드 */
        ,INVE_ST_CD                            /* 재고상태코드 */
        ,LAST_IN_OUT_NO                        /* 최종입출고번호 */
        ,EQP_PRCH_DT                           /* 단말기매입일자 */
        ,EQP_LOSS_DT
        ,FISCL_ACNTB_AMT
        ,FISCL_DEPR_SUM_AMT
        ,FISCL_REMPRC
        ,FISCL_SVCL
        ,LAST_EXPART_DT
        ,PRCH_NO
       )
VALUES
       (
         #ASSET_NO#	                              /* 자산번호 */
        ,#EQP_MDL_CD#                             /* 단말기모델코드 */
        ,#EQP_SER_NO#	                          /* 일련번호 */
        ,#EQP_IMEI_NO#	                          /* IMEI */
        ,#EQP_COLOR_CD#	                          /* 색상코드 */
        ,#PRCH_AMT#	                              /* 매입단가 */
        ,#FST_IN_DT#	                          /* 입고일자 */
        ,#DEL_YN#	                              /* 삭제여부 */
        ,#FS_REG_USER_ID#	                      /* 최초등록사용자ID */
        ,SYSDATE   	                              /* 최초등록일시 */
        ,#LS_CHG_USER_ID#	                      /* 최종변경사용자ID */
        ,SYSDATE         	                      /* 최종변경일시 */
        ,#BOX_NO#                                 /* BOX NO */
        ,#OP_CL_CD#                               /* 업무구분코드 */
        ,#INVE_ST_CD#                             /* 재고상태코드 */
        ,#LAST_IN_OUT_NO#                         /* 최종입출고번호 */
        ,#FST_IN_DT#                              /* 단말기매입일자 */
        ,''
        ,0
        ,0
        ,0
        ,2
        ,''
        ,#PRCH_NO#
       )]]>
	</insert>
