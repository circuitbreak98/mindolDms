						   ,TB_DEALCO  IC
						   ,TB_EQP_MDL  ID
				     WHERE  IA.CNSL_MGMT_NO = IB.CNSL_MGMT_NO
					   AND  IA.CNSL_DEALCO_CD = IC.DEALCO_CD
					   AND  IA.EQP_MDL_CD = ID.EQP_MDL_CD
					   AND  IA.CNSL_DT >= IC.EFF_STA_DT
					   AND  IA.CNSL_DT <= IC.EFF_END_DT
					   AND  IA.DEL_YN = 'N'
					   AND  IB.DEL_YN = 'N'
					   AND  IC.DEL_YN = 'N'
					   AND  ID.DEL_YN = 'N'
					   AND  IA.SKN_CL = 'N' /* 비현장감정 */
