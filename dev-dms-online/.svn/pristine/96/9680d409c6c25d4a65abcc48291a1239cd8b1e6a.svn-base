      ,#SELL_AMT#             /* 판매가격 */ 
      ,#FIX_PRC_YN#           /* 고정가여부 */
      ,#SELL_CNCL_YN#         /* 판매취소여부 */
      ,#SELL_CNCL_DTM#        /* 판매취소일시 */
      ,#SELL_CNCL_CL#         /* 판매취소구분 */
      ,#USERNO#               /* 최초 등록 사용자 ID */
      ,SYSDATE                /* 최초 등록 일시 */
      ,#USERNO#               /* 최종 변경 사용자 ID */
