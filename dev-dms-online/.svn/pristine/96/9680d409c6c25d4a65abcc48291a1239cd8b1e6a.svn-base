      ,#PROD_SER_NO#          /* 상품일련번호 */
      ,#DEL_YN#               /* 삭제여부 */
      ,1                      /* UPDATE COUNT */
      ,#SELL_UPRC#            /* 판매단가 */ 
      ,#SELL_PCOST#           /* 판매원가 */
      ,#SELL_MRGN#            /* 판매마진 */
      ,#SELL_CMMS#            /* 판매수수료 */ 
