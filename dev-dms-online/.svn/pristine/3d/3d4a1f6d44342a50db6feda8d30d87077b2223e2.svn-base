]]><isNotEmpty prepend="AND" property="INV_PROC_STA_DT"><![CDATA[
                 A.INV_PROC_DT BETWEEN #INV_PROC_STA_DT# AND #INV_PROC_END_DT#
]]></isNotEmpty><![CDATA[
     GROUP BY  A.INSU_INV_NO, B.UKEY_SVC_MGMT_NO, A.INV_PROC_DT, C.INVE_ST_CD ,A.INSU_COLS_SLIP_NO
               ,NVL(E.SLIP_NO,'00'),D.CNTRT_NO
        )
 WHERE  ROWNO BETWEEN #nc_firstRowIndex# AND #nc_lastRowIndex#]]>
	</select>
</sqlMap>