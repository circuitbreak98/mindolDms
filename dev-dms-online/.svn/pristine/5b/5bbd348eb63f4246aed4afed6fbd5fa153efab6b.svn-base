	<insert id="IDealCoHist" parameterClass="map" ><![CDATA[/* Biz: dms.bi.bibbase.db.DBIDealCoMgmt.IDealCoHist 거래처이력생성 */
/* IO: 이영진,2015-08-07T09:23:11(이영진,2015-07-08T17:54:47) */
INSERT 
  INTO  TH_DEAL_CO_HST 
       (
        DEAL_CO_CD              /* 거래처코드       */
       ,SEQ                     /* 순번             */
       ,AGN_ORG_CD              /* 대리점조직코드   */
       ,ORG_NM                  /* 조직명           */
       ,DEAL_BIZ_NUM            /* 사업자등록번호   */
       ,DEAL_REP_USER_NM        /* 대표자명         */
       ,DEAL_POST_NO            /* 우편번호         */
       ,DEAL_ADDR               /* 주소             */
       ,DEAL_CO_NM              /* 거래처명         */
       ,DEAL_MBL_PHON_NUM       /* 이동전화번호     */
       ,DEAL_TEL_NO             /* 전화번호         */
       ,DEAL_EML_ADDR           /* 이메일주소       */
       ,DEAL_EML_ADDR_ENPT      /* 이메일주소암호화 */
       ,DEAL_FAX_NO             /* 팩스번호         */
       ,DEAL_CO_CL1             /* 거래처구분1      */
       ,DEAL_CO_CL2             /* 거래처구분2      */
       ,BIZ_CON                 /* 업태             */
       ,TYP_OF_BIZ              /* 종목             */
       ,DEAL_CO_ST_CD           /* 거래처상태코드   */
       ,DEAL_STA_DT             /* 거래개시일       */
       ,DEAL_END_DT             /* 거래종료일       */
       ,PRPB_NM                 /* 담당자명         */
       ,PRPB_MBL_PHON           /* 담당자이동전화   */
       ,AVL_STA_DT              /* 유효시작일자     */
       ,AVL_END_DT              /* 유효종료일자     */
       ,DEAL_RMK                /* 비고             */
       ,UKEY_SUB_CD             /* UKEY서브코드     */
       ,UKEY_CHANNEL_CD         /* UKEY채널코드     */
       ,POS_AGENCY              /* 소속대리점       */
       ,SUP_DEAL                /* 상위거래처       */
       ,DEL_YN                  /* 삭제여부         */
       ,FS_REG_USER_ID          /* 최초등록사용자ID */
       ,FS_REG_DTM              /* 최초등록일시     */
       ,LS_CHG_USER_ID          /* 최종변경사용자ID */
       ,LS_CHG_DTM              /* 최종변경일시     */
       )
SELECT
        DEAL_CO_CD		        			/* 거래처코드 */
       ,(SELECT NVL(MAX(SEQ)+1,1)
           FROM TH_DEAL_CO_HST 
          WHERE DEAL_CO_CD = #DEAL_CO_CD#) 	/* 순번 */
       ,AGN_ORG_CD                          /* 대리점조직코드   */
       ,ORG_NM                              /* 조직명           */
       ,DEAL_BIZ_NUM                        /* 사업자등록번호   */
       ,DEAL_REP_USER_NM                    /* 대표자명         */
       ,DEAL_POST_NO                        /* 우편번호         */
       ,DEAL_ADDR                           /* 주소             */
       ,DEAL_CO_NM                          /* 거래처명         */
       ,DEAL_MBL_PHON_NUM                   /* 이동전화번호     */
       ,DEAL_TEL_NO                         /* 전화번호         */
       ,DEAL_EML_ADDR                       /* 이메일주소       */
       ,DEAL_EML_ADDR_ENPT                  /* 이메일주소암호화 */
       ,DEAL_FAX_NO                         /* 팩스번호         */
       ,DEAL_CO_CL1                         /* 거래처구분1      */
       ,DEAL_CO_CL2                         /* 거래처구분2      */
       ,BIZ_CON                             /* 업태             */
       ,TYP_OF_BIZ                          /* 종목             */
       ,DEAL_CO_ST_CD                       /* 거래처상태코드   */
       ,DEAL_STA_DT                         /* 거래개시일       */
       ,DEAL_END_DT                         /* 거래종료일       */
       ,PRPB_NM                             /* 담당자명         */
       ,PRPB_MBL_PHON                       /* 담당자이동전화   */
       ,AVL_STA_DT                          /* 유효시작일자     */
       ,TO_CHAR(SYSDATE,'YYYYMMDD')         /* 유효종료일자     */
       ,DEAL_RMK                            /* 비고             */
       ,UKEY_SUB_CD                         /* UKEY서브코드     */
       ,UKEY_CHANNEL_CD                     /* UKEY채널코드     */
       ,POS_AGENCY                          /* 소속대리점       */
       ,SUP_DEAL                            /* 상위거래처       */
       ,DEL_YN                              /* 삭제여부         */
       ,FS_REG_USER_ID                      /* 최초등록사용자ID */
       ,FS_REG_DTM                          /* 최초등록일시     */
       ,LS_CHG_USER_ID                      /* 최종변경사용자ID */
       ,LS_CHG_DTM                          /* 최종변경일시     */
  FROM TB_DEAL_CO
 WHERE DEAL_CO_CD = #DEAL_CO_CD#]]>
	</insert>
	<select id="SDealCoCd" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.bi.bibbase.db.DBIDealCoMgmt.SDealCoCd 거래처코드채번 */
