           ,PRCHCO_ID      = #PRCHCO_ID#
           ,LAST_MOV_IN_DT = #LAST_MOV_IN_DT#
           ,INVE_AMT       = #INVE_AMT#
           ,USED_CL        = #USED_CL#  
           ,DEL_YN         = 'N'
           ,UPDATE_COUNT   = 0
           ,LS_CHG_DTM     = SYSDATE
           ,LS_CHG_USER_ID = #LS_CHG_USER_ID#
           ,RMK = RMK||'/'||TO_CHAR(SYSDATE, 'YYYYMMDDHH24MISS')||';'||#LS_CHG_USER_ID#||';'||'IRJdgQltyJdgDisRgst'||';'||#INVE_AMT#
      WHEN NOT MATCHED THEN
    INSERT (
           EQP_MDL_CD
           ,EQP_COLOR_CD
           ,EQP_SER_NO
           ,HLD_DEALCO_ID
           ,INVE_ST
           ,EQP_ST
           ,LAST_IN_OUT_DT
           ,IN_OUT_CL
           ,IN_OUT_DTL_CL
           ,FST_IN_CONF_DT
           ,FST_IN_DEALCO_ID
           ,PRCHCO_ID
           ,LAST_MOV_IN_DT
           ,INVE_AMT
           ,USED_CL
           ,HOLD_AMT
           ,RMK
           ,DEL_YN
           ,UPDATE_COUNT
           ,FS_REG_USER_ID
           ,FS_REG_DTM
           ,LS_CHG_USER_ID
           ,LS_CHG_DTM           
           )
    VALUES
           (
           #EQP_MDL_CD#
           ,#EQP_COLOR_CD#
           ,#EQP_SER_NO#
           ,#HLD_DEALCO_ID#
           ,#INVE_ST#
           ,#EQP_ST#
           ,#LAST_IN_OUT_DT#
           ,#IN_OUT_CL#
           ,#IN_OUT_DTL_CL#
           ,#FST_IN_CONF_DT#
           ,#FST_IN_DEALCO_ID#
           ,#PRCHCO_ID#
           ,#LAST_MOV_IN_DT#
           ,#INVE_AMT#
           ,#USED_CL#
           ,#HOLD_AMT#
           ,TO_CHAR(SYSDATE, 'YYYYMMDDHH24MISS')||';'||#LS_CHG_USER_ID#||';'||'IRJdgQltyJdgDisRgst'||';'||#INVE_AMT#
           ,'N'
           ,0
           ,#FS_REG_USER_ID#
           ,SYSDATE
           ,#LS_CHG_USER_ID#
           ,SYSDATE
           )]]>
	</insert>
