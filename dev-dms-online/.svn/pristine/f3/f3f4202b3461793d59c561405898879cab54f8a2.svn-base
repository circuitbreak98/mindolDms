            (
             INSU_INV_NO
            ,INV_HST_SEQ 
            ,UKEY_INSU_NO
            ,UKEY_INSURE_MGMT_NO
            ,INV_ST_CD
            ,UKEY_INV_DT
            ,UKEY_INV_AMT
            ,DMS_INV_DT
            ,DMS_INV_AMT
            ,UNPD_RENTAL_FEE
            ,DMG_CMP_UNRTN_AMT
            ,ARR_ADD_AMT
            ,MID_TERM_PEN_AMT
            ,RTN_DELAY_PEN_AMT
            ,DMG_CMP_BKAG_AMT
            ,RECV_ST_CD
            ,UKEY_RECV_DT
            ,UKEY_RECV_AMT
            ,RSN_CD
            ,INV_PROC_DT
            ,CONF_INV_CNCL_DT
            ,COLS_REQS_DT
            ,COLS_CO_CD
            ,INSU_COLS_SLIP_NO
            ,INSU_INV_MEMO
            ,INSU_SCUR_NO
            ,SVC_MGMT_NO
            ,CUST_TYP_CD
            ,CTZ_COPREG_NO
            ,CTZ_COPREG_NO_ENPT
            ,CUST_NM
            ,CUST_NM_ENPT
            ,INSU_BIZ_REG_NO
            ,INSU_RENTAL_SER_NO
            ,INSU_RENTAL_SCRB_DT
            ,INSU_RENTAL_TERM_DT
            ,ATHO_TERM_DT
            ,INSPAY_TERM_YN
            ,TTUL_CHG_DT
            ,SVC_SCRB_DT
            ,INSU_STA_DT
