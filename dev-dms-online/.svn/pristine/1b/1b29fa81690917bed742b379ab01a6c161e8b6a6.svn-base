<sqlMap namespace="DSCBatTaskOpHstMgmt">
	<insert id="IBatTaskOpHst" parameterClass="map" ><![CDATA[/* Biz: dsm.sc.scsbase.db.DSCBatTaskOpHstMgmt.IBatTaskOpHst 일괄작업처리이력등록 */
/* IO: 배지한,2014-10-03T18:30:12(임정택,2014-09-30T13:00:55) */
INSERT
  INTO  TH_BAT_TASK_OP_HST /*일괄작업처리이력*/
        (
          TASK_DT 					/* 작업일자 */ 	
        , TASK_NO 					/* 작업번호 */ 	
        , TASK_ID 					/* 작업ID */ 	
        , TASK_NM 					/* 작업명 */ 	
        , GRP_ID 						/* 그룹ID */ 	
        , INST_CD 					/* 기관코드 */ 	
        , BAT_TASK_OP_ST_CD /* 일괄작업처리상태코드 */ 	
        , OP_STA_DTM 				/* 처리시작일시 */ 	
        , OP_END_DTM 				/* 처리종료일시 */ 	
        , OP_CNT 						/* 처리건수 */ 	
        , FS_REG_USER_ID 		/* 최초등록사용자ID */ 	
        , FS_REG_DTM 				/* 최초등록일시 */ 	
        , LS_CHG_USER_ID 		/* 최종변경사용자ID */ 	
        , LS_CHG_DTM 				/* 최종변경일시 */
        , OP_FILE_NM				/* 처리파일명 */ 
          )	
VALUES  
        (
          #TASK_DT#							/* 작업일자 */ 	           
        , #TASK_NO# 	          /* 작업번호 */ 	           
        , #TASK_ID# 	          /* 작업ID */ 	             
        , #TASK_NM# 	          /* 작업명 */ 	             
        , #GRP_ID# 	            /* 그룹ID */ 	             
        , #INST_CD# 	          /* 기관코드 */ 	           
        , #BAT_TASK_OP_ST_CD# 	/* 일괄작업처리상태코드 */ 
        , SYSDATE               /* 처리시작일시 */ 	       
        , #OP_END_DTM# 	        /* 처리종료일시 */ 	       
        , 0                     /* 처리건수 */ 	           
        , #FS_REG_USER_ID# 	    /* 최초등록사용자ID */ 	   
        , SYSDATE 	            /* 최초등록일시 */ 	       
        , #LS_CHG_USER_ID# 	    /* 최종변경사용자ID */ 	   
        , SYSDATE               /* 최종변경일시 */
        , #OP_FILE_NM#			/* 처리파일명 */         
          )]]>
	</insert>
	<update id="UBatTaskOpHst" parameterClass="map" ><![CDATA[/* Biz: dms.sc.scsbase.db.DSCBatTaskOpHstMgmt.UBatTaskOpHst 일괄작업처리이력수정 */
/* IO: 이영진,2015-08-03T12:19:26(임정택,2014-09-30T12:25:02) */
UPDATE  
        TH_BAT_TASK_OP_HST  /*일괄작업처리이력*/
   SET  BAT_TASK_OP_ST_CD 	= #BAT_TASK_OP_ST_CD#/*일괄작업처리상태코드*/
   	   ,OP_FILE_NM			= #OP_FILE_NM# 
       ,OP_END_DTM          = SYSDATE
       ,OP_CNT              = #OP_CNT#
       ,OP_FILE_NM          = #OP_FILE_NM#
       ,LS_CHG_USER_ID 		= NVL(#LS_CHG_USER_ID#, LS_CHG_USER_ID)/* 최종변경사용자ID */ 	
       ,LS_CHG_DTM 			= SYSDATE /*최종변경일시*/
 WHERE  TASK_NO = #TASK_NO#]]>
	</update>
	<select id="STaskNoSeq" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dsm.sc.scsbase.db.DSCBatTaskOpHstMgmt.STaskNoSeq 작업번호시퀀스조회 */
/* IO: 배지한,2014-10-03T18:22:34(임정택,2014-09-30T12:27:37) */
SELECT 
       SQ01_TH_BAT_TASK_OP_HST.NEXTVAL AS TASK_NO 
FROM DUAL]]>
	</select>
</sqlMap>