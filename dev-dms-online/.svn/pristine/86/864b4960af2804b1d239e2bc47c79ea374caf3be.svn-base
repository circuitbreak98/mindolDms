<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="DNRCntrtPenMgmt">
	<select id="SCntrtPenLstPaging" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.nr.nrsscbase.db.DNRCntrtPenMgmt.SCntrtPenLstPaging 계약위약금조회 */
/* IO: 김혁범,2015-07-28T09:06:28(이민재,2015-07-17T11:19:21) */
SELECT * 
  FROM(
      SELECT ROW_NUMBER() OVER(ORDER BY A.PEN_POLY_NO ASC) AS ROWNO
            ,A.CNTRT_NO                      /*계약번호*/             
            ,B.SVC_MGMT_NO           /*서비스관리번호*/                      
            ,B.SCRBR_NM                /*가입자이름*/                       
            ,B.SVC_NO                     /*이동전화번호*/                       
            ,A.PEN_GUBUN               /*위약금유형*/             
            ,D.CM_CD_NM AS PEN_GUBUN_NM                /*위약금유형명*/                     
            ,A.PEN                       /*위약금*/                      
            ,F.EQP_MDL_NM            /*모델명*/
            ,B.PET_NM                  /*펫네임*/                     
            ,H.CNTRT_PRD             /*계약기간명*/
            ,C.CNTRT_STA_DT           /*계약시작일*/                      
            ,C.CNTRT_END_DT           /*계약종료일*/                      
            ,B.OP_DT                   /*해지일*/ /*업무처리일자*/
            ,E.DEAL_CO_NM AS AGN_NM           /* 대리점명 */
            ,B.SALE_AGN_CD             /*대리점코드*/                           
            ,B.CNTRT_ST              /*계약상태*/
            ,E.CM_CD_NM AS CNTRT_ST_NM                       
        FROM TB_RENTAL_PEN A
             JOIN TB_NR_CNTRT B ON (B.CNTRT_NO = A.CNTRT_NO)
             JOIN TB_RENTAL_CNTRT C ON (C.CNTRT_NO = B.CNTRT_NO)
             JOIN  TB_RENTAL_SALE_POLY H ON (H.RENTAL_POLY_NO = A.PEN_POLY_NO)
             LEFT  OUTER JOIN TB_CM_CD D ON (D.CM_CD_ID = PEN_GUBUN AND D.CM_GRP_CD_ID = 'DMS114') /*위약금유형*/
             LEFT  OUTER JOIN TB_CM_CD E ON (E.CM_CD_ID = CNTRT_ST AND E.CM_GRP_CD_ID = 'DMS115') /*계약상태*/
             LEFT  OUTER JOIN TB_CM_CD G ON (G.CM_CD_ID = H.CNTRT_PRD AND G.CM_GRP_CD_ID = 'DMS112') /*계약기간명*/
             LEFT  JOIN TB_EQP_MDL F ON (F.EQP_MDL_CD = C.EQP_MDL_CD)
             LEFT  JOIN TB_DEAL_CO E ON (E.DEAL_CO_CD = B.SALE_AGN_CD)/*대리점명*/             
]]><dynamic prepend="WHERE"><![CDATA[
]]><isNotEmpty prepend="AND" property="SVC_MGMT_NO"><![CDATA[
B.SVC_MGMT_NO = #SVC_MGMT_NO#
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="OP_FR_DT"><![CDATA[
B.OP_DT BETWEEN #OP_FR_DT# AND #OP_TO_DT#
]]></isNotEmpty><![CDATA[
]]><isNotEqual prepend="AND" property="PEN_GUBUN" compareValue="%"><![CDATA[
A.PEN_GUBUN = #PEN_GUBUN#
]]></isNotEqual><![CDATA[
]]><isNotEmpty prepend="AND" property="SCRBR_NM"><![CDATA[
B.SCRBR_NM LIKE '%' || #SCRBR_NM# || '%'
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="SVC_NO"><![CDATA[
B.SVC_NO LIKE  #SVC_NO# || '%' 
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="SALE_AGN_CD"><![CDATA[
B.SALE_AGN_CD = #SALE_AGN_CD#	
]]></isNotEmpty><![CDATA[
]]></dynamic><![CDATA[
 )
WHERE  ROWNO BETWEEN #nc_firstRowIndex# AND #nc_lastRowIndex#]]>
	</select>
	<select id="SCntrtPenTotCnt" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.nr.nrsscbase.db.DNRCntrtPenMgmt.SCntrtPenTotCnt 계약위약금총건수 */
/* IO: 김혁범,2015-07-28T09:09:15(이민재,2015-07-17T11:19:39) */
SELECT  COUNT(1) AS TOTAL_CNT
  FROM TB_RENTAL_PEN A
             JOIN TB_NR_CNTRT B ON (B.CNTRT_NO = A.CNTRT_NO)
             JOIN TB_RENTAL_CNTRT C ON (C.CNTRT_NO = B.CNTRT_NO)
             JOIN  TB_RENTAL_SALE_POLY H ON (H.RENTAL_POLY_NO = A.PEN_POLY_NO)
             LEFT  OUTER JOIN TB_CM_CD D ON (D.CM_CD_ID = PEN_GUBUN AND D.CM_GRP_CD_ID = 'DMS114') /*위약금유형*/
             LEFT  OUTER JOIN TB_CM_CD E ON (E.CM_CD_ID = CNTRT_ST AND E.CM_GRP_CD_ID = 'DMS115') /*계약상태*/
             LEFT  OUTER JOIN TB_CM_CD G ON (G.CM_CD_ID = H.CNTRT_PRD AND G.CM_GRP_CD_ID = 'DMS112') /*계약기간명*/
             LEFT  JOIN TB_EQP_MDL F ON (F.EQP_MDL_CD = C.EQP_MDL_CD)
             LEFT  JOIN TB_DEAL_CO E ON (E.DEAL_CO_CD = B.SALE_AGN_CD)/*대리점명*/ 
]]><dynamic prepend="WHERE"><![CDATA[
]]><isNotEmpty prepend="AND" property="SVC_MGMT_NO"><![CDATA[
B.SVC_MGMT_NO = #SVC_MGMT_NO#
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="OP_DT"><![CDATA[
B.OP_DT BETWEEN #OP_FR_DT# AND #OP_TO_DT#
]]></isNotEmpty><![CDATA[
]]><isNotEqual prepend="AND" property="PEN_GUBUN" compareValue="%"><![CDATA[
A.PEN_GUBUN = #PEN_GUBUN#
]]></isNotEqual><![CDATA[
]]><isNotEmpty prepend="AND" property="SCRBR_NM"><![CDATA[
B.SCRBR_NM LIKE '%' || #SCRBR_NM# || '%'
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="SVC_NO"><![CDATA[
B.SVC_NO LIKE  #SVC_NO# || '%' 
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="SALE_AGN_CD"><![CDATA[
B.SALE_AGN_CD = #SALE_AGN_CD#	
]]></isNotEmpty><![CDATA[
]]></dynamic><![CDATA[]]>
	</select>
	<select id="SCntrtPenDtl" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.nr.nrsscbase.db.DNRCntrtPenMgmt.SCntrtPenDtl 계약위약금상세조회 */
/* IO: 김혁범,2015-07-28T09:06:45(이민재,2015-07-17T11:20:08) */
SELECT  A.CNTRT_NO                      /*계약번호*/             
        ,B.SVC_MGMT_NO           /*서비스관리번호*/                      
        ,B.SCRBR_NM                /*가입자이름*/                       
        ,B.SVC_NO                     /*이동전화번호*/                       
        ,A.PEN_GUBUN               /*위약금유형*/             
        ,D.CM_CD_NM AS PEN_GUBUN_NM                 /*위약금유형명*/                     
        ,A.PEN                       /*위약금*/                      
        ,F.EQP_MDL_NM            /*모델명*/
        ,B.PET_NM                  /*펫네임*/                     
        ,H.CNTRT_PRD             /*계약기간명*/
        ,C.CNTRT_STA_DT           /*계약시작일*/                      
        ,C.CNTRT_END_DT           /*계약종료일*/                      
        ,B.OP_DT                   /*해지일*/ /*업무처리일자*/
        ,E.DEAL_CO_NM AS AGN_NM           /* 대리점명 */
        ,B.SALE_AGN_CD             /*대리점코드*/                           
        ,B.CNTRT_ST              /*계약상태*/
        ,E.CM_CD_NM AS CNTRT_ST_NM    
 FROM TB_RENTAL_PEN A
      JOIN TB_NR_CNTRT B ON (B.CNTRT_NO = A.CNTRT_NO)
      JOIN TB_RENTAL_CNTRT C ON (C.CNTRT_NO = B.CNTRT_NO)
      JOIN  TB_RENTAL_SALE_POLY H ON (H.RENTAL_POLY_NO = A.PEN_POLY_NO)
      LEFT  OUTER JOIN TB_CM_CD D ON (D.CM_CD_ID = PEN_GUBUN AND D.CM_GRP_CD_ID = 'DMS114') /*위약금유형*/
      LEFT  OUTER JOIN TB_CM_CD E ON (E.CM_CD_ID = CNTRT_ST AND E.CM_GRP_CD_ID = 'DMS115') /*계약상태*/
      LEFT  OUTER JOIN TB_CM_CD G ON (G.CM_CD_ID = H.CNTRT_PRD AND G.CM_GRP_CD_ID = 'DMS112') /*계약기간명*/
      LEFT  JOIN TB_EQP_MDL F ON (F.EQP_MDL_CD = C.EQP_MDL_CD)
      LEFT  JOIN TB_DEAL_CO E ON (E.DEAL_CO_CD = B.SALE_AGN_CD)/*대리점명*/ 
WHERE A.CNTRT_NO = #CNTRT_NO#]]>
	</select>
</sqlMap>