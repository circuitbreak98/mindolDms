	<select id="SInsuMxclTotCnt" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.nr.nrsisbase.db.DNRInsuMxclMgmt.SInsuMxclTotCnt 보증보험금(지급)총건수 */
/* IO: 김혁범,2015-10-21T17:12:28(김혁범,2015-10-14T10:28:20) */
SELECT   COUNT(1) AS TOTAL_CNT        
  FROM
      (
       SELECT  A.DEBT_XCL_YM /*정산년월*/
              ,COUNT(A.DEBT_ITM_AMT) AS DEBT_ITM_AMT_CNT
              ,SUM(A.DEBT_ITM_AMT) AS DEBT_ITM_SUM_AMT /*총금액*/
              ,B.XCL_AMT /*DMS금액*/
              ,B.XCL_UKEY_INV_AMT /*SGI금액*/
              ,A.DEBT_SLIP_NO /*전표번호*/
              ,NVL(C.SLIP_CL_CD,'00') AS SLIP_CL_CD/*전표상태*/
              ,A.DEBT_SLIP_DT /*처리일자*/
              ,B.INVE_MATCHED /*보험금일치여부*/
              ,D.SGI_INSURE_MGMT_NO /*보험관리번호*/
              ,'' AS M_CNT
              ,'' AS M_PRC
              ,'' AS YYYYMM
              ,'' AS YYYY
         FROM  TB_XCL_DEBT A
              ,(
                SELECT  XCL_AMT /*DMS*/
                       ,XCL_UKEY_INV_AMT /*SGI*/
                       ,CASE WHEN XCL_AMT-XCL_UKEY_INV_AMT =0 THEN 'Y' ELSE 'N' END AS INVE_MATCHED /*(DMS - UKEY) 금액이 0이면 Y ,N*/
                       ,XCL_DEALCO_CD
                       ,SGI_INSU_NO
                  FROM  TB_ETC_XCL  
                 WHERE  XCL_ITM_CD = 'HP'     
               )B
              ,TB_SLIP C
              ,TB_INSU_SGI D
        WHERE  A.XCL_CL = 'HP'
          AND  B.XCL_DEALCO_CD = A.DEBT_AGN_CD
          AND  A.DEBT_SLIP_NO = C.SLIP_NO(+)
          AND  B.SGI_INSU_NO = D.SGI_INSU_NO(+) 
          AND  A.OP_CL_CD = 'NR'
          AND  B.INVE_MATCHED = 'Y' /*보험금일치여부가 'Y'일때 전표를 보여준다*/
]]><isNotEmpty prepend="AND" property="DEBT_XCL_YM"><![CDATA[
               A.DEBT_XCL_YM = #DEBT_XCL_YM#
]]></isNotEmpty><![CDATA[ 
]]><isNotEmpty prepend="AND" property="SGI_INSURE_MGMT_NO"><![CDATA[
               D.SGI_INSURE_MGMT_NO  = #SGI_INSURE_MGMT_NO#
]]></isNotEmpty><![CDATA[ 
     GROUP BY A.DEBT_XCL_YM
             ,A.DEBT_ITM_AMT
             ,B.XCL_AMT
             ,B.XCL_UKEY_INV_AMT 
             ,A.DEBT_SLIP_NO
             ,NVL(C.SLIP_CL_CD,'00')
             ,A.DEBT_SLIP_DT
             ,B.INVE_MATCHED
             ,D.SGI_INSURE_MGMT_NO
     )PA]]>
	</select>
	<select id="SInsuMxclLstPaging" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.nr.nrsisbase.db.DNRInsuMxclMgmt.SInsuMxclLstPaging 보증보험금(지급)조회 */
/* IO: 김혁범,2015-10-21T17:12:50(김혁범,2015-10-14T10:28:54) */
SELECT  SUBSTR(A.XCL_DT,0,6) AS XCL_DT  /*정산년월*/
       ,A.SGI_INSU_NO /*보증보험관리번호*/
       /*보험금요청상태*/
       ,A.XCL_AMT     /*DMS금액*/ 
       ,A.XCL_UKEY_INV_AMT  /*SGI금액*/
       ,CASE
           WHEN A.XCL_AMT - A.XCL_UKEY_INV_AMT = 0
           THEN 'Y'
        ELSE 'N'
        END AS INVE_MATCHED /*보험금일치여부*/ 
       ,A.XCL_DEALCO_CD
 FROM  TB_ETC_XCL A
      ,TB_INSU_SGI B
WHERE  A.SGI_INSU_NO = B.SGI_INSU_NO(+)
  AND  A.XCL_ITM_CD = 'HP'
  AND  A.OP_CL_CD = 'NR'
]]><isNotEmpty prepend="AND" property="DEBT_XCL_YM"><![CDATA[
       SUBSTR(A.XCL_DT,0,6) = #DEBT_XCL_YM#
]]></isNotEmpty><![CDATA[ 
]]><isNotEmpty prepend="AND" property="SGI_INSURE_MGMT_NO"><![CDATA[
       A.SGI_INSU_NO  = #SGI_INSURE_MGMT_NO#
]]></isNotEmpty><![CDATA[]]>
	</select>
