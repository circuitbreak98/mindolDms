                       ,NVL(SUM(DECODE(SUBSTR(TMP_A.CNSL_YM,5,6) ,'11',CNSL_CNT )),0)  AS  MTH_11
                       ,NVL(SUM(DECODE(SUBSTR(TMP_A.CNSL_YM,5,6) ,'12',CNSL_CNT )),0)  AS  MTH_12
