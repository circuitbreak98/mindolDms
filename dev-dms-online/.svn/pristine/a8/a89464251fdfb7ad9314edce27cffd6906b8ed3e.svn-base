                        ,NVL(SUM(DECODE(SUBSTR(TMP_A.CNSL_YM,5,6) ,'09',CLCT_RT )),0)  AS  MTH_9
                        ,NVL(SUM(DECODE(SUBSTR(TMP_A.CNSL_YM,5,6) ,'10',CLCT_RT )),0)  AS  MTH_10
                        ,NVL(SUM(DECODE(SUBSTR(TMP_A.CNSL_YM,5,6) ,'11',CLCT_RT )),0)  AS  MTH_11
