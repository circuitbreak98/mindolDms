               ,NVL(SUM(DECODE(SUBSTR(TMP_A.CNSL_YM,5,6) ,'04',CNSL_CNT )),0)  AS  MTH_4
               ,NVL(SUM(DECODE(SUBSTR(TMP_A.CNSL_YM,5,6) ,'05',CNSL_CNT )),0)  AS  MTH_5
