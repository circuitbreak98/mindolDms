              ,ROUND(E.MTH_12,3) *100 AS MTH_12
        FROM 
            (SELECT  #CNSL_YR#  AS  CNSL_YR
                    ,'06'  AS  DATA_GB /*감정율 (전체감정(건)/전체회수(건))*/
