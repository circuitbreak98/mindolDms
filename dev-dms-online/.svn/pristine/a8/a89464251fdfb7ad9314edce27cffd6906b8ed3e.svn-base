                    ,GROUPING(TMP_F.DATA_SECTION) AS G_ID 
                    ,DATA_SECTION    /*마케팅본부분류*/
                    ,NVL(SUM(TMP_F.BAD_JDG_CNT),0) AS RSLT_SUM 
                    ,NVL(TRUNC(SUM(TMP_F.BAD_JDG_CNT)/12),0) AS MTH_AVG 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'01',BAD_JDG_CNT )),0) AS MTH_1 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'02',BAD_JDG_CNT )),0) AS MTH_2 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'03',BAD_JDG_CNT )),0) AS MTH_3 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'04',BAD_JDG_CNT )),0) AS MTH_4 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'05',BAD_JDG_CNT )),0) AS MTH_5 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'06',BAD_JDG_CNT )),0) AS MTH_6 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'07',BAD_JDG_CNT )),0) AS MTH_7 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'08',BAD_JDG_CNT )),0) AS MTH_8 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'09',BAD_JDG_CNT )),0) AS MTH_9 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'10',BAD_JDG_CNT )),0) AS MTH_10
                    ,NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'11',BAD_JDG_CNT )),0) AS MTH_11 
                    ,NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'12',BAD_JDG_CNT )),0) AS MTH_12
              FROM  TMP_F
              GROUP BY #CNSL_YR#,ROLLUP(TMP_F.DATA_SECTION)
              ORDER BY TMP_F.DATA_SECTION) K1
      UNION ALL
      SELECT K2.*
        FROM (SELECT #CNSL_YR# AS CNSL_YR
                    ,'13' AS DATA_GB /* 오감정금액(천원) */
                    ,GROUPING(TMP_F.DATA_SECTION) AS G_ID
                    ,DATA_SECTION /*마케팅본부분류*/
                    ,ROUND(NVL(SUM(TMP_F.BAD_JDG_AMT_SUM)/1000,0)) AS RSLT_SUM 
                    ,NVL(ROUND((SUM(TMP_F.BAD_JDG_AMT_SUM)/12)/1000),0) AS MTH_AVG 
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'01',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_1 
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'02',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_2 
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'03',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_3 
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'04',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_4 
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'05',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_5 
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'06',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_6 
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'07',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_7 
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'08',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_8 
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'09',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_9 
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'10',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_10
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'11',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_11
                    ,ROUND(NVL(SUM(DECODE(SUBSTR(TMP_F.CNSL_YM,5,6) ,'12',BAD_JDG_AMT_SUM )),0)/1000) AS MTH_12
              FROM  TMP_F
              GROUP BY #CNSL_YR#,ROLLUP(TMP_F.DATA_SECTION)
              ORDER BY TMP_F.DATA_SECTION ) K2            
)T
ORDER BY T.DATA_GB,T.G_ID DESC ,T.DATA_SECTION]]>
	</select>
	<select id="SSaleRsltListPaging" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.ep.epsrpbase.db.DEPReportMgmt.SSaleRsltListPaging 매출실적현황조회 */
/* IO: 정동현,2015-12-17T15:06:01(정동현,2015-11-30T17:01:25) */
SELECT  PA.*
  FROM  (
         SELECT  ROW_NUMBER() OVER(ORDER BY A.CNSL_MGMT_NO ASC)  AS  ROWNO 
                ,A.CNSL_MGMT_NO                                            /* 접수관리번호 */
                ,A.PROD_CHRTIC_1                                           /* 단말구분 DMS164 */
                ,A.REPV_PROD_CD                                            /* 대표모델코드 */
                ,A.REPV_PROD_NM                                            /* 대표모델 명*/
                ,A.EQP_MDL_CD                                              /* 모델코드 */
                ,A.EQP_MDL_NM                                              /* 모델명 */
                ,A.EQP_SER_NO                                              /* 일련번호 */
                ,A.EQP_ST                                                  /* 등급 */
                ,A.EQP_ST_DTAIL                                            /* 등급(Sub) */
                ,A.CNSL_DT                                                 /* 접수일자 */
                ,A.PRCH_AMT                                                /* 매입가 */ 
                ,A.XRATE_APPLY_AMT                                         /* 매출단가() */
                ,A.SELL_PRC                                                /* 판매금액 */
                ,A.SALEPLC                                                 /* 출고처 */
                ,A.SALEPLC_NM                                              /* 출고처명 */
                ,A.XCL_SLIP_NO                                             /* 전표번호 */
                ,A.INVE_PCOST_SLIP_NO                                      /* 재고원가전표번호 */
                ,A.INVE_APLY_DT                                            /* 재고반영일 */
                ,A.XRATE_APPLY_DT                                          /* invoice날짜(선적일) */
                ,A.SALE_CL                                                 /* 재고(01)/내수(02)/수출(03) */
                ,A.INVE_CL_CD                                              /* 재고 유형 코드 DMS249 10:에코폰, 20:자수폰 */
                ,A.DEDC_TYP_CD                                             /* 공제 유형 코드 DMS244*/
                ,A.SKN_CL                                                  /* SKN 구분 Y:현장,N:비현장,NULL:비현장 */
                ,A.CROVR_INVE_YN                                           /* 이월재고여부 */
                ,A.IMEI                                                    /* IMEI */ 
           FROM  (
                  SELECT  A.CNSL_MGMT_NO
                         ,CASE WHEN (A.PROD_CHRTIC_1 IN ('S','T') OR A.PROD_CHRTIC_1 IS NULL) AND A.MFACT_CD != 'CG' THEN 'S'
                               WHEN (A.PROD_CHRTIC_1 IN ('S','T') OR A.PROD_CHRTIC_1 IS NULL) AND A.MFACT_CD = 'CG' THEN 'I'
                               ELSE 'P'
                          END AS PROD_CHRTIC_1
                         ,A.REPV_PROD_CD
                         ,(SELECT  EQP_MDL_NM 
                             FROM  TB_EQP_MDL 
                            WHERE  EQP_MDL_CD = A.REPV_PROD_CD) AS REPV_PROD_NM
                         ,A.EQP_MDL_CD
                         ,A.EQP_MDL_NM
                         ,A.EQP_SER_NO
                         ,A.EQP_ST
                         ,A.EQP_ST_DTAIL
                         ,A.CNSL_DT
                         ,A.PRCH_AMT
                         ,0 AS XRATE_APPLY_AMT
                         ,0 AS SELL_PRC
                         ,'' AS SALEPLC
                         ,'' AS SALEPLC_NM
                         ,'' AS XCL_SLIP_NO
                         ,'' AS INVE_PCOST_SLIP_NO
                         ,A.INVE_SLIP_DT AS INVE_APLY_DT
                         ,'' AS XRATE_APPLY_DT
                         ,'01' AS SALE_CL
                         ,A.INVE_CL_CD
                         ,A.DEDC_TYP_CD
                         ,A.SKN_CL
                         ,A.CROVR_INVE_YN
                         ,A.IMEI
                    FROM  (
                           SELECT  C.CNSL_MGMT_NO 
                                  ,C.EQP_SER_NO AS PROD_SER_NO
                                  ,D.EQP_SER_NO
                                  ,D.EQP_MDL_CD
                                  ,A.INVE_CL_CD
                                  ,A.DEDC_TYP_CD
                                  ,A.SKN_CL
                                  ,A.INVE_SLIP_DT
                                  ,D.EQP_ST
                                  ,D.EQP_ST_DTAIL
                                  ,D.CNSL_DT
                                  ,D.PRCH_AMT
                                  ,D.IMEI
                                  ,E.EQP_MDL_NM
                                  ,E.PROD_CHRTIC_1
                                  ,E.REPV_PROD_CD
                                  ,E.MFACT_CD
                                  ,CASE WHEN A.INVE_SLIP_DT < #INVE_SLIP_DT#||'01' THEN 'Y'
                                        ELSE 'N'
                                   END AS CROVR_INVE_YN
                             FROM  TB_EP_INVE_XCL A
                                  ,TB_EP_INVE_XCL_DTL B
                                  ,TB_EP_DIS_PROD_DIS C
                                  ,TB_EP_CST_PRCHS_MGMT D
                                  ,TB_EQP_MDL E
                            WHERE  A.INVE_XCL_NO = B.INVE_XCL_NO
                              AND  B.CNSL_MGMT_NO = C.CNSL_MGMT_NO
                              AND  C.CNSL_MGMT_NO = D.CNSL_MGMT_NO
                              AND  D.EQP_MDL_CD = E.EQP_MDL_CD
                              AND  A.INVE_TYP_CD = '10'
                              AND  A.INVE_SLIP_ST = '20'
                              AND D.PROGR_ST > '13'
		                     AND  A.DEL_YN = 'N'
		                     AND  B.DEL_YN = 'N'
		                     AND  C.DEL_YN = 'N'
		                     AND  D.DEL_YN = 'N'
		                     AND  E.DEL_YN = 'N'
                              AND  (A.INVE_SLIP_DT LIKE #INVE_SLIP_DT#||'%' OR (A.INVE_SLIP_DT < #INVE_SLIP_DT#||'01' AND NVL(C.SELL_YN,'N')='N'))
                              AND  NOT EXISTS (
                                               SELECT  IC.CNSL_MGMT_NO
                                                 FROM  TB_EP_INVE_XCL IA
                                                      ,TB_EP_INVE_XCL_DTL IB
                                                      ,TB_EP_DIS_PROD_DIS IC
                                                      ,TB_EP_CST_PRCHS_MGMT ID
                                                      ,TB_EQP_MDL IE
                                                WHERE  IA.INVE_XCL_NO = IB.INVE_XCL_NO
                                                  AND  IB.CNSL_MGMT_NO = IC.CNSL_MGMT_NO
                                                  AND  IC.CNSL_MGMT_NO = ID.CNSL_MGMT_NO
                                                  AND  ID.EQP_MDL_CD = IE.EQP_MDL_CD
                                                  AND  IC.CNSL_MGMT_NO = C.CNSL_MGMT_NO
                                                  AND  IA.INVE_TYP_CD = '10'
                                                  AND  IA.INVE_SLIP_ST = '20'
                                                  AND  IA.INVE_SLIP_DT LIKE #INVE_SLIP_DT#||'%'
                                                  AND  NVL(IC.SELL_YN,'N') = 'Y'
                                                  AND  IA.DEL_YN = 'N'
                                               )
                           ) A
                   UNION  ALL    
                  SELECT  B.CNSL_MGMT_NO
                         ,CASE WHEN (B.PROD_CHRTIC_1 IN ('S','T') OR B.PROD_CHRTIC_1 IS NULL) AND B.MFACT_CD != 'CG' THEN 'S'
                               WHEN (B.PROD_CHRTIC_1 IN ('S','T') OR B.PROD_CHRTIC_1 IS NULL) AND B.MFACT_CD = 'CG' THEN 'I'
                               ELSE 'P'
                          END AS PROD_CHRTIC_1
                         ,B.REPV_PROD_CD
                         ,(SELECT  EQP_MDL_NM 
                             FROM  TB_EQP_MDL 
                            WHERE  EQP_MDL_CD = B.REPV_PROD_CD) AS REPV_PROD_NM
                         ,B.EQP_MDL_CD
                         ,B.EQP_MDL_NM
                         ,B.EQP_SER_NO
                         ,B.EQP_ST
                         ,B.EQP_ST_DTAIL
                         ,B.CNSL_DT
                         ,B.PRCH_AMT
                         ,A.XRATE_APPLY_AMT
                         ,A.SELL_PRC
                         ,A.SALEPLC
                         ,A.SALEPLC_NM
                         ,A.XCL_SLIP_NO
                         ,A.INVE_PCOST_SLIP_NO
                         ,B.INVE_SLIP_DT AS INVE_APLY_DT
                         ,A.XRATE_APPLY_DT
                         ,DECODE(A.SELL_CL_CD,'20','03','02') AS SALE_CL
                         ,B.INVE_CL_CD
                         ,B.DEDC_TYP_CD
                         ,B.SKN_CL
                         ,B.CROVR_INVE_YN
                         ,B.IMEI                         
                    FROM  (
                           SELECT  IA.GNRL_SELL_NO                          /* 일반 판매 번호 */
                                  ,IA.GNRL_SELL_CHG_SEQ                     /* 일반 판매 변경 순번 */
                                  ,IA.SALEPLC                               /* 매출처 */
                                  ,NVL(IA.SELL_CL_CD,'10') AS SELL_CL_CD    /* 판매구분 */
                                  ,ID.DEALCO_NM AS SALEPLC_NM               /* 매출처 명*/
                                  ,IB.PROD_SER_NO                           /* 상품 일련 번호 */
                                  ,IB.SELL_PRC AS SELL_PRC                  /* 판매금액 */
                                  ,IB.SELL_PCOST                            /* 판매 원가 */
                                  ,IB.SELL_MRGN                             /* 판매 마진 */
                                  ,IB.SELL_CMMS                             /* 판매 수수료 */
                                  ,IB.SURTAX                                /* 부가세 */
                                  ,IC.XCL_SLIP_NO                           /* 정산 전표 번호 */
                                  ,IC.XRATE_APPLY_DT                        /* 환율 적용 일자 */
                                  ,IC.XRATE_APPLY_AMT                       /* 환율 적용 금액 */
                                  ,IC.INVE_PCOST_SLIP_NO                    /* 재고원가전표번호 */
                             FROM  TB_EP_SAL_GNRL_SALE IA 
                                  ,TB_EP_SAL_EQUIPMENT_SALE IB
                                  ,TB_EP_EQP_SALE IC
                                  ,TB_DEALCO ID
                            WHERE  IA.GNRL_SELL_NO = IB.GNRL_SELL_NO
                              AND  IA.GNRL_SELL_CHG_SEQ = IB.GNRL_SELL_CHG_SEQ
                              AND  IA.GNRL_SELL_NO = IC.GNRL_SELL_NO(+)
                              AND  IA.GNRL_SELL_CHG_SEQ = IC.GNRL_SELL_CHG_SEQ(+)
                              AND  IA.SALEPLC = ID.DEALCO_CD(+)
                              AND  IA.GNRL_SELL_CHG_SEQ = (SELECT  MAX(GNRL_SELL_CHG_SEQ)
                                                             FROM  TB_EP_SAL_GNRL_SALE
                                                            WHERE  GNRL_SELL_NO = IA.GNRL_SELL_NO)
                              AND  IA.SELL_DTM LIKE #INVE_SLIP_DT#||'%'                              
                              AND  IA.DEL_YN = 'N'
                              AND  IB.DEL_YN = 'N'
                              AND  IC.DEL_YN(+) = 'N'
                          ) A
                         ,(
                           SELECT  C.CNSL_MGMT_NO 
                                  ,C.EQP_SER_NO AS PROD_SER_NO
                                  ,D.EQP_SER_NO
                                  ,D.EQP_MDL_CD
                                  ,A.INVE_CL_CD
                                  ,A.DEDC_TYP_CD
                                  ,A.SKN_CL
                                  ,A.INVE_SLIP_DT
                                  ,D.EQP_ST
                                  ,D.EQP_ST_DTAIL
                                  ,D.CNSL_DT
                                  ,D.PRCH_AMT
                                  ,D.IMEI
                                  ,E.EQP_MDL_NM
                                  ,E.PROD_CHRTIC_1
                                  ,E.REPV_PROD_CD
                                  ,E.MFACT_CD
                                  ,'N' AS CROVR_INVE_YN
                             FROM  TB_EP_INVE_XCL A
                                  ,TB_EP_INVE_XCL_DTL B
                                  ,TB_EP_DIS_PROD_DIS C
                                  ,TB_EP_CST_PRCHS_MGMT D
                                  ,TB_EQP_MDL E
                            WHERE  A.INVE_XCL_NO = B.INVE_XCL_NO
                              AND  B.CNSL_MGMT_NO = C.CNSL_MGMT_NO
                              AND  C.CNSL_MGMT_NO = D.CNSL_MGMT_NO
                              AND  D.EQP_MDL_CD = E.EQP_MDL_CD
                              AND  A.INVE_TYP_CD = '10'
                              AND  A.INVE_SLIP_ST = '20'
                              AND D.PROGR_ST > '13'
			                  AND  A.DEL_YN = 'N'
			                  AND  B.DEL_YN = 'N'
			                  AND  C.DEL_YN = 'N'
			                  AND  D.DEL_YN = 'N'
			                  AND  E.DEL_YN = 'N'
                          ) B
                   WHERE  A.PROD_SER_NO = B.PROD_SER_NO(+)
                 ) A 
]]><dynamic prepend="WHERE"><![CDATA[
]]><isNotEmpty property="CNSL_MGMT_NO" prepend="AND"><![CDATA[
               A.CNSL_MGMT_NO = #CNSL_MGMT_NO#                              /* 접수관리번호 */
]]></isNotEmpty><![CDATA[
]]><isNotEmpty property="EQP_MDL_CD" prepend="AND"><![CDATA[
               A.EQP_MDL_CD = #EQP_MDL_CD#                                  /* 단말기 모델코드 */
]]></isNotEmpty><![CDATA[	
]]><isNotEmpty property="EQP_SER_NO" prepend="AND"><![CDATA[
               A.EQP_SER_NO = #EQP_SER_NO#                                  /* 단말기 일련번호 */
]]></isNotEmpty><![CDATA[
]]><isNotEmpty property="SALEPLC" prepend="AND"><![CDATA[
               A.SALEPLC = #SALEPLC#                                        /* 출고처 코드 */
]]></isNotEmpty><![CDATA[             
]]><isNotEqual property="RSLT_GB" compareValue="%" prepend="AND"><![CDATA[                            
               A.SALE_CL = #RSLT_GB#                                        /* 실적구분 */ 
]]></isNotEqual><![CDATA[
]]><isNotEqual property="PHONE_GB" compareValue="%" prepend="AND"><![CDATA[                           
               PROD_CHRTIC_1 = #PHONE_GB#                                   /* 단말구분 */
]]></isNotEqual><![CDATA[
]]></dynamic><![CDATA[
        )  PA
 WHERE  ROWNO BETWEEN #nc_firstRowIndex# AND #nc_lastRowIndex#]]>
	</select>
	<select id="SSaleRsltListTotCnt" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.ep.epsrpbase.db.DEPReportMgmt.SSaleRsltListTotCnt 매출실적현황총건수 */
