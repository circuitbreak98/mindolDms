                     AND  A.INVE_TYP_CD = '10'
                     AND  A.INVE_SLIP_ST = '20'
                     AND D.PROGR_ST > '13'
                     AND  A.DEL_YN = 'N'
                     AND  B.DEL_YN = 'N'
