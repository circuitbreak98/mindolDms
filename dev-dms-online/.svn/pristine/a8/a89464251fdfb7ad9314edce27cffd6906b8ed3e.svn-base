                                ,NVL(F.JDG_AMT_SUM/DECODE(F.TLY_CNT,0,NULL,F.TLY_CNT),0) AS JDG_UPRC     /* 감정단가 (감정금액/감정건수)*/
                                ,F.BAD_JDG_AMT_SUM   /*오감정금액(천원)*/
                           FROM (SELECT  SUBSTR(IA.CNSL_DT,0,6)  AS  CNSL_YM
                                        ,DECODE(IG.HEADQ_NM,'수도권마케팅1본부','21',
                                                            '수도권마케팅2본부','22',
                                                            '부산마케팅본부','23',
                                                            '대구마케팅본부','24',
                                                            '서부마케팅본부','25',
