	<insert id="IEqpClubTXcl" parameterClass="map" ><![CDATA[/* Biz: dms.ep.epssxbase.db.DEPEqpPpayXclMgmt.IEqpClubTXcl 단말기대금클럽T재집계 */
/* IO: 이민재,2015-10-28T16:48:38(이민재,2015-10-28T16:04:28) */
INSERT
  INTO  TB_EP_FEE_PPAY_XCL
       (
        FEE_PPAY_XCL_NO
       ,FEE_PPAY_STRD_YM
       ,INVE_TYP_CD
       ,FEE_PPAY_AMT
       ,FEE_PPAY_XCL_CNT
       ,DEL_YN
       ,FS_REG_USER_ID
       ,FS_REG_DTM
       ,LS_CHG_USER_ID
       ,LS_CHG_DTM
       ,FEE_PPAY_TYP_CD
       )
SELECT  TO_CHAR(LPAD(SQ01_TB_EP_FEE_PPAY_XCL.NEXTVAL,15,0)) AS FEE_PPAY_XCL_NO
       ,FEE_PPAY_STRD_YM
       ,INVE_TYP_CD
       ,NVL(FEE_PPAY_AMT,0) AS FEE_PPAY_AMT
       ,FEE_PPAY_XCL_CNT
       ,'N'
       ,#USERNO#
       ,SYSDATE
       ,#USERNO#
       ,SYSDATE
       ,'20' AS FEE_PPAY_TYP_CD
  FROM  (
         SELECT  #STRD_YM# AS FEE_PPAY_STRD_YM
                ,'20' AS INVE_TYP_CD 
                ,COUNT(*) AS FEE_PPAY_XCL_CNT
                ,SUM(SKN_JDG_AMT) AS FEE_PPAY_AMT
           FROM  (  
                  SELECT  NVL(C.SKN_JDG_AMT,0) AS SKN_JDG_AMT  /* 감정금액 */
                    FROM  TB_EP_CST_CONSULT_MGMT A
                         ,(SELECT  CNSL_MGMT_NO
                                  ,SUM(DDCT_AMT) DDCT_AMT
                             FROM  TB_EP_CST_CONSULT_DTL
                            WHERE  DEL_YN = 'N'
                            GROUP  BY CNSL_MGMT_NO) B
                         ,TB_EP_CST_PRCHS_MGMT C
                         ,TB_DEALCO D
                         ,TB_EQP_MDL E
                   WHERE  A.CNSL_MGMT_NO = B.CNSL_MGMT_NO
                     AND  A.CNSL_MGMT_NO = C.CNSL_MGMT_NO
                     AND  A.CNSL_DEALCO_CD = D.DEALCO_CD
                     AND  A.EQP_MDL_CD = E.EQP_MDL_CD
                     AND  A.CNSL_DT >= D.DEAL_STA_DT
                     AND  A.CNSL_DT <= D.DEAL_END_DT
                     AND  A.DEL_YN = 'N'
                     AND  C.DEL_YN = 'N'
                     AND  D.DEL_YN = 'N'
                     AND  E.DEL_YN = 'N'
                     AND  C.SCRB_MTHD = 'T'
                     AND  C.PROGR_ST = '13'
                     AND  A.CLCT_YN = 'Y'
                     AND  SUBSTR(A.CLCT_DT,0,6) = #STRD_YM#
                  )
        )]]>
	</insert>
	<insert id="IEqpClubTXclDtl" parameterClass="map" ><![CDATA[/* Biz: dms.ep.epssxbase.db.DEPEqpPpayXclMgmt.IEqpClubTXclDtl 단말기대금클럽T재집계상세 */
/* IO: 이민재,2015-10-28T17:27:25(이민재,2015-10-28T16:04:47) */
INSERT
  INTO  TB_EP_FEE_PPAY_XCL_DTL
       (
        FEE_PPAY_XCL_NO
       ,FEE_PPAY_XCL_DTL_SEQ
       ,INVE_TYP_CD
       ,CNSL_MGMT_NO
       ,PRCH_MGMT_NO
       ,EQP_MDL_CD
       ,EQP_SER_NO
       ,EQP_ST
       ,REPV_AGN_ORG_CD
       ,CNSL_DT
       ,CLCT_DT
       ,FEE_DEDC_PROC_DT
       ,XCL_AMT
       ,SKN_JDG_AMT
       ,SKN_SMPL_JDG_DAMT
       ,AGN_DDCT_AMT
       ,AGN_EXPNS_CONF_AMT
       ,DEL_YN
       ,FS_REG_USER_ID
       ,FS_REG_DTM
       ,LS_CHG_USER_ID
       ,LS_CHG_DTM
       )
SELECT  FEE_PPAY_XCL_NO
       ,FEE_PPAY_XCL_DTL_SEQ
       ,INVE_TYP_CD
       ,CNSL_MGMT_NO
       ,PRCH_MGMT_NO
       ,EQP_MDL_CD
       ,EQP_SER_NO
       ,EQP_ST
       ,AGN_ORG_CD AS REPV_AGN_ORG_CD
       ,CNSL_DT
       ,CLCT_DT
       ,FEE_DEDC_PROC_DT
       ,NVL(XCL_AMT,0) AS XCL_AMT
       ,SKN_JDG_AMT
       ,SKN_SMPL_JDG_DAMT
       ,AGN_DDCT_AMT
       ,AGN_EXPNS_CONF_AMT
       ,'N'
       ,#USERNO#
       ,SYSDATE
       ,#USERNO#
       ,SYSDATE
  FROM  (
         SELECT  A.*, B.*
           FROM  (SELECT  MAX(FEE_PPAY_XCL_NO) AS FEE_PPAY_XCL_NO 
                    FROM  TB_EP_FEE_PPAY_XCL 
                   WHERE  FEE_PPAY_STRD_YM = #STRD_YM# 
                     AND  FEE_PPAY_TYP_CD = '20'
                     AND  INVE_TYP_CD = '20') A
                ,(SELECT  ROW_NUMBER() OVER(ORDER BY A.CNSL_MGMT_NO) AS FEE_PPAY_XCL_DTL_SEQ
                         ,'20' AS INVE_TYP_CD     
                         ,A.CNSL_MGMT_NO                                                                         /* 접수관리번호 */
                         ,C.PRCH_MGMT_NO                                                                         /* 매입관리번호 */
                         ,C.EQP_MDL_CD                                                                           /* 모델코드 */
                         ,C.EQP_SER_NO                                                                           /* 일련번호 */ 
                         ,C.SKN_EQP_ST AS EQP_ST                                                                 /* 단말기상태 */ 
                         ,D.AGN_ORG_CD                                                                           /* 대리점조직코드 */
                         ,A.CNSL_DT                                                                              /* 접수일자 */
                         ,A.CLCT_DT                                                                              /* 회수일자 */
                         ,C.FEE_DEDC_PROC_DT                                                                     /* 요금공제처리일자 */
                         ,DECODE(NVL(E.FIX_PRC_YN,'N'),'Y',NVL(C.PRCH_AMT,0) 
                               - NVL(C.ADJ_AMT,0),DECODE(NVL(C.PRCH_AMT,0),0,NVL(C.PRCH_AMT,0),NVL(C.PRCH_AMT,0) 
                               + (TRUNC(NVL(B.DDCT_AMT,0),-2)-NVL(C.ADJ_AMT,0)))) AS XCL_AMT                     /* 등급산정가 */
                         ,NVL(C.SKN_JDG_AMT,0) AS SKN_JDG_AMT                                                    /* SKN감정금액 */
                         ,NVL(C.SKN_SMPL_JDG_DAMT,0) AS SKN_SMPL_JDG_DAMT                                        /* SKN샘플감정차액 */
                         ,DECODE(NVL(E.FIX_PRC_YN,'N'),'Y',0
                                ,DECODE(C.PRCH_AMT,NULL,NULL,TRUNC(B.DDCT_AMT,-2))) AS AGN_DDCT_AMT              /* 차감금액 */
                         ,NVL(C.SKN_SMPL_JDG_DAMT,0) - DECODE(NVL(E.FIX_PRC_YN,'N'),'Y',0
                         ,DECODE(C.PRCH_AMT,NULL,NULL,TRUNC(B.DDCT_AMT,-2))) AS AGN_EXPNS_CONF_AMT               /* 대리점비용확정금액 */      
                    FROM  TB_EP_CST_CONSULT_MGMT A
                         ,(SELECT  CNSL_MGMT_NO
                                  ,SUM(DDCT_AMT) DDCT_AMT
                             FROM  TB_EP_CST_CONSULT_DTL
                            WHERE  DEL_YN = 'N'
                            GROUP  BY CNSL_MGMT_NO) B
                         ,TB_EP_CST_PRCHS_MGMT C
                         ,TB_DEALCO D
                         ,TB_EQP_MDL E
                   WHERE  A.CNSL_MGMT_NO = B.CNSL_MGMT_NO
                     AND  A.CNSL_MGMT_NO = C.CNSL_MGMT_NO
                     AND  A.CNSL_DEALCO_CD = D.DEALCO_CD
                     AND  A.EQP_MDL_CD = E.EQP_MDL_CD
                     AND  A.CNSL_DT >= D.DEAL_STA_DT
                     AND  A.CNSL_DT <= D.DEAL_END_DT
                     AND  A.DEL_YN = 'N'
                     AND  C.DEL_YN = 'N'
                     AND  D.DEL_YN = 'N'
                     AND  E.DEL_YN = 'N'
                     AND  C.SCRB_MTHD = 'T'
                     AND  C.PROGR_ST = '13'
                     AND  A.CLCT_YN = 'Y'
                     AND  SUBSTR(A.CLCT_DT,0,6) = #STRD_YM#
                  ) B
        )]]>
	</insert>
