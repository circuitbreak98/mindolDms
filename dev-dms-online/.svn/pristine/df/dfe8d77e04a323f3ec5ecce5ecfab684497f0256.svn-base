SELECT  A.DEBT_XCL_YM AS YYYYMM /*정산년월일*/
       ,SUBSTR(A.DEBT_XCL_YM,0,4) AS YYYY /*정산년월*/
       ,C.CNT AS M_CNT /*총건수*/
       ,C.DMS_AMT_SUM  /*DMS금액합계*/
