   AND  A.DEBT_PRCH_TS   = C.ETC_XCL_TS
   AND  A.OP_CL_CD = D.OP_CL_CD
   AND  SUBSTR(A.XCL_DT,0,6) = D.DEBT_XCL_YM
   AND  A.XCL_DEALCO_CD = D.DEBT_DEALCO_CD
