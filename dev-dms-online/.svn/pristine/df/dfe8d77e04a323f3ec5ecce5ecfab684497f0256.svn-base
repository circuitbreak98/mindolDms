  AND  A.XCL_DEALCO_CD = C.BOND_DEALCO_CD
  AND  A.XCL_CL_CD = C.XCL_ITM_CL
  AND  A.ETC_XCL_TS = C.BOND_SALE_TS 
  AND  A.XCL_ITM_CD = 'HP'
  AND  A.OP_CL_CD = 'NR'
