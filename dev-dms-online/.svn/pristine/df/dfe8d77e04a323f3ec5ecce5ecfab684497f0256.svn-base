  FROM  TB_XCL_DEBT A 
       ,TB_SLIP B
       ,(
         SELECT  SUBSTR(XCL_DT,0,6) AS XCL_YM
                ,XCL_ITM_CD
                ,ETC_XCL_TS
                ,COUNT(ETC_XCL_NO) AS CNT
                ,NVL(SUM(XCL_AMT),0) AS SGI_AMT_SUM	/*UKEY청구금액:실정산금액 */
