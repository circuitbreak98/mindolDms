SELECT   COUNT(1) AS TOTAL_CNT        
  FROM
      (
       SELECT  A.DEBT_XCL_YM /*정산년월*/
              ,COUNT(A.DEBT_ITM_AMT) AS DEBT_ITM_AMT_CNT
              ,SUM(A.DEBT_ITM_AMT) AS DEBT_ITM_SUM_AMT /*총금액*/
              ,B.XCL_AMT /*DMS금액*/
              ,B.XCL_UKEY_INV_AMT /*SGI금액*/
