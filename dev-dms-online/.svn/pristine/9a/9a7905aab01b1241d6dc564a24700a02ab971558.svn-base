	<insert id="IEpEqpArrivClubTInveRgst" parameterClass="map" ><![CDATA[/* Biz: dms.ep.epssxbase.db.DEPInveXclMgmt.IEpEqpArrivClubTInveRgst 중고단말기확정클럽T재고등록 */
/* IO: 양재석,2016-01-25T12:59:00(양재석,2015-11-19T10:46:26) */
INSERT
  INTO TB_EP_INVE_XCL_DTL
       (
       INVE_XCL_NO,
       INVE_XCL_DTL_SEQ,
       CNSL_MGMT_NO,
       PRCH_MGMT_NO,
       CNSL_DT,
       PRCH_DT,
       XCL_AMT,
       ASMT_DMG_AMT,
       DEL_YN,
       FS_REG_USER_ID,
       FS_REG_DTM,
       LS_CHG_USER_ID,
       LS_CHG_DTM
       )
SELECT #INVE_XCL_NO#
       ,ROW_NUMBER() OVER(ORDER BY X.CNSL_DT,X.CNSL_MGMT_NO) AS ROWNO
       ,X.CNSL_MGMT_NO
       ,X.PRCH_MGMT_NO
       ,X.CNSL_DT
       ,X.PRCH_DT
       ,X.PRCH_AMT
       ,X.ASMT_DMG_AMT
       ,'N'
       ,#FS_REG_USER_ID#
       ,SYSDATE
       ,#LS_CHG_USER_ID#
       ,SYSDATE
FROM        
(
SELECT ROW_NUMBER() OVER(ORDER BY A.CNSL_DT,A.CNSL_MGMT_NO DESC) AS ROWNO
       ,A.CNSL_MGMT_NO
       ,A.CNSL_DT
       ,A.CLCT_DT
       ,A.ONING_DT
       ,B.EQP_ST
       ,NVL(B.PRCH_AMT,0) AS PRCH_AMT
       ,B.PRCH_MGMT_NO
       ,B.PRCH_DT
       ,A.EQP_MDL_CD
       ,A.EQP_SER_NO
       ,0 AS ASMT_DMG_AMT
       ,DECODE(NVL(C.CNSL_MGMT_NO,'N'),'N','N','Y') XCL_YN
  FROM TB_EP_CST_CONSULT_MGMT A
       ,TB_EP_CST_PRCHS_MGMT B
       ,(SELECT IA.CNSL_MGMT_NO, MAX(IA.INVE_XCL_NO) INVE_XCL_NO
           FROM TB_EP_INVE_XCL IB,
                TB_EP_INVE_XCL_DTL IA
          WHERE IA.DEL_YN = 'N'
            AND IB.DEL_YN = 'N'
            AND IA.INVE_XCL_NO = IB.INVE_XCL_NO
            AND IB.INVE_TYP_CD = '10'
          GROUP BY IA.CNSL_MGMT_NO
          MINUS
         SELECT ID.CNSL_MGMT_NO, ID.INVE_XCL_NO INVE_XCL_NO
           FROM TB_EP_INVE_XCL IC,
                TB_EP_INVE_XCL_DTL ID
          WHERE IC.DEL_YN = 'N'
            AND ID.DEL_YN = 'N'
            AND ID.INVE_XCL_NO = IC.INVE_XCL_NO
            AND IC.INVE_TYP_CD = '10'
            AND IC.CNCL_SLIP_YN = 'Y'
        ) C
 WHERE A.CNSL_MGMT_NO = B.CNSL_MGMT_NO
   AND A.CNSL_MGMT_NO = C.CNSL_MGMT_NO(+)
   AND A.CLCT_DT BETWEEN #CLCT_FR_DT# AND #CLCT_TO_DT# /* 회수일자 기간 */
   AND A.DEL_YN = 'N'
   AND B.DEL_YN = 'N'
   AND NVL(A.SKN_CL,'N') = 'Y'
   AND A.SCRB_MTHD = 'T'  /* 클럽T */
   AND B.TLY_DT IS NOT NULL  /* 검수완료 */
   AND TO_CHAR(TO_DATE(A.ONING_DT)+15,'YYYYMMDD') <= TO_CHAR(SYSDATE,'YYYYMMDD') /*개통일자*/
   AND (NVL(B.AGN_DDCT_YN,'N') = 'N' OR (B.AGN_DDCT_YN = 'Y' AND TO_CHAR(TO_DATE(B.TLY_DT)+15,'YYYYMMDD') <= TO_CHAR(SYSDATE,'YYYYMMDD')))
) X
WHERE X.XCL_YN = 'N']]>
	</insert>
