	<select id="SInqEpEqpArrivInveCnclYn" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.ep.epssxbase.db.DEPInveXclMgmt.SInqEpEqpArrivInveCnclYn 중고단말기확정재고취소여부 */
/* IO: 양재석,2016-01-27T09:12:59(양재석,2016-01-27T09:12:59) */
SELECT A.INVE_XCL_NO, B.INVE_SLIP_DT, NVL(C.CNCL_SLIP_YN,'N') CNCL_SLIP_YN
  FROM TB_EP_INVE_XCL_DTL A
       , TB_EP_INVE_XCL B
       , (SELECT IA.INVE_SLIP_NO     /*반제의 원전표번호*/
                 , IB.CNCL_SLIP_YN
            FROM TB_EP_INVE_XCL_DTL IA
                 , TB_EP_INVE_XCL IB
           WHERE IA.DEL_YN = 'N' 
             AND IB.DEL_YN = 'N' 
             AND IA.INVE_XCL_NO = IB.INVE_XCL_NO
             AND IB.INVE_TYP_CD = '10' /*확정재고*/
             AND IB.CNCL_SLIP_YN = 'Y'
             AND IB.INVE_SLIP_ST = '20' /*전표상태 : 회계팀승인*/
             AND IA.CNSL_MGMT_NO = #CNSL_MGMT_NO#
        ) C /*반제전표정보*/
 WHERE A.INVE_XCL_NO = B.INVE_XCL_NO
   AND B.INVE_SLIP_NO = C.INVE_SLIP_NO(+)  /*승인전표번호 = 원전표번호*/
   AND A.DEL_YN = 'N'
