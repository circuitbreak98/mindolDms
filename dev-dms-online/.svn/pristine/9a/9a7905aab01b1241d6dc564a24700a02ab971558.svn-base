	<insert id="IEpEqpNotArrivSendAmtInveRgst" parameterClass="map" ><![CDATA[/* Biz: dms.ep.epssxbase.db.DEPInveXclMgmt.IEpEqpNotArrivSendAmtInveRgst 중고단말기미착송금재고등록 */
/* IO: 양재석,2016-01-21T16:02:39(양재석,2015-10-19T18:29:52) */
INSERT
  INTO TB_EP_INVE_XCL_DTL
       (
       INVE_XCL_NO,
       INVE_XCL_DTL_SEQ,
       CNSL_MGMT_NO,
       PRCH_MGMT_NO,
       CNSL_DT,
       PRCH_DT,
       XCL_AMT,
       DEL_YN,
       FS_REG_USER_ID,
       FS_REG_DTM,
       LS_CHG_USER_ID,
       LS_CHG_DTM
       )
SELECT #INVE_XCL_NO#
       ,ROW_NUMBER() OVER(ORDER BY X.CNSL_DT,X.CNSL_MGMT_NO) AS ROWNO
       ,X.CNSL_MGMT_NO
       ,X.PRCH_MGMT_NO
       ,X.CNSL_DT
       ,X.PRCH_DT
       ,X.PRCH_AMT
       ,'N'
       ,#FS_REG_USER_ID#
       ,SYSDATE
       ,#LS_CHG_USER_ID#
       ,SYSDATE
FROM        
(SELECT  ROW_NUMBER() OVER(ORDER BY A.CNSL_DT,A.CNSL_MGMT_NO DESC) AS ROWNO
       ,A.CNSL_MGMT_NO
       ,DECODE(NVL(B.CNSL_MGMT_NO,'N'),'N','N','Y') XCL_YN
       ,A.CNSL_DT
       ,A.CLCT_DT
       ,A.ONING_DT
       ,A.EQP_ST
       ,A.SKN_JDG_AMT PRCH_AMT
       ,C.PRCH_MGMT_NO
       ,C.PRCH_DT
       ,A.EQP_MDL_CD
       ,A.EQP_SER_NO
  FROM TB_EP_CST_CONSULT_MGMT A
       ,(SELECT IA.CNSL_MGMT_NO, MAX(IA.INVE_XCL_NO) INVE_XCL_NO
           FROM   TB_EP_INVE_XCL IB,
                  TB_EP_INVE_XCL_DTL IA
          WHERE  IA.DEL_YN = 'N'
            AND  IB.DEL_YN = 'N'
            AND  IA.INVE_XCL_NO = IB.INVE_XCL_NO
            AND  IB.INVE_TYP_CD = '20'
           GROUP BY IA.CNSL_MGMT_NO
          MINUS
         SELECT ID.CNSL_MGMT_NO, ID.INVE_XCL_NO INVE_XCL_NO
           FROM   TB_EP_INVE_XCL IC,
                  TB_EP_INVE_XCL_DTL ID
          WHERE  IC.DEL_YN = 'N'
            AND  ID.DEL_YN = 'N'
            AND  ID.INVE_XCL_NO = IC.INVE_XCL_NO
            AND  IC.INVE_TYP_CD = '20'
            AND  IC.CNCL_SLIP_YN = 'Y'
       ) B
       ,TB_EP_CST_PRCHS_MGMT C
       ,TB_EQP_MDL D
 WHERE A.CLCT_DT IS NOT NULL
   AND A.CLCT_DT BETWEEN #CLCT_FR_DT# AND #CLCT_TO_DT# /* 기간 */
   AND A.DEL_YN = 'N'
   AND A.SKN_CL = 'Y' /* SKN */
   AND NVL(A.PRE_DC_CL,'N') != 'Y' /* 선할인여부 */
   AND A.IN_CONF_YN != 'N'  /* 입고제외 */
   AND NVL(A.PAY_MTHD,'') = '01' /* 계좌송금 */
   AND A.PAY_DT IS NOT NULL /* 송금일자 */
   AND A.CNSL_MGMT_NO = B.CNSL_MGMT_NO(+)
   AND A.CNSL_MGMT_NO = C.CNSL_MGMT_NO(+)
   AND C.DEL_YN (+)= 'N'
   /*AND C.TLY_DT IS NULL*/ /* 검수미완 */
   AND A.EQP_MDL_CD = D.EQP_MDL_CD
   AND D.DEL_YN = 'N'
   AND NVL(A.SCRB_MTHD,' ') != 'T'  /* 클럽T 여부*/
) X
WHERE X.XCL_YN = 'N'
]]><isNotEmpty prepend="AND" property="INVE_CL_CD"><![CDATA[   
]]><isEqual property="INVE_CL_CD" compareValue="10"><![CDATA[ 
       SUBSTR(X.CNSL_MGMT_NO,1,1) = 'C'
]]></isEqual><![CDATA[ 
]]><isNotEqual property="INVE_CL_CD" compareValue="10"><![CDATA[ 
       SUBSTR(X.CNSL_MGMT_NO,1,1) != 'C'
]]></isNotEqual><![CDATA[ 
]]></isNotEmpty><![CDATA[]]>
	</insert>
