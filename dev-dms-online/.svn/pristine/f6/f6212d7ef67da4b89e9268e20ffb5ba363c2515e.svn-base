	<select id="SSKNLoanDtlPaging" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.nr.nrscmbase.db.DNRLoanMgmt.SSKNLoanDtlPaging SKN여신 상세 페이징 조회 */
/* IO: 박홍서,2015-07-27T14:35:31(박홍서,2015-07-23T15:08:56) */
SELECT  * 
  FROM (
        SELECT  ROW_NUMBER() OVER (ORDER BY A.AGN_CD ASC, A.CNTRT_DTM ASC) AS ROWNO
              , A.AGN_CD AS SALE_AGN_CD
              , B.DEAL_CO_NM AS SALE_AGN_NM
              , B.SEQ AS SALE_AGN_SEQ
              , SUBSTR(A.CNTRT_DTM,0,6) AS CNTRT_YM
              , A.SVC_MGMT_NO
              , CASE WHEN TO_NUMBER(SUBSTR(A.CNTRT_DTM,6)) > 23 THEN '02'
                ELSE '01'
                END AS CNTRT_YM_TS
              , A.SCRBR_NM
              , A.RENTAL_SNO
              , A.CNTRT_DTM
              , FLOOR(MONTHS_BETWEEN(SYSDATE,TO_DATE(A.CNTRT_DTM,'YYYYMMDD')))||' 개월' AS CNTRT_MTH
              , A.OUT_PRC
              , A.FS_REG_USER_ID
              , A.FS_REG_DTM
              , A.LS_CHG_USER_ID
              , A.LS_CHG_DTM
              , '' AS SUM_PRC
          FROM  TB_NR_CNTRT A
              , TB_DEAL_CO B
         WHERE  A.AGN_CD = B.DEAL_CO_CD
]]><isNotEmpty prepend="AND" property="SALE_AGN_CD"><![CDATA[
                A.AGN_CD = #SALE_AGN_CD#
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="SALE_AGN_NM"><![CDATA[
                B.DEAL_CO_NM LIKE '%'||#SALE_AGN_NM#||'%'
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="SVC_MGMT_NO"><![CDATA[
                A.SVC_MGMT_NO = #SVC_MGMT_NO#
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="SCRBR_NM"><![CDATA[
                A.SCRBR_NM LIKE '%'||#SCRBR_NM#||'%'
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="CNTRT_FR_DTM"><![CDATA[
                A.CNTRT_DTM >= #CNTRT_FR_DTM#
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="CNTRT_TO_DTM"><![CDATA[
                A.CNTRT_DTM <= #CNTRT_TO_DTM#
]]></isNotEmpty><![CDATA[
       ) PA
 WHERE  PA.ROWNO BETWEEN #nc_firstRowIndex# AND #nc_lastRowIndex#]]>
	</select>
	<select id="SSKNLoanDtlLst" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.nr.nrscmbase.db.DNRLoanMgmt.SSKNLoanDtlLst SKN여신 상세 조회 */
/* IO: 박홍서,2015-07-27T11:09:36(박홍서,2015-07-23T15:10:47) */
SELECT  A.AGN_CD AS SALE_AGN_CD
      , B.DEAL_CO_NM AS SALE_AGN_NM
      , B.SEQ AS SALE_AGN_SEQ
      , SUBSTR(A.CNTRT_DTM,0,6) AS CNTRT_YM
      , A.SVC_MGMT_NO
      , TO_NUMBER(SUBSTR(A.CNTRT_DTM,6))) > TO_NUMBER(#CONST_YMTS#) THEN #CONST_2ST#
        ELSE #CONST_1ST#
        END AS CNTRT_YM_TS
      , A.SCRBR_NM
      , A.RENTAL_SNO
      , A.CNTRT_DTM
      , FLOOR(MONTHS_BETWEEN(SYSDATE,TO_DATE(A.CNTRT_DTM,'YYYYMMDD')))||' 개월' AS CNTRT_MTH
      , A.OUT_PRC
      , A.FS_REG_USER_ID
      , A.FS_REG_DTM
      , A.LS_CHG_USER_ID
      , A.LS_CHG_DTM
  FROM  TB_NR_CNTRT A
      , TB_DEAL_CO B
 WHERE  A.AGN_CD = B.DEAL_CO_CD
]]><isNotEmpty prepend="AND" property="SALE_AGN_CD"><![CDATA[
        A.AGN_CD = #SALE_AGN_CD#
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="SALE_AGN_NM"><![CDATA[
        B.DEAL_CO_NM LIKE '%'||#SALE_AGN_NM#||'%'
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="SVC_MGMT_NO"><![CDATA[
        A.SVC_MGMT_NO = #SVC_MGMT_NO#
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="SCRBR_NM"><![CDATA[
        A.SCRBR_NM LIKE '%'||#SCRBR_NM#||'%'
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="CNTRT_FR_DTM"><![CDATA[
        A.CNTRT_DTM >= #CNTRT_FR_DTM#
]]></isNotEmpty><![CDATA[
]]><isNotEmpty prepend="AND" property="CNTRT_TO_DTM"><![CDATA[
        A.CNTRT_DTM <= #CNTRT_TO_DTM#
]]></isNotEmpty><![CDATA[
 ORDER BY A.AGN_CD ASC, A.CNTRT_DTM ASC]]>
	</select>
	<select id="SSKNLoanDtlTotCnt" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.nr.nrscmbase.db.DNRLoanMgmt.SSKNLoanDtlTotCnt SKN여신 상세 총건수 조회 */
