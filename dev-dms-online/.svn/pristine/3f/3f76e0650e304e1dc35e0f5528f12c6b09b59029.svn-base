                    AND  B.CNTRT_NO                 = C.CNTRT_NO
                    AND  A.ACQR_XCL_SLIP_NO         = D.SLIP_NO(+)
                    AND  A.OP_CL_CD                 = 'NR'
                    AND  C.OP_TYP_CD NOT           IN ('06')
                    AND  SUBSTR(C.NEW_CNTRT_DT,0,6) = SUBSTR(#RENTAL_CNTRT_STA_DT#,0,6) 
