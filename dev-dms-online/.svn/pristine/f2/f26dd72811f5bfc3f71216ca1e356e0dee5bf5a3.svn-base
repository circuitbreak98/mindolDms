	<select id="SFpaProxySaleEquipmentSellSeq" parameterClass="map" resultClass="hmap" fetchSize="50" ><![CDATA[/* Biz: dms.ep.epsfsbase.db.DEPFPAProxySaleMgmt.SFpaProxySaleEquipmentSellSeq FPA판매대행상품판매단말기판매순번채번 */
